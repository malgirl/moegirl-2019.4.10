f0c\u8fd8\u6709\u82cd\u5ca9\u7239\u60ca\u9e3f\u4e00\u77a5\u7684\u66f2\u7ed8 \u81f3\u4e8ePV\u2014\u2014\u6e32\u67d320\u5c0f\u65f6\u62d6\u4e86\u8fdb\u5ea6\u5f88\u62b1\u6b49\uff0c\u6709\u4e9b\u5730\u65b9\u66f2\u7ebf\u8c03\u8272\u6765\u4e0d\u53ca\u7ec6\u8c03\u81ea\u5df1\u770b\u7740\u90fd\u8fa3\u773c\u775b23333 \u4f1a\u5c3d\u5feb\u6362\u6e90\u7684\uff01\n\u8fde\u7eed\u7b2c\u4e8c\u5e74\u4e3a\u963f\u7eeb\u505a\u751f\u8d3a\u4e86\uff0c\u8fd8\u662f\u8981\u8bf4\u4e00\u53e5\uff1a\nyou will never fade in my heart.\nHappy Birthday\uff0cLing \u751f\u65e5\u5feb\u4e50\uff0c\u963f\u7eeb\u3002\n2L\u662f\u91cd\u8981\u7684\u9884\u544a\u548c\u7f51\u6613\u4e91\u8d44\u6e90\u5730\u5740\uff013L\u79d1\u666e\u548c\u6b4c\u8bcd\u89e3\u91ca\uff0c\u62b1\u6b49\u6bd4\u8f83\u957f\uff0c\u4f46\u770b\u5b8c\u4f60\u4f1a\u771f\u6b63\u7406\u89e3\u8fd9\u9996\u6b4c\uff08\u5e76\u589e\u957f\u5f88\u591a\u59ff\u52bf\u3010\u5212\u6389\u3011\uff09\u3002\n\n\u6052\u661f\u6d88\u4ea1\u67093\u79cd\u53ef\u80fd\u7684\u4ea7\u7269,\u8fd9\u4e0e\u6052\u661f\u8d28\u91cf\u76f8\u5173\uff1a\u5c0f\u8d28\u91cf\u7684\u6052\u661f\uff08\u5982\u592a\u9633\uff09\uff0c\u8d77\u5148\u4f1a\u81a8\u80c0\uff0c\u5728\u8fd9\u4e2a\u9636\u6bb5\u7684\u6052\u661f\u6211\u4eec\u79f0\u4e4b\uff08\u7ea2\u3001\u84dd\u3001\u767d\uff09\u5de8\u661f\uff0c\u7136\u540e\u4f1a\u584c\u7f29\uff0c\u53d8\u6210\u767d\u77ee\u661f\u6216\u84dd\u77ee\u661f\uff0c\u8f90\u5c04\u3001\u4e27\u5931\u80fd\u91cf\uff0c\u6210\u4e3a\u7ea2\u77ee\u661f\uff0c\u518d\u6210\u4e3a\u9ed1\u77ee\u661f\uff0c\u6700\u7ec8\u6d88\u5931\u3002\n\u5927\u8d28\u91cf\u7684\u6052\u661f\uff0c\u22657\u4e2a\u592a\u9633\u5bc6\u5ea6\uff088\u219210M\u2299<M\uff09\u7684\u6052\u661f\u5219\u4f1a\u53d8\u6210\uff08\u84dd\u3001\u767d\u3001\u7ea2\uff09\u8d85\u5de8\u661f\uff0c\u5b83\u4f1a\u9009\u62e9\u4ee5\u8d85\u65b0\u661f\u7206\u53d1\u7684\u5f62\u5f0f\u7ed3\u675f\u751f\u547d\uff0c\u6700\u7ec8\u4f1a\u6210\u4e3a\u4e2d\u5b50\u661f\u6216\u9ed1\u6d1e\uff08\u53e4\u4ee3\u6709\u8bb0\u8f7d\uff0c \u7531\u4e8e\u8d85\u65b0\u661f\u5149\u91cf\u5927\uff0c\u4e00\u9897\u8d85\u65b0\u661f\u7206\u53d1\uff0c\u8fde\u7eed\u51e0\u4e2a\u6708\u90fd\u53ef\u4ee5\u5728\u665a\u4e0a\u770b\u4e66\uff09\uff0c\u4e2d\u5b50\u661f\u6700\u7ec8\u4e27\u5931\u80fd\u91cf\uff0c\u5f62\u6210\u9ed1\u77ee\u661f\u3002\u800c\u9ed1\u6d1e\u4f1a\u5411\u5916\u5c04\u7c92\u5b50\uff0c\u6216\u8bb8\u4f1a\u53d8\u6210\u767d\u6d1e\uff0c\u6216\u8bb8\u4f1a\u5b8c\u5168\u84b8\u53d1\u3002\n\n\u201c\u4e0d\u653b\u81ea\u7834\uff0c \u5f15\u529b\u4e2d\u8c01\u80fd\u9003\u8131 \u201d \u6052\u661f\u6d88\u4ea1\u4e0d\u56e0\u5916\u529b\uff0c\u800c\u662f\u81ea\u5df1\u7684\u6f14\u53d8\uff0c\u547d\u8fd0\u8d85\u8131\u81ea\u6211\u638c\u63a7\uff0c\u53c8\u5728\u81ea\u5df1\u624b\u4e2d\u7ec8\u7ed3\u3002\n\u201c\u6495\u7834\u661f\u56fe \u662f\u6211\u62d2\u7edd\u6c89\u9ed8\u201d \u661f\u5716\u662f\u4e0d\u540c\u65bc\u50b3\u7d71\u5730\u7406\u5716\u96c6\u6216\u8005\u5929\u9ad4\u7167\u7247\uff0c\u4e5f\u5c31\u662f\u8aaa\uff0c\u73fe\u5728\u661f\u5716\u662f\u628a\u591c\u7a7a\u4e2d\u6301\u4e45\u7684\u7279\u5fb5\u7cbe\u78ba\u63cf\u8ff0\u6216\u7e6a\u88fd\uff0c\u4f8b\u5982\u6046\u661f\u3001\u6046\u661f\u7d44\u6210\u7684\u661f\u5ea7\u3001\u9280\u6cb3\u7cfb\u3001\u661f\u96f2\u3001\u661f\u5718\u548c\u5176\u5b83\u6cb3\u5916\u661f\u7cfb\u7684\u7e6a\u5716\u96c6\uff1b\u4ea6\u5373\u662f\u300c\u661f\u661f\u7684\u5730\u5716\u300d\u3002 \u201c\u6495\u7834\u661f\u5716 \u201d\u5c31\u662f\u60f3\u5e36\u51fa\u201c\u63a8\u7ffb\u56fa\u5b9a\u601d\u60f3\uff0c\u4e0d\u88ab\u6846\u9650 \u201d\u4e4b\u985e\u7684\u610f\u601d\u3002\n\u201c\u5025\u4f97 \u201d\u662f\u61f5\u61c2\u8499\u6627\u7684\u610f\u601d \u4e0e\u7a7a\u6d1e\u540c\u97f3\u4e0d\u540c\u610f \u6240\u4ee5\u4e0d\u8981\u8bf4\u6b4c\u8bcd\u91cd\u590d\u5566\uff01\n\u201c\u91cd\u5851\u6211\uff0c\u91cd\u62fe\u6211\uff0c\u91cd\u6765\u8fc7\u201d \u6052\u661f\u6d88\u4ea1\u4e5f\u8bb8\u53ea\u662f\u53d8\u6210\u4e86\u65b0\u7684\u7269\u8d28\uff0c\u7ecf\u8fc7\u4ebf\u4e07\u5e74\u91cd\u65b0\u805a\u5408\u65b0\u751f\u3002\n\u6b4c\u8bcd\u867d\u7136\u5c11\u4f46\u662f\u5185\u6db5\u8fd8\u662f\u975e\u5e38\u6df1\u5e7f\uff0c\u5e0c\u671b\u6bcf\u4e2a\u4eba\u90fd\u53ef\u4ee5\u6709\u81ea\u5df1\u7684\u7406\u89e3\u5427\u3002\n\nQ\uff1a\u963f\u7eeb\u7684\u8bbe\u5b9a\u662f\u592a\u9633\u5417\uff1f\nA\uff1a\u5176\u5b9e\uff0c\u53ef\u4ee5\u662f2333 \u4f46\u4e0d\u505a\u9650\u5236\uff0c\u662f\u4f60\u4eec\u770b\u5230\u7684\u8fd9\u6837\u4e00\u9897\u7eef\u7ea2\u6052\u661f\u3002\nQ\uff1a\u6240\u4ee5\u8bf4\u6052\u661f\u7206\u70b8\u963f\u7eeb\u53c8\u4fbf\u5f53\u4e86\u5417\uff1f\nA\uff1a \u6807\u51c6\u7ed3\u5c40\uff08\u4e0d\uff09\u66fe\u7ecf\u7acbflag\u963f\u7eeb\u5728\u6211\u7684\u66f2\u5b50\u91cc\u4e0d\u4f1a\u4fbf\u5f53\uff0c\u6b4c\u8bcd\u91cc\u4e0d\u662f\u8bf4\u91cd\u751f\u4e4823333 \u5f3a\u884c\u4e0d\u98df\u8a00\u3002\n\u5f53\u7136\u6709\u5176\u4ed6\u95ee\u9898\u4e5f\u53ef\u7559\u8a00\u95ee~ \u770b\u5230\u4e86\u5c31\u56de\u7b54x \u53d1\u73b0\u81ea\u5df1\u5360\u4e86\u8fd9\u4e48\u5927\u4e00\u7247\u9762\u79ef\u6709\u70b9\u5413\u4ebax \u4f30\u8ba1\u4e5f\u6ca1\u6709\u4eba\u6709\u8010\u5fc3\u770b\u5230\u8fd9\u91cc\u4e86x \u3010\u6709\u5417\u3011</poem>|\u58a8\u767d\u831c\u5154\u53d1\u8868\u4e8e\u8bc4\u8bba\u533a#132&\u56de\u590d\u697c}}\n\n\n{{OQQ|collapsed}}\n{{\u4e50\u6b63\u7eeb|collapsed}}\n[[\u5206\u7c7b:\u4e2d\u56fd\u97f3\u4e50\u4f5c\u54c1]]\n[[\u5206\u7c7b:\u4f7f\u7528VOCALOID\u7684\u6b4c\u66f2]]\n[[\u5206\u7c7b:\u4e50\u6b63\u7eeb\u6b4c\u66f2]]"}}}