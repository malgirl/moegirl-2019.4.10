u5e76\u672a\u6b7b\u7edd\u3002\n\n\u672c\u6765\u5979\u5df2\u7ecf\u53d7\u4e86\u4f7f\u5168\u8eab\u52a8\u5f39\u4e0d\u5f97\u7684\u91cd\u4f24\uff0c\u4f46\u56e0\u4e3a\u6700\u8fd1\u5996\u602a\u5c71\u4e0a\u589e\u52a0\u7684\u795e\uff08\u4f30\u8ba1\u662f\u795e\u5948\u5b50\u548c\u9752\u86d9\u5b50\uff09\u7684\u5f71\u54cd\u3002\n\n\u5929\u5e1d\u7edd\u201c'''\u6697'''\u2642'''\u9ed1'''\u2642'''\u5316'''\u201d\u4e86\u3002\n\n\uff08'''\u6697'''\u2642'''\u9ed1'''\u2642'''\u5316'''\uff1a\u8eab\u4f53\u6d3b\u6027\u5316\uff0c\u6062\u590d\u80fd\u529b\u5927\u5927\u52a0\u5f3a\u3002\u5916\u8868\u53d8\u6210\u9ed1\u8272\u3002\u540c\u65f6\u4f1a\u6563\u53d1\u51fa\u4e00\u79cd\u6709\u5bb3\u7684\u7634\u6c14\u3002\uff09\n\u56e0\u4e3a\u6697\u9ed1\u5316\u540e\u6563\u53d1\u7684\u7634\u6c14\u4f1a\u5bf9\u5996\u602a\u5c71\u9020\u6210\u4e0d\u826f\u5f71\u54cd\uff0c\u5929\u5e1d\u7edd\u4e0d\u5f97\u4e0d\u79bb\u5f00\u4e86\u90a3\u91cc\u3002\n\n\u5979\u5728\u8fd9\u4e4b\u540e\u7684\u53bb\u5411\u5e76\u6ca1\u6709\u4ea4\u4ee3\uff0c\u5e94\u8be5\u662f\u5230\u5f02\u4e16\u754c\u6295\u5954\u4e86CC\u86df\uff0c\u6210\u4e3a\u4e86\u5e9f\u4eba\u519b\u56e2\u7684\u4e00\u5458\u3002\n\n\n\u4fd7\u8bdd\u8bf4\u5f97\u597d\uff1a'''\u201c\u5e9f\u4eba\u51fa\u54c1\uff0c\u5fc5\u5c5e\u94a2\u677f\u3002\u201d'''\u8fd9\u8bdd\u7528\u5728\u5929\u5e1d\u7edd\u8eab\u4e0a\u771f\u662f\u4e00\u70b9\u4e0d\u9519\u3002\n\n\u5929\u5e1d\u7edd\u6bcf\u6b21\u53d7\u5230\u653b\u51fb\u540e\uff0c\u4f1a\u6709\u4e00\u6bb5\u65f6\u95f4\u5b8c\u5168\u65e0\u654c\uff0c\u79f0\u4e3a\u201c\u8010\u6027\u65f6\u95f4\u201d\u3002\n\n\u5982\u679c\u5728\u8010\u6027\u65f6\u95f4\u91cc\u518d\u53d7\u5230\u653b\u51fb\uff0c\u8010\u6027\u65f6\u95f4\u4f1a\u53e0\u52a0\uff0c\u5bfc\u81f4\u6570\u5b57\u8d8a\u6765\u8d8a\u5927\u3002\n\n\u5f53\u8010\u6027\u65f6\u95f4\u8d85\u8fc72000\u65f6\uff0c\u5929\u5e1d\u7edd\u5c31'''\u8bba\u5916\u5316'''\u4e86\u3002\n\n\n\u53e6\u5916\uff0c\u5929\u5e1d\u7edd\u4e00\u65e6\u53d7\u5230\u53d8\u6570\u5f04\uff0c\u4e5f\u4f1a\u8bba\u5916\u5316\u3002\n\n\u7531\u4e8e\u53d8\u6570\u5f04\u662f\u5373\u6b7b\u4e94\u67f1\u6700\u7b80\u5355\u7684\u65b9\u6cd5\uff0c\u5c5e\u4e8e\u6700\u57fa\u7840\u7684\u795e\u7ea7\u6280\u672f\u3002\u53ef\u4ee5\u8bf4100\u4e2a\u795e\u91cc99\u4e2a\u90fd\u6709\u642d\u8f7d\u3002\n\n\u56e0\u6b64\u5728\u795e\u6218\u4e2d\uff0c\u5982\u679c\u6ca1\u6709\u5bf9\u7b56\uff0c\u5929\u5730\u7edd\u7684\u8bba\u5916\u5316\u51e0\u4e4e\u65e0\u6cd5\u907f\u514d\u3002\n\n\u5982\u6b64\u82db\u523b\u7684\u8bba\u5916\u5316\u6761\u4ef6\uff0c\u5373\u4f7f\u5728\u5e9f\u4eba\u4eba\u7269\u4e2d\u4e5f\u8db3\u4ee5\u6392\u5230'''\u7b2c\u4e8c\u540d'''\u3002\n\n\n\u5929\u5e1d\u7edd\u8fd8\u6709'''\u201c\u4ea7'''\u2642'''\u5375\u201d'''\u7684\u8bbe\u5b9a\u3002\u4ea7\u4e0b\u7684\u5375\u4f1a\u5728\u4e00\u6bb5\u65f6\u95f4\u540e\u6210\u4e3a\u5979\u7684\u5206\u8eab\u3002\n\n\u8fd9\u4e9b\u5206\u8eab\u672c\u8d28\u4e0a\u662fhelper\uff0c\u56e0\u6b64\u4ea7\u5375\u7684\u6700\u5927\u6570\u4e3amugen\u7684helper\u6570\u4e0a\u9650\u2014\u201456\u4e2a\u3002\n\n\u4e0d\u8fc7\u771f\u5f00\u523056\u7684\u8bdd\uff0c\u8fd0\u884c\u4f1a\u53d8\u5f97\u5341\u5206\u5361\u3002\u9ed8\u8ba4\u76843\u4e2a\u5df2\u7ecf\u8db3\u591f\u4e86\u3002\n\n\n== \u5f92\u52b3\u7684\u8865\u5145\u8bf4\u660e ==\n\n\u9065\u8fdc\u7684\u4ee5\u524d\uff0c\u5728\u5f3a\u70c8\u7684\u7206\u70b8\u88ad\u51fb\u5996\u602a\u4e4b\u5c71\u7684\u65f6\u5019\uff0c\u727a\u7272\u4e86\u81ea\u5df1\u4fdd\u62a4\u4e86\u5996\u602a\u4e4b\u5c71\u7684'''\u82f1\u96c4'''\n\n\u8eab\u4f53\u71c3\u70e7\u6b86\u5c3d\uff0c\u58f0\u97f3\u4e5f\u5931\u53bb\u4e86\uff0c\u4f46\u662f\u7531\u4e8e\u81ea\u8eab\u5f3a\u5927\u7684\u5996\u529b\uff0c\u751f\u547d\u5e76\u6ca1\u6709\u88ab\u5b8c\u5168\u6bc1\u706d\u6b86\u5c3d(\u203b1\uff09\n\n\u6536\u5230\u4e86\u5b8c\u5168\u65e0\u6cd5\u884c\u52a8\uff08\u690d\u7269{{ruby|\u4eba|yao}}\uff1f\uff09\u7a0b\u5ea6\u7684\u91cd\u4f24(\u203b2\uff09\n\n\u7531\u4e8e\u5996\u602a\u5c71\u65b0\u6765\u7684\u795e\u5e26\u6765\u7684\u5f71\u54cd\uff0c\u8eab\u4f53\u9ed1\u6697\u5316\u4e86(\u203b3\uff09\u80fd\u591f\u6d3b\u52a8\n\n\u4f46\u662f\u9ed1\u6697\u5316\u5c71\u5e26\u6765\u7684\u8d1f\u9762\u5f71\u54cd\uff0c\u5996\u602a\u4e4b\u5c71\u5f00\u59cb\u7eb7\u7eb7\u53d1\u751f\u707e\u5bb3\n\n\u82f1\u96c4\u4e0d\u80fd\u884c\u52a8\uff0c\u957f\u65f6\u95f4\u4e4b\u540e\u5996\u602a\u4e4b\u5c71\u4e5f\u6709\u4e86\u610f\u89c1(\u203b4\uff09\u5979\u88ab\u5f53\u505a\u4e86\u6709\u5bb3\u7684\u4e1c\u897f\u4e86\n\n\u51fa\u4e8e\u5bf9\u4f4f\u6240\uff08\u6545\u4e61\uff1f\uff09\u7684\u7231\uff0c\u5979\u5fc5\u987b\u8981\u79bb\u5f00\u4e86(\u203b5\uff09\n\n\n'''\u5f92\u52b3\u7684\u8865\u5145\u8bf4\u660e'''\n\n\n'''\u203b1'''\n\n\u5728\u8fd9\u91cc\u8bf4\u7684\u9065\u8fdc\u7684\u8fc7\u53bb\u548c\u521d\u4ee3\u5929\u9b54\u7684\u65f6\u4ee3\u662f\u4e8b\u60c5\n\n\u5f3a\u5927\u7684\u5996\u602a\u867d\u8bf4\u4f1a\u4ee5\u5bff\u547d\u6765\u8003\u8651\u4e1c\u897f\u7684\n\n\u5929\u9b54\u4e5f\u4ee5\u6b64\u4e3a\u57fa\u7840\u627e\u5230\u4e86\u6765\u4ee3\u66ff\u4e1c\u897f\n\n\u603b\u4e4b\u73b0\u5728\u7684\u5996\u602a\u4e4b\u5c71\uff0c\u5979\u81ea\u53e4\u4ee5\u6765\u5c31\u5b58\u5728\u7684\n\n\u5df2\u7ecf\u6b7b\u7684\uff0c\u4f46\u7531\u4e8e\u5996\u529b\u592a\u4e5f\u592a\u5f3a\u4e86\u800c\u8eab\u4f53\u6697\u9ed1\u5316\uff1f\n\n\u67d0RPG\u7684\u6062\u590d\u9b54\u6cd5\u4f24\u5bb3\u662f\u4e0d\u4f1a\u6709\u7684\n\n\n'''\u203b2'''\n\n\u5982\u5b57\u9762\u90a3\u6837\u5b8c\u5168\u4e0d\u80fd\u52a8\u5e76\u4e0d\u662f\u771f\u7684\u4e0d\u80fd\u52a8..\n\n\u624b\u8155\u662f\u53ef\u4ee5\u666e\u901a\u7684\u52a8\u4f5c\u7684\u3002\u5403\u996d\u4e5f\u80fd\u597d\u597d\u7684\u5403\u996d\uff08\u751f\u6d3b\u4e2d\uff09\n\n\u5931\u53bb\u4e86\u58f0\u97f3\u6240\u4ee5\u4f1a\u8bdd\u662f\u7528\u5199\u6587\u5b57\u6216\u624b\u8bed\u6765\u4ea4\u6d41\u7684\n\n\u8eab\u8d1f\u91cd\u7684\u4e1c\u897f\uff08\u7ea64KG\u4ee5\u4e0a\uff09\u5c31\u4f1a\u6447\u6643\u7a0b\u5ea6\u7684\u91cd\u4f24\n\n\n'''\u203b3'''\n\n\u8eab\u4f53\u6d3b\u6027\u5316\u662f\u6307\u53d7\u5230\u4f24\u5bb3\u4e5f\u9a6c\u4e0a\u5c31\u4f1a\u597d\u7684\u7a0b\u5ea6\u6cbb\u6108\u80fd\u529b\uff0c\u6574\u4f53\u5b9e\u529b\uff08\u80fd\u529b\uff09\u53d8\u5f3a\n\n\u4f46\u662f\u6697\u9ed1\u5316\uff08\u7279\u5f81\uff09\u76ae\u80a4\u53d8\u9ed1\n\n\u5468\u56f4\u4f1a\u6709\u5f88\u5f3a\u7684\u7634\u6c14\u51fa\u6765\uff08\u4e2a\u4eba\u7684\u610f\u5fd7\u6765\u6291\u5236\u7684\u8bdd\u4e5f\u4f1a\u7a0d\u5fae\u51fa\u6765\u4e00\u70b9\uff09\n\n\u8fd9\u4e2a\u7634\u6c14\u7279\u6b8a\u7684\u4e1c\u897f\uff0c\u4f1a\u5bf9\u5176\u4ed6\u6709\u7634\u6c14\u8010\u6027\u7684\u4eba\u4ea7\u751f\u4e0d\u597d\u7684\u5f71\u54cd\u800c\u5f15\u6765\u707e\u7978\n\n\u5e76\u4e14\u65e0\u8bba\u505a\u4ec0\u4e48\u4e8b\u60c5\u90fd\u662f\u4e0d\u80fd\u51c0\u5316\u7684....\uff08\u67d0\u79cd\u624b\u6bb5\u201c\u6d88\u706d\u201d\u662f\u53ef\u80fd\u7684\uff0c\u4f46\u201c\u51c0\u5316\u201d\u662f\u4e0d\u53ef\u80fd\u7684\uff09\n\n\n'''\u203b4'''\n\n\u4e3b\u8981\u73b0\u5929\u9b54\uff0c\u662f\u5927\u5929\u72d7\u4eec\u7684\u54a8\u8be2\u89d2\u8272\u4e00\u6837\u7684\u4eba\uff0c\u4e5f\u662f\u4e0a\u53f8\uff0c\u662f\u5fc5\u8981\u7684\u5546\u91cf\u7684\u5bf9\u8c61\n\n\u4ece\u81ea\u8eab\u6709\u4ec0\u4e48\u610f\u89c1\u7684\u4e8b\u65e2\u4e0d\u62b1\u6028\u4e5f\u4ec0\u4e48\u90fd\u4e0d\u8bf4\n\n\u4f46\u662f\u5f53\u5996\u602a\u4e4b\u5c71\u53d8\u7684\u5371\u9669\u7684\u65f6\u5019..\u53ea\u80fd\u53d1\u51fa\u8b66\u544a..\n\n\n'''\u203b5'''\n\n\u203b3\u8bf4\u660e\u7684\u7634\u6c14\u5bf9\u5c71\u4ea7\u751f\u8d1f\u9762\u5f71\u54cd\u4e3a\n\n\u7531\u4e8e\u62c5\u4efb\u5c71\u7684\u8b66\u536b\u6240\u4ee5\u5f71\u54cd\u8303\u56f4\u53ea\u5728\u5c71\u7684\u8303\u56f4\u5185.....\n\n\n{{\u4e1c\u65b9Project\u4e8c\u6b21\u840c\u5a18|MUGEN}}\n[[\u5206\u7c7b:\u4e1c\u65b9\u4e8c\u6b21\u521b\u4f5c\u4eba\u7269]][[\u5206\u7c7b:MUGEN]]"}}}