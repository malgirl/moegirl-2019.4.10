u9080\u8bf7\u7684\u662f\u521a\u5165\u804c\u4e0d\u4e45\u7684\u65b0\u4eba\uff0c\u8bf4\u660e\u4f60\u7684\u4e0a\u53f8\u8fd8\u7b97\u662f\u4e0d\u9519\u7684\uff0c\u81f3\u5c11\u662f\u4e2a\u4f53\u6064\u5458\u5de5\uff0c\u800c\u4e14\u4f1a\u5e0c\u671b\u65b0\u5458\u5de5\u4e5f\u62e5\u6709\u5065\u5eb7\u5feb\u4e50\u7684\u4eba\u9645\u5173\u7cfb\u7684\u4e0a\u53f8\u54e6\u3002}}\n\n\u7535\u53f0\u683c\u7433\uff1a{{color|#00CCFF|\u8fd9\u4f4d\u542c\u4f17\uff0c\u683c\u7433\u771f\u7684\u5f88\u7fa1\u6155\u4f60\uff01\u683c\u7433\u4e5f\u60f3\u8981\u53bb\u6d77\u8fb9\u5ea6\u5047\uff01}}\n\nAA12\uff1a\u554a\uff0c\u7535\u53f0\u683c\u7433\u5c0f\u59d0\u7ed9\u4e86\u975e\u5e38\u80af\u5b9a\u7684\u56de\u7b54\u5462\u2026\u2026\n::\u90a3\u6211\u73b0\u5728\u53bb\u544a\u8bc9\u6307\u6325\u5b98\u5427\u2014\u2014\n\nAA12\u521a\u8d77\u8eab\u51c6\u5907\u8981\u51fa\u95e8\uff0c\u968f\u5373\u53c8\u505c\u4e0b\u4e86\u811a\u6b65\uff0c\u8868\u60c5\u6709\u4e9b\u7ea0\u7ed3\u3002\n\nAA12\uff1a\u8fd9\u4e48\u665a\u4e86\uff0c\u4ed6\u7edd\u5bf9\u5df2\u7ecf\u7761\u4e86\u5427\u3002\n::\u800c\u4e14\u6211\u5e72\u561b\u8868\u73b0\u5f97\u50cf\u662f\u8fd9\u4e48\u5728\u4e4e\u8fd9\u4ef6\u4e8b\u4e00\u6837\u554a\u2026\u2026\n\n\u7535\u53f0\u683c\u7433\uff1a{{color|#00CCFF|\u2026\u2026\u4e0b\u9762\u6211\u4eec\u6765\u770b\u770b\u8fd9\u4f4d\u542c\u4f17\u4eca\u665a\u6709\u4ec0\u4e48\u7591\u95ee\u5462\u3002}}\n::{{color|#00CCFF|\u3010\u65b0\u6765\u7684\u5458\u5de5\u770b\u8d77\u6765\u4e0d\u592a\u5f85\u89c1\u6211\uff0c\u6211\u8be5\u600e\u4e48\u529e\uff1f\u60f3\u8981\u7ea6\u5979\u548c\u5176\u4ed6\u5458\u5de5\u4e00\u8d77\u53bb\u6d77\u8fb9\u73a9\uff0c\u4f46\u5979\u770b\u8d77\u6765\u5174\u81f4\u7f3a\u7f3a\u7684\u6837\u5b50\u3011\u2026\u2026}}\n\n\u7535\u53f0\u683c\u7433\uff1a{{color|#00CCFF|\u884c\u5427\uff0c\u679c\u7136\u662f\u590f\u65e5\u5462\uff0c\u5927\u5bb6\u90fd\u5728\u804a\u53bb\u6d77\u8fb9\u7684\u8bdd\u9898\u5462\u3002}}\n\nAA12\u7a81\u7136\u8f6c\u5411\u6536\u97f3\u673a\u3002\n\nAA12\uff1a\u54a6\uff0c\u8fd9\u6765\u4fe1\u600e\u4e48\u542c\u8d77\u6765\u50cf\u662f\u2026\u2026\n\n\u7535\u53f0\u683c\u7433\uff1a{{color|#00CCFF|\u2026\u2026\u8fd9\u79cd\u4e8b\u60c5\u5176\u5b9e\u8fd8\u662f\u8981\u5148\u597d\u597d\u786e\u8ba4\u7684\u54e6\uff1f}}\n::{{color|#00CCFF|\u65e2\u7136\u662f\u65b0\u5458\u5de5\u7684\u8bdd\uff0c\u4e5f\u8bb8\u53ea\u662f\u56e0\u4e3a\u5bb3\u7f9e\u6216\u8005\u8ba4\u751f\u4e4b\u7c7b\u7684\u539f\u56e0\u5462\u3002}}\n\n\u2026\u2026\n\n\u6536\u97f3\u673a\u91cc\u7684\u7535\u53f0\u4e3b\u64ad\u53c8\u9646\u7eed\u8bfb\u4e86\u51e0\u5c01\u542c\u4f17\u6765\u4fe1\u3002AA12\u8d76\u7d27\u5750\u4e0b\u6765\u5f00\u59cb\u7ee7\u7eed\u7f16\u8f91\u7b80\u8baf\uff0c\u70b9\u51fb\u53d1\u9001\u952e\u4e4b\u540e\uff0c\u5c31\u53c8\u4e00\u8138\u7ea0\u7ed3\u4f46\u5145\u6ee1\u671f\u5f85\u5730\u76ef\u7740\u6536\u97f3\u673a\u3002\n\n\u7535\u53f0\u683c\u7433\uff1a{{color|#00CCFF|\u2026\u2026\u597d\uff0c\u6211\u4eec\u6765\u770b\u770b\u4e0b\u9762\u8fd9\u5c01\u6765\u4fe1\u4e2d\u4f1a\u6709\u600e\u6837\u7684\u95ee\u9898\u5462\u3002}}\n\n\u7535\u53f0\u683c\u7433\uff1a{{color|#00CCFF|\u54e6\u5475\u5475\u2026\u2026\u3010\u6211\u7684\u4e0a\u53f8\u4f3c\u4e4e\u8bef\u4f1a\u6211\u4e0d\u592a\u5f85\u89c1\u4ed6\u4e86\uff0c\u53ef\u6211\u53ea\u662f\u4e0d\u592a\u4e60\u60ef\u8ddf\u522b\u4eba\u4ea4\u6d41\u800c\u5df2\uff0c\u5e0c\u671b\u53ef\u4ee5\u89e3\u5f00\u8bef\u4f1a\uff0c\u800c\u4e14\u6211\u51b3\u5b9a\u548c\u4e0a\u53f8\u4ed6\u4eec\u4e00\u8d77\u53bb\u6d77\u8fb9\u3011\u2026\u2026}}\n::{{color|#00CCFF|\u8fd9\u4f4d\u542c\u4f17\u7684\u95ee\u9898\u4f3c\u4e4e\u548c\u4e4b\u524d\u7684\u6765\u4fe1\u662f\u8054\u52a8\u5173\u7cfb\u5462\uff0c\u6700\u8fd1\u7684\u804c\u573a\u5173\u7cfb\u539f\u6765\u6709\u8fd9\u4e48\u591a\u8bef\u4f1a\u7684\u5417\u2026\u2026}}\n\n\u7535\u53f0\u683c\u7433\uff1a{{color|#00CCFF|\u2026\u2026\u4e0b\u9762\u8fd9\u5c01\u6765\u4fe1\u7684\u5185\u5bb9\u5f88\u6709\u610f\u601d\u54e6\u3002}}\n::{{color|#00CCFF|\u3010\u65b0\u5458\u5de5\u7b54\u5e94\u4e86\u6211\u4e00\u8d77\u53bb\u6d77\u8fb9\u5ea6\u5047\u7684\u8bf7\u6c42\u8036\uff01\u5e0c\u671b\u8fd9\u6b21\u5047\u671f\u53ef\u4ee5\u8ba9\u5979\u4ea4\u5230\u4e00\u4e9b\u65b0\u670b\u53cb\uff0c\u800c\u4e14\u521a\u521a\u5f97\u77e5\u539f\u6765\u5979\u6ca1\u6709\u4e0d\u5f85\u89c1\u6211\uff0c\u771f\u662f\u677e\u4e86\u4e00\u53e3\u6c14\u5462\u3011\u2026\u2026}}\n::{{color|#00CCFF|\u54e6\u5475\u5475\u5475\uff0c\u770b\u5230\u8bef\u4f1a\u89e3\u5f00\u771f\u662f\u4e00\u4ef6\u4ee4\u4eba\u5f00\u5fc3\u7684\u4e8b\u60c5\u5462\uff0c\u683c\u7433\u6211\u554a\u4e5f\u5f88\u60f3\u8981\u53bb\u6d77\u8fb9\u5ea6\u5047\u5462\u2026\u2026}}\n\n\u2026\u2026\u4e09\u5341\u5206\u949f\u540e\u3002\n\n\u7535\u53f0\u683c\u7433\uff1a{{color|#00CCFF|\u603b\u800c\u8a00\u4e4b\uff0c\u4ee5\u4e0a\u5c31\u662f\u4eca\u5929\u5168\u90e8\u7684\u8bfb\u8005\u6765\u4fe1\u4e86\u3002}}\n::{{color|#00CCFF|\u867d\u7136\u5b8c\u00b7\u5168\u00b7\u4e0d\u00b7\u77e5\u00b7\u9053\u521a\u521a\u5728\u8bf4\u53bb\u6d77\u8fb9\u8fd9\u4ef6\u4e8b\u7684\u4e24\u4f4d\u542c\u4f17\u662f\u8c01\uff0c\u4f46\u6bd4\u8d77\u5728\u6df1\u591c\u7535\u53f0\u4e92\u76f8\u53d1\u6765\u3010\u8bf7\u591a\u6307\u6559\uff01\u3011\u3010\u4e0d\u6211\u624d\u662f\uff0c\u8bf7\u591a\u6307\u6559\uff01\u3011\u8fd9\u79cd\u7b80\u8baf\uff0c\u683c\u7433\u771f\u7684\u5e0c\u671b\u4f60\u4eec\u80fd\u591f\u597d\u597d\u9762\u5bf9\u9762\u5730\u6c9f\u901a\u4e00\u6b21\u5462\u266a\u2026\u2026}}\n\n\u2026\u2026\n\n\u6b21\u65e5\u4e0a\u5348\u3002\n\n\u7b49\u5230AA12\u56de\u8fc7\u795e\u6765\u7684\u65f6\u5019\uff0c\u81ea\u5df1\u5df2\u7ecf\u6362\u4e0a\u4e86\u6cf3\u88c5\uff0c\u548c\u4e00\u4f17\u5435\u5435\u95f9\u95f9\u7684\u4eba\u5f62\u51fa\u73b0\u5728\u4e86\u5145\u6ee1\u9633\u5149\u76f4\u5c04\u7684\u6c99\u6ee9\u4e0a\u3002\n\nAA12\uff1a\u4e3a\u4ec0\u4e48\u2026\u2026\u7a81\u7136\u53d8\u6210\u8fd9\u6837\u4e86\u2026\u2026\n::\u7cdf\u7cd5\uff0c\u597d\u6652\uff0c\u800c\u4e14\u88c5\u6b63\u7ecf\u592a\u4e45\u4e86\u6709\u70b9\u6076\u5fc3\u2026\u2026\n::\u5455\uff0c\u4e3a\u4ec0\u4e48\u6211\u8fd8\u7a7f\u7740\u8fd9\u79cd\u6cf3\u88c5\u2026\u2026\n\nAA12\u770b\u7740\u4e09\u4e09\u4e24\u4e24\u5927\u547c\u5c0f\u53eb\u5206\u6563\u5f00\u6765\u7684\u540c\u4f34\uff0c\u53f9\u4e86\u53e3\u6c14\u3002\n\n\u968f\u5373\u8d70\u5411\u6c99\u6ee9\u7684\u76f8\u53cd\u65b9\u5411\uff0c\u51c6\u5907\u6491\u51e0\u628a\u906e\u9633\u4f1e\u6765\u9003\u907f\u4f17\u4eba\u548c\u592a\u9633\u7684\u89c6\u7ebf\u3002\n\nAA12\uff1a\u8fd8\u597d\u4e8b\u5148\u51c6\u5907\u4e86\u906e\u9633\u4f1e\u2026\u2026\u518d\u52a0\u4e0a\u8fd9\u4e9b\u5c06\u5c31\u4e00\u4e0b\u597d\u4e86\u3002\n::\u2026\u2026\u55ef\uff1f\n\nAA12\u7a81\u7136\u5bdf\u89c9\u5230\u81ea\u5df1\u4f3c\u4e4e\u88ab\u4e00\u675f\u89c6\u7ebf\u9501\u5b9a\u4e86\u3002\n::\u4e00\u624b\u7d27\u63e1\u7740\u906e\u9633\u4f1e\u4f1e\u67c4\uff0c\u53e6\u4e00\u53ea\u624b\u6697\u6697\u538b\u5728\u67aa\u6746\u4e0a\uff0c\u7136\u540e\u731b\u5730\u56de\u5934\u2014\u2014\n::\u53d1\u73b0\u5728\u4e0d\u8fdc\u5904\u7684\u4e00\u68f5\u6930\u5b50\u6811\u4e0b\uff0c\u4e00\u4e2a\u77ee\u5c0f\u7684\u4eba\u5f62\u9732\u51fa\u534a\u4e2a\u5934\u5728\u76ef\u7740\u81ea\u5df1\u3002\n\nAA12\uff1a\u2026\u2026\n\n\u963f\u6885\u5229\uff1a\uff08\u60ca\uff01\uff09\u2026\u2026\n\n\u50f5\u6301\u4e86\u7247\u523b\u4e4b\u540e\uff0cAA12\u6709\u70b9\u65e0\u5948\uff0c\u624b\u4e0d\u518d\u6309\u5728\u67aa\u4e0a\uff0c\u8f6c\u56de\u5934\u7ee7\u7eed\u6491\u5f00\u906e\u9633\u4f1e\u3002\n\nAA12\uff1a {{color|#A9A9A9|\uff08\u7cdf\u7cd5\uff0c\u8fd9\u4e2a\u65f6\u5019\u662f\u4e0d\u662f\u5e94\u8be5\u6253\u4e2a\u62db\u547c\u4ec0\u4e48\u7684\u2026\u2026\uff09}}\n\n\u963f\u6885\u5229\u770b\u5230AA12\u4f3c\u4e4e\u4e5f\u4e0d\u50cf\u662f\u8981\u9a71\u9010\u81ea\u5df1\u7684\u6837\u5b50\uff0c\u5c31\u8dd1\u8fc7\u53bbAA12\u7684\u8eab\u8fb9\u3002\n\n\u963f\u6885\u5229\uff1a\u90a3\u3001\u90a3\u4e2a\uff0c\u4f60\u597d\uff01\n\nAA12\u5168\u8eab\u9707\u4e86\u4e00\u4e0b\uff0c\u50f5\u786c\u5730\u56de\u5934\u770b\u5411\u963f\u6885\u5229\u3002\n\n\u963f\u6885\u5229\uff1a\u90a3\u4e2a\uff0c\u6211\u53eb\u963f\u6885\u5229\u2026\u2026\n::\u4f60\u5c31\u662f\u65b0\u6765\u7684\u540c\u4e8b\u5427\uff01\u5728\u57fa\u5730\u7684\u65f6\u5019\u90fd\u8fd8\u6ca1\u6709\u673a\u4f1a\u8bf4\u4e0a\u8bdd\u2026\u2026\n\nAA12\uff1a\u2026\u2026\u4f60\u597d\uff0c\u6211\u53ebAA12\u3002\n\n\u963f\u6885\u5229\uff1aAA12\u5c0f\u59d0\u4e00\u4e2a\u4eba\u5728\u8fd9\u8fb9\u8036\u3002\n::\u5927\u5bb6\u5728\u53e6\u4e00\u8fb9\u51c6\u5907\u73a9\u6392\u7403\u54e6\uff0c\u4f60\u8981\u53c2\u52a0\u5417\uff1f\n\n\u963f\u6885\u5229\u8ddfAA12\u8bf4\u8bdd\u7684\u65f6\u5019\uff0c\u5927\u773c\u775b\u4eff\u4f5b\u95ea\u70c1\u7740\u5149\u8292\u3002\n::AA12\u5168\u8eab\u53c8\u9707\u4e86\u4e00\u4e0b\u3002\n\nAA12\uff1a {{color|#A9A9A9|\uff08\u6df1\u591c\u7535\u53f0\u6559\u7684\u793e\u4ea4\u793c\u4eea\u90fd\u662f\u4ec0\u4e48\u6765\u7740\uff01\u5feb\u7ed9\u6211\u56de\u60f3\u8d77\u6765\u554a\uff01\uff09}}\n::\u554a\uff0c\u4e0d\u7528\u4e86\u8c22\u8c22\u2026\u2026\n::\u6211\u5176\u5b9e\u4e0d\u662f\u592a\u559c\u6b22\u4eba\u591a\u7684\u6d3b\u52a8\u3002\n\n\u963f\u6885\u5229\uff1a\uff08\u5c0f\u58f0\uff09\u8fd9\u6837\u554a\u2026\u2026\n\nAA12\uff1a {{color|#A9A9A9|\uff08\u7cdf\u7cd5\uff01\u6211\u662f\u4e0d\u662f\u8bf4\u9519\u8bdd\u4e86\uff01\u5979\u600e\u4e48\u4f4e\u4e0b\u4e86\u5934\uff01\uff09}}\n\n\u963f\u6885\u5229\uff1a\u6211\u660e\u767d\u4e86\uff01AA12\u5c0f\u59d0\u662f\u60f3\u8981\u5728\u8fd9\u91cc\u8eb2\u907f\u592a\u9633\u5427\uff1f\n::\u8bf4\u8d77\u6765\uff0c\u6d77\u8fb9\u771f\u7684\u50cf\u8d44\u6599\u91cc\u8bf4\u7684\u90a3\u6837\u53c8\u6652\u53c8\u70ed\uff0c\u6211\u4e5f\u662f\u7b2c\u4e00\u6b21\u6765\u6d77\u8fb9\uff0c\u7740\u5b9e\u88ab\u5413\u4e86\u4e00\u8df3\u5462\u2026\u2026\n\nAA12\uff1a\u662f\u3001\u662f\u554a\u3002\n\n\u963f\u6885\u5229\uff1a\u90a3\u4e2a\uff0c\u8bf7\u95ee\u2026\u2026\u6211\u53ef\u4ee5\u5e2e\u4f60\u4e00\u8d77\u642d\u906e\u9633\u4f1e\u4e48\uff1f\n::AA12\u5c0f\u59d0\u5e26\u4e86\u5f88\u591a\u906e\u9633\u4f1e\u7684\u6837\u5b50\u8036\u2026\u2026\n\nAA12\uff1a\u5f53\u3001\u5f53\u7136\u53ef\u4ee5\u3002\n\n\u963f\u6885\u5229\uff1a\u771f\u7684\u5417\uff01\u592a\u597d\u4e86\uff01\n\nAA12\u53d1\u73b0\uff0c\u963f\u6885\u5229\u4e00\u4e0b\u5b50\u53d8\u5f97\u7279\u522b\u5f00\u5fc3\u3002AA12\u867d\u7136\u611f\u5230\u6709\u4e9b\u624b\u8db3\u65e0\u63aa\uff0c\u4f46\u662f\u4e5f\u5c31\u987a\u7740\u963f\u6885\u5229\u8fd9\u6837\u505a\u4e86\u3002\n\n\u4e0d\u5f97\u4e0d\u8bf4\uff0c\u6709\u4e86\u963f\u6885\u5229\u52a0\u5165\u4e4b\u540e\u7684\u642d\u9633\u4f1e\u5de5\u7a0b\uff0c\u610f\u5916\u5730\u53d8\u5f97\u975e\u5e38\u987a\u5229\u3002\n\n\u7528\u4e86\u5e76\u4e0d\u592a\u4e45\u7684\u65f6\u95f4\uff0cAA12\u5c31\u62e5\u6709\u4e86\u4e00\u628a\u63d2\u7740\u4e09\u67c4\u5de8\u5927\u906e\u9633\u4f1e\u7528\u6765\u5168\u65b9\u4f4d\u963b\u6321\u9633\u5149\u7684\u6c99\u6ee9\u4f11\u95f2\u6905\u4e86\u3002\n\nAA12\u8ba4\u8bc6\u5230\u8fd9\u53c8\u662f\u4e00\u4e2a\u81ea\u5df1\u9700\u8981\u8fd0\u7528\u5230\u793e\u4ea4\u793c\u4eea\u7684\u8282\u70b9\u3002\n\nAA12\uff1a\u2026\u2026\u55ef\uff0c\u975e\u5e38\u611f\u8c22\u4f60\u3002\n\n\u963f\u6885\u5229\uff1a\uff08\u7740\u6025\uff09AA12\u5c0f\u59d0\u4f60\u592a\u5ba2\u6c14\u5566\uff01\u6211\u4eec\u90fd\u5728\u540c\u4e00\u4e2a\u57fa\u5730\u91cc\uff0c\u5e2e\u8fd9\u79cd\u5c0f\u5fd9\u662f\u5e94\u8be5\u7684\uff01\n\nAA12\uff1a\uff08\u8111\u5185\u5feb\u901f\u641c\u7d22\u56de\u7b54\u6837\u677f\u2026\u2026\uff09\n::\u540c\u4e00\u4e2a\u57fa\u5730\u2026\u2026\u54e6\uff0c\u662f\u3001\u662f\u554a\uff0c\u662f\u540c\u4f34\u5462\u3002\n::\u6211\u7684\u540c\u4f34\u963f\u6885\u5229\uff0c\u8bf7\u63a5\u53d7\u6211\u7684\u8c22\u610f\u3002\n\n\u963f\u6885\u5229\uff1a\uff08\u5c0f\u58f0\uff09\u540c\u4f34\u2026\u2026\u670b\u53cb\u2026\u2026\n\n\u6b63\u5f53AA12\u8fd8\u5728\u601d\u7d22\u5e94\u8be5\u518d\u8bf4\u4e9b\u4ec0\u4e48\u6765\u793c\u8c8c\u5730\u7ed3\u675f\u5bf9\u8bdd\u65f6\uff0c\u963f\u6885\u5229\u7a81\u7136\u4ece\u968f\u8eab\u5e26\u7740\u7684\u5c0f\u53e3\u888b\u4e2d\u638f\u51fa\u4e00\u4e2a\u7269\u54c1\uff0c\u6ee1\u8138\u5199\u7740\u671f\u5f85\u5730\u9012\u7ed9AA12\u3002\n\nAA12\uff1a\u2026\u2026\u6b38\uff01\u8fd9\u662f\u4ec0\u4e48\uff1f\n\n\u963f\u6885\u5229\uff1a\u662f\u7ebd\u6263\uff01\u8bf7\u3001\u8bf7\u4f60\u6536\u4e0b\uff01\n\nAA12\uff1a\u2026\u2026\u54c8\u554a\uff1f\u54e6\uff0c\u597d\u2026\u2026\n\n\u963f\u6885\u5229\uff1a\u90a3AA12\u5c0f\u59d0\uff0c\u6211\u5148\u8fc7\u53bb\u6307\u6325\u5b98\u4ed6\u4eec\u90a3\u8fb9\u4e86\u54e6\uff01\n::\u518d\u89c1\uff01\n\nAA12\uff1a\u2026\u2026\u518d\u89c1\u3002\n\nAA12\u770b\u5230\u963f\u6885\u5229\u975e\u5e38\u5f00\u5fc3\u5730\u8dd1\u8d70\u4e86\uff0c\u53c8\u76ee\u7779\u4e86\u5979\u8dd1\u7740\u8dd1\u7740\u5728\u5e73\u5730\u4e0a\u7eca\u4e86\u4e00\u8de4\u7684\u6837\u5b50\uff0c\u6700\u540e\u4f4e\u5934\u76ef\u7740\u624b\u4e2d\u7684\u7ebd\u6263\u3002\n\nAA12\uff1a\u8fd9\u5230\u5e95\u662f\u600e\u4e48\u56de\u4e8b\uff0c\u8fd9\u7ebd\u6263\u53c8\u662f\u600e\u4e48\u56de\u4e8b\uff0c\u6211\u521a\u521a\u6709\u5f88\u597d\u5b8c\u6210\u4e00\u6b21\u793e\u4ea4\u5417\u2026\u2026\n::\u554a\u597d\u9ebb\u70e6\uff0c\u6211\u8fd8\u662f\u5728\u8fd9\u91cc\u5750\u5230\u665a\u4e0a\u5427\uff0c\u597d\u70ed\uff0c\u597d\u6652\uff0c\u548c\u522b\u4eba\u8bf4\u8bdd\u597d\u7d2f\u3002\n::\u7cdf\u7cd5\uff0c\u521a\u521a\u90a3\u4e48\u6b63\u7ecf\u7684\u81ea\u5df1\u4e5f\u592a\u6076\u5fc3\u4e86\u5427\u2026\u2026\n\n\u8111\u4e2d\u5145\u6ee1\u4e86\u4e00\u4e07\u4e2a\u7591\u60d1\u7684AA12\u51b3\u5b9a\u5148\u4e0d\u53bb\u60f3\u8fd9\u4e9b\u81ea\u5df1\u60f3\u4e0d\u901a\u7684\u95ee\u9898\uff0c\u6574\u4e2a\u4eba\u762b\u5728\u4f11\u95f2\u6905\u4e0a\u3002\n\n\u6b63\u5f53\u5979\u4ee5\u4e3a\u53ef\u4ee5\u4eab\u53d7\u6ca1\u6709\u9633\u5149\u76f4\u5c04\u7684\u6e05\u51c0\u65f6\u5149\u65f6\uff0cAA12\u8fdc\u8fdc\u770b\u5230\uff0c\u6709\u4e00\u4e2a\u63d0\u7740\u6e38\u6cf3\u5708\u7684\u6a58\u7ea2\u8272\u8eab\u5f71\u4f3c\u4e4e\u6b63\u7b14\u76f4\u5730\u671d\u81ea\u5df1\u8d70\u6765\u2026\u2026\n\n\u2026\u2026\n\n\u5f53\u65e5\u665a\u4e0a\u3002\n\n\u7b49\u5230AA12\u56de\u8fc7\u795e\u6765\u7684\u65f6\u5019\uff0c\u81ea\u5df1\u5df2\u7ecf\u548c\u90a3\u4e2a\u6a58\u7ea2\u8272\u4eba\u5f62\u5435\u5b8c\u67b6\u4e86\uff0c\u5e76\u4e14\u8865\u5145\u5b8c\u7cd6\u5206\u51fa\u73b0\u5728\u4e86\u7bdd\u706b\u665a\u4f1a\u4e0a\u3002\n\nAA12\uff1a\u4e3a\u4ec0\u4e48\u2026\u2026\u7a81\u7136\u53d8\u6210\u8fd9\u6837\u4e86\u2026\u2026\n::\u8fd9\u662f\u54ea\u91cc\uff0c\u6211\u662f\u8c01\uff0c\u6211\u5728\u5e72\u4ec0\u4e48\u2026\u2026\n\nAA12\u6b63\u5728\u53d1\u6123\u7684\u65f6\u5019\uff0c\u6307\u6325\u5b98\u8d70\u4e86\u8fc7\u6765\u3002\n\n\u6307\u6325\u5b98\uff1a\u54df\uff0cAA12\u3002\n\nAA12\uff1a\u2026\u2026\u662f\u4f60\u554a\u3002\n\n\u6307\u6325\u5b98\uff1a\u600e\u4e48\u6837\uff0c\u5927\u6d77\u597d\u770b\u5417\uff1f\n\nAA12\uff1a\u2026\u2026\u60a8\u95ee\u8bdd\u7684\u65b9\u5f0f\u8d8a\u6765\u8d8a\u571f\u4e86\uff0c\u6307\u6325\u5b98\u3002\n::\u800c\u4e14\uff0c\u5750\u5728\u6c99\u6ee9\u4e0a\u770b\u6d77\uff0c\u89c2\u611f\u548c\u5750\u5728\u683c\u91cc\u82ac\u57fa\u5730\u7684\u9ad8\u5904\u770b\u5916\u9762\u7684\u5e9f\u589f\u4e00\u6837\u3002\n\n\u6307\u6325\u5b98\uff1a\u539f\u6765\u662f\u8fd9\u4e48\u82cd\u51c9\u7684\u89c2\u611f\u5417\u3002\n\nAA12\uff1a\u2026\u2026\u662f\u554a\u3002\n\n\u6307\u6325\u5b98\uff1a\u6211\u4ee5\u4e3a\u4f60\u4f1a\u56e0\u4e3a\u6709\u6211\u4eec\u5728\uff0c\u800c\u611f\u5230\u8fd9\u7247\u6d77\u3001\u6216\u8005\u683c\u91cc\u82ac\u57fa\u5730\uff0c\u6ca1\u6709\u90a3\u4e48\u8352\u51c9\u5462\u3002\n\nAA12\uff1a\uff08\u6c89\u9ed8\uff09\u2026\u2026\n\n\u6307\u6325\u5b98\uff1a\u6211\u53ef\u662f\u56e0\u4e3a\u6709\u4f60\u4eec\u5728\u6240\u4ee5\u611f\u5230\u6bcf\u5929\u90fd\u5f88\u5145\u5b9e\u800c\u4e14\u5f88\u5f00\u5fc3\u5462\uff01\n\nAA12\uff1a\u2026\u2026\u6211\u6ca1\u6709\u5411\u60a8\u95ee\u8bdd\uff0c\u6307\u6325\u5b98\u3002\n\n\u6307\u6325\u5b98\uff1a\u90a3\u6211\u5e76\u4e0d\u4ecb\u610f\u6211\u7684\u81ea\u8a00\u81ea\u8bed\u4f1a\u88ab\u4f60\u542c\u5230\u54e6\u3002\n::\u867d\u8bf4\u6211\u4eec\u90fd\u53ea\u662f\u5355\u72ec\u7684\u4e2a\u4f53\uff0c\u4f46\u662f\u5927\u5bb6\u805a\u5728\u4e00\u8d77\u7684\u5730\u65b9\uff0c\u548c\u6ca1\u6709\u5927\u5bb6\u5b58\u5728\u7684\u5730\u65b9\uff0c\u7ec8\u7a76\u8fd8\u662f\u6709\u5f88\u5927\u7684\u4e0d\u540c\u5427\uff1f\n::\u603b\u4e4b\uff0c\u6b22\u8fce\u4f60\u6765\u5230\u683c\u91cc\u82ac\uff0c\u4e5f\u5e0c\u671b\u4f60\u53ef\u4ee5\u5728\u8fd9\u91cc\u5f97\u5230\u5f88\u591a\u7f8e\u597d\u7684\u4e1c\u897f\u3002\n\nAA12\uff1a\u7f8e\u597d\u7684\u4e1c\u897f\u2026\u2026\u5728\u90a3\u4e2a\u6ee1\u662f\u5e9f\u589f\u7684\u533a\u57df\u5185\u4f1a\u6709\u5417\uff1f\n\n\u6307\u6325\u5b98\uff1a\u6709\u4e9b\u7f8e\u597d\u4e0d\u4e00\u5b9a\u6709\u5b9e\u4f53\uff0c\u800c\u662f\u88ab\u4eba\u4eec\u5236\u9020\u51fa\u6765\u7684\u6982\u5ff5\uff0c\u4f46\u662f\u5374\u51cc\u9a7e\u4e8e\u8fd9\u4e2a\u8352\u51c9\u4e16\u754c\u4e4b\u4e0a\u7684\u54e6\u3002\n\nAA12\u6123\u4e86\u4e00\u4e0b\uff0c\u7a81\u7136\u60f3\u5230\u63e1\u5728\u624b\u5fc3\u4e2d\u7684\u7ebd\u6263\uff0c\u8fd8\u6709\u53e3\u4e2d\u6b8b\u7559\u7684\u6de1\u6de1\u7684\u68d2\u68d2\u7cd6\u7684\u9178\u5473\u3002\n\n\u6307\u6325\u5b98\uff1a\u4ee5\u540e\u6709\u673a\u4f1a\u7684\u8bdd\uff0c\u518d\u8ddf\u5927\u5bb6\u4e00\u8d77\u6765\u6d77\u8fb9\u5427\uff1f\n\nAA12\uff1a\uff08\u6c89\u9ed8\uff09\u2026\u2026\n::\u5173\u4e8e\u8fd9\u4ef6\u4e8b\uff0c\u6211\u8981\u56de\u53bb\u57fa\u5730\u4e4b\u540e\u624d\u80fd\u56de\u7b54\u60a8\u3002\n\nAA12\uff1a {{color|#A9A9A9|\uff08\u5173\u4e8e\u7ebd\u6263\u554a\u670b\u53cb\u554a\u7f8e\u597d\u7684\u4e1c\u897f\u554a\u8fd9\u4e9b\u2026\u2026\u597d\u9ebb\u70e6\uff0c\u4f46\u662f\u53c8\u2026\u2026\uff09}}\n:: {{color|#A9A9A9|\uff08\u56de\u53bb\u4e4b\u540e\u4e00\u5b9a\u8981\u597d\u597d\u95ee\u4e00\u95ee\u7535\u53f0\u683c\u7433\u5c0f\u59d0\uff01\uff09}}\n::{{color|#A9A9A9|\uff08\u55ef\uff0c\u8fd9\u6b21\u6211\u8981\u4e8b\u5148\u7f16\u8f91\u7b80\u8baf\u5b58\u597d\uff0c\u7136\u540e\u88ab\u7b2c\u4e00\u4e2a\u53d1\u53bb\u7535\u53f0\uff01\uff09}}\n}}\n\n===\u6280\u80fd\u76f8\u5173===\n\u5728AA-12\u516c\u5e03\u4e4b\u521d\uff0c\u5c31\u6709\u4eba\u6ce8\u610f\u5230AA12\u7684\u6280\u80fd'''\u201c\u916e\u8840\u75c7\u201d'''\u662f\u7cd6\u5c3f\u75c5\u7684\u4e3b\u8981\u75c5\u75c7\uff0c\u540c\u65f6\u7acb\u7ed8\u4ee5\u53ca\u4eba\u5f62\u4ecb\u7ecd\u6240\u8868\u73b0\u51fa\u7684\uff0c\u90fd\u5