\n\u9ed1\u732b\u8b66\u957f\u7684\u6545\u4e8b\u57fa\u672c\u4e0a\u662f\u53d1\u751f\u5728\u4e00\u7247\u68ee\u6797\u91cc\u3002\n\n\u68ee\u6797\u516c\u5b89\u5c40\u7684\u9ed1\u732b\u8b66\u957f\u5e26\u7740\u5b83\u7684\u90e8\u5c5e\u767d\u732b\u73ed\u957f\u3001\u767d\u9e3d\u4fa6\u63a2\u53ca\u4f17\u591a\u8b66\u5458\u4fa6\u7834\u6848\u4ef6\uff0c\u7ef4\u62a4\u68ee\u6797\u7684\u548c\u5e73\u3002\n\n=== \u767b\u573a\u4eba\u7269 ===\n\uff08\u5f85\u8865\u5b8c\uff09\n=== STAFF ===\n*\u539f\u4f5c\uff1a[[\u8bf8\u5fd7\u7965]]\n*\u5bfc\u6f14\uff1a[[\u6234\u94c1\u90ce]]\u3001[[\u8303\u9a6c\u8fea]]\u3001[[\u718a\u5357\u6e05]]\n*\u7f16\u5267\uff1a[[\u6234\u94c1\u90ce]]\n*\u7f8e\u672f\u8bbe\u8ba1\uff1a\u6234\u94c1\u90ce\n*\u89d2\u8272\u8bbe\u8ba1\uff1a\u987e\u5efa\u56fd\n*\u4f5c\u753b\u76d1\u7763\uff1a\u8bb8\u677e\u5761\u3001\u4ed8\u6590\u537f\n*\u6444\u5f71\uff1a\u6bb5\u5b5d\u8431\u3001\u738b\u798f\u5eb7\n*\u526a\u8f91\uff1a\u8096\u6dee\u6d77\u3001\u674e\u5f00\u57fa\n*\u7f16\u8f91\uff1a\u6f58\u56fd\u7965\n*\u97f3\u4e50\uff1a\u7af9\u5c97\u3001\u5b89\u5e73\u3001\u6c88\u5c0f\u5c91\n*\u52a8\u753b\u5236\u4f5c\uff1a[[\u4e0a\u6d77\u7f8e\u672f\u7535\u5f71\u5236\u7247\u5382]]\n*\u5236\u7247\uff1a\u5370\u5e0c\u52c7\n*\u51fa\u54c1\uff1a\u4e0a\u6d77\u7f8e\u672f\u7535\u5f71\u5236\u7247\u5382\n\n=== CAST ===\n*\u9ed1\u732b\u8b66\u957f\uff1a\u53f2\u4e1c\u654f\n*\u767d\u732b\u73ed\u957f\uff1a\u738b\u536b\n*\u767d\u9e3d\u63a2\u957f\uff1a\u5f20\u6cc9\u7075\n\n=== \u5404\u96c6\u6807\u9898 ===\n{| class=\"wikitable\"\n|-\n! \u96c6\u6570 !! \u6807\u9898\n|-\n| \u7b2c\u4e00\u96c6 || \u75db\u6b7c\u642c\u4ed3\u9f20\n|-\n| \u7b2c\u4e8c\u96c6 || \u7a7a\u4e2d\u64d2\u654c\n|-\n| \u7b2c\u4e09\u96c6 || \u5403\u7ea2\u571f\u7684\u5c0f\u5077\n|-\n| \u7b2c\u56db\u96c6 || \u5403\u4e08\u592b\u7684\u87b3\u8782\n|-\n| \u7b2c\u4e94\u96c6 || \u4f1a\u5403\u732b\u7684\u5a18\u8205\n|}\n\n=== \u76f8\u5173\u97f3\u4e50 ===\n\u4e3b\u9898\u66f2\u300a\u554a\u54c8\uff0c\u9ed1\u732b\u8b66\u957f\u300b\uff0c\u8bcd\u66f2\u4f5c\u8005[[\u8521\u7490]]\uff0c\u5f71\u7247\u4e2d\u7684\u4e3b\u5531\u662f[[\u6c88\u5c0f\u5c91]]\u3002\n\n=== \u83b7\u5956\u8bb0\u5f55 ===\n*1985\u5e74 \u4e2d\u56fd\u513f\u7ae5\u5c11\u5e74\u7535\u5f71\u91d1\u725b\u5956 \u4f18\u79c0\u5f71\u7247\u5956\n*1988\u5e74 \u4e2d\u56fd\u5e7f\u64ad\u7535\u5f71\u7535\u89c6\u90e81986\u20141987\u5e74 \u4f18\u79c0\u5f71\u7247\u5956\n*1988\u5e74 \u4e2d\u56fd\u513f\u7ae5\u5c11\u5e74\u7535\u5f71\u6cb9\u5a03\u5956 \u4f18\u79c0\u5f71\u7247\u5956\n*2010\u5e74 \u91d1\u9f99\u5956\u539f\u521b\u6f2b\u753b\u52a8\u753b\u827a\u672f\u5927\u8d5b  \u6700\u4f73\u7ecf\u5178\u539f\u521b\u5956\n\n== \u9ed1\u732b\u8b66\u957f\u7b2c\u4e8c\u90e8 ==\n1992\u5e74\u4e2d\u592e\u7535\u89c6\u53f0\u52a8\u753b\u90e8\u5f97\u5230\u539f\u4f5c\u4f5c\u8005\u8bf8\u5fd7\u7965\u6388\u6743\u4ee5\u62cd\u6444\u7eed\u96c6\u52a8\u753b\u8fde\u7eed\u5267\uff0c\u5bfc\u6f14\u4e3a[[\u51e1\u5349]]\u3002\u8be5\u7247\u520612\u96c6\uff0c\u6bcf\u96c66\u52308\u5206\u949f\u4e0d\u7b49\u3002\n\n\u7531\u4e8e1987\u5e74\u539f\u4f5c\u4f5c\u8005\u8bf8\u5fd7\u7965\u4e0e\u4e0a\u6d77\u7f8e\u672f\u7535\u5f71\u5236\u7247\u5382\u53ca\u6234\u94c1\u90ce\u5c31\u9ed1\u732b\u8b66\u957f\u7684\u7248\u6743\u95ee\u9898\u53d1\u8d77\u8fc7\u8bc9\u8bbc<ref name=\"\u8b66\u957f\">[http://culture.ifeng.com/a/20150910/44622778_0.shtml \u300a\u9ed1\u732b\u8b66\u957f\u300b\u4e4b\u7236\u8bf8\u5fd7\u7965\u53bb\u4e16 \u5f53\u5e74\u524d\u4e3a\u4f55\u9009\u732b\u5f53\u8b66\u957f]</ref>\uff0c\u56e0\u6b64\u6b64\u4f5c\u672a\u83b7\u5f97\u7f8e\u5f71\u5382\u548c\u6234\u94c1\u90ce\u5bf9\u52a8\u753b\u4eba\u7269\u9020\u578b\uff0c\u4ee5\u53ca\u90e8\u5206\u52a8\u753b\u4eba\u7269\uff08\u5982\u201c\u4e00\u53ea\u8033\u201d\uff09\u7684\u6388\u6743\u3002\u6b64\u4f5c\u4e0e\u4e4b\u524d\u7684\u4f5c\u54c1\u4eba\u8bbe\u98ce\u683c\u5dee\u5f02\u5de8\u5927\uff0c\u7b2c\u4e8c\u90e8\u4e0e\u7b2c\u4e00\u90e8\u7684\u9ed1\u732b\u8b66\u957f\u9020\u578b\u4e0d\u540c\u3002\u7b2c\u4e8c\u90e8\u8b66\u957f\u8138\u90e8\u5168\u9ed1\uff0c\u8eab\u7a7f\u7ea2\u8272\u8b66\u670d\u3002\u4ed6\u7684\u624b\u4e0b\u4eec\u4e5f\u7531\u7b2c\u4e00\u90e8\u7684\u767d\u8272\u5236\u670d\u6539\u6210\u9ec4\u8272\u5236\u670d\u3002\n\n\u8fd9\u57fa\u672c\u4e0a\u662f\u4e00\u90e8\u7c97\u5236\u6ee5\u9020\u7684\u4f5c\u54c1\u3002\n\n=== \u5404\u96c6\u6807\u9898 ===\n{| class=\"wikitable\"\n|-\n! \u96c6\u6570 !! \u6807\u9898\n|-\n| \u7b2c\u4e00\u96c6 || \u8c01\u6bd2\u5bb3\u4e86\u5927\u9ec4\u72d7\n|-\n| \u7b2c\u4e8c\u96c6 || \u591c\u534a\u547c\u6551\u58f0\n|-\n| \u7b2c\u4e09\u96c6 || \u8b66\u957f\u906d\u7a83\n|-\n| \u7b2c\u56db\u96c6 || \u7d27\u6025\u62a2\u6551\n|-\n| \u7b2c\u4e94\u96c6 || \u6293\u4e0d\u4f4f\u7684\u51f6\u624b\n|-\n| \u7b2c\u516d\u96c6 || \u5047\u620f\u771f\u505a\n|-\n| \u7b2c\u4e03\u96c6 || \u60e8\u6848\u53d1\u751f\u4ee5\u540e\n|-\n| \u7b2c\u516b\u96c6 || \u68ee\u6797\u996d\u5e97\u7684\u707e\u96be\n|-\n| \u7b2c\u4e5d\u96c6 || \u5931\u8e2a\u7684\u7ebd\u6263\n|-\n| \u7b2c\u5341\u96c6 || \u7d27\u6025\u884c\u52a8\n|-\n| \u7b2c\u5341\u4e00\u96c6 || \u53ef\u6015\u7684\u4f10\u6728\u58f0\n|-\n| \u7b2c\u5341\u4e8c\u96c6 || \u5916\u661f\u4eba\u62a2\u52ab\u6848\n|}\n\n== \u9ed1\u732b\u8b66\u957f\u7535\u5f71\u7248 ==\n\u7ecf\u6570\u5b57\u6280\u672f\u4fee\u590d\u7684\u52a8\u753b\u7535\u5f71\u4e8e2010\u5e744\u670823\u65e5\u516c\u6620\uff0c\u603b\u5bfc\u6f14\u4ecd\u4e3a\u6234\u94c1\u90ce\u3002\u5f71\u7247\u5168\u957f78\u5206\u949f\u3002\n\n\u65b0\u7535\u5f71\u7248\u5b9e\u9645\u4e0a\u662f\u5728\u539f5\u96c6\u7535\u5f71\u57fa\u7840\u4e0a\u8fdb\u884c\u526a\u63a5\uff0c\u9488\u5bf9\u65f6\u4ee3\u53d1\u5c55\u91cd\u65b0\u7f16\u5199\u5bf9\u767d\u914d\u97f3\uff0c\u5e76\u52a0\u5165\u53cd\u6050\u7b49\u5185\u5bb9\u3002\u7535\u5f71\u4e3b\u9898\u66f2\u7531[[\u6797\u5999\u53ef]]\u4e3b\u5531\u3002<ref>[http://enjoy.eastday.com/e/20100422/u1a5164817.html \u300a\u9ed1\u732b\u8b66\u957f\u300b\u767b\u5927\u8367\u5e55 \u6797\u5999\u53ef\u81ea\u8a00\u5b66\u5230\u79d1\u666e\u77e5\u8bc6]</ref>\n\n\u7535\u5f71\u6700\u7ec8\u7968\u623f1249.3\u4e07\u3002\n\n=== CAST ===\n*\u9ed1\u732b\u8b66\u957f\uff1a\u5434\u78ca\n*\u4e00\u53ea\u8033\uff1a\u674e\u626c\n*\u975e\u6d32\u5403\u732b\u9f20\uff1a\u5927\u5c71\n*\u98df\u7334\u9e70\uff1a\u5218\u98ce\n*\u767d\u9e3d\u4fa6\u63a2\uff1a\u6731\u5f66\u5a77\n*\u7ea2\u9177\u5154\uff1a\u6797\u5999\u53ef\n*\u65c1\u767d\uff1a\u97a0\u840d\n\n== \u9ed1\u732b\u8b66\u957f\u4e4b\u7fe1\u7fe0\u4e4b\u661f ==\n{{main|\u9ed1\u732b\u8b66\u957f\u4e4b\u7fe1\u7fe0\u4e4b\u661f}}\n\n2015\u5e74\u51fa\u54c1\u7684\u5168\u65b0\u5927\u7535\u5f71\u3002\n== \u884d\u751f\u4f5c\u54c1 ==\n=== \u8fde\u73af\u753b ===\n\u5728\u4e0a\u6d77\u7f8e\u672f\u7535\u5f71\u5236\u7247\u5382\u5c06\u300a\u9ed1\u732b\u8b66\u957f\u300b\u524d5\u96c6\u6539\u6210\u4e86\u52a8\u753b\u7247\u540e\uff0c\u52a8\u753b\u5bfc\u6f14\u6234\u94c1\u90ce\u968f\u540e\u5373\u51fa\u7248\u4e86\u300a\u9ed1\u732b\u8b66\u957f\u300b\u7684\u52a8\u753b\u8fde\u73af\u753b\uff0c\u4f46\u56e0\u4e3a\u7248\u6743\u539f\u56e0\uff0c1987\u5e746\u6708\u4e0b\u65ec\uff0c\u539f\u4f5c\u8005\u8bf8\u5fd7\u7965\u5c06\u6234\u94c1\u90ce\u544a\u4e0a\u6cd5\u5ead\uff0c\u8d77\u8bc9\u6234\u94c1\u90ce\u4fb5\u72af\u4e86\u4ed6\u300a\u9ed1\u732b\u8b66\u957f\u300b\u7684\u7248\u6743\u53ca\u7f72\u540d\u6743\u3002\u6700\u7ec8\uff0c\u6cd5\u9662\u786e\u8ba4\u6234\u94c1\u90ce\u5728\u51fa\u7248\u300a\u9ed1\u732b\u8b66\u957f\u300b\u8fde\u73af\u753b\u4ee5\u53ca\u671f\u520a\u4e2d\uff0c\u786e\u5b9e\u5b58\u5728\u8fc7\u5931\u884c\u4e3a\u3002\u540e\u7ecf\u8fc7\u6cd5\u9662\u8c03\u89e3\uff0c\u6234\u94c1\u90ce\u4ece\u6587\u5b57\u7a3f\u916c\u4e2d\u8d54\u507f\u8bf8\u5fd7\u79652880\u5143\uff0c\u8bf8\u5fd7\u7965\u64a4\u8bc9\u3002<ref name=\"\u8b66\u957f\" />\n\n=== \u6e38\u620f\u7248 ===\n;\u300a\u9ed1\u732b\u8b66\u957f\u6218\u6d77\u76d7\u300b\n:2012\u5e741\u670829\u65e5\u53d1\u884c\uff0c\u662f\u4e00\u6b3e\u5c04\u51fb\u7c7b\u5c0f\u6e38\u620f\u3002\u6e38\u620f\u5185\u5bb9\u8bbe\u7f6e\u662f\u9ed1\u732b\u8b66\u957f\u7387\u9886\u8b66\u5bdf\u4e0e\u8001\u9f20\u6d77\u76d7\u519b\u56e2\u8fdb\u884c\u7684\u4e00\u573a\u963b\u51fb\u6218\u3002\n\n;\u300a\u9ed1\u732b\u8b66\u957f\u4f20\u5947\u300b\n:2012\u5e7411\u670815\u65e5\u5728\u4e2d\u56fd\u5927\u9646\u53d1\u552e\uff0c\u662f\u4e00\u6b3e\u5355\u4eba\u52a8\u4f5c\u5c0f\u6e38\u620f\uff0c\u4e3a\u201c\u9ed1\u732b\u8b66\u957f\u201d\u7cfb\u5217\u4f5c\u54c1\u3002\n\n=== \u771f\u4eba\u7535\u5f71 ===\n\u5bfc\u6f14[[\u9646\u5ddd]]\u786e\u8ba4\u5176\u516c\u53f8\u73af\u7403\u827a\u52a8\u5f71\u4e1a\u5df2\u7ecf\u62ff\u4e0b\u300a\u9ed1\u732b\u8b66\u957f\u300b\u539f\u8457\u5c0f\u8bf4\u7684\u201c\u771f\u4eba\u5f71\u89c6\u4f5c\u54c1\u6539\u7f16\u6743\u201d\u3002<ref>[http://www.1905.com/news/20150609/899195.shtml?fr=wwwnews_list_hy_201411#p1 \u9646\u5ddd\u786e\u8ba4\u5df2\u8d2d\u300a\u9ed1\u732b\u8b66\u957f\u300bIP \u6216\u4efb\u5bfc\u6f14\u62cd\u771f\u4eba\u7248]</ref>\u4f46\u5177\u4f53\u51fa\u54c1\u65f6\u95f4\u5c1a\u4e0d\u8be6\u3002\n\n{{\u4e2d\u56fd1980\u5e74\u4ee3\u52a8\u753b}}{{\u4e2d\u56fd1990\u5e74\u4ee3\u52a8\u753b}}{{\u4e2d\u56fd2010\u5e74\u52a8\u753b}}\n\n== \u6ce8\u91ca ==\n<references />\n== \u5916\u90e8\u94fe\u63a5 ==\n\n[[Category:\u4e2d\u56fd\u52a8\u753b\u4f5c\u54c1]]\n[[\u5206\u7c7b:\u4e0a\u6d77\u7f8e\u672f\u7535\u5f71\u5236\u7247\u5382\u4f5c\u54c1]]\n[[\u5206\u7c7b:\u72af\u7f6a\u9898\u6750]]\n[[Category:\u9ed1\u732b\u8b66\u957f]]\n[[Category:\u52a8\u7269\u9898\u6750]]\n[[Category:\u8b66\u5bdf\u9898\u6750]]"}}}