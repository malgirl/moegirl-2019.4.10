\n'''\u4f4f\u6240'''\uff1a\u672a\u8bbe\u5b9a\n\n'''\u804c\u4e1a'''\uff1a\u9b54\u795e\n\n'''\u80fd\u529b'''\uff1a\uff08\u6210\u4e3a\u9b54\u795e\u540e\uff09\u638c\u63a7\u6b21\u5143\u7a0b\u5ea6\u7684\u80fd\u529b\n\n'''\u840c\u5c5e\u6027'''\uff1a[[\u5de8\u4e73]]\u3001[[\u5deb\u5973\u670d]]\u3001[[S\u5c5e\u6027]]\u3001[[\u5f3a\u6c14]]\u3001[[\u8179\u9ed1]]\u3001[[\u50b2\u5a07]]\u3001[[\u767e\u5408]]\u3001[[\u65e0\u8282\u64cd]]\u3001[[\u9ed1\u5316]]\u3001[[\u59b9\u63a7]]\u3001[[\u5fa1\u59d0]]\u3001[[\u4eba\u59bb]]\u3001[[\u5de5\u53e3]]\u3001[[\u9ed1\u53d1]]\u3001[[\u767d\u53d1]]\u3001[[\u7ea2\u77b3]]\u3001[[\u6b7b\u9c7c\u773c]]\u3001[[\u7eb9\u8eab]]\u3001[[\u5deb\u5973\u670d]]\u3001[[\u5927\u8774\u8776\u7ed3]]\u3001[[\u5973\u683c\u6597\u5bb6]]\u3001[[\u4eba\u5916]]\u3001[[\u7279\u6b8a\u81ea\u79f0]]\u3001[[\u55dc\u6b66]]\u3001[[\u5fa1\u5e01]]\n\n'''\u4eba\u9645\u5173\u7cfb'''\uff1a[[\u535a\u4e3d\u7075\u68a6]]\uff08\u6bcd\u4f53\uff09\u3001[[U\u7075\u68a6]]\uff08\u4eb2\u4eba\u3001<S>[[\u6709\u4e2a\u80fd\u5e72\u7684\u59b9\u59b9\u771f\u597d|\u80fd\u5e72\u7684\u59b9\u59b9]]</S>\uff09\n\n===\u767b\u573a\u4f5c\u54c1===\n\u5e76\u975e\u539f\u4f5c\u89d2\u8272\uff0c\u767b\u573a\u4e8e\u300aMUGEN\u300b\u53ca\u5176\u884d\u751f\u4f5c\u54c1\n\n=== \u6027\u683c ===\n\u5728\u7184\u7648\u4eba\u6c0f\u7684\u9b54\u795e\u7978\u7075\u68a6\u539f\u8bbe\u5b9a\u4e2d\uff0c\u7978\u7075\u68a6\u662f\u5e7b\u60f3\u4e61\u4e00\u5207\u8d1f\u9762\u611f\u60c5\u805a\u96c6\u7684\u7ed3\u679c\uff0c\u4e00\u51fa\u751f\u4fbf\u51e0\u4e4e\u6bc1\u706d\u5e7b\u60f3\u4e61\uff0c\u800c\u9b54\u4eba\u7978\u7075\u68a6\u7684\u8bbe\u5b9a\u4e2d\uff0c\u5176\u59cb\u4f5c\u4fd1\u8005\u7978\u5fcc\u4e5f\u662f\u300a\u62f3\u7687\u300b\u4e2d\u7684\u53cd\u6d3e\u89d2\u8272\uff0c\u56e0\u6b64\u7978\u7075\u68a6\u5728\u300amugen\u300b\u4e2d\uff0c\u57fa\u672c\u4e5f\u662f\u4e00\u4e2a\u201c\u626b\u5e1a\u661f\u201d\u5f0f\u4e0d\u592a\u5999\u7684\u89d2\u8272\u3002\n\n\u4f46\u662f\uff0c\u7531\u4e8e\u5176\u9ad8\u634f\u4ed6\u5ea6\u7684\u6280\u80fd\u3001\u