b7[[\u53d1\u8f66|\u98da\u8f66]]'''\n\n'''\u00b7[[\u6280\u672f\u5b85|\u5f00\u5766\u514b]]'''\n\n}}\n\n\n\u4e07\u5e74\u9cb2\u9e4f\u4e4b\u540d\u53f7\u53d6\u81ea[[\u5218\u6148\u6b23]]\u8457\u4f5c\u300a[[\u4e09\u4f53]]\u300b\u4e2d[[\u672b\u65e5\u6218\u5f79]]\u5185\u7684\u4e00\u8258\u6218\u8230\u3002\n\n\u535a\u5ba2\u4e0d\u5e38\u7528\uff0c\u5fae\u535a\u66f4\u5df2\u629b\uff01\u4f60\u8bb0\u4e0d\u8bb0\u5f97\u4e00\u79cd\u9690\u8e2a\u533f\u8ff9\u7684\u6d3b\u6cd5\uff1f\n\n== <big>WOTer\uff1fWOTer\uff01</big> ==\n2014\u5e74\u521d\u5165\u5751WOT\u5317\u533a\uff0c\u7531\u4e8e\u79cd\u79cd\u539f\u56e0\uff0c\u5386\u65f6\u4e24\u5e74\u624d\u6210\u4e3a\u4e07\u573a\u5343\u6548\u670910\u7ea7\u7684\u8001\u53f8\u673a\uff0c\u6e38\u620f\u98ce\u683c\u4fdd\u5b88\u800c\u4e0d\u7325\u7410\uff0c\u64c5\u957f\u9632\u5b88\u591a\u4e8e\u8fdb\u653b\u3002\n\n{{\u9ed1\u5e55|\u628a\u8e72\u5751\u8bf4\u5f97\u8fd9\u822c\u51a0\u5195\u5802\u7687\uff0c[[\u6211\u4ece\u672a\u89c1\u8fc7\u6709\u5982\u6b64\u539a\u989c\u65e0\u803b\u4e4b\u4eba|\u6211\u4ece\u672a\u89c1\u8fc7\u6709\u5982\u6b64\u539a\u989c\u65e0\u803b\u4e4b\u4eba\uff01]]}}\n\n\u00b7\u5f88\u5751\n\n\u00b7\u540e\u6765\u7a0d\u597d\u4e00\u70b9\n\n\u00b7\u4f46\u662f\u4ecd\u7136\u5751\uff0c\u6548\u7387\u5927\u69821200\u5427\n\n\u7136\u800c\u6700\u8fd1\u76d2\u5b50\u6218\u6597\u529b\u6539\u7248\uff0c\u6548\u7387\u6d6e\u52a8\u6781\u5927\uff0c\u76ee\u524d\u6b63\u5904\u4e8e\u4e00\u8138\u8499\u853d\u6a21\u5f0f\u3002\n\n\u6b22\u8fce\u7ec4\u961f\u5f00\u9ed1\uff0c\u8bf7\u79c1\u4fe1\u7559ID\u3002\n\n== <big>\u5728\u5730\u7403\u5371\u96be\u4e4b\u9645\u4ece\u6b63\u4e0a\u65b9\u4fef\u51b2\u800c\u4e0b\uff01</big> ==\n2015\u5e748\u6708\u5165\u5751\u6218\u4e89\u96f7\u9706\uff0c\u7531\u4e8e\u53d7WOT\u5f71\u54cd\u8fc7\u5927\uff0c\u96be\u4ee5\u9002\u5e94\u819c\u6cd5\u9646\u6218\uff0c\u56e0\u6b64\u6210\u4e3a\u4e00\u540d{{ruby|\u4f18\u79c0|\u9017\u6bd4}}\u7684\u98de\u884c\u5458\u3002\n\n\u76f8\u8f83\u4e00\u822c\u6218\u6597\u673a\u4e0e\u6c34\u5e73\u8f70\u70b8\u673a\uff0c\u66f4\u559c\u6b22\u91cd\u6218\u548c\u653b\u51fb\u673a\u3002\n\n\u8def\u6f2b\u6f2b\u5176\u4fee\u8fdc\u516e\uff0c\u543e\u5c06\u4e0a\u4e0b\u800c[[\u4f5c\u6b7b]]\u3002\n\n\n\n{{Hide|\u6807\u9898={{ruby|\u670d\u5f79|zuo si}}\u7ecf\u5386|\u5185\u5bb9=\n\n\u88ab\u5df1\u65b9\u8f70\u70b8\u673a\u7684\u5c3e\u70ae\u51fb\u843d\u3002\n\n\u88ab\u81ea\u5df1\u7684\u70b8\u5f39\u70e7\u65ad\u5c3e\u7ffc\u3002\n\n\u7740\u8230\u65f6\u5fd8\u4e86\u81ea\u5df1\u662f\u6c34\u4e0a\u98de\u673a\u3002\n\n\u5728\u8d77\u98de\u5b8c\u5168\u79bb\u5730\u4e4b\u524d\u8bef\u6309\u6295\u5f39\u3002\n\n\u964d\u843d\u65f6\u5fd8\u8bb0\u653e\u4e0b\u8d77\u843d\u67b6\u3002\n\n\u88abAI\u63a7\u5236\u7684\u4f4e\u7ea7\u9632\u7a7a\u70ae\u4e00\u53d1\u70b9\u5c04\u8214\u65ad\u673a\u7ffc\u3002\n\n\u4ee5\u4e3a\u88ab\u51fb\u843d\u7684\u654c\u673a\u6ca1\u6709\u78b0\u649e\u6a21\u578b\u800c\u4e0d\u89c4\u907f\u3002\n\n\u5728\u654c\u4eba\u5373\u5c06\u843d\u8d25\u65f6\u4ece\u6b63\u4e0a\u65b9\u4fef\u51b2\u800c\u4e0b\uff0c\u53d1\u5c04\u7684\u706b\u7bad\u5f39\u88ab\u4f4e\u7a7a\u961f\u53cb\u5b9e\u529b\u62a2\u955c\u7a7a\u624b\u63a5\u767d\u5203\u3002\n\n\u9646\u6218\u65e0\u89c6\u725b\u987f\u7684\u6012\u543c\uff0c\u5728\u58d5\u6c9f\u7684\u52aa\u529b\u4e0b\u6210\u529f\u7ffb\u8f66\u3002\n\n\u4ee5\u4e3a\u70ae\u706b\u652f\u63f4\u4e0d\u80fd\u7ed5\u8fc7\u5efa\u7b51\u7269\u5c31\u4e0d\u8eb2\u3002\n\n\u201c\u654c\u4eba\u80af\u5b9a\u770b\u4e0d\u89c1\u6211\u3002\u201d\n\n\u4ee5\u4e0a\u3002\n\n\n}}\n\n== <big>\u7a77\u53d1\u4e4b\u5317\uff0c\u6709\u51a5\u6d77\u8005\u3002</big> ==\n\n\u9cb2\u4e43\u5317\u51a5\u4e4b\u9c7c\uff0c[[\u795e\u8bf4\u4f60\u8fd8\u4e0d\u80fd\u6b7b\u5728\u8fd9\u91cc|\u795e\u8bf4\uff1a\u9c7c\u8981\u6709\u6d77\u3002]]\n\n2015\u5e748\u6708\u5f00\u5751WOWS\n\n\u56e0\u4e3a\u662f\u4e9a\u670d\uff0c\u53c8\u6ca1\u94b1\u4e70\u52a0\u901f\u5668\uff0c\u6240\u4ee5\u2014\u2014\n\n\u6211\u7684\u5de1\u6d0b\u8230\uff0c\u65f6\u5c1a\u65f6\u5c1a\u6700\u65f6\u5c1a\uff0c\u6f02\u79fb\u80af\u5b9a\u68d2\uff0c\u5728\u6d69\u701a\u7684\u6d77\u9762\u4e0a\u65cb\u8f6c\uff01\u7529\u5c3e\uff01\u6211\u6254\u9c7c\u96f7\uff01{{\u9ed1\u5e55|[[\u4f60\u7684\u597d\u53cbxx\u5df2\u4e0a\u7ebf|\u60a8\u7684\u597d\u53cb\u3010Ping\u3011\u5df2\u4e0b\u7ebf\u3002]]}}\n\n\u540e\u56e0\u8840\u6761\u4e16\u754c\u7684\u8fdd\u548c\u611f\u7206\u68da\u800c\u5f03\u5751\u3002\n\n== <big>\u7ed9\u5c81\u6708\u4ee5\u751f\u547d\uff0c\u800c\u4e0d\u662f\u7ed9\u751f\u547d\u4ee5\u5c81\u6708\u3002</big> ==\n\n\u8fd9\u4e2a\u4eba\u6ca1\u6709\u4ec0\u4e48\u6545\u4e8b\uff0c\u6709\u6545\u4e8b\u5c31\u6709\u60b2\u6b22\u79bb\u5408\uff0c\u6240\u4ee5\u6ca1\u6709\u6545\u4e8b\u3002\n\n== [[\u53ef\u4ee5\u7684\uff0cooo|<big>\u53ef\u4ee5\uff0c\u8fd9\u5f88\u5b85\u3002</big>]] ==\n\n\u73a9\u6e38\u620f\u4e0d\u80fd\u7b97\u5b85\uff01\u2026\u2026\u73a9\u6e38\u620f\u2026\u2026[[\u81ea\u5b85\u8b66\u5907\u5458|NEET]]\u7684\u4e8b\uff0c\u80fd\u7b97\u5b85\u5417\uff1f\n\n{{ruby|\u4f17\u6240\u5468\u77e5|\u81ea\u8bf4\u81ea\u8bdd}}\uff0c\u60f3\u8981\u6210\u4e3a\u4e00\u540d\u5408\u683c\u7684<del>\u5bb6\u91cc\u8e72</del>\u4f4f\u5b85\u5b88\u62a4\u8005\uff0c\u4ec5\u4ec5\u62e5\u6709\u6e38\u620f\u662f\u4e0d\u8db3\u4ee5\u4e0e\u8fd9\u4e16\u95f4\u6240\u6709\u7684\u6076\u76f8\u6297\u8861\u7684\u3002\n\n[[\u6211\u7684\u5185\u5fc3\u6beb\u65e0\u6ce2\u52a8\uff0c\u751a\u81f3\u8fd8\u60f3\u7b11|\u6211\u7684\u5185\u5fc3\u5341\u5206Excited\uff0c\u751a\u81f3\u60f3\u770b\u4e2a\u5927\u52a8\u6f2b\u3002]]\n\n\u2014\u2014\u6240\u4ee5\u6211\u5c31\u8fdb\u5751\u4e86\u3002\n\n{{Hide|\u6807\u9898=\u5165\u5751\u8def\u5f84|\u5185\u5bb9=\n\n\u4eba\u751f\u7b2c\u4e00\u90e8\u52a8\u6f2b\u4ea7\u81ea[[11\u533a]]\uff0c\u5176\u540d\u300a\u56db\u9a71\u5144\u5f1f\u300b\u3002\n\n\u504f\u597d\u56fd\u6f2b\uff0c\u4f46\u56e0\u56fd\u6f2b\u4ea7\u91cf\u8fc7\u4f4e\u53ca\u90e8\u5206\u9898\u6750\u7684\u4e2a\u4eba\u559c\u597d\u95ee\u9898\uff0c\u4e5f\u4f1a\u5728\u95f2\u65f6\u770b\u65e5\u6f2b\uff0c\u672c\u8d28\u4e0a\u5e76\u4e0d\u6392\u65a5\u3002\n\n\u65e0\u8ffd\u756a\u4e60\u60ef\uff0c\u6ca1\u6512\u6ee1\u4e00\u5b63\u5c31\u770b\u4e86\u90a3\u548c\u54b8\u9c7c\u6709\u4ec0\u4e48\u533a\u522b\uff01\n\n\u6bd4\u8f83\u5728\u610f\u65b0\u756a\u7684\u753b\u98ce\u548c\u58f0\u4f18\u9635\u5bb9\uff0c\u5f88\u591a\u65f6\u5019\u4f1a\u6cbf\u58f0\u4f18\u7f51\u5bfb\u627e\u9002\u5408\u7684\u52a8\u6f2b\u6765\u770b\uff0c\u5f53\u4e00\u90e8\u4f5c\u54c1\u5b58\u5728\u4e09\u4f4d\u4ee5\u4e0a\u6709\u597d\u611f\u7684\u58f0\u4f18\u51fa\u6f14\u7684\u8bdd[[\u5c31\u7b97\u662f\u795e\u4e5f\u6740\u7ed9\u4f60\u770b|\u5c31\u7b97\u5168\u662f\u914d\u89d2\u4e5f\u8865\u7ed9\u4f60\u770b]]\u3002\n\n{{\u9ed1\u5e55|\u5f53\u7136\u4e86\u6709\u4e0d\u559c\u6b22\u7684\u58f0\u4f18\u65f6\u4f18\u5148\u7ea7\u4f1a\u964d\u4f4e\u5f88\u591a\u5475\u5475\u5475\u5475\u5475\u5475\u5475}}\n\n\u987a\u5e26\u4e00\u63d0\uff0c[[\u51c9\u5bab\u6625\u65e5\u7684\u5fe7\u90c1|\u6211\u7684\u56e2\u957f\u6211\u7684\u56e2]]\u662f\u7b2c\u4e00\u90e8\u4e3b\u52a8\u627e\u7684\u52a8\u6f2b\uff0c\u4ece\u8fd9\u4e4b\u540e\u8fdb\u5165\u4e86\u5947\u602a\u7684\u5957\u8def\u4e16\u754c\u3002}}\n\n'''2017\u5e747\u6708\u5f00\u59cb\u6c89\u8ff7[[\u73c2\u6735\u8389\u00b7\u8bfa\u5854\u00b7\u745f\u5c3c\u6b27\u91cc\u65af|\u73c2]]\u5b66\uff0c\u8d5e\u7f8e[[\u672b\u65e5\u65f6\u5728\u505a\u4ec0\u4e48\uff1f\u6709\u6ca1\u6709\u7a7a\uff1f\u53ef\u4ee5\u6765\u62ef\u6551\u5417\uff1f|\u58a8\u9c7c]]\uff01'''\n\n== <big>\u5e97\u957f\u7edf\u8ba1</big> ==\n\n{{info|<small>\u8fd9\u4f4d\u540c\u5fd7\u6ca1\u6709\u4efb\u4f55\u7f16\u8f91\u57fa\u7840\uff0c\u4e00\u5207\u90fd\u662f\u901a\u8fc7\u4e0d\u65ad\u63a5\u89e6\u65e2\u6709\u7684\u9875\u9762\uff08\u4e3b\u8981\u662f\u7528\u6237\u9875\uff09\u5b66\u4e60\u800c\u6765\uff0c\u6bcf\u4e00\u4f4d\u8bbf\u5ba2\u90fd\u6709\u53ef\u80fd{{ruby|\u4fc3\u8fdb\u4ed6\u7684\u8fdb\u6b65|\u8ba9\u4ed6\u5b66\u65b0\u4ee3\u7801}}\u3002</small>}}\n\n\u62c5\u5f53\u6392\u5934\u5175\u7684\u7f16\u8f91\u6761\u76ee\uff1a\uff08\u4ec5\u7edf\u8ba1\u5728\u7f16\u8f91\u5b8c\u6210\u65f6{{ruby|\u4ed8\u51fa|\u7801\u5b57}}\u5360\u6bd4\u8fc7\u534a\u7684\u6761\u76ee\uff0c\u6309\u65f6\u95f4\u987a\u5e8f\u6392\u5217\uff09\n\n{| border=1\n| [[\u516b\