u955c\u7684\u5357\u6728\u67ad\uff0c\u7279\u522b\u611f\u8c22\u53e3\u888b\u5996\u602a\u5427\u5427\u53cb{{color|red|redgraybull}}\u7684\u5e94\u63f4]]\n|}\n</poem>\n\n\u4f46\u662f\u7531\u4e8e\u5fa1\u4e09\u5bb6\u90a3\u6b8b\u5ff5\u76847\uff1a1\u516c\u6bcd\u6bd4\u4f8b\uff0c\u76f8\u4fe1\u5404\u4f4d\u73a9\u5bb6\u62ff\u5230\u624b\u7684\uff0c\u5927\u6982\u662f\u548cLL\u5b98\u65b9\u5e7f\u64ad\u5267\u634f\u4ed6\u4e00\u6837\u7684\u7537\u5c0f\u9e1f\u5427\u2026\u2026\u2026\u2026<del>\u6d88\u606f\u4e00\u51fa\u6240\u6709LLer\u517cPM\u73a9\u5bb6\u90fd\u53d1\u51fa\u4e86\u54c0\u568e\uff0c\u4e0d\u8fc7\u6ca1\u4e8b\uff0c\u4eceXY\u8d77\uff0c\u5fa1\u4e09\u5bb6\u4e5f\u80fd\u901a\u8fc7S/L\u5927\u6cd5\u5237\u6027\u522b\u548c\u95ea\u5149\u4e86</del>\n\n\u8fd9\u6b21\uff0c\u8349\u4e3b\u4e0d\u4ec5\u4eba\u6c14\u5927\u589e\uff0c\u800c\u4e14\u66f4\u56e0\u4e3a\u589e\u52a0\u4e86\u98de\u884c\u7cfb\u7684\u526f\u5c5e\u6027\u800c\u53d8\u5f97\u4e0d\u518d\u60b2\u50ac\u3002<del>\u96d6\u7136\u6700\u7d42\u9032\u5316\u5f8c\u5c31\u4e0d\u662f\u4e86</del>{{\u9ed1\u5e55|\u5728\u73a9\u5bb6\u4e4b\u95f4\uff0c\u81ea\u53e4\u6d41\u4f20\u7740\u8fd9\u6837\u4e00\u6761\u94c1\u5219\uff1a\u9009\u62e9\u706b\u4e3b\uff1a\u666e\u901a\u96be\u5ea6\u3002\u9009\u62e9\u6c34\u4e3b\uff1a\u56f0\u96be\u96be\u5ea6\u3002\u9009\u62e9\u8349\u4e3b\uff1a\u5730\u72f1\u96be\u5ea6\u2190\u7136\u800c\u521d\u4ee3\u5c0f\u521a\u7684\u5927\u5ca9\u86c7\u4e0d\u77e5\u846c\u9001\u4e86\u591a\u5c11\u9009\u62e9\u5c0f\u706b\u9f99\u7684\u65b0\u4eba\uff0c\u5b9d\u77f3\u7cfb\u5217\u76f8\u6bd4\u770b\u7a7f\u4e00\u5207\u7684\u6cbc\u8dc3\u9c7c\u4e00\u5bb6\u9009\u62e9\u706b\u9e21\u7684\u8bad\u7ec3\u5e08\u5219\u4f1a\u5728\u8d85\u80fd\u548c\u98de\u884c\u9053\u9986\u53d7\u632b\uff0c\u6240\u4ee5\u8fd9\u6761\u89c4\u5219\u4e5f\u5e76\u4e0d\u662f\u7edd\u5bf9\u7684\u3002\u5176\u5b9e\u8fd9\u5f88\u5927\u7a0b\u5ea6\u4e0a\u6e90\u4e8e\u53e3\u5996\u91cc\u6700\u5f31\uff08\u65e0\u8bef\uff09\u7684NPC\u6349\u866b\u5c0f\u5b50\u3002}}\u6e38\u620f\u524d\u671f\uff0c\u5f80\u5f80\u73a9\u5bb6\u8fd8\u6ca1\u53d1\u5c55\u597d\uff0c\u4f46\u5927\u91cf\u7684\u6349\u866b\u5c0f\u5b50\u5374\u6cdb\u6ee5\u5728\u8349\u4e1b\u548c\u68ee\u6797\u91cc\uff0c\u800c\u4e14\u73a9\u7684\u4e00\u624b\u597d\u51cf\u901f\uff0c\u76f4\u63a5\u5bfc\u81f4\u8349\u4e3b\u60b2\u50ac\u3002\u8fd9\u6b21\u6709\u4e86\u98de\u884c\u7cfb\u7684\u526f\u5c5e\u6027\uff0c\u5988\u5988\u518d\u4e5f\u4e0d\u6015\u6211\u7684\u8349\u4e3b\u88ab[[\u7eff\u6bdb\u866b\u5a18|\u7eff\u6bdb\u866b]]<del>\u5403\u6389</del>\u6253\u8d25\u5566\uff01\n\u968f\u7740\u589e\u52a0\u4e86\u98de\u884c\u7cfb\u7684\u526f\u5c5e\u6027\u968f\u4e4b\u800c\u6765\u7684\u53e6\u4e00\u4e2a\u95ee\u9898\u5c31\u662f\uff1a\u88ab\u51b0\u7cfb\u56db\u500d\u514b!\u800c\u60f3\u5bf9\u4ed8\u51b0\u7cfb\u6700\u5e38\u89c1\u7684\u5c31\u662f\u706b\u7cfb\u548c\u683c\u6597\u7cfb\u653b\u51fb\uff0c\u706b\u7cfb\u663e\u7136\u4e0d\u73b0\u5b9e\uff0c\u4f46\u8054\u7cfb\u7434\u68a8\u67ad\u7684\u67d0\u6bb5\u8bf4\u660e\u201c\u5728\u7a7a\u4e2d\u98de\u884c\u65f6\u4e1d\u6beb\u4e0d\u4f1a\u53d1\u51fa\u62cd\u52a8\u7fc5\u8180\u7684\u58f0\u54cd\u3002\u80fd\u5728\u4e0d\u88ab\u53d1\u73b0\u7684\u60c5\u51b5\u4e0b\u6162\u6162\u9760\u8fd1\u5bf9\u624b\u5e76\u4f7f\u51fa\u5f3a\u529b\u7684\u811a\u8e22\uff0c \u6216\u53d1\u5c04\u4e0e\u5243\u5200\u822c\u950b\u5229\u7684\u6811\u53f6\u878d\u4e3a\u4e00\u4f53\u7684\u7fbd\u6bdb\uff0c\u4ece\u8fdc\u8ddd\u79bb\u53d1\u52a8\u653b\u51fb\uff01\u201d\u7684\u8bdd... ...\u8bb8\u591a\u4eba\u8111\u4e2d\u6d6e\u73b0\u4e86\u4e00\u53ea<del>\u5144\u8d35</del>\u9e1f\u7a81\u7136\u51fa\u73b0\u5728\u4f60\u80cc\u540e\u5e76\u4f7f\u7528\u4e0b\u8e22\u7684\u753b\u9762\uff0c\u4f17\u4eba\u518d\u6b21\u53d1\u51fa<del>\u6740\u732a\u822c</del>\u7edd\u671b\u7684\u60e8\u53eb\u3002\u4f46\u8bf4\u4e0d\u5b9a\u662f\u50cf\u5947\u9c81\u8389\u5b89\u90a3\u6837\u7684\u53cc\u8fdb\u5316\u8def\u7ebf\uff1f<del>\uff08\u2190\u4f46\u53ea\u8981\u8001\u4efb\u4e0d\u540c\u610f\u8fd9\u5c31\u53ea\u662f\u81ea\u6211\u5b89\u6170\u7f62\u4e86\uff09</del>\n\n\u662f\u5426\u62e5\u6709\u51fa\u8272\u7684\u683c\u6597\u6280\u80fd\u5148\u4e0d\u8bba\uff0c\u5012\u50cf\u662f\u7ee7\u627f\u4e86\u4e0a\u4e00\u5c4a\u6c34\u4e3b\uff08XY\u7248\uff0c[[\u7532\u8d3a\u5fcd\u86d9\u5a18|\u7532\u8d3a\u5fcd\u86d9]]\uff09\uff0c\u6709\u4e86\u65b0\u7684\u672c\u7cfb\uff08\u597d\u5427\u5fcd\u86d9\u7684\u8bdd\u7531\u4e8e\u68a6\u7279\u53d8\u5e7b\u81ea\u5728\uff0c\u7528\u4ec0\u4e48\u90fd\u662f\u672c\u7cfb\uff09\u6280\u80fd\uff1a\u6811\u53f6\u3002\u76ee\u524d\u53ef\u4ee5\u4e60\u5f97\u201c\u6811\u53f6\u201d\u8fd9\u4e00\u6280\u80fd\u7684\u53ea\u6709\u7434\u68a8\u67ad\u9171\uff0c\u4e0d\u8fc7\u4e5f\u5c31\u662f\u548c\u706b\u82b1\u6c34\u67aa\u4e00\u6837\u768440\u5a01\u529b\u521d\u59cb\u6280\u80fd\u3002\n\n\u9690\u85cf\u7279\u6027\u4e5f\u662f\u4ec5\u6b64\u4e00\u5bb6\u7684\u65b0\u7279\u6027\u201c\u8fdc\u9694\u201d\uff0c\u4f7f\u6240\u6709\u8fd1\u8eab\u6280\u80fd\u5168\u90e8\u53d8\u4e3a\u975e\u8fd1\u8eab\u6280\u80fd<s>\u8c8c\u4f3c\u6ca1\u5565\u5375\u7528</s>\u3002\n\n\u7531\u4e8e\u7cbe\u7075\u5b9d\u53ef\u68a6\u8fd9\u6b21\u7684\u65b0\u7248\u540d\u4e3a\u201c\u592a\u9633\uff08SUN\uff09\u201d\u548c\u201c\u6708\u4eae\uff08MOON\uff09\u201d\uff0c\u6240\u4ee5\u4e5f\u88ab\u79f0\u4e3a\u201c\u65e5{{\u9ed1\u5e55|\uff08S\uff09}}\u201d\u548c\u201c\u6708{{\u9ed1\u5e55|\uff08M\uff09}}\u201d\u3002\u8349\u7cfb\u548c\u98de\u884c\u7cfb\u7684\u7ec4\u5408\u4e5f\u88ab\u7b80\u79f0\u4e3a\u201c\u8349\u98de\u201d\u3002\u4e8b\u5b9e\u4e0a\u672c\u6765\u662f\u6ca1\u6709\u4ec0\u4e48\u4e0d\u59a5\u7684{{\u9ed1\u5e55|\uff08\u9ed1\u5e55\u9664\u5916\uff09}}\uff0c\u4f46\u81ea\u4ece\u5fa1\u4e09\u5bb6\u8d44\u6599\u66dd\u5149\u540e\u8d34\u5427\u4e0a\u5c31\u51fa\u73b0\u4e86\u5404\u79cd<del>\u4e0d\u582a\u5165\u76ee\u7684</del>\u5237\u5c4f\u3002\n{{\u9ed1\u5e55|\u201c\u6211\u8981\u65e5\u5357\u5c0f\u9e1f\uff01\u201d\u201c\u6211\u8981S/M\u5357\u5c0f\u9e1f\uff01\u201d\u201c\u6211\u8981\u8349\u98de\u5357\u5c0f\u9e1f\uff01\u201d}}\n\u5bf9\u6b64\u6211\u5b9e\u5728\u8868\u793a#\uff08\u6342\u8138\uff09\n\n\u5728\u5176\u8d44\u6599\u4e2d\uff0c\u6709\u4e00\u6761\u4f3c\u4e4e\u