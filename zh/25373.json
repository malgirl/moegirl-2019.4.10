f\u3002\nX\u670812\u65e5\uff0c\u4e00\u540dX\u59d3\u516d\u65ec\u8001\u6c49\u5728\u91cd\u5e86\u6768\u5bb6\u576a\u67d0\u8bc1\u5238\u8425\u4e1a\u90e8\u731d\u6b7b\u3002\u9001\u533b\u9662\u4e0d\u6cbb\u8eab\u4ea1\uff0c\u5728\u5012\u5730\u65f6\uff0c\u53e3\u91cc\u5728\u4e0d\u505c\u5730\u8bf4\u201c25\u4e07\u53d8\u62105\u4e07\u4e86\uff01\u201d</poem>\n<h4><center>\u80a1\u7968\u662f\u628a\u6740\u4eba\u4e0d\u89c1\u8840\u7684\u5200\uff0c\u6211\u4eec\u5982\u679c\u4f1a\u800d\u5200\u90a3\u4e48\u5c31\u53ef\u4ee5\u7528\u5b83\u6765\u6740\u4eba\uff0c\u5982\u679c\u4e0d\u4f1a\u800d\u5200\uff0c\u90a3\u4e48\u5c31\u4f1a\u800d\u4e86\u81ea\u5df1</center></h4>\n<poem>X\u670821\u65e5\uff0c\u5b81\u6ce2\u4e1c\u5357\u5546\u62a5\u62a5\u9053\uff0c\u5b81\u6ce2\u6709\u4e00\u7537\u5b50\u56e0\u80a1\u7968\u4e8f\u635f\uff0c\u53d1\u751f\u7cbe\u795e\u9519\u4e71\uff0c\u62e8\u6253110\u62a5\u8b66\u8981\u6c42\u8b66\u5bdf\u6551\u5e02\uff0c\u8b66\u5bdf\u54ed\u7b11\u4e0d\u5f97\u3002\nX\u670812\u65e5\uff0c\u6d1b\u9633\u4e00\u540d\u5458\u5de5\u7092\u80a1\u5de8\u4e8f\uff0c\u7a81\u7136\u4ece\u5382\u533a\u51856\u5c42\u5bbf\u820d\u697c\u8df3\u4e0b\u6b7b\u4ea1\uff0c\u6b7b\u800525\u5c81\u5de6\u53f3\u3002\nX\u67081\u65e5\uff0c\u611a\u4eba\u8282\uff0c\u4f46\u4eca\u5929\u53d1\u751f\u7684\u4e8b\u60c5\u5374\u4e0d\u662f\u73a9\u7b11\u3002\u4eca\u5929\u4e0b\u5348\u4e24\u70b9\u534a\uff0c\u5728\u9f50\u9c81\u8bc1\u5238\u5a01\u6d77\u6ee8\u6d77\u5317\u8def\u8425\u4e1a\u90e8\u91cc\u7684\u7ea6300\u591a\u4eba\u96c6\u4f53\u5931\u58f0\u75db\u54ed\u3002\u8d77\u5148\u53ea\u662f\u51e0\u4e2a\u4eba\u6389\u773c\u6cea\uff0c\u54ed\u7740\u54ed\u7740\u5c31\u54ed\u51fa\u4e86\u58f0\u97f3\uff0c\u8fd9\u58f0\u97f3\u77ac\u95f4\u5c31\u611f\u67d3\u4e86\u80a1\u7968\u4ea4\u6613\u5927\u5385\u91cc\u7684\u6240\u6709\u4eba\uff0c\u4e8e\u662f\u8fd9\u4e09\u767e\u591a\u7537\u5973\u8001\u5c11\uff0c\u4e00\u8d77\u75db\u54ed\u4e86\u2026\u2026\nX\u670830\u65e5\uff0c\u5c71\u4e1c\u5fb7\u5dde\u4e00\u540dX\u59d3\u80a1\u6c11\u56e0\u80a1\u7968\u66b4\u8dcc\u800c\u8df3\u697c\u3002\nX\u670823\u65e5\uff0c\u7092\u80a1\u8df3\u697c\u91cd\u5e86\u4e00\u5973\u80a1\u6c11\u5356\u4e86\u623f\u5b50\u7092\u80a1\u4e8f\u635f35\u4e07\u5143\uff0c\u8be5\u540d\u5987\u5973\u8bf4\u8981\u4e0a\u5395\u6240\uff0c\u6ca1\u60f3\u5230\u518d\u6b21\u51c6\u5907\u8df3\u697c\u81ea\u6740\uff0c\u5728\u5916\u9762\u5b88\u7740\u7684\u6c11\u8b66\u7acb\u523b\u51b2\u4e86\u8fdb\u53bb\uff0c\u628a\u5979\u67b6\u56de\u4e86\u5927\u5385\u3002\nX\u670825\u65e5\uff0c\u7092\u80a1\u8df3\u697c\u53a6\u95e8\u5973\u80a1\u6c11\u53d1\u75af\uff0c\u4e3e\u5200\u780d\u4eba\uff1b\u2014\u2014\u201c\u597d\u597d\u7684\u5bb6\uff0c\u90fd\u662f\u88ab\u80a1\u7968\u7ed9\u5bb3\u7684\uff01\u201d\nX\u6708\u5e95\uff0c\u7092\u80a1\u8df3\u697c\u4e0a\u6d77\u4e00\u5bb6\u5916\u4f01\u7684X\u59d3\u4e2d\u5c42\u7ba1\u7406\u804c\u5458\u56e0\u7092\u80a1\u5de8\u4e8f\uff0c\u670d\u5b89\u7720\u836f\u81ea\u6740\u3002\u5bb6\u4f4f\u6768\u6d66\u533a\u8425\u53e3\u8def\u7684X\u963f\u5a46\u4eca\u5e7467\u5c81\uff0c\u201c\u80a1\u9f84\u201d\u5df2\u6709\u597d\u51e0\u5e74\uff0c\u201c\u8001\u4eba\u6295\u4e8620\u4e07\u4f59\u5143\u5728\u80a1\u5e02\u91cc\uff0c\u6700\u8fd1\u80a1\u5e02\u63a5\u8fde\u4e0b\u8dcc\uff0c\u8d44\u91d1\u88ab\u8170\u65a9\uff0c\u542c\u8bf4\u76ee\u524d\u53ea\u5269\u4e0b9\u4e07\u5143\u5de6\u53f3\u4e86\u3002\u201d \u4e00\u540d\u90bb\u5c45\u731c\u6d4b\uff0c\u6628\u65e5\u7684\u5927\u8dcc\u8ba9\u8001\u4eba\u539f\u672c\u4f4e\u843d\u7684\u5fc3\u60c5\u96ea\u4e0a\u52a0\u971c\uff0c\u5bfc\u81f4\u8001\u4eba\u72af\u75c5\u6655\u5012\u6b7b\u4ea1\u3002\n1\u65e5\uff0c\u6c5f\u82cf\u4e00\u540dX\u59d3\u80a1\u6c11\u5728\u5f53\u65e5\u80a1\u7968\u66b4\u8dcc\u540e\u7cbe\u795e\u5931\u5e38\uff0c\u5f53\u665a\u8df3\u697c\u81ea\u6740\u3002\n21\u65e5\u51cc\u6668\uff0c\u676d\u5dde\u65b9\u6b63\u8bc1\u5238\uff08\u884c\u60c5\u7814\u62a5\uff09\u5206\u6790\u5e08XX\u4e5f\u8df3\u697c\u4e86\u3002\u4ed6\u9009\u62e9\u4e86\u4ece\u81ea\u5bb6\u7684\u697c\u4e0a\u7eb5\u8eab\u8dc3\u4e0b\uff0c\u7559\u4e0b\u4e86\u4e00\u4e2a\u5e74\u5e7c\u7684\u513f\u5b50\uff0c\u540d\u6c14\u4e0d\u5927\uff0c\u4ed6\u5230\u6b7b\u6ca1\u6709\u5f15\u8d77\u592a\u591a\u7684\u5173\u6ce8\u3002\u4f46\u6070\u6070\u662f\u56e0\u4e3a\u4ed6\u5f88\u666e\u901a\uff0c\u53ea\u662f\u8bc1\u5238\u516c\u53f8\u7684\u4e00\u4e2a\u5c0f\u89d2\u8272\uff0c\u56e0\u6b64\u66f4\u663e\u5f97\u80a1\u5e02\u51f6\u9669\u3002</poem>\n<h4><center>\u4ece\u4f17\u6708\u520a\u7f16\u8f91\u90e8\u7adf\u51fa\u547d\u6848</center></h4>\n<poem>2017\u5e74X\u6708X\u65e5\u5728\u4ece\u4f17\u6708\u520a\u7684\u7f16\u8f91\u90e8\u53d1\u73b0\u4e00\u5177\u5c38\u4f53\uff0c\u7ecf\u67e5\u8bc1\u6b64\u4eba\u662f\u8d1f\u8d23\u7f8e\u5de5\u7684\u5458\u5de5\u201c{{\u9ed1\u5e55|\u963f\u64b8}}\u201d\u3002\n\u88ab\u5bb3\u4eba\u88ab\u53d1\u73b0\u65f6\u9762\u90e8\u671d\u4e0b\u8db4\u5728\u529e\u516c\u684c\u4e0a\u3002\n\u6cd5\u533b\u58f0\u660e\u88ab\u5bb3\u4eba\u662f[[\u7206\u809d]]\u800c\u6b7b\u3002\n\u4f46\u88ab\u5bb3\u4eba\u80cc\u540e\u7684\u5899\u4e0a\u7531\u88ab\u5bb3\u4eba\u5199\u4e0b\u4e86\u4e09\u5343\u5b57\u7684\u8840\u4e66\u3002\n\u58f0\u79f0\u201c\u6211\u662f\u88ab\u4eba\u5bb3\u6b7b\u7684\uff01\u201d</poem>}}\n\n{{Toggle\n|id=1\n|button= <center><span style=\"font-size:100%\">\n<table style=\"width:100%\">\n<tr>\n  <td style=\"width:80px\"><br />\u4e94\u6708\u53f7</td>\n  <td style=\"text-align:center;font-size:30px\">'''\u4ece \u4f17 \u6708 \u520a'''</td>\n  <td style=\"width:80px;font-size:6px\">\u7f16\u8f91 \u7edb\u821e\u4e71\u4e38<br/>\u7f8e\u5de5 \u963f\u64b8<br/>\u6392\u7248 \u67ad\u76ee</td>\n</tr>\n</table>\n------  </span></center>\n|content=\n<h3><center><big>'''\u7537\u4eba\u770b\u4e86\u6c89\u9ed8\u5973\u4eba\u770b\u4e86\u6d41\u6cea'''</big> </center></h3>\n<h4><center>\u80a1\u7968\u662f\u628a\u6740\u4eba\u4e0d\u89c1\u8840\u7684\u5200</center></h4>\n<poem>1\u3001\u9f13\u80c0\u7684\u94b1\u5305\u5c31\u50cf\u9f13\u80c0\u7684\u8180\u80f1\uff0c\u603b\u662f\u4f1a\u6709\u5c3d\u5feb\u6e05\u7a7a\u7684\u51b2\u52a8\u3002\n2\u3001\u5bf9\u4f01\u4e1a\u548c\u9000\u4f11\u8005\u800c\u8a00\uff0c\u73b0\u5728\u6709\u5f88\u591a\u94b1\u5c45\u7136\u4e5f\u6210\u4e86\u4e00\u4e2a\u95ee\u9898\uff08\u65e0\u5904\u6295\u8d44\uff09\uff0c\u53ea\u8d5a0.25%\u7684\u5229\u606f\u51e0\u4e4e\u5c31\u548c\u4e8f\u635f0.25%\u4e00\u6837\u8ba9\u4eba\u75db\u82e6\u3002\n3\u3001\u7ba1\u7406\u5c42\u4e0d\u6b63\u76f4\u7684\u4e0a\u5e02\u516c\u53f8\u5c31\u662f\u4e0b\u4e09\u6ee5\u3002\n4\u3001\u5982\u679c\u96c7\u4f63\u4e13\u4e1a\u6295\u8d44\u7ecf\u7406\uff0c\u4ed6\u4eec\u4e00\u5b9a\u4f1a\u8ba9\u4f60\u7684\u56de\u62a5\u843d\u540e\u4e8e\u5e02\u573a\uff0c\u534e\u5c14\u8857\u63a8\u9500\u5458\u7684\u8868\u73b0\u90fd\u6bd4\u4e13\u4e1a\u6295\u8d44\u8005\u597d\u5f97\u591a\u3002\n5\u3001\u4f01\u4e1a\u54c1\u8d28\u4e0e\u4eba\u5458\u7d20\u8d28\u6bd4\u5c3d\u804c\u8c03\u67e5\u91cd\u8981\u5f97\u591a\uff0c\u6311\u4eba\u7684\u773c\u5149\u6bd4\u6311\u516c\u53f8\u66f4\u51c6\u786e\u3002\n6\u3001\u5bf9\u4e2d\u56fd\u7684\u957f\u8fdc\u53d1\u5c55\u6709\u4fe1\u5fc3\u3002\n\n\u80a1\u7968\u8dcc\u505c\u4e86\uff0c\u4f60\u53ef\u4ee5\u9009\u62e9\u6253\u4e00\u4e2a\u7535\u8bdd\uff0c\u4f60\u662f\u8981\u6253\u7ed9110\u8fd8\u662f120\uff1f\n\u4e0a\u5c71\u722c\u5761\u7f13\u6162\u8d70\uff0c\u70d8\u4e91\u6258\u6708\u662f\u5c0f\u725b\u3002\n\u6295\u8d44\u8005\u91cd\u7ea2\u5229\uff0c\u6295\u673a\u8005\u91cd\u6d88\u606f\u3002\n\u5468k\u7ebf\u8d70\u51fa\u7b2c\u4e09\u6b21\u542b\u4e49\uff0c\u4e70\u5165\u6b64\u80a1\u5c31\u53d1\u5bb6\u3002\n\u5e95\u90e8\u8df3\u7a7a\u5411\u4e0a\u8d70\uff0c\u5929\u6253\u96f7\u5288\u4e0d\u653e\u624b\u3002\n\u5927\u725b\u53d8\u75af\u725b\uff0c\u5929\u91cf\u5230\u4e86\u5934\u3002\n\u80a1\u5e02\u4e0b\u8dcc\u65f6\u6210\u4ea4\u91cf\u53ef\u4ee5\u4e0d\u7528\u653e\u5927\uff0c\u80a1\u5e02\u4e0a\u6da8\u65f6\u6210\u4ea4\u91cf\u5fc5\u987b\u653e\u5927\u3002\n\u9ad8\u4f4d\u8df3\u7a7a\u5411\u4e0a\u8d70\uff0c\u795e\u4ed9\u62db\u624b\u5374\u4e0d\u7559\u3002\n\u4e70\u5165\u524d\u8981\u5c0f\u5fc3\u6c42\u8bc1\uff0c\u4e70\u5165\u65f6\u8981\u4e09\u601d\u800c\u884c\u3002\n\u8d62\u5229\u624d\u662f\u786c\u9053\u7406\uff0c\u4e0d\u8bba\u7ee9\u4f18\u7ee9\u5dee\uff0c\u4e0d\u8bba\u6295\u8d44\u6295\u673a\uff0c\u4e0d\u8bba\u957f\u7ebf\u77ed\u7ebf\uff0c\u53ea\u8981\u80fd\u8d62\u5229\u5c31\u610f\u5473\u7740\u6295\u8d44\u6210\u529f\u3002</poem>\n<h4><center>\u7f16\u8f91\u90e8\u547d\u6848\u7ed3\u6848\u51f6\u624b\u65e0\u7f6a\u91ca\u653e </center></h4>\n<poem>\u7ecf\u67e5\u88ab\u5bb3\u4eba\u5f53\u5929\u672c\u5e94\u8be5\u6309\u65f6\u4e0b\u73ed\n\u4f46\u56e0\u6392\u7248\u5458\u5de5\u201c{{\u9ed1\u5e55|\u67ad\u76ee}}\u201d\u547d\u4ed6\u6539\u7a3f\u624d\u917f\u6210\u60b2\u5267\n\u8b66\u5bdf\u5c06\u5acc\u7591\u4eba\u902e\u6355\u540e\u56e0\u8bc1\u636e\u4e0d\u660e\u786e\u540e\u91ca\u653e\u5acc\u7591\u4eba</poem>}}\n\n== \u6b4c\u8bcd ==\n{{Vocaloid Songbox Introduction\n|lbgcolor = #FF7799\n|ltcolor = #9B5CAF\n|group1 = \u4f5c\u7f16\u66f2<br />\u4f5c\u8bcd<br/>\u8c03\u6821<br />\u6df7\u97f3\n|list1 = [[\u7edb\u821e\u4e71\u4e38]]\n|group2 = \u66f2\u7ed8\n|list2 = \u963f\u64b8\uff08\u5e7b\u6653\u4f0a\u7248\uff09<br/>TIKO\uff08\u58a8\u6e05\u5f26\u7248\uff09\n|group3 = PV\n|list3 = \u67ad\u76eemoku\n|group4 = \u6f14\u5531\n|list4 = [[\u5e7b\u6653\u4f0a]]\uff0f[[\u58a8\u6e05\u5f26]]\n|group5 = \u51fa\u54c1\n|list5 = HEROAR\n}}\n[[File:\u5982\u610f\u7b97\u76d8\u58a8\u6e05\u5f26.jpg|300px|thumb|right|\u58a8\u6e05\u5f26\u7248\u66f2\u7ed8 by TIKO]]\n<poem>\n\n\u7ea2\u7eff\u8272\u7684\u53cc\u7ebf\u3000\u6709\u65f6\u5019\u5f88\u7f8e\n\u6709\u65f6\u5019\u4f34\u968f\u7740\u7a7a\u60f3\u6ecb\u5473\n\u554a\u3000\u628a\u70ed\u70b9\u53d8\u6210\u788e\u7247\n\n\u88ab\u505a\u7a7a\u7684\u611f\u89c9\u3000\u88ab\u5957\u7262\u7684\u65f6\u95f4\n\u968f\u65f6\u5c06\u6765\u4e34\u7684\u5929\u584c\u5730\u9677\n\u554a\u3000\u524d\u4ec6\u540e\u7ee7\u7edc\u7ece\u4e0d\u7edd\n\n\u6211\u4eec\u7b97\u8fc7\u7684\u7b97\u8fc7\u7684\u3000\u73b0\u5728\u5462\uff1f\u7b97\u4ec0\u4e48\uff1f\n\u7b97\u591a\u4e86\u7b97\u5c11\u4e86\u3000\u5982\u679c\u4ed8\u4e4b\u4e00\u70ac\u8fd8\u5269\u4ec0\u4e48\n\u7b97\u65e9\u7684\u7b97\u665a\u7684\u3000\u535a\u5f08\u7684\u7b79\u7801\u4e0a\u8fd8\u6709\u6ca1\u6709\u6