3063\u305f10\u6587\u5b57\u306e\u81ea\u5206\u306e\u540d\u3067\u3055\u3048\u3000\u4eca\u306e\u72b6\u614b\u3067\u306f\u624b\u653e\u3057\u3066\u3057\u307e\u3044\u305d\u3046\u3060\u3088}}|\u5c31\u8fde\u53ea\u670910\u4e2a\u5b57\u7b26\u7684\u81ea\u5df1\u7684\u540d\u5b57 \u5728\u8fd9\u4e2a\u72b6\u6001\u4e0b\u4e5f\u5feb\u8981\u6293\u4e0d\u4f4f\u4e86}}\uff0c\uff08\u5341\u4e2a\u5b57\u7b26\u6307\u5176\u539f\u540d\u201c\u62dc\u9686\u00b7\u963f\u514b\u96f7\u5fb7\u201d\u3002\uff09<br/>\n{{ruby|{{lj|1\u756a\u76ee\u306e\u3042\u306e\u5b50\u306f\u3000\u7121\u4e8b\u306b\u5e30\u308c\u305f\u306e\u3060\u308d\u3046\u304b}}|\u7b2c\u4e00\u4f4d\u7684\u90a3\u4e2a\u5b69\u5b50\uff0c\u80fd\u5e73\u5b89\u5730\u56de\u6765\u5417}}\u3002\uff08\u539f\u4f5c\u4e2d\u5c06[[CNo.6 \u5148\u53f2\u9057\u4ea7 \u6df7\u6c8c\u5927\u897f\u6d32\u5de8\u4eba]]\u8d50\u4e88\u5c0f\u513f\u5b50[[\u7c73\u6b47\u5c14\u00b7\u963f\u514b\u96f7\u5fb7|\u2162]]\uff0c\u56e0\u4e3a\u529b\u91cf\u8fc7\u4e8e\u5f3a\u5927\uff0c\u5631\u5490\u5176\u8c28\u614e\u4f7f\u7528\u3002\uff09<br/>\n{{ruby|{{lj|\u3042\u308c\u306f\u5b9f\u306b\u5f93\u9806\u3060\u304b\u3089\u3000\u4f7f\u3044\u6368\u3066\u306b\u3057\u3066\u3082\u3044\u3044\u304b\u306a\u3093\u3066\u8003\u3048\u3066\u3044\u305f\u3000\uff08\u306f\u306f\u3001\u307e\u3055\u304b\uff09}}|\u4ed6\u5b9e\u5728\u662f\u592a\u8fc7\u4e8e\u987a\u4ece\uff0c\u5e72\u8106\u5229\u7528\u5b8c\u5c31\u4e22\u6389\u597d\u4e86\uff08\u54c8\u54c8\uff0c\u5f00\u73a9\u7b11\u7684\uff09}}\uff0c\uff08\u539f\u4f5c\u4e2d\u79f0\u547c\u4ed6\u4e3a\u201c\u5584\u826f\u7684\u5b69\u5b50\u201d\u3002\uff09<br/>\n{{ruby|{{lj|2\u756a\u76ee\u306e\u3042\u306e\u5b50\u306f\u3000\u3075\u3066\u304f\u3055\u308c\u3066\u306f\u3044\u306a\u3044\u3060\u308d\u3046\u304b}}|\u7b2c\u4e8c\u4f4d\u7684\u90a3\u4e2a\u5b69\u5b50\uff0c\u662f\u4e0d\u662f\u5728\u95f9\u7740\u522b\u626d\u5462}}\uff0c\uff08<del>\u539f\u4f5c\u4e2d[[\u6258\u9a6c\u65af\u00b7\u963f\u514b\u96f7\u5fb7|\u2163]]\u5c31\u662f\u718a\u5b69\u5b50\u8868\u73b0\u3002</del>\uff09<br/>\n{{ruby|{{lj|\u305d\u308c\u306b\u64cd\u308a\u7cf8\u3092\u3064\u3051\u3066\u3000\u6a2a\u3067\u60aa\u3092\u6f14\u3058\u3055\u305b\u3088\u3046\u306a\u3093\u3066\u545f\u3044\u3066\u305f\u3000\uff08\u306f\u306f\u3001\u307e\u3055\u304b\uff09}}|\u7ed9\u4ed6\u7cfb\u4e0a\u6728\u5076\u63d0\u7ebf\uff0c\u6559\u5506\u4ed6\u53bb\u4e0a\u6f14\u4e00\u4e2a\u6076\u4eba\u7684\u89d2\u8272\u5427\uff08\u54c8\u54c8\uff0c\u5f00\u73a9\u7b11\u7684\uff09}}\uff0c\uff08\u539f\u4f5c\u4e2d[[\u6258\u9a6c\u65af\u00b7\u963f\u514b\u96f7\u5fb7|\u2163]]\u4f7f\u7528\u201c\u673a\u5173\u4eba\u5076\u201d\u5361\u7ec4\uff0c\u5e76\u4e14\u6559\u5506[[\u6258\u9a6c\u65af\u00b7\u963f\u514b\u96f7\u5fb7|\u2163]]\u9677\u5bb3[[\u795e\u4ee3\u51cc\u7259|\u51cc\u7259]]\u3002<del>\u56e0\u4e3a\u8ba4\u4e3a[[\u6258\u9a6c\u65af\u00b7\u963f\u514b\u96f7\u5fb7|\u2163]]\u53e3\u98ce\u677e\uff0c\u53ea\u9002\u5408\u5e72\u8fd9\u4e2a\u3002</del>\uff09<br/>\n{{ruby|{{lj|3\u756a\u76ee\u306e\u3042\u306e\u5b50\u306f\u3000\u6ce3\u304d\u3058\u3083\u304f\u3063\u3066\u306f\u3044\u306a\u3044\u3060\u308d\u3046\u304b}}|\u7b2c\u4e09\u4f4d\u7684\u90a3\u4e2a\u5b69\u5b50\uff0c\u662f\u5426\u5728\u4f4e\u58f0\u62bd\u6ce3\u7740\u5462}}\uff0c\uff08\u539f\u4f5c\u4e2d[[\u514b\u91cc\u65af\u591a\u5f17\u00b7\u963f\u514b\u96f7\u5fb7|\u2164]]\u56e0\u4e3a\u7236\u4eb2\u5931\u8e2a\uff0c\u4e0d\u5f97\u4e0d\u5c06\u4e24\u4e2a\u5f1f\u5f1f\u9001\u5f80\u5b64\u513f\u9662\u3002\uff09<br/>\n{{ruby|{{lj|\uff08\u307b\u3089\u3001\u3082\u3046\u540d\u524d\u3082\u98df\u3055\u308c\u305f\u3088\u3046\u3067\u3000\u9854\u3092\u898b\u3066\u3082\u3000\u547c\u3093\u3067\u3084\u308c\u306a\u3044\u3000\u529b\u306a\u304f\u7b11\u3063\u305f\uff09}}|\uff08\u4e0d\u8fc7\uff0c\u5df2\u7ecf\u88ab\u5403\u6389\u4e86\u540d\u5b57\u7684\u4ed6\u4eec\uff0c\u5373\u4f7f\u89c1\u9762\u4e5f\u65e0\u6cd5\u4e92\u76f8\u547c\u5524\u7684\u3002\u6211\u65e0\u529b\u5730\u7b11\u9053\uff09}}\uff0c\uff08\u4e3a\u4e86\u590d\u4ec7\uff0c\u5168\u5bb6\u4eba\u820d\u5f03\u4e86\u539f\u6765\u7684\u540d\u5b57\uff0c\u513f\u5b50\u7684\u79f0\u547c\u5168\u90e8\u6539\u4e3a\u6570\u5b57\u4ee3\u53f7\u3002<del>\u201c\u6570\u5b57\u5bb6\u201d\u7684\u540d\u5934\u7684\u7531\u6765</del>\uff09<br/>\n{{ruby|{{lj|\uff08\u305f\u3060\u3044\u307e\uff09}}|\uff08\u6211\u56de\u6765\u4e86\uff09}}\uff08\u6700\u540e[[\u4e5d\u5341\u4e5d\u6e38\u9a6c|\u6e38\u9a6c]]\u6218\u80dc\u4e86\u7389\u5ea7\uff0c\u7389\u5ea7\u7684\u4ec7\u6068\u4e5f\u5f97\u4ee5\u51b0\u91ca\u3002\uff09\u3002\n</poem>\n{{clear}}\n*'''\u304f\u308b\u308a\u3093\u3054P\u540c\u65f6\u4e5f\u4ee5\u7389\u5ea7\u4e00\u5bb6\u4e3a\u539f\u578b\u521b\u4f5c\u4e86\u300a\u3068\u3042\u308b\u4e00\u5bb6\u306e\u5fa1\u8336\u4f1a\u8b70\u300b\u3002'''\n<poem>\n{{ruby|{{lj|\u3057\u3044\u30a2\u30d7\u30ea\u30b3\u30c3\u30c8\u30c6\u30a3\u30fc}}|\u4e0d\u7528\u62c5\u5fc3\u5462\u8fd9\u6bd4\u8c01\u90fd\u66f4\u8981\u6e29\u67d4\u7684\u674f\u4ec1\u8336}}\uff0c\uff08\u5c0f\u513f\u5b50[[\u7c73\u6b47\u5c14\u00b7\u963f\u514b\u96f7\u5fb7|\u2162]]\u7684\u4ee3\u8868\u8272\u4e4b\u4e00\u3002\uff09<br/>\n{{ruby|{{lj|\u5927\u4e08\u592b\u3060\u3088\u3001\u3082\u3046\u8ab0\u3082\u5f7c\u3082\u50b7\u3064\u3051\u305f\u308a\u3060\u306a\u3093\u3066\u306d\u3057\u306a\u3044\u304b\u3089}}|\u6ca1\u95ee\u9898\u7684\u54e6 \u56e0\u4e3a\u4e0d\u8bba\u8c01\u90fd\u518d\u4e0d\u4f1a\u505a\u51fa\u4f24\u5bb3\u522b\u4eba\u7684\u4e8b\u5462}}\uff0c\uff08\u539f\u4f5c\u4e2d\u6d3e\u9063\u5176\u4f7f\u7528[[CNo.6 \u5148\u53f2\u9057\u4ea7 \u6df7\u6c8c\u5927\u897f\u6d32\u5de8\u4eba]]\u53bb\u6253\u8d25[[\u4e5d\u5341\u4e5d\u6e38\u9a6c|\u6e38\u9a6c]]\uff0c\u4f46\u56e0\u4e3a\u5931\u8d25\u88ab\u5265\u593a\u4e86\u7075\u9b42\u3002\uff09<br/>\n{{ruby|{{lj|\u89d2\u7802\u7cd6\uff13\u3064\u30ab\u30c3\u30d7\u306b\u6295\u3052\u5165\u308c\u304f\u308b\u304f\u308b\u5efb\u3059}}|\u5c06\u4e09\u7247\u65b9\u7cd6\u653e\u8fdb\u676f\u4e2d\u6405\u4f34}}\uff0c\uff08\u5c0f\u513f\u5b50[[\u7c73\u6b47\u5c14\u00b7\u963f\u514b\u96f7\u5fb7|\u2162]]\u7684\u4ee3\u53f7\u3002\uff09<br/>\n{{ruby|{{lj|\u9280\u306e\u3055\u3058\u3067\u30bd\u30fc\u30b5\u30fc\u53e9\u3051\u3070\u53e4\u4ee3\u9b5a\u3068\u3001\u6d77\u306e\u5e95\u306e\u907a\u8de1\u3078\u30c8\u30ea\u30c3\u30d7\uff01}}|\u7528\u94f6\u5319\u53bb\u6572\u6572\u8336\u6258\u7684\u8bdd\u5c31\u4f1a\u4e0e\u53e4\u4ee3\u9c7c\uff0c\u4e00\u8d77\u5230\u6d77\u5e95\u7684\u9057\u8ff9\u65c5\u884c}}\uff01\uff08\u539f\u4f5c\u4e2d[[\u7c73\u6b47\u5c14\u00b7\u963f\u514b\u96f7\u5fb7|\u2162]]\u4f7f\u7528\u4e86\u539f\u521b\u5361\u7247\uff0c\u573a\u5730\u9b54\u6cd5\u6df1\u6d77\u738b\u56fd-\u6c89\u6ca1\u7684\u56fd\u5ea6\u3002\uff09<br/>\n{{ruby|{{lj|\u5730\u306b\u3064\u304b\u306a\u3044\u8db3\u3001\u6c17\u5206\u304c\u826f\u3044\u3068\u306f\u3068\u3066\u3082\u8a00\u3048\u306a\u3044\u306a}}|\u89e6\u4e0d\u5230\u5730\u9762\u7684\u53cc\u811a\uff0c\u5b9e\u5728\u8bf4\u4e0d\u4e0a\u4ee4\u4eba\u611f\u89c9\u826f\u597d\u5462}}\uff0c\uff08\u56e0\u4e3a\u5728\u5f02\u4e16\u754c\u6e38\u8361\uff0c\u8eab\u4f53\u53d1\u751f\u53d8\u5316\u800c\u53d8\u4e3a\u513f\u7ae5\u5f62\u6001\u3002\uff09<br/>\n{{ruby|{{lj|\u305a\u3063\u3068\u30a6\u30a3\u30ea\u30a2\u30e0\u3068\u30b8\u30e7\u30bb\u30d5\u304c\u5275\u308a\u3042\u3052\u305f\u540d\u4f5c\u304c\u3001\u30c6\u30ec\u30d3\u306e\u4e2d\u30a8\u30f3\u30c9\u30ec\u30b9\u30eb\u30fc\u30d7\u518d\u751f}}|\u5a01\u5ec9\u548c\u7ea6\u745f\u6240\u4f5c\u7684\u90a3\u540d\u4f5c\uff0c\u4e0d\u65ad\u5730\u5728\u7535\u89c6\u65e0\u5c3d\u91cd\u64ad\u7740}}\uff0c\uff08\u7389\u5ea7\u6240\u770b\u7684\u52a8\u753b\u7247\u4e4b\u4e00\uff0c\u6839\u636e\u753b\u9762\u5e94\u8be5\u634f\u4ed6\u81ea\u300a\u732b\u548c\u8001\u9f20\u300b\u3002\uff09<br/>\n{{ruby|{{lj|\u5fc3\u914d\u3057\u3066\u304f\u308c\u308b\u306e\u53cd\u6297\u30e2\u30fc\u30c9\u306e\u30ec\u30e2\u30f3\u30ad\u30e3\u30f3\u30c7\u30a3\u30fc}}|\u53eb\u4eba\u62c5\u5fc3\u7684\u53cd\u6297\u6a21\u5f0f\u7684\u67e0\u6aac\u8336}}\uff0c\uff08\u4e8c\u513f\u5b50[[\u6258\u9a6c\u65af\u00b7\u963f\u514b\u96f7\u5fb7|\u2163]]\u7684\u4ee3\u8868\u8272\u4e4b\u4e00\u3002\uff09<br/>\n{{ruby|{{lj|\u5927\u4e08\u592b\u3060\u3088\u3001\u3082\u3046\u8ab0\u3082\u5f7c\u3082\u9665\u308c\u305f\u308a\u3060\u306a\u3093\u3066\u3057\u306a\u3044\u304b\u3089}}|\u6ca1\u95ee\u9898\u7684\u54e6\uff0c\u56e0\u4e3a\u8c01\u4e5f\u90fd\u518d\u4e0d\u4f1a\u505a\u9677\u5bb3\u522b\u4eba\u7684\u4e8b\u5462}}\uff0c\uff08\u6559\u5506[[\u6258\u9a6c\u65af\u00b7\u963f\u514b\u96f7\u5fb7|\u2163]]\u9677\u5bb3[[\u795e\u4ee3\u51cc\u7259|\u51cc\u7259]]\u3002\uff09<br/>\n{{ruby|{{lj|\u89d2\u7802\u7cd6\uff14\u3064\u30ab\u30c3\u30d7\u306b\u6295\u3052\u5165\u308c\u304f\u308b\u304f\u308b\u5efb\u3059}}|\u5c06\u56db\u7247\u65b9\u7cd6\u653e\u8fdb\u676f\u4e2d\u6405\u4f34}}\uff0c\uff08\u4e8c\u513f\u5b50[[\u6258\u9a6c\u65af\u00b7\u963f\u514b\u96f7\u5fb7|\u2163]]\u7684\u4ee3\u53f7\u3002\uff09<br/>\n{{ruby|{{lj|\u91d1\u306e\u30d5\u30a9\u30fc\u30af\u30ab\u30c1\u30ab\u30c1\u3068\u9cf4\u3089\u305b\u3070\u7d61\u7e70\u3001\u7247\u76ee\u5272\u308c\u305f\u4eba\u5f62\u3068\u30b7\u30e3\u30eb\u30a6\u30a3\u30fc\u30c0\u30f3\u30b9\uff01}}|\u82e5\u7528\u91d1\u53c9\u5b50\u6ef4\u7b54\u6ef4\u7b54\u5730\u4f5c\u54cd\u7684\u8bdd\uff0c\u5c31\u4f1a\u4e0e\u574f\u6389\u4e86\u4e00\u53ea\u773c\u775b\u7684\u673a\u68b0\u4eba\u5076\u8d77\u821e\uff01}}\uff08\u539f\u4f5c\u4e2d[[\u6258\u9a6c\u65af\u00b7\u963f\u514b\u96f7\u5fb7|\u2163]]\u4f7f\u7528\u7684\u5361\u7247[[\u673a\u5173\u5080\u5121-\u6b7b\u7075\u4eba\u5076]]\u3002\uff09<br/>\n{{ruby|{{lj|\u5fc3\u914d\u3044\u3089\u306a\u3044\u3088\u6e05\u304f\u6b63\u3057\u3044\u30d6\u30eb\u30fc\u30d9\u30ea\u30fc\u30b8\u30e3\u30e0}}|\u4e0d\u7528\u62c5\u5fc3\u54e6\u8fd9\u7eaf\u6d01\u76f4\u6b63\u7684\u84dd\u8393\u679c\u9171}}\uff0c\uff08\u5927\u513f\u5b50[[\u514b\u91cc\u65af\u591a\u5f17\u00b7\u963f\u514b\u96f7\u5fb7|\u2164]]\u7684\u4ee3\u8868\u8272\u4e4b\u4e00\u3002\uff09<br/>\n{{ruby|{{lj|\u5927\u4e08\u592b\u3060\u3088\u3001\u3082\u3046\u8ab0\u3082\u5f7c\u3082\u6068\u3093\u3060\u308a\u546a\u3063\u305f\u308a\u3057\u306a\u3044\u304b\u3089}}|\u6ca1\u95ee\u9898\u7684\u54e6\uff0c\u56e0\u4e3a\u8c01\u4e5f\u90fd\u4e0d\u4f1a\u518d\u618e\u6068\u5492\u6028\u5f7c\u6b64\u5462}}\uff0c\uff08\u5728\u5f97\u77e5[[\u83f2\u53