2f''' \u5352\u2026\u2026\n*'''\u6211\u6295\u2642\u964d\u4e86 \u771f\u662f\u7684'''\n\u6295\u964d\u7528\u8bed\uff0c\u4e00\u822c\u4ec5\u7279\u5b9a\u4eba\u5458\u4f7f\u7528<br />\n\u53ef\u4ee5\u56de\u590d\u201c\u54fc\uff01\u201d\u53d7\u964d(\u50b2\u5a07)(\u53d7\u964d\u8005\u4e00\u822c\u4e3a\u67d0\u5154\u8033\u7ba1\u7406\u5458)\n*'''\u6211\u5df2\u7ecf\u7ffb\u4e0d\u4e86\u8eab\u4e86'''\n\u81ea\u5632\u7528\u8bed\uff0c\u9690\u542b\u7740<del>\u4eba\u7684</del>\u5815\u843d\u7b49\u6df1\u523b\u7684\u601d\u8003\n*'''{{ruby|mikanmikanmi|\u6c61kan\u6c61kan\u6c61}}'''\n\u8c03\u620f\u7528\u8bed\uff0cmikanmikanmi\u672c\u4eba\u4e5f\u4f7f\u7528\uff0c\u6709\u6210\u4e3a\u90aa\u2640\u6559\u7684\u6f5c\u8d28\n*'''\u6342\u8138play'''\n\u7531\u4e8e\u6c61kan\u5b9e\u5728\u592a\u6c61\u4e86\uff0c\u4ee5\u81f3\u4e8e\u6c61\u5230\u6c61kan\u81ea\u5df1\u90fd\u770b\u4e0d\u4e0b\u53bb\u4e86\u5f97\u6342\u8138\u624d\u884c\u3002\n\n\u804a\u5929\u4e2d\u5e38\u51fa\u73b0\u6c61kan\u53d1:'''(\u6342\u8138'''\n\n\u6342\u8138play\u6682\u65f6\u5b9a\u4e49\u4e3a\u4e00\u79cd\u81ea\u6c61\u81ea\u4e50\u7684\u884c\u4e3a\n\n=== X ===\n*'''\u5c0f\u9ed1\u5c4b'''\n\u585e\u53e3\u7403\u7684\u540c\u4e49\u8bcd\u3002\u6ce8:\u5c0f\u9ed1\u5c4b\u548c\u5c0f\u9ed1\u5c4bplay\u4e0d\u540c\n=== Y ===\n*'''\u566bo'''\n\u566b\u540e\u52a0\u540d\u8bcd\uff0c\u4e00\u822c\u4e3a\u52a8\u7269\u540d\uff0c\u6765\u6e90\u4e8e[[User:\u5154\u5154\u8033\u5b9d\u5b9d|\u5fc6\u5154]]\u3002\n:*'''\u566b\u515a'''\n:<del>\u88abshuibi\u656c\u754f</del>\u88abshuibi\u75db\u65a5\u7684\u4e00\u4e2a\u56e2\u4f53\uff0c\u65e0\u660e\u786e\u754c\u5b9a\u6761\u4ef6\uff0c\u4f46\u53ef\u4ee5\u80af\u5b9a\u7684\u662f\uff0c\u80af\u5b9a\u5305\u62ec\u566b\u5154\u548c\u566b\u9e21\u3002\n*'''\u4e00\u8138\u840c\u767e'''\n\u201c\u4e00\u8138\u61f5\u903c\u201d\uff0c\u51fa\u5904\u4e3a\u67d0\u679a\u7f16\u8f91[[User:PLAcenturion|Ether]]\u7684\u7b14\u8bef\uff0c\u7531\u4e8e\u4e00\u5f00\u59cb\u5728\u8f93\u5165\u6cd5\u91cc\u6ca1\u6709\u76f4\u63a5\u627e\u5230\u61f5\u903c\u8fd9\u4e24\u4e2a\u5b57\uff0c\u5c31\u628aMB\u9996\u5b57\u6bcd(\u840c\u767e\u7f16\u8f91\u7684\u8f93\u5165\u6cd5\u91cc\u666e\u904d\u524d\u4e24\u4f4d\u4e3a\u840c\u767e\u3001\u6a21\u677f\uff0c\u6ca1\u6709\u6a21\u677f\u7684\u57fa\u672c\u5c31\u662f\u7eafshuibi)\u7684\u7b2c\u4e00\u4e2a\u6253\u4e0a\u53bb\u4e86\u3002\n\n{{\u9ed1\u5e55|\u4f3c\u4e4e\u5df2\u7ecf\u6210\u4e3a\u4e86\u4e00\u4e2a\u6897\uff0c\u5e76\u4e14\u5df2\u7531[[user:\u5815\u4e36\u5927\u5929\u4f7f|\u7834\u574f\u59ec]]\u5f00\u542f\u4e86\u8868\u60c5\u6a21\u5f0f}}\u671f\u95f4\u51fa\u73b0\u4e86'''\u4e00\u8138\u59b9\u51b0'''\u8fd9\u79cd\u51f6\u9669\u7684\u9b54\u6539\u578b\u53f7\u3002\u73b0\u7531[[User:Nnmushroom|\u819c\u83c7]]\u5c06\u5176\u6ca1\u6709\u5927\u610f\u7684\u50cf\u7d20\u5316\uff0c\u5e76\u9690\u9690\u6709\u505a\u6210\u8868\u60c5\u5305\u4f01\u5212\u7684\u51b2\u52a8\u2026\u2026\n*'''\u4e00\u7fa4\u57fa\u4f6c\u641e\u4ec0\u4e48\u767e\u5408'''\n\u5410\u69fd\u7528\u8bed\uff0c\u51fa\u5904\u4e3a[[User:Chenxuuu|Chenxuuu]]\u5410\u69fd\u4e00\u7fa4\u4f3c\u4e4e\u5728\u641e[[user:\u5815\u4e36\u5927\u5929\u4f7f|\u59ec]]\u7684\u57fa\u4f6c\u3002\n*'''\u59e8\u592a'''\n[[User:PLAcenturion|\u67d0\u4e2a\u5b66\u751f\u5361]]\u56e0\u4e3a\u4e00\u76f4\u88ab\u5237\u5361\uff0c\u4e8e\u662f\u9ed8\u9ed8\u7684\u63a5\u53d7\u4e86\u8fd9\u4e2a\u8bbe\u5b9a\uff0c\u628a\u5708\u540d\u6539\u6210\u4e86{{ruby|\u59e8\u592a\u5361|\u4ee5\u592a\u5361}}\n\n{{\u9ed1\u5e55|\u8d1f\u9505\u5927\u4fa0\u59e8\u592b\u59e8\u7ec8\u4e8e\u6709CP\u4e86\uff01}}\n{{Cquote|'''\u592b\u59bb\u53cc\u53cc\u628a\u9505\u80cc'''|}}\n*'''\u56e0/\u56e0\u4e3a'''\n\u5e38\u7528\u4e8e\u7fa4\u540d\u7247\uff0c\u52a0\u5728shuibi\u7684\u6b63\u5f0f\u540d\u79f0\u4e4b\u540e\u3002\u5728\u5403\u53e3\u7403\u65f6\u4f1a\u663e\u793a\u4e3a\u201c{{ruby|ooo|shuibi}} \u56e0{{ruby|xxx|\u5e72\u4e86\u7f9e\u7f9e\u7684\u4e8b}} \u88ab\u7ba1\u7406\u5458\u7981\u8a00{{ruby|ooxxoxox|\u597d\u957f\u597d\u957f\u7684\u4e00\u6bb5\u65f6\u95f4}}\u201d\n\n=== Z ===\n*'''\u5728\u5927\u840c\u767e\uff0cooo'''\n\u88c5B\u7528\u8bed\uff0c\u4f46\u5e38\u88ab\u4ed6\u4eba\u4e0e\u201c\u6211\u5df2\u7ecf\u7ffb\u4e0d\u4e86\u8eab\u4e86\u201d\u8fde\u7528\u4ee5\u5f70\u663e\u5176\u524d\u540e\u53cd\u5dee<br />\ne.g. (\u4ece\u524d)-'''\u5728\u5927\u840c\u767e\uff0c\u5c31\u7b97\u662f\u6652\u51b0\u4e5f\u6253\u4e0d\u8fc7\u6211\uff01'''<br />\n(\u73b0\u5728)'''-\u6211TM\u5df2\u7ecf\u7ffb\u4e0d\u4e86\u8eab\u4e86_(:\u0437\u300d\u2220)_'''\n*'''\u65e9\u4e0aH'''\n\u65e9\u5b89\u7528\u8bed\uff0c\u53ef\u56de\u590d'''\u540cH\u540cH'''\n\n'''\u7279\u6b8a\u7528\u6cd5:'''\n:<poem>\u5154\u5b50:\u65e9\u4e0aH\n\u5341\u516d:\u65e9\u4e0aH\nEther:\u5154\u5b50\u548c\u5341\u516d\u65e9\u4e0aH\n\u4e00\u822c\u4f1a\u793edoge:\u5154\u5b50\u548c\u5341\u516d\u65e9\u4e0aH\n\u9e4f:\u5154\u5b50\u548c\u5341\u516d\u65e9\u4e0aH\n\u63d0\u7763:\u5154\u5b50\u548c\u5341\u516d\u65e9\u4e0aH\n\u8a00:\u5154\u5b50\u548c\u5341\u516d\u5929\u5929H\n\u9e4f:\u5154\u5b50\u548c\u5341\u516d\u5929\u5929H\nGHO:\u5154\u5b50\u548c\u5341\u516d\u5929\u5929H\n\u67d0\u83c7:Good Morning {{\u9ed1\u5e55|Wood}}\n</poem>\n:*\u6ce8:\u67d0\u53ea\u6f5c\u85cf\u5728\u7fa4\u91cc\u7684\u66f4\u65b0\u59ec\u4eba\u683c\u9ed8\u9ed8\u63a5\u53d7\u4e86\u8fd9\u4e2a\u8bbe\u5b9a\uff0c\u5e76\u4e8e2016\u5e744\u670819\u65e50854[http://m.weibo.cn/2029177923/3965898086375694 \u65e9\u4e0aH]\u4e86\u4e00\u4e0b\uff0c\u8bc4\u8bba\u91cc\u7b80\u76f4\u50cf\u72c2\u6b22\u4e00\u6837\u3002\u987a\u4fbf\u63d0\u4e00\u53e5\uff0c\u8bc4\u8bba\u91cc\u7684\u67d0\u76ee\u5f55\u59b9\u4e8e0920\u628a\u672c\u9875\u9762\u66dd\u5149\u4e86\uff0c\u867d\u7136\u770b\u8bc4\u8bba\u91cc\u7684\u53cd\u5e94\uff0c\u4f3c\u4e4e\u6ca1\u6709\u51e0\u4e2a\u4eba\u6ce8\u610f\u5230\u8fd9\u4e2a\u9875\u9762\uff0c\u4f46\u662f1652\u65f6\u5154\u5b50\u8fd8\u662f\u8868\u793a\u5c34\u5c2c\uff0c\u663e\u7136\u8fd9\u4e2a\u9875\u9762\u9690\u542b\u7684\u4fe1\u606f\u91cf\u592a\u5927\u4e86\u3002\n*'''\u69a8\u5e72'''\n\u6c61kan\u7528\u8eab\u4f53\u69a8\u5e72\u4e86\u65e0\u6570\u7684\u7537\u4eba\uff0c\u8fde\u594e\u7237\u3001\u5b99\u65af\u3001\u7389\u7687\u5927\u5e1d\u3001\u9f50\u5929\u5927\u5723\u90fd\u60e8\u906d\u5176\u6bd2\u624b\uff0c\u4e2d\u56fd\u795e\u8bdd\u4f53\u7cfb\u4e0e\u5965\u6797\u5339\u4e9a\u795e\u7cfb\u5d29\u6e83\u7684\u7f6a\u9b41\u7978\u9996\n<br />\u53e6\u5916\u6700\u8fd1\u6c61kan\u5df2\u7ecf\u6b32\u6c42\u4e0d\u6ee1\u4e86\uff0c\u5f00\u59cb\u628a\u76ee\u5149\u653e\u5728\u4e86\u9f99\u7684\u8eab\u4e0a\uff0c\u6210\u4e3a\u4e86\u6c61\u4e4b\u706d\u9f99\u9b54\u5bfc\u58eb{{\u9ed1\u5e55|\u53c8\u4e00\u4e2a\u5e7b\u60f3\u79cd\u8981\u706d\u7edd\u4e86}}<br />\n\n\u5df2\u7ecf\u64b0\u5199\u4e86\u300a\u57fa\u4e8e\u751f\u7269\u6d3b\u4f53\u7684\u5feb\u901f\u69a8\u6c41\u6280\u672f\u300b\n\nshuibi:{{lj|\u7adc\u304c\u5f7c\u304cmikan\u3092\u55b0\u3089\u3046\uff01}}\n\n\u6c61kan:{{lj|\u7adc\u795e\u306e\u25a0\u3092\u55b0\u3089\u3048! }}\n*'''\u54f2\u2642\u5b66'''\n\u4e0e\u25a0\u25a0\u3001\u25a0\u25a0\u3001\u25a0\u25a0\uff0c\u5e76\u5217\u4e3ashuibi\u7fa4\u7684\u56db\u5927\u8bdd\u9898\n*'''\u6349'''\n\u5728\u591c\u95f4\u6f5c\u4f0f\uff0c\u7b49\u5f85\u68a8\u82b1\u9171\u51fa\u73b0\u540e\u52a8\u624b\u6355\u6349\u7684\u884c\u4e3a<br />\n(\u636e\u8bf4\u5728\u4e0a\u53e4\u65f6\u671f\uff0c\u68a8\u82b1\u9171\u8fd8\u4e0d\u662f\u5b8c\u5168\u663c\u4f0f\u591c\u51fa\u7684\u751f\u7269\uff0c\u5728\u767d\u5929\u4e5f\u4f1a\u51fa\u73b0)\n\n\u73b0\u5df2\u7ecf\u8fdb\u5316\u4e3a'''\u6251'''\uff0c\u7528\u4e8e\u5c06\u6349\u5230\u68a8\u82b1\u7684shuibi\u548c\u68a8\u82b1\u4e00\u628a\u5168\u90e8\u6251\u5165\u6000\u4e2d\uff0c\u53ef\u65e0\u9650\u53e0\u52a0\u4f7f\u7528\n\n{{\u9ed1\u5e55|\u6210\u529f\u6355\u6349\u68a8\u82b1\u9171\u540e\u83b7\u5f97\u7684\u7279\u6b8a\u9053\u5177\u4e3a\u3010\u53e3\u7403\u3011\uff0c\u81ea\u52a8\u7ed1\u5b9a\uff0c\u65e0\u6cd5\u4e22\u5f03}}"}}}