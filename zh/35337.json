u3051\u3069\u3044\u3044\u306e\uff1f}}||\u80f8\u90e8\u9732\u51fa\u4f86\u4e5f\u6c92\u554f\u984c\u55ce\uff1f||\u4e0a\u91ce\u5353\u5fd7\n|-\n|#08||{{lang|ja|\u306f\u3058\u3081\u3066\u3060\u304b\u3089\u512a\u3057\u304f\u3057\u3066\u304f\u3060\u3055\u3044}}||\u56e0\u4e3a\u662f\u7b2c\u4e00\u6b21\u6240\u4ee5\u8bf7\u6e29\u67d4\u4e00\u70b9||{{lang|ja|\u306e\u308a\u307f\u305d\u306e\u307f}}\n|-\n|#09||{{lang|ja|\u3067\uff1f\u7d50\u5c40\u306f\u307f\u51fa\u3066\u305f\u306e\uff1f}}||\u65bc\u662f\uff1f\u7d50\u679c\u9732\u51fa\u4f86\u4e86?||\u67f3\u7028\u96c4\u4e4b\n|-\n|#10||{{lang|ja|\u6b63\u8ad6\u3060\u3051\u3069\u9ed9\u308d\u3046\u306d}}||\u96d6\u7136\u6b63\u78ba\u4f46\u9084\u662f\u5225\u8aaa\u4e86||\u5e02\u5ddd\u656c\u4e09\n|-\n|#11||{{lang|ja|\u3044\u308f\u3086\u308b\u30ac\u30b5\u5165\u308c\u3067\u3059\u308f}}||\u9019\u5c31\u662f\u6240\u8b02\u7684\u5165\u5ba4\u641c\u67e5||\u67f3\u7028\u96c4\u4e4b||\u5927\u585a\u9686\u5bec||rowspan=\"2\"|\u5fb7\u7530\u5927\u8cb4\n|-\n|#12||{{lang|ja|\u3072\u3081\u306b\u3043\u3001\u751f\u5f92\u4f1a\u3084\u3081\u308b\u3063\u3066\u3088}}||\u59ec\u54e5\u8981\u9000\u51fa\u5b78\u751f\u6703||rowspan=\"2\" colspan=\"2\"|\u67f3\u7028\u96c4\u4e4b\n|-\n|#13||{{lang|ja|\u3054\u3081\u3093\u306d\u3001\u3072\u3081\u304f\u3093}}||\u5c0d\u4e0d\u8d77\uff0c\u59ec||\u68ee\u4e0b\u6607\u543e\n|}\n==== \u5404\u8bdd\u5267\u60c5 ====\n{{Hide|\u6807\u9898={{\u5267\u900f\u63d0\u9192}}|\u5185\u5bb9=\n'''#1 \u611f\u89c9\u4f5c\u4e3a\u5973\u4eba\u8f93\u4e86'''<br />\n\u5728\u971c\u79d1\u9ad8\u4e2d\u4e0a\u5b66\u7684\u6709\u5ddd\u59ec\uff0c\u88ab\u8ba8\u503a\u7684\u4eba\u8ffd\u6740\u7684\u4e8b\u60c5\u88ab\u5b66\u751f\u4f1a\u7684\u6210\u5458\u77e5\u9053\u4e86\u3002\u4e8e\u662f\u5b66\u751f\u4f1a\u51b3\u5b9a\u5e2e\u52a9\u4ed6\u3002\u8fd9\u672c\u6765\u662f\u5f88\u597d\u7684\uff0c\u4f46\u662f\u2026\u3002<br /><br />\n'''#2 \u73b0\u5728\u7684\u4f60\u80fd\u884c'''<br />\n\u53d8\u6210\u7a7f\u7740\u5973\u88c5\u7684\u5ea6\u8fc7\u751f\u6d3b\u7684\u6709\u5ddd\u59ec\uff0c\u4e0a\u5b66\u65f6\u4e5f\u88ab\u5468\u56f4\u7684\u89c6\u7ebf\u6240\u6ce8\u610f\u3002\u6765\u5230\u5b66\u6821\u540e\uff0c\u5b66\u751f\u4f1a\u7684\u6210\u5458\u51fa\u73b0\u4e86\u3002\u800c\u4e14\uff0c\u8fde\u98ce\u7eaa\u59d4\u5458\u4e5f\u767b\u573a\u4e86\u2026\uff1f<br /><br />\n'''#3 \u597d\u53ef\u7231\u554a\uff0c\u53ef\u6076\u2026'''<br />\n\u5728\u7a81\u7136\u7684\u6307\u4ee4\u4e0b\uff0c\u5b66\u751f\u4f1a\u6210\u5458\u4eec\u88ab\u6d3e\u53bb\u652f\u63f4\u793e\u56e2\u6d3b\u52a8\u3002\u5728\u5404\u79cd\u5404\u6837\u7684\u793e\u56e2\u6d3b\u52a8\u5de1\u56de\uff0c\u90e8\u5458\u4eec\u7684\u89c6\u7ebf\u90fd\u6307\u5411\u6709\u5ddd\u59ec\u2026\u3002<br /><br />\n'''#4 \u90a3\u662f\u4ec0\u4e48\u8bbe\u5b9a'''<br />\n\u5b66\u751f\u4f1a\u6210\u5458\u4eec\u53bb\u652f\u63f4\u7f8e\u672f\u90e8\u7684\u6d3b\u52a8\u3002\u7a81\u7136\u88ab\u62dc\u6258\u8981\u88f8\u4f53\u6a21\u7279\uff0c\u6ca1\u80fd\u62d2\u7edd\u7684\u6709\u5ddd\u59ec\u7ad9\u5728\u4e86\u5927\u5bb6\u7684\u76ee\u5149\u7684\u9762\u524d\u2026\u3002<br /><br />\n'''#5 \u60f3\u8981\u5927\u5c3a\u5ea6\u7684\u4e1c\u897f\uff01'''<br />\n\u8003\u8bd5\u7ed3\u679c\u5f88\u7cdf\u7cd5\uff0c\u6709\u5ddd\u59ec\u611f\u5230\u6709\u70b9\u6cae\u4e27\u3002\u8eab\u4e3a\u79c0\u624d\u7684\u5b66\u751f\u4f1a\u7684\u6210\u5458\u4eec\uff0c\u51b3\u5b9a\u6559\u6709\u5ddd\u59ec\u5b66\u4e60\u3002\u4f46\u662f\uff0c\u90a3\u4e2a\u6240\u8c13\u7684\u6559\u5b66\u65b9\u5f0f\u2026\u3002<br /><br />\n'''#6 \u4f4e\u89d2\u5ea6\u6444\u5f71\u4e0d\u8981\u7d27\u5417\uff1f'''<br />\n\u6709\u5ddd\u59ec\u6765\u5e2e\u5fd9\u8d1d\u9732\u524d\u8f88\u7684\u540c\u4eba\u6d3b\u52a8\u3002\u8bf4\u8d77\u540c\u4eba\u6d3b\u52a8\u7684\u4e50\u8da3\uff0cCOSPLAY\u5f53\u7136\u4e0d\u80fd\u5c11\u2026\u3002\u5f53\u7136\u59ec\u4e5f\u88ab\u903c\u7740\u7a7f\u2026\u3002<br /><br />\n'''#7 \u80f8\u90e8\u5b8c\u5168\u9732\u51fa\u6765\u6ca1\u5173\u7cfb'''<br />\n\u5b66\u751f\u4f1a\u6210\u5458\u53d8\u6210\u4e86\u6e38\u6cf3\u90e8\u7684\u7ec3\u4e60\u5bf9\u624b\u3002\u6beb\u65e0\u7591\u95ee\u8981\u6362\u6cf3\u88c5\u4e0a\u9635\uff0c\u4f46\u662f\u7ed9\u59ec\u51c6\u5907\u7684\u6cf3\u88c5\u5374\u662f\u5973\u751f\u7528\u7684\u3002\u59ec\u4e0d\u80fd\u7406\u89e3\u2026\u3002<br /><br />\n'''#8 \u8fd9\u662f\u7b2c\u4e00\u6b21\u6240\u4ee5\u8bf7\u6e29\u67d4\u70b9'''<br />\n\u5047\u65e5\uff0c\u5b66\u751f\u4f1a\u6210\u5458\u53bb\u8d2d\u7269\u4e2d\u5fc3\u6e38\u73a9\u3002\u6709\u5ddd\u59ec\u88ab\u547d\u4ee4\u7a7f\u7740\u5973\u88c5\u8fc7\u6765\uff0c\u4f46\u662f\u5728\u90a3\u91cc\u5b66\u751f\u4f1a\u6210\u5458\u6709\u7740\u975e\u540c\u5bfb\u5e38\u7684\u8ba1\u5212\u2026\u3002<br /><br />\n'''#9 \u4e8e\u662f\uff1f\u7ed3\u679c\u8fd8\u662f\u9732\u51fa\u6765\u4e86\u5417\uff1f'''<br />\n\u4e3a\u4e86\u5f04\u6d3b\u6587\u5316\u796d\uff0c\u59ec\u4e0d\u60c5\u613f\u5374\u88ab\u903c\u7740\u7a7f\u6cf3\u88c5\u5728\u7f8e\u5c11\u5973\u9009\u62d4\u8d5b\u51fa\u573a\u4e86\u3002\u5b66\u751f\u4f1a\u6210\u5458\u548c\u98ce\u7eaa\u59d4\u5458\u7b49\u4eba\u4e5f\u51fa\u573a\u4e86\uff0c\u4ece\u5f1f\u5f1f\u8f89\u591c\u90a3\u91cc\u542c\u5230\u91cd\u5927\u7684\u4e8b\u5b9e\u7684\u59ec\u2026\u3002<br /><br />\n'''#10 \u867d\u7136\u8bf4\u5f97\u5f88\u5bf9 \u4f46\u8bf7\u95ed\u5634'''<br />\n\u505a\u98ce\u7eaa\u59d4\u5458\u4f1a\u7684\u5de5\u4f5c\u7684\u8f89\u591c\u548c\u4e00\u53f7\u88ab\u6258\u4ed8\u68c0\u67e5\u65e9\u6668\u7684\u670d\u88c5\u3002\u4e00\u8fb9\u64e6\u7740\u56f0\u5026\u7684\u773c\u775b\u4e00\u8fb9\u53d1\u7262\u9a9a\u7684\u8f89\u591c\uff0c\u5728\u90a3\u91cc\u76ee\u51fb\u5230\u4e86\u2026\u3002\u5c31\u8fd9\u6837\u98ce\u7eaa\u59d4\u5458\u4f1a\u7684\u51b2\u51fb\u7684\u4e8b\u5b9e\u9646\u7eed\u88ab\u63ed\u5f00\uff01<br /><br />\n'''#11 \u8fd9\u5c31\u662f\u6240\u8c13\u7684\u5927\u641c\u67e5\u5427'''<br />\n\u59ec\u3001\u8f89\u591c\u548c1\u53f7\u88ab\u62db\u5f85\u53bb\u5149\u6c38\u7684\u5bb6\uff0c\u4f46\u662f\u88ab\u542c\u5230\u8fd9\u4ef6\u4e8b\u7684\u5b66\u751f\u4f1a\u6210\u5458\u4e5f\u6765\u96c6\u5408\u4e86\u3002\u52c9\u52c9\u5f3a\u5f3a\u5168\u5458\u4e00\u8d77\u53bb\u5149\u6c38\u7684\u5bb6\uff0c\u6765\u5230\u5149\u6c38\u7684\u623f\u95f4\u4ee5\u540e\uff0c\u5927\u5bb6\u51b3\u5b9a\u505a\u4e00\u4ef6\u7406\u6240\u5f53\u7136\u7684\u4e8b\u60c5\u2026\u3002<br /><br />\n'''#12 \u59ec\u54e5 \u4e0d\u5728\u5b66\u751f\u4f1a\u5e72\u4e86'''<br />\n\u59ec\u6ce8\u610f\u5230\u4e86\u5077\u7aa5\u5973\u5b50\u66f4\u8863\u5ba4\u7684\u4e0d\u826f\u5c11\u5e74\u3002\u867d\u7136\u611f\u89c9\u5230\u4e86\u90a3\u4e9b\u4e0d\u826f\u5c11\u5e74\u4eec\u7684\u6b32\u671b\uff0c\u4f46\u662f\u4e5f\u5f97\u6267\u884c\u5b66\u751f\u4f1a\u7684\u5de5\u4f5c\u3002\u7136\u800c\u4ece\u67d0\u4e2a\u4e8b\u5b9e\u4e0a\uff0c\u5b66\u751f\u4f1a\u6210\u5458\u4e4b\u95f4\u4ea7\u751f\u4e86\u88c2\u75d5\u2026\u3002<br /><br />\n'''#13 \u5bf9\u4e0d\u8d77 \u59ec\u541b'''<br />\n\u59ec\u79bb\u53bb\u7684\u7b2c\u4e8c\u5929\u3002\u5348\u4f11\u7684\u5b66\u751f\u4f1a\u5ba4\uff0c\u88ab\u4e0e\u5f80\u5e38\u622a\u7136\u4e0d\u540c\u7684\u5b89\u9759\u7684\u6c14\u6c1b\u5305\u56f4\u7740\u3002\u201c\u53bb\u5bfb\u627e\u6210\u4e3a\u4ee3\u66ff\u59ec\u7684\u65b0\u4eba\u5427\u201d18\u7981\u5bf9\u7740\u5979\u4eec\u8bf4\uff0c\u800c\u4f1a\u957f\u8fd0\u5b50\u8bf4\u201c\u6ca1\u4eba\u80fd\u4ee3\u66ff\u59ec\u201d\u3002\u7136\u800c\uff0c\u59ec\u518d\u6b21\u88ab\u4e0d\u826f\u5b66\u751f\u9a9a\u6270\uff0c\u51fa\u6765\u76f8\u6551\u7684\u662f\u2026\u3002\n}}\n==== \u76f8\u5173\u97f3\u4e50 ====\n{{color|red|\u7247\u5934\u66f2(OP)}}\n*'''[[Trouble Maker|{{lang|ja|\u3068\u3089\u3076\u308b\u3081\u30fc\u304b\u30fc}}]]'''\uff08\u9ebb\u70e6\u5236\u9020\u8005\uff09\n**\u6b4c\uff1a\u3042\u3044\u307e\u3044\u307f\u30fc\u307e\u3044\u3093\uff08\u6851\u539f\u7531\u6c14\u3001\u6469\u5929\u697c\u7531\u9999\u3001\u5c0f\u91ce\u65e9\u7a00\u3001\u4e1c\u57ce\u65e5\u6c99\u5b50\uff09\n**\u4f5c\u8bcd\uff1a\u30df\u30ba\u30ce\u30b2\u30f3\u30ad\uff0c\u4f5c\u66f2\uff1a\u7766\u6708\u5468\u5e73\n{{color|blue|\u7247\u5c3e\u66f2(ED)}}\n*'''[[\u5316\u5986!(\u641e\u59ec\u65e5\u5e38\u6b4c\u66f2)|{{lang|ja|\u3081\u30fc\u304d\u3083\u3063\u3077!}}]]'''\uff08\u5316\u5986\uff01\uff09\n**\u6b4c\uff1a\u3042\u3044\u307e\u3044\u307f\u