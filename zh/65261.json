f6'''\u662f[[\u864e\u8d70\u7fd4|\u864e\u8d70\u304b\u3051\u308b]]\u6240\u521b\u4f5c\u7684\u8f7b\u5c0f\u8bf4'''\u300a[[\u4ece\u96f6\u5f00\u59cb\u7684\u9b54\u6cd5\u4e66]]\u300b'''\u53ca\u5176\u884d\u751f\u4f5c\u54c1\u7684\u767b\u573a\u89d2\u8272\u3002\n\n== \u7b80\u4ecb ==\n\n\u672c\u4f5c\u5973\u4e3b\u89d2\uff0c\u6709\u7740\u4e00\u5934\u94f6\u53d1\u53ca\u9752\u7d2b\u8272\u77b3\u5b54\u7684\u6ce5\u6697\u9b54\u5973\u3002\n\n\u9b54\u6cd5\u7684\u521b\u59cb\u4eba\uff0c\u89c6\u5176\u4f7f\u7528\u65b9\u6cd5\u751a\u81f3\u80fd\u6bc1\u706d\u4e16\u754c\u7684\u9b54\u6cd5\u4e66\u300a\u96f6\u4e4b\u4e66\u300b\u7684\u4f5c\u8005\u3002\n\n\u6709\u7740\u5f3a\u5927\u7684\u9b54\u529b\u3002\u6027\u683c\u5929\u7136\u5584\u826f\uff0c\u4ea6\u6709\u8179\u9ed1\u7684\u65f6\u5019\uff0c\u662f\u4e2a\u5403\u8d27\u3002\n\n\u4e0e\u6700\u9ad8\u4f4d\u7684\u6076\u9b54\u7b7e\u8ba2\u4e86\u5951\u7ea6\uff0c\u80fd\u300c\u9a73\u56de\u300d\u5176\u4ed6\u9b54\u6cd5\u4f7f\u4f7f\u7528\u96f6\u4e4b\u4e66\u5185\u7684\u9b54\u6cd5\u3002\n\n{{\u9ed1\u5e55|\u7531\u4e8e\u5973\u4e3b\u548c\u8230\u961fCollection\u7684[[\u8230\u961fCollection:\u5c9b\u98ce|\u5c9b\u98ce]]\u957f\u5f97\u592a\u50cf\uff0c\u518d\u52a0\u4e0a\u753b\u5e08\u662f\u540c\u4e00\u4e2a\u4eba\uff08\u3057\u305a\u307e\u3088\u3057\u306e\u308a\uff09\uff0c\u56e0\u6b64\u88ab\u620f\u79f0\u4e3a\u9b54\u6cd5\u5c11\u5973\u5c9b\u98ce\u3002}}\n\n<gallery>\n\u96f6(\u4ece\u96f6\u5f00\u59cb\u7684\u9b54\u6cd5\u4e66).jpg|\u50ad\u5175\u6389\u843d\u4e2d\n</gallery>\n\u96f6\u662f\u6ce5\u6697\u7cfb\u7edf\u4e2d\u547d\u540d\u7684\u7279\u4f8b<ref>\u6ce5\u6697\u7684\u7cfb\u7edf\u4e4b\u4e2d\uff0c\u6709\u7740\u4ee3\u8868\u4e8c\uff0c\u4e09\uff0c\u4e94\uff0c\u4e03\uff0c\u5341\u4e00\uff0c\u5341\u4e09\u7684\u9b54\u5973\uff0c\u4ee5\u53ea\u6709\u81ea\u5df1\u80fd\u89e3\u6784\u81ea\u5df1\u7684\u7d20\u6570\u540d\u5b81\u610f\u5473\u7740\u4ed6\u4eec\u662f\u80fd\u591f\u6210\u4e3a\u201c\u5e08\u201d\u7684\u5b58\u5728\uff0c\u6bcf\u4e2a\u6570\u5b57\u6709\u81ea\u5df1\u7684\u610f\u4e49\u3002</ref>\uff0c\u662f\u8868\u73b0\u6839\u672c\u865a\u65e0\u7684\u6982\u5ff5\u3002\u96f6\u5728\u5341\u5c81\u65f6\u88ab\u5e08\u957f\u6000\u7740\u656c\u754f\u6388\u4e88\u4e86\u96f6\u7684\u540d\u5b57\u3002\n\n{{clear}}\n\u540c\u65f6\u96f6\u4e5f\u662f\u4ee3\u8868\u672c\u4eba\u5929\u8d4b\u9ad8\uff0c\u4ee5\u53ca\u5176\u4ed6\u7684\u2026\n\n== \u7ecf\u5386 ==\n{{\u5267\u900f}}\n\n\u70ba\u4e86\u5c0b\u627e\u88ab\u76dc\u8d70\u7684\u300a\u96f6\u4e4b\u4e66\u300b\uff0c\u72ec\u81ea\u8e0f\u4e0a\u4e86\u65c5\u9014\uff0c\u671f\u95f4\u4e0e\u517d\u4eba\u4f63\u5175\u7b7e\u4e0b\u5951\u7ea6\u7b54\u5e94\u534f\u52a9\u89e3\u5f00\u5176\u517d\u5316\u3002\n\n\u5bf9\u4f63\u5175\u6709\u7740\u975e\u4e00\u822c\u7684\u6267\u7740\uff0c\u5e76\u8ba4\u5b9a\u5176\u662f\u81ea\u5df1\u4e3a\u6570\u4e0d\u591a\u7684\u670b\u53cb\u3002\n{{\u9ed1\u5e55|\u4e4b\u5f8c\u6642\u4e0d\u6642\u627e\u6a5f\u6703\u7761\u5728\u4f63\u5175\u8eab\u4e0a\u3002}}\n(\u5f85\u8865)\n\n{{\u864e\u8d70\u7fd4}}\n{{\u66ff\u6362\u4fa7\u8fb9\u680f\u5e95\u56fe|url=zero0.png|width=200px}}\n\n== \u6ce8\u91ca ==\n<references />\n== \u5916\u90e8\u94fe\u63a5 ==\n*[http://zeronosyo.com/ \u5b98\u65b9\u7f51\u9875]\n\n[[\u5206\u7c7b:\u4ece\u96f6\u5f00\u59cb\u7684\u9b54\u6cd5\u4e66]]\n[[\u5206\u7c7b:\u9b54\u5973]][[\u5206\u7c7b:\u5929\u7136\u5446]][[\u5206\u7c7b:\u8179\u9ed1]][[\u5206\u7c7b:\u5403\u8d27]][[\u5206\u7c7b:\u957f\u76f4]][[\u5206\u7c7b:\u5446\u6bdb]][[\u5206\u7c7b:\u8fc7\u819d\u889c(\u767d)]][[\u5206\u7c7b:\u8d2b\u4e73]][[\u5206\u7c7b:\u7b2c\u4e00\u4eba\u79f0Wagahai]][[\u5206\u7c7b:\u59b9\u59b9]]"}}}