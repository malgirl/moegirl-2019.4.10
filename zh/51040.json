\n\u5c45\u4f4f\u5728\u7cbe\u795e\u75c5\u9662\u4e2d\u63a5\u53d7\u6cbb\u7597\u7684\u5e7b\u60f3\u75c7\u5c11\u5973\uff0c\u5b8c\u5168\u5c06\u73b0\u5b9e\u548c\u5e7b\u60f3\u6df7\u6dc6\u3002\n\n\u51fa\u751f\u5728\u4e00\u4e2a\u4e61\u4e0b\u519c\u5bb6\uff0c\u7ee7\u6bcd\u5bf9\u5979\u975e\u5e38\u6076\u52a3\uff0c\u52a8\u8f84\u6253\u9a82\u3002\n\n\u6027\u683c\u975e\u5e38\u602f\u5f31\uff0c\u6ca1\u6709\u4efb\u4f55\u73a9\u5177\uff0c\u4e5f\u6ca1\u80fd\u4ea4\u5230\u670b\u53cb\uff0c\u5168\u51ed\u5e7b\u60f3\u548c\u81ea\u5df1\u73a9\u800d\u3002\n\n\u6bcf\u5f53\u53d7\u5230\u65a5\u8d23\u6253\u9a82\u5c31\u4f1a\u4e0d\u65ad\u544a\u8bc9\u81ea\u5df1\u8fd9\u662f\u68a6\uff0c\u81ea\u5df1\u6ca1\u6709\u53d7\u5230\u4f24\u5bb3\uff0c\u7236\u6bcd\u4e5f\u6ca1\u6709\u751f\u6c14\u3002\n\n\u9010\u6e10\u65e0\u6cd5\u5206\u6e05\u5e7b\u60f3\u548c\u73b0\u5b9e\u7684\u5979\uff0c\u88ab\u9001\u5f80\u533b\u9662\u3002\u5728\u533b\u751f\u7684\u5e2e\u52a9\u4e0b\u9010\u6e10\u597d\u8f6c\u3002\n\n\u4f46\u540e\u6765\u67d0\u4e00\u5929\u2026\u2026\u82b1\u56ed\u91cc\u957f\u51fa\u4e00\u9897\u6811\uff0c\u53ee\u94c3\u94c3\u5730\u7ed3\u6ee1\u4e86\u91d1\u82f9\u679c\uff0c\u4e00\u53ea\u7bee\u5b50\u602a\u7269\u4e5f\u51fa\u73b0\u5728\u5730\u4e0b\u3002\u5979\u4ee5\u4e3a\u8fd9\u53c8\u662f\u5979\u7684\u5e7b\u89c9\uff0c\u4f46\u5176\u4ed6\u4eba\u90fd\u544a\u8bc9\u5979\uff0c\u8fd9\u662f\u771f\u7684\u3002\n\n\u5728\u5979\u7684\u773c\u91cc\uff0c\u73b0\u5b9e\u548c\u5e7b\u60f3\uff0c\u53c8\u5f00\u59cb\u4e86\u6df7\u6dc6\u2026\u2026\n\n\n== \u6570\u636e ==\n{| class=\"wikitable\" style=\"margin:0 auto;text-align:center;\"\n|-\n!colspan=\"4\"| '''\u300c\u68a6\u4e4b\u6447\u7bee\u300d\u2014\u2014\u7a46\u5a05'''\n|-\n!colspan=\"1\"| '''CV'''\n|colspan=\"1\"|'''\u7530\u4e2d\u5343\u60e0\u7f8e'''\n!colspan=\"1\"| ''' \u4eba\u8bbe/\u7acb\u7ed8''' \n|colspan=\"1\"| ''' \u672a\u77e5/\u672a\u77e5'''\n|-\n!colspan=\"1\"| ''' \u804c\u4e1a'''\n|colspan=\"1\"|'''\u8f85\u52a9'''\n!colspan=\"1\"| ''' \u5c5e\u6027'''\n|colspan=\"1\"|'''[[File:\u521a\u5c5e\u6027.png|25px]]\u521a/\u6cd5\u672f'''\n|-\n!colspan=\"1\"| '''\u521d\u59cb\u661f\u7ea7'''\n|colspan=\"1\"|'''[[File:7\u65e5\u4e4b\u90fdB\u7b49\u7ea7.png|38px]]'''\n!colspan=\"1\"| '''\u795e\u5668'''\n|colspan=\"1\"|'''\u5b9d\u7269\u30fb\u91d1\u82f9\u679c'''<br />\n|-\n!colspan=\"4\"| '''\u83b7\u5f97\u9014\u5f84'''\n|-\n|colspan=\"4\" style=\"text-align:left;\"|\n\u767d\u591c\u9986\u62bd\u53d6\u83b7\u5f97\n|-\n!colspan=\"4\"| '''\u624d\u80fd'''\n|-\n|colspan=\"4\" style=\"text-align:left;\"|\n#\u5de1\u67e5\u529b\uff1a2\n#\u5efa\u8bbe\u529b\uff1a5\u21927\n#\u5f00\u53d1\u529b\uff1a9\u219212\n|-\n!colspan=\"4\"| '''\u6280\u80fd'''\n|-\n|colspan=\"4\" style=\"text-align:left;\"|\n#'''\u77ed\u6682\u7684\u4f11\u61a9'''\uff08\u521d\u59cb1\u7ea7/\u88ab\u52a8\u6280\u80fd\uff09<br />\u7b49\u7ea71 \u8840\u91cf\u4f4e\u4e8e30%\u662f\u7a46\u5a05\u5c06\u5403\u6389\u4e00\u4e2a\u82f9\u679c\uff0c1.5\u79d2\u5ef6\u8fdf\u540e\u6062\u590d40%\u8840\u91cf\uff0c\u51b7\u5374\u65f6\u95f490\u79d2\u3002<br />\u2192\u51b7\u5374\u65f6\u95f475\u79d2[Lv.2]<br />\u2192\u6280\u80fd\u6062\u590d\u8840\u91cf45%[Lv.3]<br />\u2192\u6280\u80fd\u6062\u590d\u8840\u91cf50%\u51b7\u5374\u65f6\u95f460\u79d2[Lv.4]\n#'''\u9ec4\u91d1\u82f9\u679c'''\uff08\u521d\u59cb2\u7ea7/\u51b7\u53747\u79d2\uff09<br />\u5411\u76ee\u6807\u533a\u57df\u4e22\u4e0b\u4e00\u9897\u6301\u7eed\u5632\u8bbd\u5468\u56f4\u7684\u654c\u4eba\u7684\u91d1\u82f9\u679c\u3002\u91d1\u82f9\u679c\u53d7\u52306\u6b21\u666e\u901a\u653b\u51fb\u4f24\u5bb3\u6216\u6301\u7eed2\u79d2\u540e\u7206\u70b8\u9020\u6210\u8303\u56f4\u4f24\u5bb3.<br />\u2192\u82f9\u679c\u53ef\u53d7\u51fb8\u6b21\u5e76\u4e14\u4f24\u5bb3\u63d0\u9ad812.8%[Lv.3]<br />\u2192\u82f9\u679c\u53ef\u53d7\u51fb10\u6b21\u5e76\u4e14\u4f24\u5bb3\u63d0\u9ad813.6%[Lv.4]\n#'''\u5973\u795e\u7684\u8912\u5956 '''\uff08\u521d\u59cb1\u7ea7/\u51b7\u537410\u79d2\uff09<br /> \u5411\u76ee\u6807\u533a\u57df\u4e22\u4e0b\u4e00\u9897\u9752\u82f9\u679c\uff0c\u6062\u590d\u533a\u57df\u5185\u53cb\u65b9\u5355\u4f4d\u751f\u547d\u503c\u3002<br />\u2192\u6062\u590d\u751f\u547d\u503c\u63d0\u9ad811.4%[Lv.2]<br />\u2192\u6062\u590d\u751f\u547d\u503c\u63d0\u9ad812.8%[Lv.3]<br />\u2192\u6062\u590d\u751f\u547d\u503c\u63d0\u9ad813.6%[Lv.4]\n#''' \u6eda\u77f3\u82f9\u679c '''\uff08\u521d\u59cb1\u7ea7/\u51b7\u537425\u79d2\uff09<br /> \u53ec\u5524\u6eda\u77f3\u82f9\u679c\u7f13\u6162\u5411\u524d\u6eda\u52a8\uff0c\u6cbf\u9014\u5bf9\u654c\u65b9\u5355\u4f4d\u6700\u4f4e\u9020\u6210\u4f24\u5bb3\u548c1\u79d2\u7729\u6655\uff0c\u4f24\u5bb3\u91cf\u548c\u7729\u6655\u65f6\u95f4\u968f\u8ddd\u79bb\u589e\u52a0\u800c\u589e\u52a0\uff0c\u6700\u591a\u7729\u66552\u79d2\u3002<br />\u2192\u4f24\u5bb3\u63d0\u9ad811.4%[Lv.2]<br />\u2192\u63d0\u9ad8\u4f24\u5bb312.8%\u548c0.5\u79d2\u6700\u5927\u7729\u6655\u65f6\u95f4[Lv.3]<br />\u2192\u63d0\u9ad8\u4f24\u5bb313.6%\u548c0.5\u79d2\u6700\u5927\u7729\u6655\u65f6\u95f4[Lv.4]\n|-\n!colspan=\"4\"| '''\u795e\u5668\u6280\u80fd'''\n|-\n|colspan=\"4\" style=\"text-align:left;\"|\n*'''\u7978\u6839'''\uff08\u88ab\u52a8\u6280\u80fd\uff09<br />\u4f7f\u53cb\u65b9\u795e\u5668\u4f7f\u7684\u6cd5\u969c\u7387\u63d0\u9ad81.6%\u3002<br />\u2192\u6cd5\u969c\u7387\u63d0\u9ad83.2%[Lv.2]<br />\u2192\u6cd5\u969c\u7387\u63d0\u9ad84.8%[Lv.3]<br />\u2192\u6cd5\u969c\u7387\u63d0\u9ad86.4%[Lv.4]\n|-\n!colspan=\"4\"| '''\u7a81\u7834\u6d88\u8017\u5f71\u88c5''' \n|-\n| A\u7ea7\u2014S\u7ea7\u7a81\u7834\n| colspan=3 |{{color|green|\u4e91\u7aef\u6f2b\u6e38}}\u300c2cost\u300d\n|-\n| S\u7ea7\u7a81\u7834\n| colspan=3 |{{color|blue|\u9ed1\u8272\u7b14\u8bb0\u672c}}\u300c5cost\u300d\n|-\n| \u795e\u5668\u7a81\u7834\uff089-10\uff09\n| colspan=3 |{{color|violet|\u6076\u6076\u679c\u5b9e}}\u300c6cost\u300d\n|-\n| \u795e\u5668\u7a81\u7834\uff0819-20\uff09\n| colspan=3 | {{color|gold|\u91d1\u7f8a\u6bdb}}\u300c1cost\u300d\n|-\n| \u795e\u5668\u7a81\u7834\uff0829-30\uff09\n| colspan=3 |{{color|gold|\u5fd8\u6211\u4e4b\u97f3}}\u300c2cost\u300d\n|-\n|}\n\n\n* '''\u8bbe\u5b9a'''\n\n\n{{tabs\n|DefaultTab=1\n|color= green\n|bt1=\u68a6\u4e4b\u6447\