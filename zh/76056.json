u3001\u9a6c\u6625\u7136\u3001\u8bb8\u5927\u8fb0\u3001\u738b\u674e\u8fbe   !! \u738b\u674e\u8fbe\u3001\u5218\u536b\u5170\u3001\u9648\u7fd4\u3001\u738b\u8d85\n|}\n\n==\u4e2a\u4eba\u7ecf\u5386<del>\u5e7b\u53ef\u53ef\u5927\u5e1d\u672c\u7eaa</del>==\n\u5e7b\u53ef\u53ef\uff0c\u7537\uff0c2005\u5e74\u51fa\u751f\uff0c\u5c0f\u5b66\u65f6\u5f00\u59cb\u63a5\u89e6\u52a8\u753b\u6f2b\u753b\uff0c\u521d\u4e2d\u65f6\u63a5\u89e6\u8f7b\u5c0f\u8bf4\uff0c\u62e5\u6709\u4e30\u5bcc\u7684\u9605\u7247\u91cf\u3002<br \\>\n\u5c0f\u5b66\u65f6\u5f00\u59cb\u72ec\u7acb\u521b\u4f5c\u5267\u672c\uff0c\u83b7\u5f97\u8fc7\u5468\u56f4\u4eba\u58eb\u7684\u4e00\u81f4\u597d\u8bc4\u3002<br \\>\n\u5386\u4efb\u4e2d\u56fd\u5c11\u5e74\u5148\u950b\u961f\u961f\u5458\u3001\u8bed\u6587\u7ec4\u957f\u3001\u798f\u5dde\u7b2c\u4e03\u4e2d\u5b66\u521d\u4e00\u5341\u73ed\u5ba3\u4f20\u59d4\u5458\u3002\u73b0\u4efb\u798f\u5dde\u7b2c\u4e03\u4e2d\u5b66\u521d\u4e00\u5341\u73ed\u540d\u8a89\u5ba3\u4f20\u59d4\u5458\u3002\n\n==\u751f\u547d\u611f\u8a00==\n\u6211\u5df2\u7ecf\u7f16\u4e86\u4e24\u4e2a\u8bcd\u6761\u4e86\uff0c\u5728\u9ad8\u68a8\u7c73\u590f\u8bcd\u6761\u6211\u4e86\u89e3\u7684\u66f4\u6df1\uff0c\u4e00\u4e2a\u5f39\u5e55\u91cc\u9762\u7684\u6897\uff0c\u4e3a\u4ec0\u4e48\u5c31\u4e0d\u80fd\u7528\u5462\uff1f\u4e00\u4e2a\u6211\u5bf9\u4e8e\u8fd9\u4e2a\u4eba\u7269\u7684\u5410\u69fd\uff0c\u4e3a\u4ec0\u4e48\u5c31\u4e0d\u80fd\u73a9\u5462\uff1f\n\u840c\u5a18\u767e\u79d1\uff0c\u5b83\u6ca1\u6709\u7ef4\u57fa\u767e\u79d1\u4e13\u4e1a\uff0c\u6ca1\u6709\u767e\u5ea6\u767e\u79d1\u5927\u4f17\u5316\uff0c\u5b83\u80fd\u505a\u7684\u4f30\u8ba1\u5c31\u53ea\u6709\u5a31\u4e50\u6027\u3002\n\u6211\u81ea\u5df1\u7528\u840c\u5a18\u767e\u79d1\uff0c\u5176\u5b9e\u5c31\u662f\u4e3a\u4e86\u627e\u4e50\u5b50\uff0c\u6211\u628a\u6211\u8ba4\u4e3a\u597d\u73a9\u7684\u4e1c\u897f\u5206\u4eab\u7ed9\u522b\u4eba\uff0c\u8fd9\u53c8\u6709\u4ec0\u4e48\u9519\u5462\uff1f\n\u8fd9\u4e2a\u95ee\u9898\uff0c\u7ed9\u6211\u81ea\u5df1\uff0c\u4e5f\u7ed9\u5404\u4f4d\u8fd9\u4e9b\u4e50\u610f\u770b\u4e00\u4e2a\u4e11\u7537\u5df4\u62c9\u5df4\u62c9\u4e00\u5927\u5806\u7684\u89c2\u4f17\u59e5\u7237\u4eec\u3002<br \\>\n\u4e3a\u4ec0\u4e48\u840c\u767e\u7684\u7528\u6237\u8fd9\u4e48\u4ec7\u6068\u5171\u9752\u56e2\u5462\uff1f\u8fd9\u662f\u4e00\u4e2a\u95ee\u9898\uff0c\u603b\u4e4b\u6211\u662f\u4e0d\u4ec7\u6068\u7684\uff0c\u81f3\u5c11\u6211\u5bf9\u4e8e\u5171\u9752\u56e2\u662f\u4e2d\u7acb\u7684\u6001\u5ea6\uff0c\u4ec0\u4e48\u4e1c\u897f\u5176\u5b9e\u5df2\u7ecf\u4e0d\u91cd\u8981\u4e86\uff0c\u53ea\u5e0c\u671b\u4e60\u8fd1\u5e73\u53ef\u4ee5\u653e\u5f00\u70b9\u5c3a\u5ea6\u3002\u6216\u8005\u8bf4\u6211\u4eec\u73b0\u5728\u5c31\u53ef\u4ee5\u9a82\u9a82\u738b\u5c90\u5c71\uff1f\u6240\u6709\u4eba\u90fd\u6e34\u671b\u5206\u7ea7\uff0c\u4f46\u6709\u4eba\u4e0d\u613f\uff0c\u600e\u4e48\u8bb2\u5462\uff0c\u5468\u603b\u7406\u8bb2\u8fc7\u6c42\u540c\u5b58\u5f02\uff0c\u8fd9\u662f\u6bcf\u4e2a\u4eba\u90fd\u8981\u60f3\u7684\u95ee\u9898\uff0c\u5305\u62ec\u5bb6\u957f\uff0c\u5305\u62ec\u4e60\u8fd1\u5e73\uff0c\u8fd8\u662f\u4e94\u5e74\u540e\u7684\u738b\u5c90\u5c71\u3002<br \\>\n\u8bdd\u8bf4\u73b0\u5728\u7684\u7406\u8bba\u7c7b\u8282\u76ee\u662f\u4e0d\u662f\u7d20\u6750\u592a\u5c11\uff0c\u73b0\u5728\u90fd\u62ff\u603b\u4e66\u8bb0\u8bf4\u8fc7\u7684\u8bdd\u5f53\u5185\u5bb9\u4e86\uff0c\u6211\u4eec\u770b\u7684\u662f\u515a\u8bfe\u4e0d\u662f\u603b\u4e66\u8bb0\u7684\u4e2a\u4eba\u79c0\uff0c\u4e5f\u6ca1\u6709\u60f3\u8fc7\u5b89\u53ef\uff0c\u5982\u679c\u8fd9\u6837\u7684\u8bdd\uff0c\u53ef\u80fd\u5171\u9752\u56e2\u5728\u5927\u4f17\u4e2d\u7684\u98ce\u8bc4\u4f1a\u8d8a\u6765\u8d8a\u4f4e\u5427\uff0c\u4e0d\u8fc7\u6211\u8fd8\u662f\u4e50\u4e8e\u770b\u6b64\u7c7b\u8282\u76ee\u7684\uff0c\u53ea\u8981\u505a\u5f97\u597d\uff0c\u4e0d\u8ba9\u6211\u89c9\u5f97\u5c34\u5c2c\u5c31\u884c\u4e86\uff0c\u592e\u89c6\u505a\u7684\u8fd8\u662f\u5f88\u597d\u7684\uff0c\u4e0d\u8fc7\u6e56\u5357\u53f0\u7684\u4e00\u8a00\u96be\u5c3d\uff0c\u5728\u6b64\u6211\u6482\u7ed3\u8bba\uff0c\u53ea\u8981\u8282\u76ee\u505a\u5f97\u597d\uff0c\u7ba1\u4f60\u662f\u7528\u4ec0\u4e48\u505a\u7d20\u6750\uff0c\u4f46\u662f\u8282\u76ee\u505a\u4e0d\u597d\uff0c\u5c31\u4e0d\u8981\u602a\u522b\u4eba\u6311\u523a\u54e6\uff01\n\n==\u6076\u641e\u5c0f\u6bb5\u5b50==\n*\u840c\u5a18\u767e\u79d1\u89c6\u9891\u5e7f\u544a\n**\u4e24\u4e2a\u7537\u5b69\u5728\u9910\u5385\u5403\u996d\uff0c\u7a81\u7136\u53d1\u73b0\u83dc\u5355\u4e0a\u6709\u4e24\u4e2a\u9ed1\u6761\u6761\uff0c\u4ed6\u4eec\u522e\u6765\u522e\u53bb\uff0c\u4ed6\u4eec\u8be2\u95ee\u7eff\u5934\u53d1\u7684\u670d\u52a1\u5458\u4e3a\u4ec0\u4e48\u8fd9\u6837\uff0c\u670d\u52a1\u5458\u7b11\u800c\u4e0d\u8bed\uff0c\u62ff\u51fa\u624b\u673a\uff0c\u6253\u5f00\u840c\u5a18\u767e\u79d1\uff0c\u4e24\u4e2a\u5927\u7537\u5b69\u987f\u65f6\u660e\u767d\u4e86\u9ed1\u6761\u6761\u7684\u7528\u5904\uff0c\u628a\u670d\u52a1\u5458\u7684\u624b\u673a\u62a2\u8d70\uff0c\u6253\u4e86\u534a\u5c0f\u65f6\u6e38\u620f\u2026\u2026\n**\u4e00\u4f4d\u957f\u76f8\u6e05\u79c0\u7684\u7537\u5b69\u88ab\u602a\u517d\u8ffd\u6740\uff0c\u7a81\u7136\u4ece\u5929\u800c\u964d\u4e00\u4f4d\u7f8e\u4e3d\u7684\u5c11\u5973\uff0c\u53ea\u89c1\u8fd9\u4f4d\u5c11\u5973\u6253\u5f00\u4e86\u840c\u5a18\u767e\u79d1app\uff0c\u77ac\u95f4\u5413\u8dd1\u4e86\u602a\u517d\uff0c\u602a\u517d\u5927\u558a\uff1a\u201c\u539f\u6765\u4f26\u5bb6\u8fd9\u4e48\u840c\u54c7\uff01\u201d<del>\u4f60\u4ec0\u4e48\u65f6\u5019\u4ea7\u751f\u4e86\u81ea\u5df1\u5f88\u597d\u770b\u7684\u9519\u89c9</del>\n**\u4e24\u4e2a\u5564\u9152\u74f6\u5b50\u5728\u6d41\u6c34\u7ebf\u4e0a\u8d70\u554a\u8d70\uff0c\u8d70\u554a\u8d70\uff0c\u8d70\u5230\u4e00\u534a\uff0c\u4e00\u4e2a\u5564\u9152\u74f6\u5b50\u7d2f\u4e86\uff0c\u60f3\u6b47\u4f1a\uff0c\u53e6\u4e00\u4e2a\u5564\u9152\u74f6\u5b50\u6253\u5f00\u4e86\u840c\u5a18\u767e\u79d1\uff0c\u7136\u540e\u4e24\u4e2a\u4eba\u5728\u6d41\u6c34\u7ebf\u4e0a<del>\u6b89\u4e86\u60c5</del>\n*\u4e1c\u5357\u536b\u89c6\u89c6\u9891\u5e7f\u544a\n**\u4e24\u4e2a\u5e74\u8f7b\u4eba\u6b63\u5728\u5899\u4e0a\u505a\u7740\u6d82\u9e26\uff0c\u8fd9\u4e2a\u65f6\u5019\uff0c\u4e00\u4e2a\u4e25\u8083\u7684\u8001\u4eba\u671d\u4ed6\u4eec\u8d70\u4e86\u8fc7\u6765\uff0c\u5728\u5899\u4e0a\u753b\u4e86\u4e2a\u6d77\u6d6a\u7ffb\u6eda\u7684\u573a\u666f\uff0c\u7136\u540e\u4e09\u4e2a\u4eba\u5c31\u4e00\u8d77\u770b\u6d77\u804a\u5929\u3002<del>\u6cf3\u88c5\u56de</del>\u5c4f\u5e55\u4e0a\u51fa\u73b0\u51e0\u4e2a\u5927\u5b57\uff1a\u6d77\u6d0b\u65f6\u4ee3\uff0c\u4e2d\u534e\u60c5\u6000\u3002"}}}