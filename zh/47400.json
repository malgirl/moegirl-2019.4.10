u65bd\u2192\u4e03\u4e03\u5f0f\u4e09\u949b\u786c\u94c0\u5408\u91d1\u8bfa\u57fa\u4e9a\u9632\u7a7a\u6816\u59ec\u6781\u5316\u88c5\u7532+\u57c3\u65af\u5854\u6885\u5c14\u6c0f\u81ea\u9002\u5e94\u6b20\u63cd\u62a4\u76fe\n|-\n| 0\u21920\u21920\n| colspan=3 | \u88c5\u5907\u4e0d\u53ef\u2192\u4e03\u4e94\u5f0f\u6df1\u6d77\u88c5\u7532\u5f00\u6d1e\u5668\u6539\u4e8c\u2192\u4e03\u4e03\u5f0f\u6df1\u6d77\u9632\u7a7a\u4e9a\u7a7a\u95f4\u4f20\u611f\u5668\n|}\n*\u62ec\u53f7\u5185\u6570\u636e\u4e3a\u8be5\u9879\u80fd\u529b\u6240\u80fd\u8fbe\u5230\u7684\u6700\u5927\u503c(\u8fd1\u4ee3\u5316\u6539\u4feeMAX\u3001Lv99)\n{{Hide|\u6807\u9898=\u6570\u636e\u8bbe\u5b9a|\u5185\u5bb9=\n*\u7a00\u6709\u5ea6\uff1a\u7530\u4e2d\u4e00\u822c\u5728\u65b0\u5b9e\u88c5\u7684\u9b3c\u755c\u6d77\u57df\u6389\u843d\uff0c\u6389\u843d\u4e0d\u7b97\u5c11\u4f46\u975e\u5e38\u96be\u635e\uff0c\u6240\u4ee5\u9009\u62e9\u4e86\u5c0f\u5b66\u751f\u4e2d\u7a00\u6709\u5ea6\u6700\u9ad8\u4e5f\u662f\u5728\u4e0a\u9762\u622a\u56fe\u4e2d\u548c\u7530\u4e2d\u4e00\u8d77\u51fa\u73b0\u7684\u9a71\u9010\u738b\u4f5c\u4e3a\u53c2\u8003\u3002\n*\u8010\u4e45\uff1a\u7530\u4e2d\u7684\u4eba\u8bbe\u611f\u89c9\u50cf\u662f\u5904\u4e8e\u8f7b\u5de1\u4e0e\u91cd\u5de1\u4e4b\u95f4\u7684\u5e74\u9f84\uff0c\u6545\u91c7\u7528\u4e86\u6e38\u620f\u4e2d\u5904\u4e8e\u8f7b\u5de1\u4e0e\u91cd\u5de1\u4e4b\u95f4\u7684\u5927\u6dc0\u548c\u6700\u4e0a\u7ea7\u7684\u8010\u4e45\u3002\n*\u706b\u529b\uff1a\u53c2\u8003\u88c5\u5907\uff0c\u4e8c\u6539\u536b\u661f\u770b\u8d77\u6765\u5f88\u5c4c\u5b9e\u9645\u4e0a\u5e76\u6ca1\u6709\u4ec0\u4e48\u5375\u7528\uff0c\u6240\u4ee5\u9009\u62e9\u4e86\u540c\u6837\uff08\u663c\u6218\u4e2d\uff09\u7136\u5e76\u5375\u7684poi\u3002\u7136\u800c\u7530\u4e2d\u4e4b\u6bcd\u88c5\u5907\u7684\u6c89\u8239\u4e4b\u7b14\u5374\u662f\u8d85\u8d8a\u6e38\u620f\u5236\u5ea6\u7684\u5b58\u5728\uff0c\u6240\u4ee5\u7ed9\u4e86\u6bd4\u6e38\u620f\u4e2d\u6700\u5f3a\u7684\u7406\u4e8b\u957f\u9ad8\u4e00\u70b9\u7684\u706b\u529b\u3002\n*\u88c5\u7532\uff1a\u7530\u4e2d\u53d7\u73a9\u5bb6\u65e0\u6570\u53e3\u8bdb\u7b14\u4f10\u800c\u4e0d\u5012\uff0c\u88c5\u7532\u65e0\u7591\u662f\u5f88\u5f3a\u7684\uff0c\u521a\u597d\u7530\u4e2d\u662f\u4e2a\u80a5\u5b85\uff0c\u6545\u4ee5\u4ed6\u7684\u4f53\u91cd\u7684\u76ee\u6d4b\u503c\u4f5c\u4e3a\u88c5\u7532\u3002\n*\u96f7\u88c5\uff1a\u53c2\u8003\u706b\u529b\uff0c\u7530\u4e2d\u6218\u6597\u529b\u8981\u7136\u5e76\u5375\u6240\u4ee5\u5e76\u4e0d\u80fd\u7ed9\u4ed6\u96f7\u88c5\uff0c\u7530\u4e2d\u4e4b\u6bcd\u5219\u662f\u9700\u8981\u591c\u6218\u7684\uff0c\u800c\u4ffe\u65af\u9ea6\u7684\u96f7\u88c5\u5bf9\u5176\u591c\u6218\u7684\u610f\u4e49\u6709\u591a\u91cd\u8981\u5927\u5bb6\u90fd\u662f\u77e5\u9053\u7684\u3002\n*\u56de\u907f\uff1a\u7530\u4e2d\u80cc\u4e86\u53e3\u9505\uff0c\u6240\u4ee5\u7528\u540c\u6837\u81ea\u5e26\u80cc\u9505\u7684\u5929\u6d25\u98ce\u6539\u7684\u6570\u636e\uff1b\u7530\u4e2d\u4e4b\u6bcd\u5728\u5929\u4e0a\u98de\u7684\uff0c\u56de\u907f\u5f53\u7136\u4e0d\u80fd\u548c\u8230\u5a18\u540c\u65e5\u800c\u8bed\uff0c\u6240\u4ee5\u9009\u62e9\u4e86\u540c\u6837\u8ddf\u522b\u4eba\u6ca1\u6cd5\u6bd4\u7684\u79cb\u6708\u7684\u5bf9\u7a7a\u503c\u3002\n*\u642d\u8f7d\uff1a\u7530\u4e2d\u642d\u8f7d\u7684\u662f\u536b\u661f\uff0c\u7f16\u8f91\u5f53\u65f6\u6ca1\u843d\u5730\u7684\u536b\u661f\u662f\u4e00\u53ea\u8f7b\u5de1\u548c\u4e94\u822a\u6539\u4e8c\uff0c\u6240\u4ee5\u662f\u4e09\u53ea\uff08\u73b0\u5728\u65e9\u5c31\u843d\u5730\u4e86\uff09\uff0c\u7530\u4e2d\u4e4b\u6bcd\u2026\u2026\u672c\u8eab\u5c31\u662f\u4e2a\u642d\u8f7d\u3002\n*\u901f\u529b\uff1a\u7530\u4e2d\u662f\u5de1\u6d0b\u8230\uff0c\u8fd8\u80cc\u4e86\u4e2a\u56de\u907f\u9505\uff0c\u9ad8\u901f\u5e94\u8be5\u6ca1\u4ec0\u4e48\u60ac\u5ff5\uff1b\u4ed6\u5988\u2026\u2026\u5728\u5929\u4e0a\u98de\u7740\u5462\n*\u8fd0\uff1a\u8230C\u80fd\u706b\u6210\u73b0\u5728\u8fd9\u4e2a\u6837\u5b50\u9664\u4e86\u5b83\u672c\u8eab\u548c\u7530\u4e2d\u8fd0\u8425\u80fd\u529b\u7684\u4f18\u79c0\u4e4b\u5904\u5916\u8fd0\u6c14\u65e0\u7591\u4e5f\u662f\u6781\u597d\u7684\u3002\n*\u88c5\u5907\uff1a\u7f36\u662f\u80cc\u9505\u7684\u610f\u601d\uff0c\u7af9\u873b\u8713\u8868\u793a\u98de\u5988\uff0c\u6c89\u8239\u4e4b\u7b14\u6307\u5982\u6708\u90a3\u6ce1\u5c4e\uff0c\u53ec\u5524\u8bbe\u65bd\u8868\u793a\u7530\u4e2d\u8bbe\u8ba1\u7684\u6e38\u620f\u5173\u5361\u3002\u4e03\u4e09\u4e03\u56db\u4e03\u4e94\u662f\u7687\u5386\uff0c\u5c31\u662f\u4e5d\u4e03\u8230\u653b\u4e5d\u4e5d\u8230\u7206\u90a3\u4e2a\u6570\u5b57\u7684\u610f\u601d\u3002\u4eca\u5e74\u662f\u7687\u53862677\u5e74\uff0c\u9662\u957f\u662f13\u5e74\u51fa\u6765\u7684\uff0c\u7406\u4e8b\u957f\u548c\u6c89\u5982\u6708\u90fd\u662f15\u5e74\uff0c\u536b\u661f\u5e74\u5e74\u6709\u53ea\u662f\u4e3a\u4e86\u7559\u4e2a\u5b58\u5728\u611f\u3002\n\n\u4ee5\u4e0a\u6458\u81ea\u539f\u7f16\u8f91\u8005\u8bc4\u8bba\u533a\u3002\n\n<del>\u4f55\u6b62\u90a33\u53ea\u536b\u661f\uff0c\u8fde\u53cb\u519b\u8230\u961f\u5927\u6982\u9a6c\u4e0a\u90fd\u8981\u536b\u661f\u843d\u5730\u4e86</del>\n}}\n\n{{Hide|\u6807\u9898=\u4eba\u8bbe\u7acb\u7ed8|\u5185\u5bb9=\n[[File:\u7530\u4e2d_\u4eba\u8bbe\u56fe.jpg|400px]]\n}}\n\n==\u7530\u4e2d\u76f8\u5173==\n\n'''\u300c\u7530\u4e2d\u5c07\u964d\u5927\u4efb\u65bc\u662f\u63d0\u7763\u4e5f\uff0c\u5fc5\u5148\u82e6\u5176\u63a8\u5716\uff0c\u52de\u5176\u958b\u767c\uff0c\u9913\u5176\u8cc7\u6e90\uff0c\u7a7a\u4e4f\u6c34\u6876\uff0c\u884c\u62c2\u4e82\u5176\u822a\u5411\uff0c\u6240\u4ee5\u8840\u5fc3\u5c3f\u6027\uff0c\u98db\u5abd\u5176\u6240\u4e0d\u80fd\u3002\u300d '''\n\n===\u7530\u4e2d\u4e4b\u540d===\n\n\u4f5c\u4e3a\u5546\u4e1a\u6e38\u620f\uff0c\u8230\u961fCollection\u7684\u8fd0\u8425\u65b9\u66f4\u591a\u7684\u4ee5\u96c6\u4f53\u7684\u540d\u4e49\u8fdb\u884c\u6d3b\u52a8\uff0c\u521a\u5f00\u59cb\u65f6\u7530\u4e2d\u5728\u666e\u901a\u73a9\u5bb6\u4e2d\u7684\u77e5\u540d\u5ea6\u5e76\u4e0d\u7b97\u7279\u522b\u9ad8\u3002\n\n2015\u5e74\u64ad\u653e\u7684\u8230\u961fCollection\u52a8\u753b\u5f15\u8d77\u4e86\u5e7f\u6cdb\u7684\u5173\u6ce8\u4e0e\u4e89\u8bae\uff0c\u4f5c\u4e3a\u4f5c\u54c1\u4e3b\u50ac\u5e76\u64b0\u5199\u4e86\u52a8\u753b\u5267\u60c5\u5927\u7eb2\u7684\u7530\u4e2d\u8c26\u4ecb\u4e5f\u56e0\u6b64\u66dd\u5149\u5ea6\u5927\u589e\uff0c\u4e0e\u52a8\u753b\u7684\u5176\u4ed6\u4e3b\u8981\u5236\u4f5c\u4eba\u4e00\u8d77\uff0c\u6210\u4e3a\u4e86\u7231\u597d\u8005\u4eec\u53e3\u8bdb\u7b14\u4f10\u7684\u5bf9\u8c61{{\u9ed1\u5e55|\u5982\u6708 \u4f60\u6b7b\u5f97\u597d\u60e8\u554a}}\u3002\u8fd9\u5176\u4e2d\uff0c\u7530\u4e2d\u7684\u59d3\u6c0f\u4f5c\u4e3a\u65e5\u672c\u7b2c\u4e00\u5927\u59d3\uff0c\u5176\u6717\u6717\u4e0a\u53e3\u7a0b\u5ea6\uff0c\u5176\u53ef\u7528\u7d20\u6750\u4e4b\u591a\uff0c\u5728\u9b3c\u755c\u6076\u641e\u65b9\u9762\u7acb\u523b\u663e\u793a\u51fa\u4e86\u65e0\u4e0e\u4f26\u6bd4\u7684\u4fbf\u5229\u6027\uff1a\n\n*<del>\u5c71\u672c</del>\u7530\u4e2d\u6211\u65e5\u4f60\u5148\u4eba\uff01\uff08\u7535\u89c6\u5267\u300a\u4eae\u5251\u300b\u4e2d\u695a\u4e91\u98de\u7684\u53c2\u8c0b\u957f\u6797\u5fd7\u5f3a\u53f0\u8bcd-\u539f\u53f0\u8bcd\u662f\u5c71\u672c\uff09\n*\u7530\u65e5\u5148\uff08\u4e0a\u9762\u53f0\u8bcd\u7684\u7b80\u5316\u7248\uff09\n*\u98de\u6bcd\u8230\u7530\u4e2d\n*\u4e0d\u843d\u4e4b\u98de\u5988\u7530\u4e2d\uff08\u7136\u800c\u8fd9\u4e2a\u7530\u4e2d\u548c[[\u8230\u961fCollection:\u957f\u6ce2|\u957f\u6ce2]]\u7684\u63d0\u7763\u201c\u4e0d\u5c48\u4e4b\u731b\u5c06\u7530\u4e2d\u201d\u6ca1\u6709\u534a\u6bdb\u94b1\u5173\u7cfb\uff0c\u5927\u6982\uff09\n*\u6b63\u89c4\u98de\u6bcd\u7530\u4e2d\uff0c\u51fa\u51fb\n\uff08\u5f85\u8865\u5145\uff09\n\n===\u7530\u4e2d\u98de\u5988===\n\u5177\u4f53\u51fa\u73b0\u65f6\u95f4\u4e0d\u8be6\uff0c\u4f46\u786e\u5b9e\u5728\u8230\u961fcollection\u5427\u4e2d\u5e7f\u6cdb\u5b58\u5728\uff0c\u65e9\u671f\u5b58\u5728\u7740\u8c29\u9a82\u8272\u5f69\uff0c\u4f46\u7531\u4e8e\u6e38\u620f\u4e2d\u8d8a\u6765\u8d8a\u591a\u51fa\u73b0\u7684\u9b3c\u755c\u4efb\u52a1\uff0c\u5982\u65b0\u4e09\u5ddd\uff0c\u6d77\u7a81\u4efb\u52a1\u7b49\uff0c\u53ca4-5\u5b9e\u88c5\u540e\uff0c\u65a9\u6740\u9636\u6bb5\u4e0e\u78e8\u8840\u9636\u6bb5\u96be\u5ea6\u7684\u5de8\u5927\u53cd\u5dee\uff0c\u9010\u6e10\u53d8\u6210\u4e00\u79cd\u8c03\u4f83\u7530\u4e2d\u7684\u8bed\u8a00\u3002\u76f8\u4fe1\u968f\u7740\u65e5\u540e\u66f4\u591a\u7684\u7530\u4e2d\u8111\u6d1e\u7684\u51fa\u73b0\uff0c\u7530\u4e2d\u98de\u5988\u8fd9\u53e5\u8bdd\u4f1a\u8d8a\u6765\u8d8a\u591a\u7684\u51fa\u73b0\u3002<del>\u63a8\u56fe\u6216\u8005\u505a\u4efb\u52a1\u7684\u65f6\u5019\u6765\u4e00\u53e5\u7530\u4e2d\u98de\u5988\u603b\u662f\u6ca1\u9519\u7684\u3002</del>\n\u7530\u4e2d\uff1a\u603b\u6709\u975e\u63d0\u98de\u6715\u5988\n\n===\u7a00\u6709\u5ea6===\n\n\u65e5\u6587\u8230C\u7ef4\u57fa\u7684\u6389\u843d\u7edf\u8ba1\u4e2d\uff0c\u7530\u4e2d\u662f\u5e38\u89c1\u7684\u9650\u5b9a\u6389\u843d\u4e4b\u4e00\uff0c\u5f80\u5f80\u51fa\u73b0\u5728\u5404\u79cd\u65b0\u5b9e\u88c5\u7684\u96be\u5ea6\u53cd\u4eba\u7c7b\u7684\u5730\u56fe\u3002\n\n{{Hide|\u6807\u9898=\u65b0\u7c734-5boss\u6389\u4e86\u7530\u4e2d\uff0c\u8bf7\u95ee\u597d\u7528\u5417\uff1f|\u5185\u5bb9=[[File:\u7530\u4e2d_\u6389\u843d\u62a5\u544a.jpg|400px]]}}\n\n===\u7530\u4e2d\u5b50\u548c\u5979\u6109\u5feb\u7684\u5c0f\u4f19\u4f34\u4eec===\n\n\u7530\u4e2d\u7684\u5176\u4ed6\u840c\u5316\u5f62\u8c61\uff0c\u4ee5\u53ca\u5176\u4ed6\u8230C\u5b98\u65b9\u76f8\u5173\u4eba\u58eb\u7684\u840c\u5316\u8bf7\u53c2\u89c1\uff1a[http://matome.naver.jp/odai/2140487940112099101 \u7530\u4e2dP\u3068\u6109\u5feb\u306a\u4ef2\u9593\u305f\u3061\u307e\u3068\u3081]\u3002\n\n<s>\u7136\u800c\u4f3c\u4e4e\u591a\u6570\u65e5\u672c\u5b85\u6c11\u5bf9\u5927\u53d4\u4eec\u7684\u8fd9\u79cd\u65e0\u8282\u64cd\u840c\u5316\u884c\u4e3a\u7684\u611f\u60f3\u662f\u201c\u54e6\u54e6\u597d\u6076\u5fc3\u201d</s>\n\n===\u5728{{ruby|DDH184|\u52a0\u8d3a}}\u9644\u8fd1\u73b0\u8eab?===\n\u57288\u670827\u53f7\u51e4\u51f0\u7f51\u7684\u8bb0\u8005\u524d\u5f80\u4e86\u73b0\u573a\u62cd\u6444\u65f6,\u53d1\u73b0\u62cd\u6444\u65c1\u8fb9\u7684\u4e00\u4e2a\u8def\u4eba\u9177\u4f3c\u7530\u4e2d(\u5728\u5b98\u65b9\u7684\u63a8\u7279\u4e0a\u4e5f\u6709{{ruby|DDH184|\u52a0\u8d3a}}\u7684\u76f8\u5173\u7167\u7247,\u4e5f\u8bb8\u771f\u7684\u662f\u672c\u4eba?)\u4f46\u4e0d\u7ba1\u600e\u4e48\u8bf4,\u63d0\u7763\u4eec\u770b\u5230\u540e,\u4eb2\u5207\u611f(\u96