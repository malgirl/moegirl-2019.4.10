u3044\u3044\u5b50\u3060\u3088\u306d\u3002}}\n}}\n\n== \u7b80\u4ecb ==\n[[File:\u3010\u5927\u4eba\u306e\u968e\u6bb5\u3011\u79cb\u5c71 \u96bc\u4eba.jpg|290px|thumb|\u5b66\u56ed\u796d\uff0c\u6253\u5f00\u4e86\u65b0\u4e16\u754c\u7684\u5927\u95e8]]\n17\u5c81\u7684\u73b0\u5f79[[\u9ad8\u4e2d\u751f|\u9ad8\u4e2d\u4e8c\u5e74\u7ea7\u751f]]\uff0c\u7ec4\u5408\uff08\u517c\u5b66\u6821[[\u8f7b\u97f3\u90e8]]\u4e50\u961f\uff09\u300c[[High\u00d7Joker]]\u300d\u7684\u521b\u59cb\u4eba\u548c\u961f\u957f\uff0c\u8d1f\u8d23\u5f39\u5409\u4ed6\u548c\u4f5c\u8bcd\u3002\u540c\u793e\u56e2\u7684\u6210\u5458\u662f[[\u51ac\u7f8e\u65ec]]\u3001[[\u698a\u590f\u6765]]\u3001[[\u82e5\u91cc\u6625\u540d]]\u3001[[\u4f0a\u6fd1\u8c37\u56db\u5b63]]\uff0c\u4e94\u4eba\u4e00\u8d77\u8f6c\u804c\u6210\u4e3a\u5076\u50cf\u3002\u5409\u4ed6\u6c34\u5e73\u4f3c\u4e4e\u5f88\u9ad8\uff0c\u6b63\u662f\u96bc\u4eba\u7684\u8868\u6f14\u5438\u5f15\u4e86[[\u4f0a\u6fd1\u8c37\u56db\u5b63]]\u5165\u90e8\u3002\n\n\u516c\u5f0f\u8ba4\u5b9a\u7684[[\u65e5\u672c\u666e\u901a\u9ad8\u4e2d\u751f|\u5e73\u51e1\u666e\u901a\u9ad8\u4e2d\u751f]]\uff0c\u6ca1\u6709\u4ec0\u4e48\u8fc7\u53bb\u7684\u5305\u88b1\uff0c\u5e94\u8be5\u662f\u5168315\u4e8b\u52a1\u6240\u6700\u666e\u901a\u7684\u4e00\u4e2a\u4eba\u4e86{{\u9ed1\u5e55|\uff0c\u666e\u901a\u5230OVA\u7684315\u7248\u9a6c\u8def\u987b\u52a0\u5b66\u56ed\uff08\u96fe\uff09\u9884\u544a\u7247\u91cc\u626e\u6f14\u7684\u89d2\u8272\u5b9a\u4f4d\u5c31\u662f\u300c\u666e\u901a\u4eba\u300d<del>\uff08\u8fd8\u51fa\u573a\u5c31\u4fbf\u5f53\u4e86\uff09</del>}}\u3002\u6027\u683c\u70ed\u60c5\u3001\u76f4\u7387\u3001\u53cb\u5584\uff0c\u5f88\u7b80\u5355\u5c31\u80fd\u5174\u9ad8\u91c7\u70c8\u3002\u53d7\u5230\u5938\u5956\u7684\u65f6\u5019\u4f1a\u5bb3\u7f9e\uff0c\u4f46\u5176\u5b9e\u5f88\u9ad8\u5174\u3002\u5bb9\u6613\u611f\u52a8\u54ed\u7684[[\u7231\u54ed\u9b3c]]\u3002\n\n\u56e0\u4e3a\u60f3\u53d7\u6b22\u8fce\u800c\u7ec4\u5efa\u4e86\u4e50\u961f\uff0c\u4e5f\u7ecf\u5e38\u8003\u8651\u5982\u4f55\u66f4\u53d7\u6b22\u8fce\u3002\u4e0d\u8fc7\u968f\u7740\u5076\u50cf\u6d3b\u52a8\u7684\u5f00\u5c55\u9010\u6e10\u6210\u957f\uff0c\u73b0\u5728\u6bd4\u8d77\u4e2a\u4eba\u7684\u6210\u529f\u66f4\u5e0c\u671b\u4e50\u961f\u672c\u8eab\u53d6\u5f97\u6210\u529f\u3002\u4f5c\u4e3a\u961f\u957f\u8ba4\u771f\u8d1f\u8d23\uff0c\u4f1a\u7ecf\u5e38\u5173\u5fc3\u6210\u5458\u7684\u72b6\u51b5\uff0c\u968f\u65f6\u51c6\u5907\u4f38\u51fa\u63f4\u624b\u3002\u5177\u6709\u5e26\u9886\u7ec4\u5408\u594b\u53d1\u5411\u4e0a\u7684\u610f\u5fd7\u529b\uff0c\u80fd\u591f\u534f\u8c03\u597d\u6027\u683c\u5404\u5f02\u7684\u6210\u5458\uff0c\u5b9e\u9645\u662f\u4e2a\u4f18\u79c0\u7684\u9886\u5bfc\u8005\u3002\n\n\u867d\u7136\u89d2\u8272\u672c\u4eba\u81ea\u8ba4\u6beb\u65e0\u7279\u8272\uff0c\u89c9\u5f97\u81ea\u5df1\u62fc\u4eba\u6c14\u80af\u5b9a\u62fc\u4e0d\u8fc7\u5176\u4ed6\u4eba\u8bbe<del>\u4e0a\u5929\u5165\u5730</del>\u4e30\u5bcc\u7684\u5076\u50cf\uff0c\u5e73\u51e1\u666e\u901a\u9ad8\u4e2d\u751f\u7684\u4eb2\u5207\u611f\u53cd\u800c\u5907\u53d7\u73a9\u5bb6\u559c\u7231\u3002\u7b2c\u4e00\u56de315\u4e8b\u52a1\u6240\u4eba\u6c14\u603b\u9009\u4e3e\u4ece\u4e2d\u95f4\u53d1\u8868\u5c31\u662f\u7b2c\u4e00\uff0c\u6700\u7ec8\u9ad8\u7968\u5f53\u9009'''{{color|red|\u7b2c\u4e00\u540d}}'''\u3002<del>\u7136\u540e\u5728\u4f5c\u4e3a\u51a0\u519b\u83b7\u5f97\u7684\u65b0SR\u5361\u9762\u4e2d\u559c\u6781\u800c\u6ce3\u4e86\u3002</del>\n\n== \u8da3\u95fb\u8f76\u4e8b ==\n*\u96bc\u4eba\u7684\u5370\u8c61\u8272\u662f{{color|#fe6b02|\u6a59\u8272}}\u3002<span style=\"background-color:#fe6b02; border:1px solid #000000; text-align:center;\">{{#if:{{{2|}}}|&nbsp;{{{2}}}&nbsp;|&nbsp;&nbsp;&nbsp;&nbsp;}}</span>\n*\u5728[[\u4f0a\u6fd1\u8c37\u56db\u5b63|\u56db\u5b63]]\u5165\u90e8\u524d\u662f\u4e50\u961f\u7684\u4e3b\u5531\u3002\n*\u559c\u6b22\u6536\u96c6\u6c90\u6d74\u9732\u3002\n*SR\u5361\u300cOff-shot\u300d\u7279\u8bad\u540e\u5361\u9762\u4e2d\u4ee5\u4e3a\u6536\u5230\u4e86\u60c5\u4e66\u53c8\u7f9e\u53c8\u559c\uff0c\u4f46\u4ed4\u7ec6\u770b\u56fe\u4f1a\u53d1\u73b0\u4fe1\u5c01\u80cc\u9762\u7684\u7f72\u540d\u662f\u300c\u6e90\u6b21\u90ce\u300d\u2026\u2026\n*\u300cH\u00d7J's Special Summer!\u300d\u6d3b\u52a8\u4e2d\u5224\u660e\u6709\u4e2a\u4e07\u80fd\u7684\u51b7\u50b2\u54e5\u54e5\uff0c\u96bc\u4eba\u591a\u534a\u56e0\u6b64\u6709\u4e9b\u81ea\u5351\u3002\u300c{{lj|H\u00d7J\u6674\u5929! Marine Blue Live!}}\u300d\u6d3b\u52a8\u4e2d\u5224\u660e\u53cc\u4eb2\u90fd\u5916\u6d3e\u51fa\u56fd\u4e86\u3002\n*\u548c[[\u5927\u6cb3\u6b66]]\u5173\u7cfb\u5f88\u597d\u3002\u8fd9\u4e24\u4eba\u518d\u52a0\u4e0a[[\u9e70\u57ce\u606d\u4e8c]]\u548c[[\u515c\u5927\u543e]]\u90fd\u559c\u6b22\u6253\u6e38\u620f\uff0c\u56db\u4eba\u88ab\u79f0\u4e3a\u300c\u6e38\u620f\u7ec4\u300d\u3002\u540e\u67652018\u767d\u8272\u60c5\u4eba\u8282\u6d3b\u52a8\u4e2d\u786e\u5b9a\u300c\u6e38\u620f\u7ec4\u300d\u8fd8\u5305\u542b[[\u6a58\u5fd7\u72fc]]\u3002\n*[[\u5927\u80c3\u738b]]\u3002\u8c6a\u723d\u7684\u5403\u76f8\u88ab\u5b66\u6821\u7684\u7537\u540c\u5b66\u8bc4\u4ef7\u4e3a\u300c\u7537\u5b50\u6c49\u4e2d\u7684\u7537\u5b50\u6c49\u300d\u3002\n*\u662f\u9694\u58c1[[\u5b89\u90e8\u83dc\u83dc]]\u7684\u58f0\u4f18[[\u4e09\u5b85\u9ebb\u7406\u60e0]]\u7684\u62c5\u5f53\u5076\u50cf\u3002\u96bc\u4eba\u4e0a\u4f4dSR\u6d3b\u52a8\u7684\u65f6\u5019\uff0c\u4e09\u5b85P\u4e00\u5f00\u5c40\u5c31\u53d6\u5f97\u4e86\u7b2c4\u540d\u7684\u6392\u540d\u3002<del>17\u5c81\u62c5\u5f5317\u5c81\uff0c\u6ca1\u6bdb\u75c5</del>\n**[[\u6a58\u5fd7\u72fc]]\u7684\u58f0\u4f18[[\u53e4\u7551\u60e0\u4ecb]]\u4e5f\u662f\u96bc\u4ebaP\u3002\n*\u58f0\u4f18[[\u5343\u53f6\u7fd4\u4e5f]]\u9ad8\u4e2d\u65f6\u5f39\u8fc7\u5409\u4ed6\u3002\u540e\u67653rd Live\u7684\u4ed9\u53f0\u516c\u6f14\u4e2d[[High\u00d7Joker]]\u4f5c\u4e3a\u5b9e\u6253\u5b9e\u7684\u4e50\u961f\u8868\u6f14\u4e86\u3002<del>\u8fd9\u5e74\u5934\u58f0\u4f18\u9886\u5de5\u8d44\u771f\u96be</del>\n\n== \u52a8\u753b\u76f8\u5173 ==\n\nTV\u52a8\u753b\u4e3b\u89d2\u7ec4\u4e4b\u4e00\u3002\u5176\u4e2d\u7b2c7\u8bdd\u548c\u7b2c10\u8bdd\u662fHigh\u00d7Joker\u56de\u3002\n\n\u7b2c7\u8bdd\u4ee5\u6210\u5458\u81ea\u5df1\u62cd\u6444\u51fa\u9053PV\u4e3a\u7ebf\u7d22\uff0c\u8bb2\u8ff0\u4e86High\u00d7Joker\u7ed3\u6210\u7684\u7ecf\u8fc7\u3002\n\n\u7b2c10\u8bdd\u4e2d\uff0cHigh\u00d7Joker\u5728\u81ea\u5df1\u9ad8\u4e2d\u7684\u6587\u5316\u796d\u4e0a\u4e3e\u884c\u4e86\u6f14\u5531\u4f1a\u3002\n\n== \u6b4c\u66f2 ==\n[[File:\u3010Star Melody\u3011\u79cb\u5c71 \u96bc\u4eba.jpg|\u65e0\u6846|\u53f3]]\n=== \u4e2a\u4eba\u66f2 ===\n*[[PRECIOUS TONE]]\n\n=== \u7ec4\u5408\u66f2 ===\n*[[HIGH JUMP NO LIMIT]]\n*[[\u5168\u80fd\u5c0f\u4e11|JOKER\u2197\u30aa\u30fc\u30eb\u30de\u30a4\u30c6\u30a3]]\n*[[OUR SONG \u90a3\u662f\u4e16\u754c\u4e0a\u552f\u4e00\u7684|{{lj|OUR SONG -\u305d\u308c\u306f\u4e16\u754c\u3067\u3072\u3068\u3064\u3060\u3051-}}]]\n*[[Sunset Colors|Sunset\u2605Colors]]\n*[[\u4e07\u82b1\u7b52TOURHYTHM|\u30ab\u30ec\u30a4\u30c9 TOURHYTHM]]\uff08\u7ffb\u5531\uff09\n\n=== \u5408\u5531\u66f2 ===\n*[[DRIVE A LIVE]]\n*[[Beyond The Dream]]\n*[[\u5982\u540c\u5728\u591c\u7a7a\u4e2d\u95ea\u8000\u7684\u661f\u661f|{{lj|\u591c\u7a7a\u3092\u714c\u3081\u304f\u661f\u306e\u3088\u3046\u306b}}]]\n*[[Reason|Reason!!]]\n*[[\u590f\u65f6\u95f4\u6d82\u9e26|{{lj|\u590f\u6642\u9593\u30b0\u30e9\u30d5\u30a3\u30c6\u30a3}}]]\n*[[\u5982\u679c\u67d0\u4e00\u5929\u63d0\u53ca\u8fd9\u4e2a\u77ac\u95f4|{{lj|\u3044\u3064\u304b\u3053\u306e\u77ac\u9593\u306b\u540d\u524d\u3092\u3064\u3051\u308b\u306a\u3089}}]]\n*[[GLORIOUS ROAD|GLORIOUS RO@D]]\n\n== [[\u5076\u50cf\u5927\u5e08 SideM LIVE ON ST@GE|LOS]]\u5361\u9762\u7cbe\u9009 ==\n\n<gallery mode=packed-hover>\n\u3010\u9752\u6625\u30c9\u771f\u3093\u4e2d\u3011\u79cb\u5c71 \u96bc\u4eba.png|{{lj|\u9752\u6625\u30c9\u771f\u3093\u4e2d}}\n\u3010\u9752\u6625\u30c9\u771f\u3093\u4e2d\u3011\u79cb\u5c71 \u96bc\u4eba+.png|{{lj|\u9752\u6625\u30c9\u771f\u3093\u4e2d+}}\nSSR\u3010\u672c\u6c17\u306e\u30e1\u30ed\u30c7\u30a3\u3011\u79cb\u5c71 \u96bc\u4eba.png|{{lj|\u672c\u6c17\u306e\u30e1\u30ed\u30c7\u30a3}}\nSSR\u3010\u672c\u6c17\u306e\u30e1\u30ed\u30c7\u30a3\u3011\u79cb\u5c71 \u96bc\u4eba+.png|{{lj|\u672c\u6c17\u306e\u30e1\u30ed\u30c7\u30a3+}}\n</gallery>\n\n{{\