\n| \u4e30\u81e3\u79c0\u5409 || \u65e5\u672c || \u5e1d\u653f\u3001\u519b\u7565 || \u4e2d\u56fd\u3001\u671d\u9c9c || \u5e1d\u653f\u3001\u519b\u7565\n|-\n| \u62ff\u7834\u4ed1 || \u6cd5\u56fd || \u5e1d\u653f\u3001\u519b\u7565 || \u82f1\u56fd || \u519b\u7565\u3001\u4f20\u5947\n|-\n| \u5e0c\u7279\u52d2 || \u5fb7\u56fd || \u519b\u7565\u3001\u4f20\u5947 || \u4fc4\u56fd\u3001\u7f8e\u56fd || \u519b\u7565\uff08\u4fc4\u56fd\uff09\u4f20\u5947\uff08\u7f8e\u56fd\uff09\n|}\n\n\n== \u5bbf\u547d\u4e4b\u4eba ==\n\n\u738b\u6743\u662f\u4eba\u7c7b\u9636\u7ea7\u5dc5\u5cf0\u7684\u6743\u529b\uff0c\u800c\u9876\u70b9\uff0c\u6c38\u8fdc\u53ea\u6709\u4e00\u4eba\u7684\u7acb\u8db3\u4e4b\u5730\u3002\u56e0\u6b64\u540c\u65f6\u4ee3\u3001\u540c\u4e00\u9886\u571f\u4e0a\u7684\u541b\u738b\u4e0e\u541b\u738b\u4e4b\u95f4\uff0c\u4e3a\u4e86\u4e89\u593a\u7edf\u6cbb\u6743\u76f8\u7231\u76f8\u6740\u3002\u8fd9\u4e9b\u5bbf\u547d\u4e4b\u4eba\u9760\u7684\u8fd1\u4e86\uff0c\u5f7c\u6b64\u4f1a\u4ea7\u751f\u5947\u5999\u7684\u5316\u5b66\u53cd\u5e94\u3002\n\n\u5217\u4e2a\u7b80\u5355\u7684\u8303\u4f8b\uff1a\n\n{| class=\"wikitable\"\n|-\n! \u5bbf\u547d\u4e4b\u4eba !! \u654c\u5bf9\u6548\u679c !! \u53cb\u519b\u6548\u679c\n|-\n| \u674e\u715cvs\u8d75\u5321\u80e4 || \u7740\u8ff7\uff1a\u8d75\u5321\u80e4\u627f\u53d7\u674e\u715c\u7684\u63a7\u5236\u6548\u679c\u7684\u65f6\u95f4\u5ef6\u957f\uff1b\u72c2\u70ed\uff1a\u674e\u715c\u53d7\u5230\u8d75\u5321\u80e4\u9020\u6210\u7684\u4f24\u5bb3\u63d0\u5347\uff1b||\u9f13\u821e\uff1a\u8d75\u5321\u80e4\u653b\u51fb\u63d0\u5347\u3002\u611f\u5316\uff1a\u674e\u715c\u7684\u589e\u76ca\u7c7b\u6548\u679c\u6301\u7eed\u65f6\u95f4\u589e\u52a0\u3002\n|}\n\n\n== \u541b\u738b\u89d2\u8272\u5361\u8303\u4f8b ==\n[[File:\u5e0c\u7279\u52d2-\u89d2\u8272\u63d2\u753b.jpg|300px]]\n\n\u541b\u738b\uff1a\u5e0c\u7279\u52d2\n\n\u522b\u540d\uff1a\u94a2\u94c1\u4e4b\u6708\n\n\u65f6\u671f\uff1a\u7b2c\u56db\u5e1d\u56fd\uff08\u6708\u7403\uff09\u65f6\u671f\n\n\n{{color|red|'''\u2014\u2014\u738b\u6743\u91cf\u7ea7\u2014\u2014'''}}\n\n'''\u738b\u6743\u91cf\u7ea7'''\uff1a\u25cf\u25cf\u25cf\u25cf\u25cb\n\n\n'''\u5e1d\u653f''' \u25cf\u25cf\u25cf\u25cf\u25cb\uff08\u65f6\u4ee3\u7684\u5b9d\u51a0\uff09\n\n\u5f3a\u76db\u738b\u671d\u7684\u7f14\u9020\u8005\uff0c\u4f7f\u4e00\u56fd\u8d70\u5411\u5dc5\u5cf0\u7684\u541b\u738b\uff0c\u6216\u662f\u5371\u673a\u65f6\u523b\u7684\u6551\u4e3b\u3002\n\n\u5e0c\u7279\u52d2\u5728\u4f4d\u65f6\u8fc5\u901f\u63d0\u5347\u4e86\u56fd\u529b\uff0c\u4ee4\u5fb7\u56fd\u8dfb\u8eab\u4e16\u754c\u5f3a\u56fd\u3002\n\n\n'''\u519b\u7565''' \u25cf\u25cf\u25cf\u25cf\u25cb\uff08\u4e07\u519b\u7684\u5de1\u793c\uff09\n\n\u64c5\u957f\u6218\u4e89\u827a\u672f\uff0c\u6216\u662f\u5728\u6218\u573a\u4e0a\u6709\u7a81\u51fa\u8868\u73b0\u7684\u541b\u738b\u3002\n\n\u95ea\u7535\u6218\u4e4b\u540d\u4ee4\u4eba\u5370\u8c61\u6df1\u523b\u3002\n\n\n'''\u6cd5\u7edf''' \u25cf\u25cf\u25cf\u25cb\u25cb\uff08\u5168\u5883\u7684\u7389\u5ea7\uff09\n\n\u6cbb\u7406\u5f3a\u5927\u56fd\u5ea6\u7684\u541b\u738b\uff0c\u5e76\u672a\u6301\u6709\u201c\u7687\u5e1d\u201d\u7684\u6cd5\u7edf\u3002\n\n\u7b2c\u4e09\u5e1d\u56fd\u65f6\u671f\u5143\u9996\u66fe\u5177\u6709\u7f57\u9a6c\u6cd5\u7edf\uff0c\u4f46\u672a\u52a0\u5195\u4e3a\u7687\u5e1d\u3002\u5982\u4eca\u867d\u7136\u52a0\u5195\u4e3a\u6708\u9762\u7687\u5e1d\uff0c\u4f46\u5df2\u7ecf\u5931\u53bb\u4e86\u7f57\u9a6c\u5e1d\u56fd\u7684\u6cd5\u7edf\uff0c\u56e0\u6b64\u6cd5\u7edf\u5c5e\u6027\u964d\u4f4e\u4e86\u3002\n\n\n'''\u4f20\u5947''' \u25cf\u25cf\u25cf\u25cf\u25cf\uff08\u5c18\u4e16\u7684\u795e\u8bdd\uff09\n\n\u5df2\u88ab\u5168\u4e16\u754c\u6240\u719f\u77e5\u7684\u4f20\u5947\u541b\u738b\uff0c\u7559\u4e0b\u6570\u4e0d\u6e05\u7684\u6545\u4e8b\u548c\u8f76\u8bdd\u4e3a\u4eba\u4eec\u6d25\u6d25\u4e50\u9053\uff0c\u751a\u81f3\u6210\u4e3a\u67d0\u79cd\u6587\u5316\u7684\u4ee3\u8a00\u8005\u3002\n\n\u5143\u9996\u56e0\u4e8c\u6218\u800c\u94ed\u523b\u4e8e\u4eba\u7c7b\u7684\u5386\u53f2\u8bb0\u5fc6\u4e0a\u3002\u4ed6\u4e00\u624b\u5efa\u7acb\u7684\u7b2c\u4e09\u5e1d\u56fd\u6210\u4e3a\u8bb8\u591a\u6e38\u620f\u3001\u5c0f\u8bf4\u4e0e\u6f2b\u753b\u7684\u9898\u6750\u3002\n\n\n{{color|red|'''\u2014\u2014\u738b\u6743\u70d9\u5370\u2014\u2014'''}}\n\n'''\u3010\u9b54\u738b\u3011'''\n\n\u5f3a\u5ea6\uff1a\u25cf\u25cf\u25cf\u25cf\u25cf\n\n\u76f8\u5173\u5c5e\u6027\uff1a\u519b\u7565\n\n\u8bf4\u660e\uff1a\u5927\u91cf\u5c60\u6740\u5e73\u6c11\uff0c\u9020\u6210\u6570\u91cf\u60ca\u4eba\u7684\u4f24\u4ea1\u6570\u76ee\u3002\n\n\u4f5c\u4e3a\u4e8c\u5341\u4e16\u7eaa\u6700\u53ef\u6015\u7684\u79cd\u65cf\u5c60\u6740\u884c\u4e3a\u7684\u5143\u51f6\uff0c\u5e0c\u7279\u52d2\u5bf9\u6240\u6709\u4e2d\u7acb\u5355\u4f4d\u989d\u5916\u9020\u6210\u4f24\u5bb3\u3002\n\n\n'''\u3010\u8d22\u5bcc\uff1a\u7eb3\u7cb9\u9057\u4ea7\u3011'''\n\n\u5f3a\u5ea6\uff1a\u25cf\u25cb\u25cb\u25cb\u25cb\n\n\u76f8\u5173\u5c5e\u6027\uff1a\u5e1d\u653f\n\n\u8bf4\u660e\uff1a\u5728\u4f4d\u65f6\u66fe\u655b\u805a\u5927\u91cf\u8d22\u5bcc\u3002\u8fd9\u4e00\u70d9\u5370\u80fd\u4e3a\u541b\u738b\u5e26\u6765\u5927\u5c0f\u4e0d\u4e00\u7684\u91d1\u94b1\u6536\u5165\u3002\n\n\u636e\u8bf4\u7eb3\u7cb9\u64a4\u9000\u65f6\u5c06\u641c\u522e\u6765\u7684\u8d22\u5bcc\u96c6\u4e2d\u6210\u4e3a\u5b9d\u85cf\u3002\u4f46\u7531\u4e8e\u5176\u4e2d\u5927\u90e8\u5206\u5df2\u88ab\u5404\u56fd\u653f\u5e9c\u6240\u7f5a\u6ca1\uff0c\u56e0\u6b64\u53ea\u80fd\u63d0\u4f9b\u96f6\u82b1\u94b1\u7a0b\u5ea6\u7684\u82b1\u9500\u3002\n\n\n'''\u3010\u95ea\u7535\u6218\uff1a\u5df2\u786e\u8ba4\u98de\u884c\u4f53\u3011 '''\n\n\u5f3a\u5ea6\uff1a\u25cf\u25cf\u25cf\u25cf\u25cb\n\n\u76f8\u5173\u5c5e\u6027\uff1a\u519b\u7565\u3001\u4f20\u5947\n\n\u8bf4\u660e\uff1a\u4ee5\u98de\u789f\u8fdb\u884c\u95ea\u7535\u6218\u3002\u56e0\u4e3a\u6781\u9ad8\u7684\u98de\u884c\u901f\u5ea6\uff0c\u4ee4\u5143\u9996\u53ef\u4ee5\u65e0\u89c6\u7a7a\u95f4\u8ddd\u79bb\u5feb\u901f\u79fb\u52a8\u4f5c\u6218\u3002\n\n\n'''\u3010\u9ed1\u79d1\u6280\uff1a\u53e6\u5916\u768494\u4e2a\u3011'''\n\n\u5f3a\u5ea6\uff1a\u25cf\u25cf\u25cf\u25cf\u25cf\n\n\u76f8\u5173\u5c5e\u6027\uff1a\u4f20\u5947\n\n\u8bf4\u660e\uff1a\u6709\u7814\u7a76\u4e0d\u53ef\u601d\u8bae\u7684\u8d85\u73b0\u5b9e\u79d1\u6280\u7684\u4f20\u8bf4\u3002\u541b\u738b\u4f1a\u638c\u63e1\u4e00\u79cd\u6216\u591a\u91cd\u5f3a\u5927\u7684\u79d1\u6280\u3002\n\n\u8fd9\u662f\u5c5e\u4e8e\u7b2c\u56db\u5e1d\u56fd\u7684\u9ed1\u79d1\u6280\uff0c\u8fde\u8bb0\u5fc6\u548c\u4e2a\u6027\u90fd\u80fd\u5b8c\u7f8e\u590d\u5236\u7684\u514b\u9686\u6280\u672f\u3002\u8ba9\u5143\u9996\u5728\u53d7\u5230\u81f4\u547d\u4f24\u540e\u518d\u6b21\u5b8c\u597d\u65e0\u635f\u5730\u51fa\u73b0\u3002\n\n\n{{color|red|'''\u2014\u2014\u738b\u6743\u707e\u96be\u2014\u2014'''}}\n\n'''\u3010\u5175\u8d25\u3011'''\n\n\u5f3a\u5ea6\uff1a\u25cf\u25cf\u25cf\u25cf\u25cb\n\n\u76f8\u5173\u5c5e\u6027\uff1a\u519b\u7565\n\n\u8bf4\u660e\uff1a\u5728\u6218\u4e89\u4e2d\u906d\u9047\u4e86\u626d\u8f6c\u541b\u738b\u547d\u8fd0\u7684\u5927\u5931\u8d25\u3002\n\n\u5143\u9996\u5728\u4e8c\u6218\u4e2d\u7684\u5931\u8d25\u4ee4\u7b2c\u4e09\u5e1d\u56fd\u5f7b\u5e95\u6bc1\u706d\uff0c\u6240\u5e78\u4ed6\u672c\u4eba\u64a4\u9000\u5230\u4e86\u6708\u9762\u7684\u7b2c\u56db\u5e1d\u56fd\u3002\u5728\u5931\u8d25\u540e\u65e0\u6cd5\u83b7\u5f97\u4efb\u4f55\u7ecf\u9a8c\u4e0e\u6536\u76ca\u3002\n\n\n'''\u3010\u56f4\u527f\u3011'''\n\n\u5f3a\u5ea6\uff1a\u25cf\u25cf\u25cf\u25cf\u25cf\n\n\u76f8\u5173\u5c5e\u6027\uff1a\u519b\u7565\n\n\u8bf4\u660e\uff1a\u88ab\u591a\u4e2a\u654c\u5bf9\u56fd\u5bb6\u6240\u56f4\u653b\u3002\n\n\u5143\u9996\u9762\u5bf9\u7740\u540c\u76df\u56fd\u7684\u5168\u529b\u56f4\u527f\u3002\u5728\u9762\u5bf9\u5176\u4ed6\u541b\u738b\u653b\u51fb\u65f6\uff0c\u989d\u5916\u627f\u53d7\u4f24\u5bb3\u3002\n\n\n'''\u3010\u5996\u9b54\u5316\uff1a666\u542f\u793a\u5f55\u4e4b\u517d\u3011'''\n\n\u5f3a\u5ea6\uff1a\u25cf\u25cf\u25cf\u25cf\u25cf\n\n\u76f8\u5173\u5c5e\u6027\uff1a\u4f20\u5947\n\n\u8bf4\u660e\uff1a\u88ab\u56fd\u6c11\u89c6\u4e3a\u6076\u9b54\u6216\u602a\u7269\u3002\n\n\u5143\u9996\u88ab\u56fd\u6c11\u89c6\u4e3a666\u542f\u793a\u5f55\u4e4b\u517d\u7684\u5316\u8eab\u3002\u5728\u9886\u754c\u4f5c\u6218\u65f6\uff0c\u5e1d\u653f\u76f8\u5173\u7684\u6548\u679c\u4f1a\u51cf\u5f31\u3002\n\n\n{{color|red|'''\u2014\u2014\u9886\u754c / \u654c\u533a\u2014\u2014'''}}\n\n\u9886\u754c\uff1a\u5fb7\u56fd\uff08\u63d0\u5347\u519b\u7565\u3001\u4f20\u5947\uff09\u3002\n\n\u719f\u6089\u5fb7\u56fd\u7684\u5730\u7406\uff0c\u56e0\u6b64\u4f9d\u7136\u80fd\u53d1\u6325\u5f3a\u5927\u7684\u519b\u4e8b\u5b9e\u529b\u3002\u540c\u65f6\u5728\u672c\u571f\u5177\u6709\u76f8\u5f53\u5927\u7684\u4f20\u5947\u5f71\u54cd\u529b\u3002\n\n\u654c\u533a\uff1a\u4fc4\u56fd\uff08\u964d\u4f4e\u519b\u7565\uff09\uff0c\u7f8e\u56fd\uff08\u964d\u4f4e\u4f20\u5947\uff09\n\n\u66fe\u88ab\u82cf\u8054\u653b\u9677\u67cf\u6797\uff0c\u56e0\u6b64\u5728\u4fc4\u56fd\u4f5c\u6218\u65f6\u964d\u4f4e\u519b\u7565\u5c5e\u6027\u3002\u540c\u65f6\u957f\u671f\u5728\u7f8e\u56fd\u8d85\u7ea7\u82f1\u96c4\u6f2b\u753b\u548c\u6e38\u620f\u91cc\u62c5\u4efb\u88ab\u6253\u8d25\u7684\u53cd\u6d3e\uff0c\u56e0\u6b64\u5728\u7f8e\u56fd\u4f5c\u6218\u65f6\u4f20\u5947\u5c5e\u6027\u4e0b\u964d\u3002\n\n\n{{color|red|'''\u2014\u2014\u5bbf\u547d\u4e4b\u4eba\u2014\u2014'''}}\n\n\u5bbf\u547d\u4e4b\u4eba\uff1a\u65af\u5927\u6797\u3002\n\n\u5728\u9762\u5bf9\u65af\u5927\u6797\u65f6\uff0c\u5e0c\u7279\u52d2\u5177\u6709\u4e0b\u5217\u6548\u679c\uff1a\n\n\u654c\u5bf9\uff1a\u3010\u5e1d\u56fd\u7684\u6bc1\u706d\u3011\u5e0c\u7279\u52d2\u5728\u4e0e\u65af\u5927\u6797\u654c\u5bf9\u65f6\uff0c\u5e1d\u653f\u3001\u519b\u7565\u5e26\u6765\u7684\u6548\u679c\u51cf\u5f31\u3002\n\n\u53cb\u519b\uff1a\u3010\u5143\u9996\u7684\u6124\u6012\u3011\u5e0c\u7279\u52d2\u5728\u4e0e\u65af\u5927\u6797\u540c\u961f\u65f6\uff0c\u4f1a\u63d0\u5347\u9020\u6210\u81f4\u547d\u4f24\u5bb3\u7684\u51e0\u7387\u3002\n\n{{\u840c\u738bEX}}\n\n[[\u5206\u7c7b:\u840c\u738bEX]]"}}}