n|\u5174\u8da3=\u73a9\u5177\u5c4b\u5236\u4f5c\n|\u58f0\u4f18=\n|\u521d\u767b\u573a=2012/05/14\n|\u603b\u9009\u4e3e1=\u5708\u5916\n|\u603b\u9009\u4e3e2=\u5708\u5916\n|\u603b\u9009\u4e3e3=\u5708\u5916\n|\u603b\u9009\u4e3e4=\u5708\u5916\n|\u603b\u9009\u4e3e5=\u5708\u5916\n|\u603b\u9009\u4e3e6=\u5708\u5916\n|\u603b\u9009\u4e3e7=\u5708\u5916\n|\u89d2\u8272\u66f2=\n|\u89d2\u8272\u66f2\u539f\u540d=\n|\u5e8f\u53f7\u65e5\u671f=\n|\u81ea\u6211\u4ecb\u7ecd=<small>{{lj|\u5b50\u3069\u3082\u306e\u9803\u304b\u3089\u305a\u3063\u3068\u82b8\u80fd\u754c\u3067\u751f\u304d\u3066\u304d\u305f\u3093\u3067\u3059\u3002}}<br>{{lj|\u3060\u304b\u3089\u83ef\u3084\u304b\u306a\u3060\u3051\u306e\u4e16\u754c\u3058\u3083\u306a\u3044\u3063\u3066\u5206\u304b\u3063\u3066\u308b\u2026\u3067\u3082\u79c1\u305f\u3061\u306a\u3089\u3084\u308c\u307e\u3059\u3088\u306d\u3002}}<br>{{lj|\u30d7\u30ed\u30c7\u30e5\u30fc\u30b5\u30fc\u3082\u79c1\u3092\u4fe1\u3058\u3066\u304f\u308c\u307e\u3059\u304b\uff1f}}</small><br>\u6211\u4ece\u5f88\u5c0f\u7684\u65f6\u5019\u5c31\u4e00\u76f4\u5728\u827a\u80fd\u754c\u751f\u5b58\u4e86\u3002<br>\u8fd9\u4e16\u754c\u4e0d\u53ea\u6709\u7f8e\u597d\uff0c\u8fd9\u6211\u660e\u767d\u2026\u2026\u4f46\u662f\u6211\u4eec\u4e00\u8d77\u7684\u8bdd\uff0c\u4e00\u5b9a\u6ca1\u95ee\u9898\u3002<br>\u5236\u4f5c\u4eba\u5148\u751f\u80fd\u76f8\u4fe1\u6211\u5417\uff1f\n|\u81ea\u6211\u4ecb\u7ecd2=<small>{{lj|\u5ca1\u5d0e\u6cf0\u8449\u3067\u3059\u3002\u30e2\u30c7\u30eb\u3092\u3057\u3066\u3044\u3066\u3001\u82b8\u80fd\u754c\u306e\u3053\u3068\u306f\u305d\u308c\u306a\u308a\u306b\u2026\u3002}}<br>{{lj|\u30a2\u30a4\u30c9\u30eb\u3082\u5927\u5909\u3067\u3057\u3087\u3046\u3051\u3069\u3001\u4eca\u307e\u3067\u306e\u7d4c\u9a13\u3092\u751f\u304b\u3057\u3066\u9811\u5f35\u308d\u3046\u3068\u601d\u3044\u307e\u3059\u3002}}<br>{{lj|\u2026\u529b\u3092\u8cb8\u3057\u3066\u304f\u308c\u307e\u3059\u304b\uff1f}}</small><br>\u6211\u662f\u5ca1\u5d0e\u6cf0\u8449\u3002\u4f5c\u4e3a\u6a21\u7279\u513f\uff0c\u539f\u672c\u5c31\u5728\u827a\u80fd\u754c\u4e86\u2026\u3002<br>\u6210\u4e3a\u5076\u50cf\u4e5f\u5341\u5206\u8f9b\u82e6\uff0c\u4f46\u662f\u4f1a\u5229\u7528\u4ee5\u524d\u7684\u7ecf\u9a8c\u52a0\u6cb9\u7684\u3002<br>\u2026\u53ef\u4ee5\u5e2e\u52a9\u6211\u5417\uff1f\n|\u4f20\u95fb1=<small>{{lj|\u624b\u66f8\u304d\u306e\u5b57\u306f\u3068\u3066\u3082\u30ad\u30ec\u30a4\u3089\u3057\u3044\u3002}}</small><br>\u624b\u5199\u7684\u5b57\u4f3c\u4e4e\u975e\u5e38\u6f02\u4eae\u3002\n|\u4f20\u95fb2=<small>{{lj|\u4eba\u524d\u3067\u8aad\u3080\u306e\u306f\u53e4\u5178\u6587\u5b66\u3060\u304c\u3001\u5b50\u4f9b\u5411\u3051\u306e\u7d75\u672c\u3092\u96c6\u3081\u3066\u3044\u308b\u3089\u3057\u3044\u3002}}</small><br>\u5728\u516c\u5171\u573a\u5408\u8bfb\u7684\u662f\u53e4\u5178\u6587\u5b66\uff0c\u4f46\u4f3c\u4e4e\u6709\u6536\u96c6\u513f\u7ae5\u5411\u7684\u7ed8\u672c\u3002\n|\u4f20\u95fb3=<small>{{lj|\u5bc6\u304b\u306b\u3001\u81ea\u64ae\u308a\u306e\u7df4\u7fd2\u3092\u3057\u3066\u3044\u308b\u3089\u3057\u3044\u3002}}</small><br>\u4f3c\u4e4e\u5728\u5077\u5077\u7ec3\u4e60\u81ea\u62cd\u3002\n}}\n\n== \u7b80\u4ecb ==\n[[File:\u30d7\u30e9\u30cd\u30c3\u30c8\u30b9\u30bf\u30fc \u5ca1\u5d0e\u6cf0\u8449.png|\u7f29\u7565\u56fe]]\n2012\u5e745\u670814\u65e5\u8ffd\u52a0\u5de5\u4f5c\u300c\u795e\u6237\u5730\u533a\u300d\u65f6\u521d\u767b\u573a\u3002\n\n16\u5c81\u7684\u5c11\u5973\uff0c\u5916\u8868\u770b\u4e0a\u53bb\u5374\u5f88\u5e7c\u5c0f\u3002\u4ece\u5f88\u5c0f\u5c31\u5f00\u59cb\u4f5c\u4e3a\u513f\u7ae5\u6f14\u5458\u548c\u6a21\u7279\u5728\u6f14\u827a\u754c\u91cc\u5de5\u4f5c\uff0c\u9047\u5230\u5236\u4f5c\u4eba\u540e\u8f6c\u804c\u6210\u4e86\u5076\u50cf\u3002\u7ecf\u9a8c\u4e30\u5bcc\uff0c\u6709\u7740\u300c\u8bba\u7ecf\u9a8c\u4e0d\u4f1a\u8f93\u7ed9\u4efb\u4f55\u4eba\u300d\u7684\u53d1\u8a00\u3002\n\n\u8bed\u6001\u5e73\u9759\u4f46\u662f\u6027\u683c\u597d\u5f3a\uff0c\u5bf9\u5f85\u5de5\u4f5c\u5145\u6ee1\u70ed\u60c5\uff0c\u8f93\u4e86\u4e5f\u4e0d\u4f1a\u6c14\u9981\u3002\n\n\u88ab\u8ba4\u4e3a\u662f\u7070\u59d1\u5a18\u7cfb\u5217\u91cc\u7ecf\u9a8c\u6700\u4e30\u5bcc\u7684\u5076\u50cf\uff0c\u56e0\u6b64\u4e5f\u6709\u7740\u300c\u5188\u5d0e\u524d\u8f88\u300d\u7684\u79f0\u547c\u3002\u5728\u6f2b\u753b\u300aNew Generations\u300b\u91cc\uff0c[[\u5c9b\u6751\u536f\u6708]]\u548c[[\u672c\u7530\u672a\u592e]]\u8fd8\u56e0\u300c\u4ee5\u524d\u5728\u7535\u89c6\u4e0a\u89c1\u8fc7\u300d\u800c\u611f\u5230\u5174\u594b\u548c\u61a7\u61ac\u3002\n\n\u4f46\u662f\u5de5\u4f5c\u4e4b\u5916\u4e5f\u6709\u5931\u843d\u5b64\u72ec\u7684\u4e00\u9762\u3002\u4ece\u5c0f\u5c31\u6210\u4e3a\u827a\u4eba\u8ba9\u6cf0\u53f6\u7ecf\u5386\u4e86\u4e00\u4e2a\u5b64\u72ec\u7684\u7ae5\u5e74\uff0c\u6ca1\u6709\u4ec0\u4e48\u7279\u522b\u7f8e\u597d\u7684\u7ae5\u5e74\u56de\u5fc6\uff0c\u4ee5\u5de5\u4f5c\u4e3a\u4e3b\u3002\u8fd9\u4e5f\u5bfc\u81f4\u4e86\u5979\u4e0d\u5bfb\u5e38\u7684\u5b69\u5b50\u6c14\u884c\u4e3a\uff0c\u4f8b\u5982\u559c\u6b22\u5236\u4f5c\u73a9\u5177\u5c4b\u3002\n\n== \u8da3\u95fb\u8f76\u4e8b ==\n*\u548c[[\u5173\u88d5\u7f8e]]\u3001[[\u767d\u83ca\u8424]]\u3001[[\u677e\u5c3e\u5343\u9e64]]\u7ec4\u6210\u300cGIRLS BE NEXT STEP\u300d\u7ec4\u5408\uff0c\u6709\u5171\u901aR\u5361\u670d\u88c5\u3002{{\u9ed1\u5e55|\u56db\u4e2a\u4eba\u8df3[[Heart Voice]]\u7684MV\u6548\u679c\u4e0d\u9519}}\n*CGSS\u7684\u4e2a\u4eba\u5267\u60c5\u4e2d\u81ea\u8a00\u300c\u6f14\u827a\u7ecf\u538611\u5e74\u300d\u3002\u6b64\u5267\u60c5\u5b9e\u88c5\u662f2016\u5e741\u670831\u65e5\uff0c\u800c[[\u5076\u50cf\u5927\u5e08(\u521d\u4ee3)|\u5076\u50cf\u5927\u5e08\u8857\u673a\u7248]]\u63a8\u51fa\u4e8e2005\u5e747\u670826\u65e5\uff0c\u6240\u4ee5\u6cf0\u53f6\u7684\u8d44\u5386\u4e0e\u5076\u50cf\u5927\u5e08\u6574\u4e2a\u7cfb\u5217\u7b49\u957f\u3002\n\n== \u5361\u7247 ==\n\u76ee\u524d\u62e5\u670910\u5f20R\u5361\uff08\u5176\u4e00\u4e3a\u5355\u72ec\u7684R+\u5361\uff09\uff0c9\u5f20SR\u5361\u3002\n\nRare\uff1a\u521d\u59cb\u5361\u7247\u3002\n\nSRare[\u70c2\u6f2b\u4eba\u5076\u5c11\u5973]\uff1a2013\u5e742\u670827\u65e5\u6d3b\u52a8\u300c\u5076\u50cf\u751f\u5b58 \u5973\u513f\u82822013\u300d\u7684\u5361\u7247\u3002\n\nRare[\u5c0f\u5c0f\u7684\u4e00\u6b65]\uff1a2013\u5e746\u670813\u65e5\u6d3b\u52a8\u300cDream Live Festival\u300d\u7684\u5361\u7247\u3002\n\nSRare[Planet Star]\uff1a2013\u5e7410\u670830\u65e5\u6d3b\u52a8\u300cLIVE\u5de1\u6f14\u5609\u5e74\u534e in \u5b87\u5b99\u4e16\u754c\u300d\u7684\u5361\u7247\u3002\n\nRare[\u5de7\u514b\u529b\u4e4b\u5473/\u5de7\u514b\u529b\u4e4b\u5473\u00b7S]\uff1a2014\u5e742\u67087\u65e5\u6d3b\u52a8\u300c\u5076\u50cf\u57f9\u80b2 Chocolate for You!\u300d\u7684\u5361\u7247\u3002\n\nRare[\u7ae5\u8bdd\u52a8\u7269]\uff1a2014\u5e748\u670831\u65e5\u671f\u95f4\u9650\u5b9a\u626d\u86cb\u300c\u8f6f\u7ef5\u7ef5\u6bdb\u8338\u8338\u7ae5\u8bdd\u52a8\u7269\u300d\u7684\u5361\u7247\u3002\n\nRare+/SRare/SRare[\u65b0\u6625I.C/\u9640\u87ba\u8fbe\u4eba/\u9640\u87ba\u8fbe\u4eba\u00b7S]\uff1a2014\u5e7412\u670831\u65e5\u6d3b\u52a8\u300c\u5076\u50cf\u6311\u6218 \u65b0\u5e74\u624d\u827aSP\u300d\u7684\u5361\u7247\u3002\n\nSRare[{{lj|\u5149\u3055\u3059\u5834\u6240}}]\uff1a2017\u5e749\u670811\u65e5\u7b2c33\u56de\u300cDream Live Festival\u300d\u767b\u573a\u7684\u5361\u7247\u3002\n\n{| align=center\n|- \n| <gallery mode=packed-hover>\nFile:\u5ca1\u5d0e\u6cf0\u8449+.png|R\nR\uff3b\u5c0f\u3055\u306a\u4e00\u6b69\uff3d\u5ca1\u5d0e\u6cf0\u8449+.jpg|\u5c0f\u5c0f\u7684\u4e00\u6b65\nFile:\uff3b\u30b7\u30e7\u30b3\u30e9\u30d5\u30ec\u30fc\u30d0\u30fc\uff3d\u5ca1\u5d0e\u6cf0\u8449+.jpg|\u5de7\u514b\u529b\u4e4b\u5473\nFile:R\uff3b\u30e1\u30eb\u30d8\u30f3\u30a2\u30cb\u30de\u30eb\u30ba\uff3d\u5ca1\u5d0e\u6cf0\u8449+.jpg|\u7ae5\u8bdd\u52a8\u7269\nFile:R\uff3b\u65b0\u6625I.C\uff3d\u5ca1\u5d0e\u6cf0\u8449+.jpg|\u65b0\u6625I.C\nFile:\uff3b\u3055\u308f\u3084\u304b\u30b9\u30dd\u30fc\u30c4\uff3d\u5ca1\u5d0e\u6cf0\u8449+.jpg|{{lj|\u3055\u308f\u3084\u304b\u30b9\u30dd\u30fc\u30c4}}\nFile:\uff3bT.B.\u30c1\u30a2\u30fc\u30ba\uff3d\u5ca1\u5d0e\u6cf0\u8449+.jpg|{{lj|T.B.\u30c1\u30a2\u30fc\u30ba}}\nFile:R\uff3b\u30ac\u30fc\u30ea\u30fc\u30dd\u30c3\u30b7\u30d6\uff3d\u5ca1\u5d0e\u6cf0\u8449+.jpg|{{lj|\u30ac\u30fc\u30ea\u30fc\u30dd\u30c3\u30b7\u30d6}}\n