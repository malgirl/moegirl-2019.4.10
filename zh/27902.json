u8ba1\u5212\u8868\uff0c\u4eca\u5929\u9700\u8981\u53bb\u9a8c\u6536\u8282\u65e5\u670d\u88c5\u7684\u51c6\u5907\u60c5\u51b5\u3002\n\nJS05\uff1a\u8bf6\uff0c\u6307\u6325\u5b98\uff1f\u60a8\u600e\u4e48\u6765\u5566\uff01\n\n\u6307\u6325\u5b98\uff1a\u516b\u4e00\u5f0f\u9a6c\u62dc\u6258\u6211\u6765\u9a8c\u6536\u7684\u3002\n::\u4f60\u8fd9\u8fb9\u662f\u8d1f\u8d23\u51c6\u5907\u8fc7\u5e74\u7684\u65b0\u8863\u670d\u5427\uff1f\n\nJS05\uff1a\u6307\u6325\u5b98\u60a8\u6765\u7684\u6b63\u597d\u3002\n::\u6211\u521a\u51c6\u5907\u53bb\u62ff\u8ba2\u505a\u7684\u8863\u670d\u5462\uff0c\u8981\u4e0d\u4e00\u8d77\u53bb\u770b\u770b\uff1f\n::\u60a8\u5728\u7684\u8bdd\u4e5f\u597d\u5e2e\u6211\u628a\u628a\u5173\u3002\n\n\u6307\u6325\u5b98\uff1a\u597d\u554a\uff0c\u90a3\u5c31\u4e00\u8d77\u53bb\u5427\u3002\n\n\u5e97\u94fa\u5185\u3002\n\n\u6307\u6325\u5b98\uff1aJS05\uff0c\u8fd8\u6ca1\u597d\u5417\uff1f\n\nJS05\uff1a\u7a0d\u7b49\u4e00\u4e0b\uff0c\u9a6c\u4e0a\u5c31\u597d\uff01\n::\u8fd9\u4e2a\u6263\u5b50\u6709\u70b9\u513f\u9ebb\u70e6\u2026\u2026\u597d\u4e86\uff0c\u6211\u8fd9\u5c31\u51fa\u6765\uff01\n\nJS05\u8d70\u51fa\u66f4\u8863\u5ba4\u3002\n\nJS05\uff1a\u6307\u6325\u5b98\uff0c\u60a8\u89c9\u5f97\u600e\u4e48\u6837\uff1f\n\n\u6307\u6325\u5b98\uff1a\u5f88\u597d\u770b\u554a\uff0cJS05\u3002\n::\u76f8\u5f53\u9002\u5408\u4f60\u7684\u98ce\u683c\u5462\u3002\n\nJS05\uff1a\u5514\u2026\u2026\u4e3a\u4e86\u914d\u5408\u8fd9\u4ef6\u65d7\u888d\uff0c\u5f97\u7a0d\u5fae\u6574\u7406\u4e0b\u5218\u6d77\u5462\u3002\n\nJS05\uff1a\u73b0\u5728\u5462\uff1f\n\n\u6307\u6325\u5b98\uff1a\u5443\uff0c\u548c\u521a\u624d\u6709\u533a\u522b\u5417\uff1f\n\nJS05\uff1a\u6211\u628a\u5218\u6d77\u653e\u4e0b\u6765\u4e86\u4e00\u4e9b\uff0c\u548c\u521a\u624d\u533a\u522b\u5927\u4e86\uff01\n::\u6307\u6325\u5b98\uff0c\u60a8\u7684\u89c2\u5bdf\u529b\u6709\u70b9\u5dee\u554a\u3002\n\n\u6307\u6325\u5b98\uff1a\u660e\u660e\u662f\u4f60\u5bf9\u89c2\u5bdf\u529b\u7684\u8981\u6c42\u592a\u9ad8\u4e86\u2026\u2026\n\nJS05\uff1a\u563f\u563f\uff0c\u6bd5\u7adf\u6211\u7684\u5de5\u4f5c\u8981\u6c42\u7684\u662f\u5341\u8db3\u7684\u7cbe\u51c6\u561b\u3002\n::\u6307\u6325\u5b98\u5148\u6765\u5e2e\u6211\u68c0\u67e5\u8fd9\u4e9b\u8863\u670d\u5427\uff0c\u5c31\u5f53\u7ec3\u7ec3\u624b\uff01\n\n\u6307\u6325\u5b98\uff1a\u597d\u5427\u597d\u5427\uff0c\u90a3\u5c31\u8ba9\u6211\u6765\u8bd5\u8bd5\u3002\n::\u8bf4\u8d77\u6765\u8fd9\u4e9b\u8863\u670d\u548c\u5927\u5bb6\u5e73\u65f6\u7684\u7740\u88c5\u98ce\u683c\u5dee\u8ddd\u6709\u4e9b\u5927\u2026\u2026\u5979\u4eec\u4f1a\u613f\u610f\u7a7f\u5417\uff1f\n\nJS05\uff1a\u6307\u6325\u5b98\uff0c\u8fd9\u70b9\u60a8\u653e\u5fc3\uff0c\u6ca1\u6709\u4eba\u5f62\u4f1a\u62d2\u7edd\u65b0\u8863\u670d\u7684\u3002\n::\u800c\u4e14\u8fd9\u4e48\u65b0\u9896\u53c8\u597d\u770b\u7684\u6b3e\u5f0f\uff0c\u8981\u662f\u8c01\u4e0d\u4e50\u610f\u7a7f\uff0c\u90a3\u4e00\u5b9a\u662f\u5979\u6ca1\u773c\u5149\uff01\n\n\u6307\u6325\u5b98\uff1a\uff08\u5979\u8bf4\u8bdd\u8fd8\u771f\u662f\u76f4\u6765\u76f4\u53bb\u554a\u2026\u2026\uff09\n::\u542c\u4f60\u8fd9\u4e48\u8bf4\uff0c\u6211\u5c31\u66f4\u671f\u5f85\u6625\u8282\u4e86\u3002\n::\u80fd\u770b\u5230\u5927\u5bb6\u548c\u5e73\u65f6\u4e0d\u540c\u7684\u6a21\u6837\uff0c\u4e00\u5b9a\u662f\u76f8\u5f53\u6709\u8da3\u7684\u4e8b\u3002\n\nJS05\uff1a\u55ef\uff0c\u6307\u6325\u5b98\u5c31\u7b49\u7740\u8fc7\u8282\u597d\u5566\uff0c\u8fd9\u6b21\u4e5f\u4e00\u5b9a\u4e0d\u4f1a\u8ba9\u60a8\u5931\u671b\u7684\u3002\n::\u5bf9\u4e86\u2026\u2026\u8bf4\u8d77\u6765\uff0c\u6307\u6325\u5b98\u60a8\u4e70\u4e86\u65b0\u8863\u670d\u5417\uff1f\n\n\u6307\u6325\u5b98\uff1a\u6211\uff1f\u6211\u5012\u662f\u6ca1\u4ec0\u4e48\u51c6\u5907\uff0c\u6bd5\u7adf\u8fd9\u6b21\u8fc7\u8282\u662f\u4eba\u5f62\u4eec\u7684\u63d0\u8bae\u2026\u2026\n\nJS05\uff1a\u65e2\u7136\u516b\u4e00\u5f0f\u9a6c\u62dc\u6258\u6307\u6325\u5b98\u6765\u505a\u9a8c\u6536\u5de5\u4f5c\uff0c\u90a3\u60a8\u4e5f\u662f\u6211\u4eec\u6625\u8282\u7b79\u5907\u5c0f\u7ec4\u7684\u4e00\u5458\u4e86\u3002\n::\u4e4b\u524d\u6211\u4eec\u8bf4\u597d\u7684\uff0c\u53c2\u4e0e\u7b79\u529e\u7684\u4eba\u90fd\u8981\u6709\u65b0\u8863\u670d\u3002\n\u800c\u4e14\u8fc7\u5e74\u7684\u65f6\u5019\u8981\u7a7f\u65b0\u8863\u670d\uff0c\u8fd9\u4e60\u4fd7\u672c\u6765\u5c31\u662f\u4f60\u4eec\u4eba\u7c7b\u5b9a\u7684\u561b\uff01\n\n\u6307\u6325\u5b98\uff1a\u662f\u8fd9\u6837\u7684\u5417\uff1f\n::\u65e7\u65f6\u4ee3\u7684\u5e86\u795d\u65b9\u5f0f\u6211\u4e0d\u662f\u5f88\u4e86\u89e3\u2026\u2026\n\nJS05\uff1a\u6211\u7684\u6570\u636e\u5e93\u662f\u4e0d\u4f1a\u51fa\u9519\u7684\u3002\n::\u4e0d\u8fc7\u73b0\u5728\u8ba2\u505a\u4e1c\u65b9\u98ce\u683c\u7684\u6015\u662f\u6765\u4e0d\u53ca\u4e86\uff0c\u7b49\u4f1a\u513f\u6211\u4eec\u4e0a\u8857\u53bb\u4e70\u5427\u3002\n\n\u6307\u6325\u5b98\uff1a\u4e0d\u7528\u5566\uff0c\u6211\u672c\u6765\u4e5f\u4e0d\u8fc7\u8fd9\u4e2a\u8282\uff0c\u53ea\u662f\u548c\u4f60\u4eec\u4e00\u8d77\u51d1\u51d1\u70ed\u95f9\u3002\n::\u800c\u4e14\u4e0b\u5348\u8fd8\u6709\u5de5\u4f5c\uff0c\u6015\u662f\u6ca1\u6709\u901b\u8857\u7684\u7a7a\u95f2\u4e86\u3002\n\nJS05\uff1a\u6307\u6325\u5b98\u2026\u2026\u4f60\u662f\u5bb3\u6015\u901b\u8857\u5417\uff1f\n\n\u6307\u6325\u5b98\uff1a\u4e5f\u3001\u4e5f\u4e0d\u662f\u5566\uff0c\u4e0b\u5348\u8fd8\u6709\u603b\u90e8\u7684\u4f1a\u8bae\u3002\n::\u6bd4\u8d77\u901b\u8857\uff0c\u8fd8\u662f\u751f\u6c14\u7684\u8d6b\u4e3d\u5b89\u66f4\u53ef\u6015\u554a\uff01\n\nJS05\uff1a\u90a3\u597d\u5427\uff0c\u7b49\u6211\u628a\u8981\u6539\u7684\u5730\u65b9\u544a\u8bc9\u88c1\u7f1d\uff0c\u7136\u540e\u5c31\u56de\u57fa\u5730\u53bb\u3002\n\n\u6307\u6325\u5b98\uff1a\u8fd8\u8981\u4fee\u6539\u554a\uff1f\n\nJS05\uff1a\u5f53\u7136\uff0c\u8fc7\u5e74\u7684\u65b0\u8863\u670d\u5fc5\u987b\u597d\u597d\u51c6\u5907\uff01\n::\u771f\u662f\u7684\uff0c\u660e\u660e\u90fd\u662f\u4f60\u4eec\u4eba\u7c7b\u81ea\u5df1\u7684\u8282\u65e5\u2026\u2026\u7a0d\u5fae\u91cd\u89c6\u70b9\u554a\u3002\n\n\u6307\u6325\u5b98\uff1a\u5404\u4e2a\u5730\u65b9\u7684\u98ce\u4fd7\u4e0d\u4e00\u6837\u561b\u3002\n::\u5982\u679c\u662f\u5723\u8bde\u6216\u8005\u5143\u65e6\uff0c\u60f3\u5fc5\u5728\u8fd9\u91cc\u4f1a\u66f4\u53d7\u5173\u6ce8\u5427\u3002\n\nJS05\uff1a\u4f46\u662f\u6307\u6325\u5b98\u5728\u8fd9\u4e24\u4e2a\u8282\u65e5\u4e5f\u6ca1\u6709\u6362\u65b0\u8863\u670d\u5427\uff1f\n::\u6211\u542cDSR\u5979\u4eec\u8bf4\uff0c\u8fd9\u91cc\u7684\u4eba\u4e5f\u6709\u65b0\u5e74\u6362\u65b0\u8863\u670d\u7684\u4e60\u4fd7\u5462\u3002\n\n\u6307\u6325\u5b98\uff1a\u6bd5\u7adf\u73b0\u5728\u8fd9\u4e2a\u72b6\u51b5\u2026\u2026\n::\u8fd8\u80fd\u6709\u5e86\u795d\u8282\u65e5\u7684\u4f59\u529b\u5c31\u4e0d\u9519\u4e86\uff0c\u6ca1\u6cd5\u5728\u610f\u592a\u591a\u7684\u7ec6\u8282\u95ee\u9898\u554a\u3002\n\nJS05\uff1a\u542c\u4e0a\u53bb\uff0c\u4eba\u7c7b\u8fc7\u5f97\u5f88\u8f9b\u82e6\u554a\u2026\u2026\n\n\u6307\u6325\u5b98\uff1a\u662f\u554a\u2026\u2026\u54ce\uff0c\u600e\u4e48\u8dd1\u5230\u8fd9\u4e48\u6c89\u91cd\u7684\u8bdd\u9898\u4e0a\u6765\u4e86\u3002\n::\u9a6c\u4e0a\u5c31\u8981\u5e86\u795d\u8282\u65e5\u4e86\uff0c\u8fd8\u662f\u5f00\u5fc3\u70b9\u5427\uff01\n\nJS05\uff1a\u8bf4\u7684\u4e5f\u662f\uff0c\u90a3\u6307\u6325\u5b98\u60a8\u5c31\u671f\u5f85\u7740\u6211\u4eec\u7684\u6625\u8282\u5427\uff01\n::\u4e00\u5b9a\u4e0d\u4f1a\u6bd4\u5723\u8bde\u4ec0\u4e48\u7684\u5dee\u7684\uff01\n::\u79bb\u8fc7\u8282\u8fd8\u6709\u51e0\u5929\u5462\uff0c\u6307\u6325\u5b98\u60a8\u8bb0\u5f97\u62bd\u7a7a\u53bb\u4e70\u65b0\u8863\u670d\u554a\u3002\n\n\u6307\u6325\u5b98\uff1a\u597d\u7684\u597d\u7684\uff0c\u8fd9\u6bb5\u65f6\u95f4\u6709\u7a7a\u7684\u8bdd\uff0c\u6211\u5c31\u53bb\u57ce\u5e02\u91cc\u901b\u901b\u3002\n\n\u548cJS05\u56de\u5230\u57fa\u5730\u540e\uff0c\u6211\u4e5f\u5f88\u5feb\u5730\u6295\u8eab\u4e8e\u5e74\u521d\u548c\u5e74\u672b\u4ea4\u66ff\u671f\u95f4\u7684\u5404\u9879\u5de5\u4f5c\u91cc\u3002\n\n\u6ca1\u8fc7\u591a\u4e45\uff0c\u4fbf\u628a\u4e70\u65b0\u8863\u670d\u7684\u4e8b\u60c5\u7529\u5230\u8111\u540e\u4e86\u3002\n\n\u9664\u5915\u5f53\u5929\u4e0b\u5348\u3002\n\n\u5904\u7406\u5b8c\u610f\u5916\u5f15\u71c3\u7684\u7206\u7af9\u5e26\u6765\u7684\u9a9a\u4e71\u540e\uff0c\u6211\u56de\u5230\u4e86\u529e\u516c\u5ba4\u5185\u3002\n\n\u6307\u6325\u5b98\uff1a\u547c\u2026\u2026\u8282\u65e5\u5e86\u5178\u5f00\u59cb\u524d\u51fa\u8fd9\u79cd\u4e8b\uff0c\u8fd8\u771f\u662f\u7d2f\u4eba\u3002\n\nJS05\uff1a\u6307\u6325\u5b98\uff0c\u60a8\u5728\u8fd9\u513f\u554a\u3002\n::\u2026\u2026\u60a8\u770b\u4e0a\u53bb\u76f8\u5f53\u6ca1\u7cbe\u795e\u554a\u3002\n\n\u6307\u6325\u5b98\uff1a\u55ef\uff0c\u521a\u624d\u53d1\u751f\u4e86\u70b9\u610f\u5916\u72b6\u51b5\uff0c\u6211\u60f3\u7a0d\u5fae\u4f11\u606f\u4e00\u4e0b\u3002\n\nJS05\uff1a\u662f\u56e0\u4e3aART556\u5f04\u51fa\u7684\u4e71\u5b50\u5427\uff0c\u771f\u662f\u8f9b\u82e6\u60a8\u4e86\u3002\n::\u6ca1\u60f3\u5230\u90a3\u4e2a\u5bb6\u4f19\u4e3a\u4e86\u597d\u73a9\uff0c\u80fd\u505a\u51fa\u8fd9\u4e48\u4e0d\u8ba1\u540e\u679c\u7684\u4e8b\u2026\u2026\n\n\u6307\u6325\u5b98\uff1a\u4f60\u7684\u6d88\u606f\u5012\u662f\u633a\u7075\u901a\u7684\u2026\u2026\n\nJS05\uff1a\u563f\u563f\uff0c\u6211\u53ea\u662f\u4ece\u5916\u8fb9\u56de\u6765\u7684\u65f6\u5019\uff0c\u987a\u8def\u5411\u5176\u4ed6\u4eba\u6253\u542c\u4e86\u4e00\u4e0b\u3002\n::\u6211\u6765\u627e\u60a8\u662f\u4e3a\u4e86\u8fd9\u4e2a\u2026\u2026\n\nJS05\u5c06\u4e00\u4e2a\u76d2\u5b50\u9012\u7ed9\u6211\u3002\n\n\u6307\u6325\u5b98\uff1a\u8fd9\u662f\u4ec0\u4e48\uff1f\n\nJS05\uff1a\u8fc7\u5e74\u7a7f\u7684\u65b0\u8863\u670d\u554a\u3002\n::\u6211\u60f3\u6307\u6325\u5b98\u60a8\u6700\u8fd1\u516b\u6210\u662f\u6ca1\u7a7a\u53bb\u4e70\u8863\u670d\uff0c\u521a\u597d\u8d76\u4e0a\u4eca\u5929\u51fa\u95e8\uff0c\u5c31\u987a\u4fbf\u5e2e\u60a8\u4e70\u4e86\u3002\n::\u4e0d\u592a\u6e05\u695a\u60a8\u559c\u6b22\u4ec0\u4e48\u6b3e\u5f0f\u7684\u8863\u670d\uff0c\u60f3\u4e86\u60f3\u5c31\u4e70\u4e86\u4ef6\u886c\u886b\u3002\n::\u65b0\u7684\u4e00\u5e74\u521a\u521a\u5f00\u5934\uff0c\u603b\u5f52\u5f97\u51c6\u5907\u4ef6\u65b0\u8863\u670d\u7684\u3002\n\n\u6307\u6325\u5b98\uff1a\u8c22\u8c22\u2026\u2026\u8fd9\u4e8b\u6211\u81ea\u5df1\u90fd\u5fd8\u4e86\u3002\n\nJS05\uff1a\u5c31\u77e5\u9053\u4f60\u4eec\u4eba\u7c7b\u7231\u5fd8\u4e8b\uff0c\u6240\u4ee5\u6211\u4eec\u624d\u8981\u66ff\u4f60\u4eec\u8bb0\u4f4f\u554a\u3002\n::\u65b0\u5e74\u7a7f\u65b0\u8863\u670d\u80fd\u53bb\u9664\u524d\u4e00\u5e74\u7684\u6666\u6c14\uff0c\u8fd8\u80fd\u5e26\u6765\u597d\u8fd0\u3002\n::\u65e0\u8bba\u662f\u5bf9\u4eba\u5f62\u8fd8\u662f\u4eba\u7c7b\u6765\u8bf4\uff0c\u8fd9\u90fd\u662f\u4e2a\u597d\u5146\u5934\u5462\uff01\n\n\u6307\u6325\u5b98\uff1a\u54c8\u54c8\uff0c\u7684\u786e\u662f\u8fd9\u6837\u3002\u90a3\u6211\u5c31\u6536\u4e0b\u5566\u3002\n\nJS05\uff1a\u8fd9\u5c31\u5bf9\u4e86\uff0c\u6307\u6325\u5b98\u3002\n::\u770b\u60a8\u73b0\u5728\u7cbe\u795e\u4e86\u8d77\u6765\uff0c\u90a3\u6211\u5c31\u653e\u5fc3\u4e86\u3002\n::\u6211\u8fd8\u8981\u53bb\u5e2e\u5176\u4ed6\u4eba\u7684\u5fd9\uff0c\u7b49\u4f1a\u513f\u5e74\u591c\u996d\u4e0a\u89c1\uff01\n::\u8bb0\u5f97\u8981\u7a7f\u65b0\u8863\u670d\u6765\u554a\uff01\n\n\u6211\u76ee\u9001JS05\u7684\u80cc\u5f71\u6d88\u5931\u5728\u95e8\u53e3\u3002\n\n\u90a3\u4e9b\u8fde\u4eba\u7c7b\u90fd\u5df2\u7ecf\u5feb\u8981\u9057\u5fd8\u7684\u5173\u4e8e\u65e7\u65f6\u4ee3\u7684\u8bb0\u5fc6\uff0c\u5979\u4eec\u6b63\u5c0f\u5fc3\u5730\u5c06\u5b83\u4eec\u4e00\u4e00\u62fe\u8d77\uff0c\u5e76\u8ba4\u8ba4\u771f\u771f\u5730\u5b88\u62a4\u7740\u3002\n\n\u5728\u672a\u6765\u7684\u67d0\u4e00\u5929\uff0c\u8fd9\u4e9b\u8bb0\u5fc6\u4e00\u5b9a\u4f1a\u6563\u53d1\u51fa\u66f4\u52a0\u70ab\u76ee\u7684\u5149\u5f69\u5427\u3002\n\n\u6307\u6325\u5b98\uff1a\u597d\u4e86\uff0c\u4f11\u606f\u7ed3\u675f\u3002\n::\u5148\u53bb\u6362\u8863\u670d\u2026\u2026\u7136\u540e\u548c\u5979\u4eec\u4e00\u8d77\u5e86\u795d\u8fd9\u4e2a\u96be\u5f97\u7684\u8282\u65e5\u5427\u3002\n}}\n\n===JS05\u7684\u5355\u53d1\u4f20\u8bf4(\u53f0\u670d\u9650\u5b9a)===\n\u5728JS05\u525b\u51fa\u7684\u6642\u5019\u66fe\u56e0\u5176\u6280\u80fd\u6548\u76ca\u4e0d\u5927\u751a\u81f3\u9084\u6703\u6253\u7a7a\uff0c\u56e0\u6b64\u6709\u4e86\u201c\u65b0\u4e94\u661fRF\u4e4b\u6065\u201d\u7684\u7a31\u865f\u800c\u906d\u5230{{\u9ed1\u5e55|\u5927}}\u90e8\u5206\u6307\u63ee\u5b98\u7684\u51b7\u843d\uff0c\u7136\u800c\u5728\u67d0\u4e9b\u6709\u611b\u7684\u6307\u63ee\u5b98\u5c07\u5176\u653e\u5165\u968a\u4f0d\