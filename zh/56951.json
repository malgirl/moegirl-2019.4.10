uff0c\u5728\u4efb\u641c\u67e5\u5b98\u65f6\u4e0e\u4e9a\u5386\u514b\u65af\u00b7\u8fbe\u5fb7\u5229\u548c\u4e9a\u91cc\u6b27\u65af\u00b7\u9a6c\u514b\u83b1\u56e0\u4e3a\u540c\u4e8b\uff0c\u96b6\u5c5e\u4e8e\u79d1\u957f\u8d5b\u5c14\u76d6\u00b7\u7f57\u3002\u4ed6\u662f\u4f20\u8bf4\u6545\u4e8b\u201c\u767d\u72fc\u4e0e\u5723\u5973\u201d\u4e2d\u6cbb\u7597\u5e08\u7684\u5973\u513f\u4e4c\u5c14\u65af\u62c9\u7684\u540e\u88d4[[\u585e\u831c\u5c14\u00b7\u8bfa\u4f0a\u827e\u4e1d]]\u7684\u672a\u5a5a\u592b\u3002[[\u7f07\u6b27]]\u7684\u6551\u547d[[\u6069\u4eba]]\u3002\u4f0a\u5b89\u5f8b\u5e08\u5728\u51ef\u610f\u56fe\u8bf4\u670d\u4e9a\u91cc\u6b27\u65af\u4e4b\u65f6\uff0c\u4ece\u80cc\u540e\u67aa\u51fb\u51ef\u5bfc\u81f4\u5176\u8eab\u4ea1}}</ref>\uff0c[[\u7f57\u4f0a\u5fb7\u00b7\u73ed\u5b81\u65af]]\u524d\u5f80\u5171\u548c\u56fd\u6295\u9760\u4eb2\u621a || \u60c5\u62a5\u90e8\u8bbe\u7acb\uff0c\u96f7\u514b\u7279\uff08[[\u79d1\u6d1b\u4e1d]]\u7684\u5b66\u957f\uff09\u4ece\u6770\u5c3c\u4e1d\u738b\u7acb\u5b66\u9662\u9000\u5b66 || \n|-\n| 1202 || \u54aa\u897f\u4e50\u56ed\u5f00\u5efa\uff0c\u74e6\u5409\u00b7\u8d6b\u7c73\u65af\u83f2\u4e9a\u5efa\u7acb\u5723\u4e66\u4f1a || \u300a[[\u82f1\u96c4\u4f20\u8bf4VI\u7a7a\u4e4b\u8f68\u8ff9FC|\u7a7a\u4e4b\u8f68\u8ff9FC]]\u300b\u3001\u300a[[\u82f1\u96c4\u4f20\u8bf4VI\u7a7a\u4e4b\u8f68\u8ff9SC|\u7a7a\u4e4b\u8f68\u8ff9SC]]\u300b<br />\u5229\u8d1d\u5c14\u738b\u56fd\u00b7\u5361\u5c14\u74e6\u5fb7\u5171\u548c\u56fd\u00b7\u57c3\u96f7\u6ce2\u5c3c\u4e9a\u5e1d\u56fd\u7b7e\u7f72\u4e0d\u6218\u6761\u7ea6 || \u5170\u8fea\u00b7\u5965\u5170\u591a\u8131\u79bb\u730e\u5175\u56e2<br />\u57c3\u96f7\u6ce2\u5c3c\u4e9a\u5e1d\u56fd\u6e38\u51fb\u58eb\u534f\u4f1a\u906d\u88ad\u4e8b\u4ef6\n|-\n| 1203 || [[\u5170\u8fea\u00b7\u5965\u5170\u591a]]\u52a0\u5165[[\u514b\u6d1b\u65af\u8d1d\u5c14\u8b66\u5907\u961f|\u8b66\u5907\u961f]]<br />[[\u5409\u5229\u4e9a\u65af\u00b7\u5965\u65af\u672c]]\u5bb0\u76f8\u975e\u6b63\u5f0f\u8bbf\u95ee<br />\u9ed1\u6708\u8d38\u6613\u516c\u53f8\u5f00\u8bbe\u5206\u516c\u53f8<br />[[\u73b2\u00b7\u6d77\u74e6\u65af|\u73b2]]\u6ede\u7559\u4e8e\u7f57\u8d5e\u8d1d\u5c14\u514b\u5de5\u623f\uff08\u4eba\u5076\u5de5\u623f\uff09 || \u300a[[\u82f1\u96c4\u4f20\u8bf4VI\u7a7a\u4e4b\u8f68\u8ff9SC|\u7a7a\u4e4b\u8f68\u8ff9SC]]\u300b\u30013rd\u300a[[\u82f1\u96c4\u4f20\u8bf4VI\u7a7a\u4e4b\u8f68\u8ff9the 3rd|\u7a7a\u4e4b\u8f68\u8ff9the 3rd]]\u300b || \u201c[[\u8d64\u8272\u661f\u5ea7]]\u201d\u4e0e\u201c\u9ed1\u6708\u8d38\u6613\u516c\u53f8\u201d\u4e8e\u4e1c\u65b9\u8857\u53d1\u751f\u4e89\u6597\n|-\n| 1204 || [[\u7f57\u4f0a\u5fb7\u00b7\u73ed\u5b81\u65af]]\u56de\u5230\u514b\u6d1b\u65af\u8d1d\u5c14\uff0c\u52a0\u5165[[\u7279\u52a1\u652f\u63f4\u79d1]]<br />[[\u4ea8\u5229\u00b7\u9ea6\u514b\u9053\u5c14]]\u6697\u6740\u8ba1\u5212\u5931\u8d25<br />\u897f\u98ce\u65c5\u56e2[\u730e\u5175\u738b]\u548c\u8d64\u8272\u661f\u5ea7[\u6597\u795e]\u51b3\u6597\u76f8\u7ee7\u6b7b\u4ea1<br />[[\u742a\u96c5]]\u4ece\u62cd\u5356\u4f1a\u88ab[[\u7f57\u4f0a\u5fb7\u00b7\u73ed\u5b81\u65af]]\u7b49\u4eba\u6551\u51fa<br />\u6e2f\u6e7e\u533a\u53d1\u751f\u300a\u9ed1\u6708\u88ad\u51fb\u4e8b\u4ef6\u300b<br />\u53d1\u751f\u201c\u514b\u6d1b\u65af\u8d1d\u5c14D\u2234G\u6559\u56e2\u4e8b\u4ef6\u201d<br />D\u2234G\u6559\u56e2\u4e8b\u4ef6\u7ed3\u675f<br />[[\u201c\u5c0f\u4e11\u201d\u80af\u5e15\u96f7\u62c9]]\u5ba3\u5e03\u201c\u5e7b\u708e\u8ba1\u5212\u201d\u5f00\u59cb<br />\u514b\u6d1b\u65af\u8d1d\u5c14\u5ba3\u5e03\u72ec\u7acb || [[\u827e\u4e1d\u8482\u5c14\u00b7\u5e03\u83b1\u7279]]\u3001[[\u7ea6\u4fee\u4e9a\u00b7\u5e03\u83b1\u7279]]\u3001[[\u73b2\u00b7\u6d77\u74e6\u65af]]\u56de\u56fd || \u6258\u5c14\u5179\u58eb\u5b98\u5b66\u9662VII\u73ed\u6210\u7acb\uff0c[[\u9ece\u6069\u00b7\u8212\u534e\u6cfd]]\u7b49\u4eba\u8fdb\u5165VII\u73ed<br />VII\u73ed\u963b\u6b62\u201c\u5217\u8f66\u70ae\u201d\u7684\u53d1\u52a8<br />\u201c\u94c1\u8840\u5bb0\u76f8\u201d[[\u5409\u5229\u4e9a\u65af\u00b7\u5965\u65af\u672c]]\u906d\u6697\u6740\u67aa\u51fb\u91cd\u4f24\n|}\n===\u6ce8\u91ca===\n<references/>\n\n== \u53e4\u4ee3\u9057\u7269 ==\n\u62e5\u6709\u6781\u5148\u8fdb\u6280\u672f\u7684\u53e4\u4ee3\u585e\u59c6\u91cc\u4e9a\u6587\u660e\u9057\u7269\u3002\u4e03\u8000\u6559\u4f1a\u8ba4\u4e3a\u90a3\u662f\u5973\u795e\u8d50\u4e88\u4eba\u7c7b\u7684\u793c\u7269\uff0c\u7531\u5c01\u5723\u7701\u8d1f\u8d23\u7ba1\u7406\u548c\u56de\u6536\u3002\u4e00\u65e6\u542f\u52a8\u53e4\u4ee3\u9057\u7269\uff0c\u80fd\u591f\u53d1\u6325\u51fa\u8fdc\u8fdc\u51cc\u9a7e\u4e8e\u73b0\u4ee3\u6280\u672f\u4e4b\u4e0a\u7684\u6548\u679c\uff0c\u56e0\u6b64\uff0c\u4e03\u8000\u6559\u4f1a\u7edd\u5bf9\u7981\u6b62\u56e0\u4e2a\u4eba\u7406\u7531\u800c\u4efb\u610f\u7684\u4f7f\u7528\u53e4\u4ee3\u9057\u7269\u3002\u89e6\u72af\u7981\u5fcc\u7684\u5723\u804c\u8005\u5c06\u88ab\u4f5c\u4e3a\u5f02\u7aef\u9010\u51fa\u6559\u4f1a\u5e76\u4e88\u4ee5\u60e9\u5904\u3002\n\n===\u4e03\u81f3\u5b9d===\n\u4e03\u81f3\u5b9d\u662f\u8fc7\u53bb\u5973\u795e\u8d50\u4e88\u4eba\u7c7b\uff0c\u5bf9\u5e94\u4e03\u79cd\u5c5e\u6027\u7684\u53e4\u4ee3\u9057\u7269\uff0c\u53e4\u4ee3\u7684\u4eba\u4eec\u5f97\u5230\u4e03\u81f3\u5b9d\u540e\u5206\u4e3a\u4e03\u6d3e\u3002\u5229\u8d1d\u5c14\u5f02\u53d8\u65f6\u51fa\u73b0\u7684\u6d6e\u6e38\u90fd\u5e02\u5229\u8d1d\u5c14\u65b9\u821f\u7684\u6838\u5fc3\u300c\u8f89\u4e4b\u73af\u300d\u662f'''\u7a7a\u4e4b\u81f3\u5b9d'''\uff0c\u53ef\u4ee5\u5b9e\u73b0\u4eba\u4eec\u7684\u613f\u671b\u3002\n\n\u300a[[\u96f6\u4e4b\u8f68\u8ff9]]\u300b\u4e0e\u300a[[\u78a7\u4e4b\u8f68\u8ff9]]\u300b\u4e2d\u63d0\u53ca'''\u5e7b\u4e4b\u81f3\u5b9d'''\uff0c\u88ab\u79f0\u4f5c\u6267\u638c\u5e7b\u4e4b\u529b\u7684\u300c\u865a\u795e\u300d\u4ee5\u5e93\u6d1b\u4f0a\u65af\u5bb6\u65cf\u4e3a\u4e2d\u5fc3\u7684\u4e00\u6d3e\u59d4\u6258\u81f3\u5b9d\u4ee3\u66ff\u5973\u795e\u62c5\u4efb\u300c\u4eba\u95f4\u4e4b\u795e\u300d\u3002\u4e3a\u4e86\u5728\u5b9e\u73b0\u4eba\u7c7b\u6b32\u671b\u7684\u540c\u65f6\u4e3a\u4eba\u7c7b\u6307\u5f15\u6b63\u786e\u7684\u65b9\u5411\uff0c\u81f3\u5b9d\u88ab\u8d4b\u4e88\u4e86\u9ad8\u4f4d\u7684\u4eba\u683c\uff0c\u4f46\u5e7b\u4e4b\u81f3\u5b9d\u957f\u671f\u627f\u62c5\u7740\u4eba\u7c7b\u7684\u7f6a\u4e1a\u548c\u4e16\u754c\u7684\u4e0d\u5408\u7406\u4e4b\u5904\uff0c\u5fc3\u4e2d\u7684\u75db\u82e6\u4e0e\u65e5\u4ff1\u589e\uff0c\u6700\u540e\u9009\u62e9\u81ea\u6211\u6bc1\u706d\uff0c\u5c06\u81ea\u5df1\u7684\u56e0\u679c\u4ece\u4e16\u754c\u7684\u56e0\u679c\u4e2d\u6d88\u53bb\uff0c\u4ee5\u81f4\u6210\u4e3a\u5931\u843d\u7684\u81f3\u5b9d\u3002\u5e93\u6d1b\u4f0a\u65af\u5bb6\u65cf\u4e3a\u91cd\u73b0\u5e7b\u4e4b\u81f3\u5b9d\uff0c\u5236\u9020\u4e86[[\u742a\u96c5]]\u8fd9\u4e2a\u81f3\u5b9d\u6838\u5fc3\u5e76\u4ea4\u7531D\u2234G\u6559\u56e2\u57f9\u80b2\uff08\u8bf1\u62d0\u3001\u7ed1\u67b6\u513f\u7ae5\u90fd\u662f\u4e3a\u4e86\u7ed9\u742a\u96c5\u63d0\u4f9b\u4eba\u683c\uff0c\u6700\u7ec8\u6210\u4e3a\u5e7b\u4e4b\u81f3\u5b9d\uff09\uff0c\u78a7\u4e4b\u8f68\u8ff9\u4e2d\u501f\u7531\u5e93\u6d1b\u4f0a\u65af\u5bb6\u65cf\u7684\u672f\u5f0f\uff0c\u7ecf\u739b\u5229\u4e9a\u8d1d\u5c14\u4e4b\u624b\u800c\u89c9\u9192\u6210\u4e3a\u96f6\u4e4b\u81f3\u5b9d\u3002\u5229\u7528\u4e86\u5e93\u6d1b\u4f0a\u65af\u5bb6\u65cf\u957f\u5e74\u4ee5\u6765\u91cd\u73b0\u5e7b\u4e4b\u81f3\u5b9d\u7684\u8ba1\u5212\uff0c\u518d\u5176\u539f\u672c\u6240\u6709\u7684\u5e7b\u4e4b\u529b\u7684\u57fa\u7840\u4e0a\uff0c\u540c\u65f6\u517c\u5177\u7a7a\u3001\u65f6\u4e4b\u529b\uff0c\u8d85\u8d8a\u4e86\u539f\u578b\u3002\n\n===\u5176\u4ed6\u9057\u7269===\n*''' \u6d1b\u4e9a\u7684\u9b54\u67aa ''' \n\u80fd\u4f7f\u6301\u6709\u8005\u53d1\u72c2\u7684\u53e4\u4ee3\u5f29\u67aa\uff0c\u539f\u88ab\u5c01\u5370\u5728\u4e03\u8000\u6559\u4f1a\u798f\u5229\u9662----\u7d2b\u82d1\u4e4b\u5bb6\u7684\u8d77\u6e90\u533a\u57df\u4e2d\u3002\u7d2b\u82d1\u4e4b\u5bb6\u906d\u53d7\u730e\u5175\u653b\u51fb\u65f6\uff0c\u51ef\u6587\u72ec\u8eab\u4e00\u4eba\u95ef\u5165\u59cb\u6e90\u4e4b\u5730\u4e0e\u593a\u5f97\u6d1b\u4e9a\u7684\u9b54\u67aa\u730e\u5175\u5bf9\u5cd9\uff0c\u7206\u53d1\u5723\u75d5\u5438\u6536\u8be5\u9057\u7269\u6240\u6709\u529b\u91cf\u3002\u6545\u73b0\u5df2\u5931\u53bb\u4f5c\u7528\uff0c\u4e0b\u843d\u4e0d\u660e\u3002\u5b83\u95f4\u63a5\u4fc3\u4f7f\u7a7a\u4e4b\u8f68\u8ff93rd\u6545\u4e8b\u53d1\u751f\u3002\n*''' {{fact|\u82cd\u4e4b\u77f3\u677f|\u6765\u6e90\u8bf7\u6c42}}'''\n\u74e6\u5409\u5bb6\u4e61\u6240\u4f9b\u5949\u7684\u77f3\u677f\uff0c\u662f\u88ab\u5168\u6751\u89c6\u4e3a\u795e\u7684\u8c61\u5f81\u3002\u5f53\u5deb\u5b50\u74e6\u5409\u53d1\u73b0\u8be5\u77f3\u677f\u6b63\u5728\u5438\u53d6\u5168\u6751\u4eba\u6c11\u7684\u751f\u547d\u540e\uff0c\u4ed6\u5c1d\u8bd5\u529d\u8bf4\u6751\u6c11\u653e\u5f03\u4fe1\u4ef0\u3002\u65e0\u679c\uff0c\u74e6\u5409\u8bd5\u56fe\u6bc1\u6389\u77f3\u677f\u3002\u5728\u88ab\u77f3\u677f\u5438\u53d6\u751f\u547d\u529b\u7684\u5371\u673a\u65f6\u523b\uff0c\u4ed6\u7206\u53d1\u5723\u75d5\u53cd\u566c\u77f3\u677f\u3002\u8be5\u9057\u7269\u73b0\u5df2\u5931\u53bb\u529b\u91cf\uff0c\u6389\u5165\u5ce1\u8c37\uff0c\u4e0b\u843d\u4e0d\u660e\u3002\n*'''\u96f7\u514b\u9c81\u65af\u65b9\u77f3'''\n\u4e03\u81f3\u5b9d\u4e4b\u4e00\u8f89\u4e4b\u73af\u6240\u521b\u5f71\u4e4b\u56fd\u7684\u7ec8\u7aef\uff0c\u5728\u5f71\u4e4b\u56fd\u91cc\u53ef\u4ee5\u4f53\u73b0\u5f3a\u5927\u529b\u91cf\u7684\u65b9\u5f62\u9057\u7269\u3002\u5343\u5e74\u524d\u5c01\u5370\u5c0f\u7ec4\u66fe\u5229\u7528\u5b83\u4e0e\u5f71\u4e4b\u56fd\u7684\u77f3\u7891\u7ed9\u8f89\u4e4b\u73af\u9020\u6210\u6781\u5927\u8d1f\u62c5\u5e76\u6210\u529f\u5c06\u81f3\u5b9d\u5c01\u5370\u3002\u672c\u5e94\u88ab\u6559\u4f1a\u56de\u6536\uff0c\u4f46\u57283rd\u7684\u6545\u4e8b\u540e\uff0c\u5f71\u4e4b\u56fd\u5b8c\u5168\u5c01\u95ed\u81f4\u4f7f\u65b9\u77f3\u5931\u53bb\u5168\u90e8\u673a\u80fd\uff0c\u51ef\u6587\u4f9d\u7167\u6559\u4f1a\u7684\u89c4\u5b9a\uff0c\u5c06\u5176\u4ea4\u4ed8\u535a\u58eb\u827e\u8389\u5361\u00b7\u62c9\u8d5b\u5c14\u3002\n*'''\u611a\u8005\u6302\u5760'''\n\u80fd\u4f7f\u542c\u4f17\u76f8\u4fe1\u643a\u5e26\u8005\u8bf4\u7684\u5927\u90e8\u5206\u8c0e\u8a00\u7684\u6302\u5760\u3002\u539f\u4e3a\u5e1d\u56fd\u519b\u706b\u5546\u8d6b\u5c14\u66fc\u00b7\u5eb7\u62c9\u5fb7\u901a\u8fc7\u7ed3\u793e\u83b7\u5f97\uff0c\u540e\u88ab\u51ef\u6587\u4e8e\u9732\u897f\u5854\u5c3c\u4e9a\u53f7\u7a7a\u8247\u4e0a\u56de\u6536\u3002\n*'''\u5c01\u5370\u4e4b\u5b9d\u6756'''\n\u5229\u8d1d\u5c14\u5362\u5b89\u539f\u5e02\u957f\u6234\u5c14\u8499\u7684\u4f20\u5bb6\u5b9d\uff0c\u80fd\u4f7f\u4e00\u5b9a\u8303\u56f4\u5185\u9664\u6301\u6709\u8005\u4ee5\u5916\u6240\u6709\u4eba\u5458\u4e0d\u80fd\u884c\u52a8\u7684\u6743\u6756\u3002\u662f\u672c\u5e94\u4f9d\u7167\u89c4\u5b9a\u4e0a\u4ea4\u6559\u4f1a\u7684\u53e4\u4ee3\u9057\u7269\u3002\u7ecf\u738b\u56fd\u519b\u6536\u7f34\u540e\u8f6c\u4ea4\u51ef\u6587\uff0c\u540e\u4e3a\u5e72\u6270\u5a01\u80c1\u738b\u90fd\u65b0\u578b\u6218\u8f66\u4e0a\u7684\u798f\u97f3\u88ab\u51ef\u6587\u6bc1\u6389\u3002\n\n{{\u8f68\u8ff9}}\n== \u6ce8\u91ca ==\n<references/>\n== \u5916\u90e8\u94fe\u63a5 ==\n*Falcom\u5b98\u7f51\uff1ahttp://www.falcom.com/\n{{\u5230\u7dad\u57fa\u767e\u79d1}}\n{{DEFAULTSORT:Ying}}\n[[Category:\u65e5\u672c\u6e38\u620f\u4f5c\u54c1]][[\u5206\u7c7b:Falcom\u4f5c\u54c1]][[Category:\u82f1\u96c4\u4f20\u8bf4\u7cfb\u5217]][[\u5206\u7c7b:\u8f68\u8ff9\u7cfb\u5217]]\n[[\u5206\u7c7b:\u5f02\u4e16\u754c\u9898\u6750]][[\u5206\u7c7b:\u9b54\u6cd5\u9898\u6750]][[\u5206\u7c7b:\u6218\u4e89\u9898\u6750]][[Category:\u5192\u9669\u9898\u6750]][[Category:\u84b8\u6c7d\u670b\u514b\u9898\u6750]][[Category:\u5947\u5e7b\u9898\u6750]]"}}}