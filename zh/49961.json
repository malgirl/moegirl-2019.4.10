\u4e5d\u6b4c\u00b7\u4e1c\u541b\u300b\uff1a\u201c\u9752\u4e91\u8863\u516e\u767d\u9713\u88f3\uff0c\u4e3e\u957f\u77e2\u516e\u5c04\u5929\u72fc\u3002\u201d</ref>\u9526\u88d8<ref>\u3010\u9526\u88d8\u3011\u7528\u9526\u7f1d\u5236\u7684\u76ae\u8863\u3002\u300a\u8bf4\u6587\u89e3\u5b57\u300b\uff1a\u201c\u88d8\uff0c\u76ae\u8863\u4e5f\u3002\u201d</ref>\u3000\u6b4c\u821e\u4f55\u66fe\u4f11\n\u987b\u81fe<ref>\u3010\u987b\u81fe\u3011\u7247\u523b\uff0c\u77ed\u65f6\u95f4\u3002\u300a\u8340\u5b50\u00b7\u529d\u5b66\u300b\uff1a\u201c\u543e\u5c1d\u7ec8\u65e5\u800c\u601d\u77e3\uff0c\u4e0d\u5982\u987b\u81fe\u4e4b\u6240\u5b66\u4e5f\u3002\u201d</ref>\u6625\u79cb\u3000\u7738\u6ce2<ref>\u3010\u7738\u6ce2\u3011\u6e90\u4e8e\u201c\u660e\u7738\u79cb\u6ce2\u201d\u3002\u5b8b\u00b7\u738b\u89c2\u300a\u535c\u7b97\u5b50\u300b\uff1a\u201c\u6c34\u662f\u773c\u6ce2\u6a2a\uff0c\u5c71\u662f\u7709\u5cf0\u805a\u3002\u6b32\u95ee\u884c\u4eba\u53bb\u90a3\u8fb9\uff0c\u7709\u773c\u76c8\u76c8\u5904\u3002\u201d</ref>\u7eb5\u5982\u65e7\n\u4f0a\u4eba<ref>\u3010\u4f0a\u4eba\u3011\u610f\u4e2d\u4eba\uff0c\u672c\u4e49\u4e3a\u201c\u90a3\u4e2a\u4eba\u201d\u3002\u300a\u8bd7\u00b7\u56fd\u98ce\u00b7\u84b9\u846d\u300b\uff1a\u201c\u84b9\u846d\u82cd\u82cd\uff0c\u767d\u9732\u4e3a\u971c\uff0c\u6240\u8c13\u4f0a\u4eba\uff0c\u5728\u6c34\u4e00\u65b9\u3002\u201d</ref>\u4e0d\u5019\u3000\u60c5\u4e1d<ref>\u3010\u60c5\u4e1d\u3011\u5728\u6b64\u5904\u6307\u5bf9\u68a6\u4e2d\u795e\u5973\u7684\u7559\u604b\u4e4b\u60c5\u3002\u6e05\u00b7\u5b54\u5c1a\u4efb\u300a\u6843\u82b1\u6247\u300b\uff1a\u201c\u5230\u4eca\u65e5\u60c5\u4e1d\u5272\u65ad\uff0c\u82b3\u8349\u5929\u6daf\u3002\u201d</ref>\u4e3a{{ruby|\u8c01|shei}}\u7559\n\n\u6fb9\u6fb9<ref>\u3010\u6fb9\u6fb9\u3011\u6c34\u6ce2\u5fae\u5fae\u8361\u6f3e\u7684\u6837\u5b50\u3002\u300a\u8bf4\u6587\u89e3\u5b57\u300b\uff1a\u201c\u6fb9\uff0c\u6c34\u6447\u4e5f\u3002\u201d\u4e1c\u6c49\u00b7\u66f9\u64cd\u300a\u6b65\u51fa\u4e1c\u95e8\u884c\u300b\uff1a\u201c\u6c34\u4f55\u6fb9\u6fb9\uff0c\u5c71\u5c9b\u7ae6\u5cd9\u3002\u201d</ref>\u6d9f\u6f2a\u8d77\u3000\u5f92\u60f9\u3000\u98de\u9e25\n\u56de\u9996{{ruby|\u8c01|shei}}\u5b9b\u5728<ref>\u3010\u5b9b\u5728\u3011\u4eff\u4f5b\u5728\u3002\u300a\u5eb7\u7199\u5b57\u5178\u300b\uff1a\u201c\u5b9b\uff0c\u53c8\u5b9b\u7136\u72b9\uff0c\u4f9d\u7136\u3002\u201d</ref>\u3000\u6789\u6e1a<ref>\u3010\u6789\u6e1a\u3011\u5f2f\u66f2\u6cb3\u6c34\u4e2d\u7684\u5c0f\u5757\u9646\u5730\u3002\u300a\u8bf4\u6587\u89e3\u5b57\u300b\uff1a\u201c\u6789\uff0c\u90aa\u66f2\u4e5f\u3002\u201d \u300a\u695a\u8f9e\u00b7\u4e5d\u7ae0\u00b7\u6d89\u6c5f\u300b\uff1a\u201c\u671d\u53d1\u6789\u6e1a\u516e\u5915\u5bbf\u8fb0\u9633 \u3002\u201d</ref>\u3000\u6c99\u6d32\n\n\u6b8b\u68a6\u65ad\u70df\u7b3c\u6c34{{ruby|\u69ad|sie}}<ref>\u3010\u6c34\u69ad\u3011\u5efa\u4e8e\u6c34\u8fb9\u6216\u6c34\u4e0a\u7684\u4ead\u53f0\u3002\u5510\u00b7\u675c\u752b\u300a\u6625\u591c\u5ce1\u5dde\u7530\u4f8d\u5fa1\u957f\u53f2\u6d25\u4ead\u7559\u5bb4\u300b\uff1a\u201c\u6756\u85dc\u767b\u6c34\u69ad\uff0c\u6325\u7ff0\u5bbf\u6625\u5929\u3002\u201d</ref>\n\u591c\u9611\u73ca<ref>\u3010\u9611\u73ca\u3011\u5c06\u5c3d\u3002\u5b8b\u00b7\u8f9b\u5f03\u75be\u300a\u9752\u7389\u6848\u300b\uff1a\u201c\u4f17\u91cc\u5bfb\u5979\u5343\u767e\u5ea6\uff0c\u84e6\u7136\u56de\u9996\uff0c\u90a3\u4eba\u5374\u5728\u706f\u706b\u9611\u73ca\u5904\u3002\u201d</ref>\u79cb\u5fc3{{ruby|\u8c01|shui}}\u582a{{ruby|\u89e3|zie}}<ref>\u3010\u79cb\u5fc3\u8c01\u582a\u89e3\u3011\u79cb\u5fc3\u4e8c\u5b57\u5408\u800c\u4e3a\u201c\u6101\u201d\uff0c\u89e3\u201c\u79cb\u5fc3\u201d\u610f\u4e3a\u89e3\u201c\u6101\u60c5\u201d\u3002\u5b8b\u00b7\u5434\u6587\u82f1\u300a\u5510\u591a\u4ee4\u300b\uff1a \u201c\u4f55\u5904\u5408\u6210\u6101\uff1f\u79bb\u4eba\u5fc3\u4e0a\u79cb\u3002\u201d</ref>\n\u6267{{ruby|\u6cea|luoi}}\u7b14\u5f92\u5c06\u76f8\u601d{{ruby|\u5199|sie}}\n\u58a8\u5c3d\u7a7a\u7559\u4eba{{ruby|\u60b2|buoi}}{{ruby|\u5207|cie}}\n\u4e89\u5948{{ruby|\u4f55|huo}}<ref>\u3010\u4e89\u5948\u4f55\u3011\u600e\u5948\u4f55\u3002\u4e89\uff1a\u600e\u3002\u5510\u00b7\u987e\u51b5\u300a\u4ece\u519b\u884c\u300b\u4e4b\u4e00\uff1a\u201c\u98ce\u5bd2\u6b32\u782d\u808c\uff0c\u4e89\u5948\u88d8\u8884\u8f7b\uff1f\u201d</ref> \u6b64\u4e00\u53bb\u6c38\u751f\u522b\n\n\n\u6eaf\u56de\u8def\u53f3<ref>\u3010\u6eaf\u56de\u8def\u53f3\u3011\u9006\u6d41\u8ffd\u5bfb\uff0c\u5374\u9053\u8def\u574e\u5777\u3002\u53f3\uff1a\u4e0b\uff0c\u4e00\u8bf4\u5f2f\u66f2\u3002\u672c\u53e5\u5f15\u81ea\u300a\u8bd7\u00b7\u56fd\u98ce\u00b7\u84b9\u846d\u300b\uff1a\u201c\u6eaf\u6d04\u4ece\u4e4b\uff0c\u9053\u963b\u4e14\u53f3\u3002\u201d</ref>\u3000\u795e\u5973\u516e\u7109\u6c42<ref>\u3010\u7109\u6c42\u3011\u4f55\u5904\u8ffd\u6c42\u3002\u7109\uff0c\u517c\u8bcd\uff0c\u4e8e\u4f55\u3002\u4e0a\u53e4\u6c49\u8bed\u4e2d\uff0c\u7591\u95ee\u53e5\u8ff0\u8bed\u5728\u8c13\u8bcd\u524d\u3002</ref>\n\u534e\u88fe\u7f57\u8896<ref>\u3010\u534e\u88fe\u7f57\u8896\u3011\u534e\u7f8e\u7684\u5916\u8863\uff0c\u9526\u4e3d\u7684\u8863\u8896\u3002\u6b64\u53e5\u4ea6\u53ef\u7406\u89e3\u4e3a\u4e92\u6587\u53e5\u3002\u88fe\uff0c\u6307\u6709\u5bf9\u895f\u7684\u7f69\u886b\u3002\u300a\u8bf4\u6587\u89e3\u5b57\u300b\uff1a\u201c\u88fe\uff0c\u8863\u888d\u4e5f\u3002\u201d \u5510\u00b7\u674e\u8d3a\u300a\u9ad8\u8f69\u8fc7\u300b\uff1a\u201c\u534e\u88fe\u7ec7\u7fe0\u9752\u5982\u8471\uff0c\u91d1\u73af\u538b\u8f94\u6447\u73b2\u73d1\u3002\u201d</ref>\u3000\u6cef\u5374\u51e0\u591a\u5fe7\n\u8f7b\u7ee2<ref>\u3010\u8f7b\u7ee2\u3011\u8f7b\u8584\u7684\u7ee2\u5e1b\u3002\u7ee2\uff0c\u9ea6\u830e\u8272\u7684\u4e1d\u7ec7\u54c1\u3002\u6e05\u00b7\u6bb5\u7389\u88c1\u300a\u8bf4\u6587\u89e3\u5b57\u6ce8\u300b\uff1a\u201c\u7ee2\uff0c\u7f2f\u5982\u9ea6\u414c\u8272\u3002\u201d </ref>\u7d20\u624b<ref>\u3010\u7d20\u624b\u3011\u7ed3\u767d\u7684\u53cc\u624b\uff0c\u591a\u5f62\u5bb9\u5973\u5b50\u3002\u300a\u53e4\u8bd7\u5341\u4e5d\u9996\u00b7\u9752\u9752\u6cb3\u7554\u8349\u300b\uff1a\u201c\u5a25\u5a25\u7ea2\u7c89\u5986\uff0c\u7ea4\u7ea4\u51fa\u7d20\u624b\u3002\u201d</ref>\u3000\u5a49\u79c9<ref>\u3010\u5a49\u79c9\u3011\u6e29\u5a49\u5730\u79c9\u6301\u7740\u3002</ref>\u7389\u7c2a\u5934\n\u6b32\u559c\u8fd8\u6101\u3000\u972d\u53bb\u4e0d\u76f8\u9058<ref>\u3010\u972d\u53bb\u4e0d\u76f8\u9058\u3011\u4e91\u96fe\u6d88\u6563\uff0c\u4fbf\u4e0d\u518d\u4e0e\u4e4b\u76f8\u9047\u3002\u972d\uff0c\u4e91\u6c14\u3001\u70df\u96fe\u3002\u76f8\u9058\uff0c\u76f8\u9047\u3002\u300a\u8bf4\u6587\u89e3\u5b57\u300b\uff1a\u201c\u9058\uff0c\u9047\u4e5f\u3002\u201d\u300a\u695a\u8f9e\u00b7\u4e25\u5fcc\u54c0\u65f6\u547d\u300b\uff1a\u201c\u592b\u4f55\u4e88\u751f\u4e4b\u4e0d\u9058\u65f6\uff01\u201d</ref>\n\n\u7fe9\u7fe9\u60ca\u9e5c<ref>\u3010\u60ca\u9e5c\u3011\u56e0\u60ca\u800c\u8d77\u7684\u91ce\u9e2d\u3002\u5510\u00b7\u738b\u52c3\u300a\u6ed5\u738b\u9601\u5e8f\u300b\uff1a\u201c\u843d\u971e\u4e0e\u5b64\u9e5c\u9f50\u98de,\u79cb\u6c34\u5171\u957f\u5929\u4e00\u8272\u3002\u201d</ref>\u8d77\u3000\u6d41\u7704<ref>\u3010\u6d41\u7704\u3011\u6d41\u8f6c\u76ee\u5149\u89c2\u770b\u3002\u6218\u56fd\u00b7\u695a\u00b7\u5b8b\u7389\u300a\u767b\u5f92\u5b50\u597d\u8272\u8d4b\u300b\uff1a\u201c\u542b\u559c\u5fae\u7b11\uff0c\u7a83\u89c6\u6d41\u7704\u201d\u3002</ref>\u3000\u795e\u6e38\n\u4f73\u4eba\u5df2\u4e0d\u5728\u3000\u56de\u6c40\u3000\u5740\u4e18<ref>\u3010\u56de\u6c40\u5740\u4e18\u3011\u540c\u4e0a\u6587\u201c\u6789\u6e1a\u6c99\u6d32\u201d\u3002</ref>\n\n\u9500\u957f\u68a6<ref>\u3010\u9500\u957f\u68a6\u3011\u201c\u9500\u201d\u540c\u201c\u6d88\u201d\uff0c\u6d88\u6563\u3002</ref>\u826f\u4f1a\u6c38{{ruby|\u8bc0|zue}}<ref>\u3010\u826f\u4f1a\u6c38\u8bc0\u3011\u5982\u6b64\u7f8e\u597d\u7684\u76f8\u4f1a\u4e0d\u4f1a\u518d\u6709\u4e86\u3002\u5f15\u81ea\u4e09\u56fd\u9b4f\u00b7\u66f9\u690d\u300a\u6d1b\u795e\u8d4b\u300b\uff1a\u201c\u60bc\u826f\u4f1a\u4e4b\u6c38\u7edd\u516e \u54c0\u4e00\u901d\u800c\u5f02\u4e61\u3002\u201d</ref>\n\u5149\u9634\u9519\u9b13\u751f\u6e05\u6c5f{{ruby|\u96ea|sue}}<ref>\u3010\u9b13\u751f\u6e05\u6c5f\u96ea\u3011\u7528\u9b13\u751f\u971c\u96ea\u4ee3\u6307\u4e24\u9b13\u6591\u767d\u3002\u5510\u00b7\u675c\u752b\u300a\u767b\u9ad8\u300b\uff1a\u201c\u8270\u96be\u82e6\u6068\u7e41\u971c\u9b02\uff0c\u6f66\u5012\u65b0\u505c\u6d4a\u9152\u676f\u3002\u201d\u6e05\u6c5f\uff1a\u6307\u96d2\u6c34\u3002</ref>\n\u628a\u6a3d\u76cf<ref>\u3010\u628a\u6a3d\u76cf\u3011\u628a\uff0c\u52a8\u8bcd\uff0c\u79c9\u3001\u6301\u3002\u6a3d\u76cf\uff0c\u9152\u5668\u3002</ref>\u865a\u5bf9\u955c\u4e2d\u6708\n{{ruby|\u676f|buoi}}\u7a7a\u5f92\u9980\u5fc3\u6ef4{{ruby|\u8840|sue}}\n{{ruby|\u4f55|huo}}\u6d88{{ruby|\u8bf4|shyue}}<ref>\u3010\u4f55\u6d88\u8bf4\u3011\u4e0d\u5fc5\u8bf4\u3002\u4f55\u6d88\uff0c\u4f55\u987b\u3002\u6c11\u56fd\u00b7\u674e\u53d4\u540c\uff1a\u201c\u4e00\u4e8b\u65e0\u6210\u4eba\u6e10\u8001\uff0c\u4e00\u94b1\u4e0d\u503c\u4f55\u6d88\u8bf4\u3002\u201d</ref> \u4eca\u4e00\u4eba\u770b\u76c8{{ruby|\u9619|cue}}<ref>\u3010\u76c8\u9619\u3011\u6307\u591c\u6708\u3002\u6708\u76c8\u6708\u7f3a\u4e3a\u6574\u6708\uff0c\u6b64\u5904\u4ee5\u770b\u6708\u53cd\u886c\u4e3b\u4eba\u5b64\u5355\u843d\u5bde\u3002\u5510\u00b7\u5f20\u4e54\u300a\u5bf9\u6708\u300b\u8bd7\u4e4b\u4e00:\u201c\u76c8\u7f3a\u9752\u51a5\u5916\uff0c\u4e1c\u98ce\u4e07\u53e4\u5439\u3002\u201d</ref>\n\u4f3c\u76f8\u89c1\u3000\u5374\u4e0d\u89c1\u3000\u5e7b\u6c34\u6708\n\u68a6\u4e2d\u4eba\u3000\u68a6\u4e2d\u60c5\u3000\u6c38\u7edd\u522b\n</poem>\n\n{{\u6ca1\u6709\u9f9f\u58f3\u7684\u4e4c\u9f9f|collapsed}}\n{{\u7eb3\u5170\u5bfb\u98ce|collapsed}}\n{{\u6d1b\u5929\u4f9d/\u6bbf\u5802\u66f2|collapsed}}\n\n== \u6ce8\u91ca ==\n<references/>\n[[\u5206\u7c7b:\u4e2d\u56fd\u97f3\u4e50\u4f5c\u54c1]]\n[[Category:\u4f7f\u7528VOCALOID\u7684\u6b4c\u66f2]]\n[[\u5206\u7c7b:\u6d1b\u5929\u4f9d\u6b4c\u66f2]]"}}}