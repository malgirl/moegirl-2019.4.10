ff0c\u4e5f\u4f20\u6709\u8eab\u4e3a\u8bf8\u517d\u4e4b\u7956\u7684\u9e92\u9e9f\u6b63\u786e\u5bf9\u4f4d\u662f\u8bf8\u9e1f\u4e4b\u7956\u9e3e\u7684\u8bf4\u6cd5\u3002\n\n\u5bf9\u88ab\u67d0\u4e9b\u5730\u65b9\u5f53\u6210\u957f\u9888\u9e7f\u4e00\u4e8b\u975e\u5e38\u751f\u6c14\u2026\u2026\n|}\n\n== \u6e38\u620f\u8868\u73b0 ==\n\n\u706b\u961f\u7b2c\u4e00\u7269\u7406\u901f\u653b\u624b\uff0c\u975e\u6d32\u7b2c\u4e00\u6218\u795e\uff0c\u5f00\u8352\u961f\u4e3b\u529b\uff0c\u622a\u6b62\u76ee\u524d\u7efc\u5408\u8282\u594f\u699c\u4e0a\u552f\u4e00\u7a33\u5750T0\u4f4d\u7f6e\u7684\u8a93\u7075\u3002\n\n\u9762\u677f\u4f18\u79c0\uff0c\u503e\u5411\u5148\u53d1\u5236\u4eba\uff1b\u5929\u8d4b\u7b80\u5355\u66b4\u529b\u8fd8\u597d\u7528\uff1b\u5bf9\u5723\u5668\u8981\u6c42\u4e0d\u9ad8\uff0c\u9002\u7528\u66b4\u51fb\u6d41\u548c\u7269\u7406\u52a0\u4f24\u6d41\uff0c\u5b9e\u5728\u62ff\u4e0d\u51fa\u66b4\u51fb\u5200\u548c\u5929\u4e1b\u4e91\u5251\u7684\u8bdd\u7089\u5b50\u4e5f\u80fd\u5bf9\u4ed8\u4e00\u4e0b\u3002\n\n\u6280\u80fd\u7ec4\u5f88\u662f\u4e00\u822c\uff0c\u708e\u738b\u5251\u5a01\u529b\u5f3a\u5927\u4f46\u526f\u4f5c\u7528\u582a\u79f0\u81f4\u547d\uff0c\u589e\u5e45\u6280\u80fd\u76f8\u5f53\u666e\u901a{{\u9ed1\u5e55|\u6216\u8005\u8bf4\u7b80\u76f4\u5f02\u5e38\u7684\u5f31}}\uff0cAOE\u65b9\u9762\u672c\u7cfb\u6280\u80fd\u4e0d\u5f3a\uff0c\u5f3a\u7684\u53c8\u4e0d\u662f\u672c\u7cfb\u6280\u80fd\uff0c\u6240\u4ee5\u73b0\u5728\u57fa\u672c\u4e0a\u5c31\u662f\u878d\u7532\u4e4b\u722a\u5305\u6253\u5929\u4e0b\u7684\u5c34\u5c2c\u5c40\u9762\u3002\n\n\u83b7\u5f97\u95e8\u69db\u4f4e\uff0c\u6210\u578b\u5feb\uff0c\u524d\u671f\u6218\u6597\u529b\u625b\u628a\u5b50\uff0c\u540e\u671f\u5404\u79cdUR\u6210\u578b\u540e\u4e5f\u80fd\u6709\u4e00\u5e2d\u4e4b\u5730\uff0c\u5c5e\u4e8e\u7ec3\u4e86\u7edd\u5bf9\u4e0d\u4e8f\u7684\u7c7b\u578b\u3002\n\n== \u8a93\u7075\u4e00\u4e8c\u4e09 ==\n===\u5168\u6e38\u7b2c\u4e00\u52b3\uff08\u4eb2\uff09\u6a21\uff08\u7239\uff09===\n\u73b0\u5728\u5047\u5982\u6709\u65b0\u5165\u73a9\u5bb6\u53bb\u95ee\u4e0a\u4e00\u53e5\u201c\u65b0\u624b\u5e94\u8be5\u7528\u4ec0\u4e48\u8a93\u7075\u597d\uff1f\u201d\u5f97\u5230\u7684\u56de\u7b54\u516b\u6210\u90fd\u662f\u201c\u522b\u95ee\uff0c\u95ee\u5c31\u9e92\u9e9f\u3002\u201d\u3002\n\n\u7406\u7531\uff1f\u7b80\u5355\uff0c\u597d\u5165\u624b\uff0c\u597d\u57f9\u517b\uff0c\u5f3a\uff08\u91cd\u70b9\uff09\u3002\n\n\u4f5c\u4e3a\u4e00\u4e2a\u6700\u4f4e1-4\u5c31\u80fd\u635e\u5230\u7684SR\uff0c\u9e92\u9e9f\u7684\u5f3a\u5ea6\u8fdc\u9ad8\u4e8e\u5979\u6240\u5728\u7684\u5173\u5361\u7684\u96be\u5ea6\u3002S\u7ea7\u7684\u7269\u653b\u548c\u901f\u5ea6\uff0c2\u500d\u66b4\u51fb\u4f24\u5bb3\u7684\u5929\u8d4b\uff08\u6b63\u5e38\u66b4\u4f24\u662f1.5\u500d\uff09\uff0c\u52a0\u4e0a\u547d\u4e2d\u5fc5\u66b4\u51fb\u7684\u878d\u7532\u4e4b\u722a\uff0c\u5728\u8fd9\u4e2a\u66b4\u51fb\u673a\u5236\u662f\u65e0\u89c6\u5bf9\u624b\u5168\u90e8\u80fd\u529b\u5f3a\u5316\u503c\u7684\u7248\u672c\u9636\u6bb5\uff0c\u9e92\u9e9f\u582a\u79f0\u5f53\u4e4b\u65e0\u6127\u7684\u7269\u7406\u7cfb\u5934\u53f7\u6253\u624b\u3002\u5982\u679c\u518d\u52a0\u4e0a\u4e0e\u4e4b\u642d\u914d\u7684[[\u65b9\u821f\u6307\u4ee4:\u51e4\u51f0|\u51e4\u51f0]]\uff0c\u5373\u4fbf\u4e0d\u7528\u9ad8\u7ea7\u5723\u5668\u52a0\u6210\uff0c\u9e92\u9e9f\u4e5f\u80fd\u591f\u6253\u51fa\u6210\u5428\u7684\u8f93\u51fa\u3002{{\u9ed1\u5e55|\u878d\u7532\u4e4b\u722a\u9762\u524d\u4eba\u4eba\u5e73\u7b49.jpg}}\n\n\u800c\u4e14\u7531\u4e8e\u7b49\u7ea7\u53ea\u662fSR\uff0c\u4f5c\u6218\u80fd\u529b\u5f3a\uff0c\u83b7\u5f97\u9014\u5f84\u591a\u8fdb\u5316\u6750\u6599\u5c11{{\u9ed1\u5e55|\u5bf9\u6bd4UR\u5361\u591a\u7684\u5413\u4eba\u7684\u6750\u6599\u800c\u8a00}}\uff0c\u9e92\u9e9f\u5f88\u5bb9\u6613\u5c31\u80fd\u6ee1\u7ea7\u6ee1\u7834\u8fbe\u5230\u6700\u5f3a\u72b6\u6001\uff0c\u57fa\u672c\u4e0a\u6bcf\u4e2a\u5fa1\u7075\u58eb\u90fd\u4f1a\u6709\u4e00\u53ea\u591a\u5c11\u7ec3\u8fc7\u7684\u9e92\u9e9f\u3002\u5728\u63a8\u56fe\u5f00\u8352\u4e43\u81f3\u7ade\u6280\u573a\u9e92\u9e9f\u90fd\u6709\u4e00\u5e2d\u4e4b\u5730\uff0c\u662f\u65e0\u6570\u840c\u65b0\u7684\u7b2c\u4e00\u6253\u624b\uff0c\u4e5f\u662f\u65e0\u6570\u5927\u4f6cUR\u4e4b\u5916\u7684\u7b2c\u4e00\u5907\u9009{{\u9ed1\u5e55|\u52b3\u82e6\u529f\u9ad8}}\u3002\n\n\u56e0\u4e3aSR\u7684\u4f53\u91cf\u9650\u5236\uff0c\u9e92\u9e9f\u8fd8\u662f\u6ca1\u6cd5\u548c\u6210\u578b\u4e4b\u540e\u7684UR\u76f8\u6bd4\uff0c\u4f46\u662f\u8003\u8651\u5230\u5305\u62ec\u5165\u624b\u6e20\u9053\u6210\u578b\u901f\u5ea6\u7b49\u7b49\u7684\u7efc\u5408\u80fd\u529b\u7684\u8bdd\u2026\u2026\u966a\u4f34\u5fa1\u7075\u58eb\u4eec\u8d70\u8fc7\u65e0\u6570\u98ce\u96e8\u7684\u9e92\u9e9f\uff0c\u8db3\u591f\u79f0\u5f97\u4e0a\u662f\u201c\u7239\u201d\u7ea7\u7684\u5b58\u5728\u3002\n\n===\u6c38\u8fdc\u7684\u4e00\u7ebf===\n\n\u5728\u4f60\u6e38\u81f3\u4eca\u4e3a\u6b62\u7684\u8282\u594f\u699c\u4e0a\uff0c\u53ea\u6709\u9e92\u9e9f\u4ece\u672a\u6389\u51faT0\u7684\u5b9d\u5ea7\u3002\n\n\u867d\u7136\u5728\u5f88\u591aUR\u8a93\u7075\u5373\u5c06\u6210\u578b\u7684\u9636\u6bb5\u9e92\u9e9f\u5f00\u59cb\u663e\u51fa\u9893\u52bf\uff0c\u8f93\u51fa\u4e5f\u7531\u4e8e\u53ea\u662f\u7d2b\u5361\u7684\u9650\u5236\u4ece\u800c\u6ca1\u6709\u4ee5\u524d\u90a3\u4e48\u51f6\u72e0\uff0c\u4f46\u8003\u8651\u5230\u5305\u62ec\u83b7\u5f97\u96be\u5ea6\u7b49\u7684\u5176\u4ed6\u5404\u79cd\u65b9\u9762\u800c\u8a00\uff0c\u9e92\u9e9f\u4f9d\u7136\u591f\u5f97\u4e0aT0\u7684\u53f0\u9636\u3002\n\n\u800c\u4e14\uff0c\u4e0d\u53ea\u662f\u6e38\u620f\u5f3a\u5ea6\uff0c\u5f88\u591a\u4eba\u7b2c\u4e00\u4e2a\u7ec3\u7684\u662f\u9e92\u9e9f\uff0c\u7b2c\u4e00\u4e2a\u6ee1\u7834\u7684\u662f\u9e92\u9e9f\uff0c\u7b2c\u4e00\u4e2a\u9000\u4f11\u7684\u4e5f\u662f\u9e92\u9e9f\uff0c\u9e92\u9e9f\u5df2\u7ecf\u5728\u5f88\u591a\u73a9\u5bb6\u5fc3\u4e2d\u6811\u7acb\u8d77\u4e86\u6bd4\u5f3a\u5ea6\u66f4\u751a\u7684\u826f\u597d\u5370\u8c61\u3002\n\n\u5f53\u7136\uff0cT0\u4e0d\u4f1a\u6c38\u8fdc\u4fdd\u6301{{\u9ed1\u5e55|\u51e4\u51f0\u5728\u67d0\u6bb5\u65f6\u95f4\u5185\u4e5f\u56e0\u4e3a\u6280\u80fd\u7684\u539f\u56e0\u6389\u4e0b\u8fc7T0}}\u3002\u4f46\u53ef\u4ee5\u8bf4\uff0c\u54ea\u6015\u4eca\u540e\u9762\u5bf9\u518d\u5f3a\u5927\u7684\u654c\u4eba\uff0c\u9762\u5bf9\u518d\u96be\u7684\u5173\u5361\uff0c\u8fd9\u4e2a\u4e00\u76f4\u5e26\u7740\u7b11\u5bb9\u7684\u8a93\u7075\u53ea\u8981\u5fa1\u7075\u58eb\u4e00\u58f0\u4ee4\u4e0b\uff0c\u7acb\u9a6c\u80fd\u591f\u62ab\u7532\u4e0a\u9635\uff0c\u518d\u6b21\u51b2\u950b\u5728\u6700\u524d\u7ebf\u7684\u6218\u573a\u4e0a\u3002\n\n===\u5bf9\u661f\u7a7a\u7684\u597d\u5947===\n\n\u5982\u679c\u628a\u9e92\u9e9f\u653e\u5728\u534f\u4f1a\u91cc\uff0c\u5979\u6709\u53e5\u8bdd\u662f\u8fd9\u4e48\u8bf4\u7684\uff1a\u201c\u542c\u8bf4\u5929\u7a7a\u4e2d\u4e5f\u6709\u4e00\u53ea\u95ea\u95ea\u53d1\u5149\u7684\u9e92\u9e9f\u5462\u3002\u201d\u3002\n\n\u8fd9\u53e5\u8bdd\u6240\u6307\u7684\u5e94\u8be5\u662f\u4f4d\u4e8e\u53cc\u5b50\u5ea7\u4ee5\u5357\uff0c\u5927\u72ac\u5ea7\u4ee5\u5317\uff0c\u5c0f\u72ac\u5ea7\u4e0e\u730e\u6237\u5ea7\u4e4b\u95f4\u7684\u94f6\u6cb3\u4e2d\u7684\u9e92\u9e9f\u661f\u5ea7\uff0c\u5bf9\u8fd9\u4e2a\u661f\u5ea7\u7684\u8bb0\u8f7d\u6700\u65e9\u53ef\u4ee5\u8ffd\u6eaf\u5230\u6ce2\u65af\u3002\u770b\u6765\u9e92\u9e9f\u5bf9\u4e8e\u661f\u5ea7\u5565\u7684\u4e5f\u6709\u4e00\u5b9a\u7684\u4e86\u89e3\uff1f\n\n\u4e0d\u8fc7\uff0c\u9e92\u9e9f\u5ea7\u7684\u5f62\u8c61\u7c7b\u4f3c\u4e8e\u72ec\u89d2\u517d\uff0c\u548c\u4e1c\u65b9\u4f20\u8bf4\u4e2d\u7684\u9e92\u9e9f\u8fd8\u662f\u5dee\u522b\u6bd4\u8f83\u5927\u7684\u3002\n\n===\u60e8\u906d\u9488\u5bf9\uff1f===\n\n\u81ea[[\u65b9\u821f\u6307\u4ee4:\u5b81\u8299|\u5b81\u8299]]\u5b9e\u88c5\u4ee5\u6765\uff0c\u773c\u5c16\u7684\u5fa1\u7075\u58eb\u4eec\u53d1\u73b0\u4e86\u4e00\u4e2a\u6280\u80fd\u7684\u63cf\u8ff0\u975e\u5e38\u7279\u522b\uff1a\n\n{{Hide|\u6807\u9898=<del>\u6211\u770b\u4f60\u5c31\u662f\u5728\u9488\u5bf9\u6211\u9e92\u9e9f</del>|\u5185\u5bb9=[[File:23333\u6280\u80fd.png|200px|center]]}}\n\n\u9e92\u9e9f\u7684\u88ab\u52a8\u540d\u5b57\u6b63\u597d\u53eb\u505a\u201c\u7965\u745e\u8d50\u798f\u201d\uff0c\u800c\u4e14\u6548\u679c\u6b63\u597d\u88ab\u514b\u5236\uff0c\u56e0\u6b64\u8fd9\u4e2a\u6280\u80fd\u53ef\u4ee5\u8bf4\u662f\u5bf9\u9e92\u9e9f\u6ee1\u6ee1\u7684\u