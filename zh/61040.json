\n<TD BGCOLOR=\"#d9d9d9\" height=\"20\" width=\"20\"> \u6539\u88c5\n<TD BGCOLOR=\"#57ba47\" height=\"20\" width=\"20\"> \u5916\u8eab\n<TD BGCOLOR=\"#7ffff4\" height=\"20\" width=\"20\"> \u5185\u8231\n<TD BGCOLOR=\"#ffb24e\" height=\"20\" width=\"20\"> \u5185\u58c1\n<TD BGCOLOR=\"#ffb24e\" height=\"20\" width=\"20\"> \u5185\u58c1\n<TD BGCOLOR=\"#ff4e4e\" height=\"20\" width=\"20\"> \u70ae\u67b6\n<TD BGCOLOR=\"#f557ff\" height=\"20\" width=\"20\"> \u7279\u6b8a\n</TR>\n</TABLE>\n|-\n! colspan=4 | '''\u519b\u65c5\u5c5e\u6027''' \n|-\n| '''\u6f14\u827a''' || 3 || '''\u827a\u672f''' || 2\n|-\n| '''\u8fd0\u52a8''' || 1|| '''\u53a8\u827a''' || 3\n|-\n| '''\u62a4\u7406''' || 3 || '''\u6280\u5de5''' || 3\n|-\n! colspan=4 | '''\u5165\u624b\u65b9\u5f0f'''\n|-\n| '''\u5f00\u53d1''' || colspan=3 | '''\uff08\u65e0\u6cd5\u5efa\u9020\uff09'''\n|-\n| '''\u6389\u843d''' || colspan=3 | '''\uff08\u65e0\u6cd5\u6389\u843d\u83b7\u5f97\uff09'''\n|-\n| '''\u5176\u4ed6''' || colspan=3 | \u643a\u5e26[[\u7ef4\u5361\u00b7\u963f\u683c\u4e3d\u666e\u76ae\u5a1c|\u7ef4\u5361]]\u901a\u5173\u6d3b\u52a8\u5173\u5361E8-4(\u201c\u98ce\u66b4\u9a91\u5175\u201d)\u540e\u6709\u51e0\u7387\u6389\u843d\u3002\n|}\n'''\u5907\u6ce8\uff1a\uff08*1\uff09'''\uff1a\u201c\u98ce\u66b4\u201d\u9a91\u5175\u53d8\u5f0f\uff0c\u72d9\u51fb\u70ae\u53f0\u6a21\u5f0f\u7684\u5f39\u836f\n\n== \u767b\u573a\u65f6\u95f4 ==\n==== \u9996\u6b21\u767b\u573a ====\n197x\u5e749\u6708\u4e2d\u65ec\uff0c\u6b63\u5f53\u4f0f\u9f99\u829d\u4e0e\u65c5\u884c\u8005\u53d1\u751f\u51b2\u7a81\u65f6\u4ecb\u5165\u6218\u6597\uff0c\u5229\u7528\u8d85\u8fdc\u7a0b\u706b\u70ae\u963b\u65ad\u4e86\u65c5\u884c\u8005\u7684\u8ffd\u51fb\u540e\u79bb\u5f00\u6218\u573a\u3002\n==== \u4e8c\u6b21\u767b\u573a ====\n197x\u5e7411\u6708\u672b\uff0c\u6b7b\u6c34\u4f63\u5175\u4e00\u884c\u52a8\u5c0f\u7ec4\u4e8e\u5317\u65b9\u5931\u8e2a\uff0c\u201c\u98ce\u66b4\u201d\u9a91\u5175\u56e2\u521d\u6b21\u53d7\u7d27\u6025\u59d4\u6258\u65f6\u4eb2\u81ea\u51fa\u52a8\u3002\n==== \u6b63\u5f0f\u767b\u573a ====\n197x\u5e741\u6708\uff0c\u8d85\u6f58\u5ba3\u5e03\u201c\u98ce\u66b4\u201d\u9a91\u5175\u56e2\u6b63\u5f0f\u6210\u7acb\uff0c\u5bf9\u5916\u5f00\u653e\u4e14\u5f00\u59cb\u63a5\u53d7\u59d4\u6258\u3002\n== \u6027\u80fd\u8bc4\u4ef7 ==\n*\u9762\u677f'''{{color|red|\u9632\u5fa1\u503c\u6781\u9ad8}}'''\u5e76\u4e14\u5bf9\u4e8e\u5927\u591a\u6570\u5f39\u79cd\u90fd\u62e5\u6709\u6297\u6027\uff0c\u81ea\u8eab\u7684\u3010\u5f15\u64ce\u3011\u4e0e\u3010\u8f66\u4f53\u3011\u53ef\u4ee5\u62b5\u6297\u6781\u5927\u591a\u6570\u7684\u8d1f\u9762\u6548\u679c\u3002\n*\u7a7f\u7532\u4e0e\u4f24\u5bb3\u8f83\u9ad8\uff0c\u5728\u540c\u671f\u4e2d\u62e5\u6709'''{{color|red|\u975e\u5e38\u6050\u6016}}'''\u7684\u8f93\u51fa\u80fd\u529b\u3002\n*\u9002\u5e94\u7684\u5f39\u79cd\u5c04\u7a0b\u6781\u8fdc\u4e14\u62e5\u6709'''{{color|red|\u826f\u597d\u7684\u7a7f\u7532}}'''\u6548\u679c\uff0c\u5728AP/APCR/HE\u8fd9\u7c7b\u91cd\u578b\u8f66\u79cd\u4e2d\u5360\u6709\u5f88\u5927\u7684\u6e38\u620f\u3002\n*\u826f\u597d\u7684\u8f93\u51fa\u52a0\u6781\u9ad8\u7684\u9632\u5fa1\uff0c\u8bbe\u5b9a\u4e0a\u662f\u975e\u5e38\u5f3a\u529b\u7684\u5b58\u5728\uff0c\u4f46\u662f\u7279\u522b\u5bb9\u6613\u53d7\u5230\u71c3\u70e7\u7684\u6548\u679c{{\u9ed1\u5e55|\u7eaf\u6b63\u7f8e\u7cfb\u8840\u7edf}}\n*\u603b\u4f53\u800c\u8a00\u662f\u9632\u5fa1\u6027\u80fd\u548c\u653b\u51fb\u80fd\u529b\u90fd\u975e\u5e38\u4f18\u79c0\u7684\u5766\u514b\uff0c\u4f46\u662f\u83b7\u5f97\u9014\u5f84\u662f\u4e2a\u95ee\u9898{{\u9ed1\u5e55|\u6bd5\u7adf\u201c\u98ce\u66b4\u201d\u5175\u56e2\u56e2\u957f\u592a\u61d2\uff0c\u5927\u591a\u6570\u65f6\u95f4\u90fd\u4e0d\u51fa\u95e8}}\n\n{{Template:\u94a2\u94c1\u534e\u5c14\u5179|\u7f8e\u56fd}}\n== \u5916\u90e8\u6ce8\u91ca\u4e0e\u94fe\u63a5 ==\n[[Category:\u673a\u5a18]]\n[[Category:\u94a2\u94c1\u534e\u5c14\u5179]]"}}}