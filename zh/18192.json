u65e0\u6570\u7247\u60b2\u54c0\u7684\u6e56\u6c34\u3000\u5979\u65e9\u5df2\u65e0\u8def\u53ef\u9000}}\n\uff08\u4e3a\u62ef\u6551\u666e\u8def\u540c\u57c3\u5df2\u7ecf\u8fc7\u65e0\u6570\u4e2a\u4e16\u754c\uff0c\u65e0\u6cd5\u56de\u5230\u5f00\u59cb\uff09\n{{color|#EF404A|\u8fd9\u4e00\u5207\u5f52\u529f\u4e8e\u4f60\u4eec\u7684\u539f\u7f6a}}\n\uff08\u8fd9\u90fd\u662f\u5a01\u8bfa\u5e0c\u548c\u8428\u817e\u52aa\u65af\u7684\u8fc7\u9519\uff09\n{{color|#EF404A|\u6211\u8bdb\u6740\u7406\u60f3\u8005\u6b4c\u9882\u7684\u5b8c\u7f8e\u3000\u6211\u4fe1\u5979\u7ec8\u5c06\u6c89\u7761}}\n\u3010\u666e\u8def\u540c\u76f8\u4fe1\u57c3\u5df2\u7ec8\u6709\u4e00\u5929\u4f1a\u611f\u5230\u538c\u5026\u3011\n{{color|#EF404A|\u4f55\u5fc5\u8981\u8ddf\u968f\u6211\u5760\u5165\u90a3\u8f6e\u56de}}\n\uff08\u666e\u8def\u540c\u4e0d\u5e0c\u671b\u57c3\u5df2\u50cf\u81ea\u5df1\u90a3\u6837\u3010\u88ab\u62b9\u9664\u8bb0\u5fc6\u6295\u8eab\u4e8e\u5404\u4e2a\u4e16\u754c\u3011\uff09\n\n{{color|#999999|\u300c\u4f60\u7684\u4f7f\u547d\u53ea\u6709\u4e00\u4e2a\u300d\n\u300e\u597d\u7684\u6bcd\u4eb2\uff0c\u8bf7\u544a\u8bc9\u6211\u8be5\u600e\u4e48\u505a\u300f}}\n\uff08\u8428\u817e\u52aa\u65af\u8ba9\u57c3\u5df2\u62ef\u6551\u666e\u8def\u540c\uff09\n{{color|#999999|\u300c\u4f60\u662f\u5728\u8fdd\u6297\u6211\u5417\u300d\n\u300e\u662f\u7684\u6bcd\u4eb2\uff0c\u6211\u6b63\u51c6\u5907\u8fd9\u6837\u505a\u300f}}\n\uff08\u57c3\u5df2\u56e0\u3010\u770b\u5230\u666e\u8def\u540c\u548c[[\u6fc1\u4ee4]]\u5728\u4e00\u8d77\u800c\u6000\u7591\u81ea\u5df1\u6240\u8c13\u7684\u62ef\u6551\u6839\u672c\u4e0d\u662f\u666e\u8def\u540c\u60f3\u8981\u7684\uff0c\u662f\u65e0\u610f\u4e49\u7684\u3011\uff09\n\n{{color|#EF404A|\u6211\u4e0e\u5979\u4e0d\u9700\u8981\u6d45\u8584\u7684\u7406\u89e3\u3000\u5728\u751f\u547d\u71c3\u5c3d\u4e4b\u524d}}\n\uff08\u6211\u548c\u5979\u5230\u6b7b\u90fd\u7406\u89e3\u5f7c\u6b64\uff09\n{{color|#EF404A|\u8fd9\u4e00\u5207\u4e0d\u8fc7\u662f\u6c38\u6052\u7684\u6b3a\u9a97}}\n\u3010\u2460\u666e\u8def\u540c\u4e0e\u6fc1\u4ee4\u5728\u4e00\u8d77\u4ec5\u662f\u4e3a\u6b3a\u9a97\u57c3\u5df2\uff0c\u8ba9\u5979\u7070\u5fc3\uff1b\n\u3000\u2461\u666e\u8def\u540c\u4e0a\u4e00\u53e5\u8bf4\u7684\u7406\u89e3\u53ea\u662f\u6b3a\u9a97\u81ea\u5df1\uff1b\n\u3000\u2462\u795e\u5bf9\u4e8e\u795e\u4f7f\u7684\u6b3a\u9a97\uff0c\u8054\u7cfb\u201c\u662f\u90a3\u4e9b\u795e\u590d\u4ec7\u7684\u65b9\u5f0f\u201d\u3011\n{{color|#EF404A|\u6211\u8bdb\u6740\u638c\u6743\u8005\u8106\u5f31\u7684\u5c0a\u4e25\u3000\u6211\u4fe1\u5979\u7ec8\u5c06\u6bc1\u706d}}\n\u3010\u57c3\u5df2\u5bf9\u666e\u8def\u540c\u7684\u7231\u5c06\u4f7f\u5979\u6bc1\u706d\u3011\n{{color|#EF404A|\u4f55\u5fc5\u8981\u62e5\u62b1\u6211\u5760\u5165\u90a3\u7231\u604b}}\n\u3010\u666e\u8def\u540c\u4e0d\u60f3\u7231\u4e0a\u57c3\u5df2\uff0c\u4e0d\u60f3\u770b\u5979\u548c\u767d\u6d1b\u4e00\u6837\u3011\n\n\u5bf9\u666e\u8def\u540c\u6740\u6b7b\u57c3\u5df2\u7684\u79c1\u4eba\u7406\u89e3\uff1a\n\u4e0e\u5176\u8ba9\u4f60\u4e3a\u6551\u6211\u800c\u6b7b\uff0c\u4e0d\u5982\u6211\u5c06\u4f60\u4eb2\u624b\u6740\u6b7b\u3002\uff08\u8054\u7cfb\u751f\u6daf\u7cfb\u5217\uff09\n\u8fd9\u6837\u4f60\u4e0e\u6211\u6740\u6b7b\u7684\u666e\u901a\u4eba\u65e0\u5f02\uff0c\u6211\u4e5f\u4e0d\u5fc5\u4f24\u5fc3\n\n\u5bf9\u6b4c\u8bcd\u6f5c\u610f\u601d\u7684\u7406\u89e3\uff1a\n\u666e\u8def\u540c\u5c06\u57c3\u5df2\u4e4b\u6b8b\u7834\u5f52\u529f\u4e8e\u5a01\u8bfa\u5e0c\u548c\u8428\u817e\u52aa\u65af\uff0c\u65e0\u6570\u904d\u6211\u4fe1\u53ea\u4e3a\u9003\u8131\u81ea\u5df1\u5185\u5fc3\u7275\u8fde\u5979\u53d7\u8fc7\u7684\u4e0d\u5b89\u7684\u8c34\u8d23\uff0c\u4ece\u672a\u60f3\u8fc7\u5979\u4e4b\u4e0d\u5e78\u548c\u6bc1\u706d\u7686\u7531\u81ea\u5df1\u800c\u751f\u3002\n</poem>\n\n\n{{JUSF\u5468\u5b58|collapsed}}\n{{\u6d1b\u5929\u4f9d/\u6bbf\u5802\u66f2|collapsed}}\n{{\u300cSUPER_HIT!!\u300dVocaloid\u4e2d\u6587\u66f2|collapsed}}\n==\u6ce8\u91cb==\n<references/>\n[[\u5206\u7c7b:\u4e2d\u56fd\u97f3\u4e50\u4f5c\u54c1]]\n[[\u5206\u7c7b:\u4f7f\u7528VOCALOID\u7684\u6b4c\u66f2]]\n[[\u5206\u7c7b:\u6d1b\u5929\u4f9d\u6b4c\u66f2]]"}}}