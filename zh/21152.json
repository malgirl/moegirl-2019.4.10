\n==\u8c03\u6574==\n<poem>\n[\u548c\u7766\u7684\u4f7f\u8005]<\u548c\u7766\u306e\u4f7f\u8005>\n\n\u25c7\u53ef\u4ee5\u5bf9\u5e94\u5492\u6587\u901f\u5ea6\u4e3a1.2\u7684\u6548\u679c\u8fdb\u884c\u8fde\u9501\u53d1\u52a8\n\u25c7\u53ef\u4ee5\u5bf9\u5e94\u602a\u517d\u7684\u653b\u6483\u5ba3\u8a00\u00b7\u53ec\u559a\u6642\u53d1\u52a8\u3002\n\u25c7\u4e0d\u80fd\u5728\u4f24\u5bb3\u8ba1\u7b97\u9636\u6bb5\u53d1\u52a8\u3002 \n\u25c7\u53d1\u52a8\u540e\u8fd9\u4e2a\u56de\u5408\u5185\u300c\u548c\u7766\u306e\u4f7f\u8005\u300d\u4e0d\u4f1a\u7559\u5728\u573a\u4e0a\u3002\n\n\u25c7\u53d1\u52a8\u7684\u56de\u5408\u5bf9\u624b\u7684\u602a\u517d\u5168\u90e8\u7684\u6218\u6597\u4f24\u5bb3\u4e3a0\n\u3010\u5bf9\u624b\u7684\u602a\u517d\u5168\u90e8\u7684\u6218\u6597\u4f24\u5bb3\u3011\n\u25c6\u53d7\u5230\u653b\u51fb\u7684\u602a\u517d\u88ab\u7834\u574f\n\u25c6\u602a\u517d\u4e4b\u95f4\u7684\u6218\u6597\u4ea7\u751f\u7684\u4f24\u5bb3\n\u25c6\u5bf9\u73a9\u5bb6\u76f4\u63a5\u653b\u51fb\u4ea7\u751f\u7684\u4f24\u5bb3\n\u53ef\u4ee5\u9632\u6b62\u4ee5\u4e0a\u6240\u6709\u7684\u4f24\u5bb3\u3002\n\u00b7\u53ef\u4ee5\u9632\u6b62[\u6d41\u661f\u76f4\u51fb/\u30e1\u30c6\u30aa\u00b7\u30b9\u30c8\u30e9\u30a4\u30af][\u731b\u8fdb\u7684\u5251\u89d2\u517d/\u731b\u9032\u3059\u308b\u5263\u89d2\u7363]\u7b49\u52b9\u679c\u4ea7\u751f\u7684\u6218\u6597\u4f24\u5bb3\u3002\n\u00b7\u53ef\u4ee5\u9632\u6b62[\u767e\u8db3\u866b/\u30ec\u30c3\u30b0\u30eb][\u4eba\u9020\u4eba7\u53f7/\u4eba\u9020\u4eba\u9593\uff17\u53f7][\u4e58\u9f99\u7684\u7ffc\u9f99\u6218\u58eb/\u30c9\u30e9\u30b4\u30f3\u306b\u4e57\u308b\u30ef\u30a4\u30d0\u30fc\u30f3]\u7b49\u52b9\u679c\u4ea7\u751f\u7684\u6218\u6597\u4f24\u5bb3\u3002\n\u00b7\u4e0d\u80fd\u9632\u6b62[\u52a0\u519c\u70ae\u5175/\u30ad\u30e3\u30ce\u30f3\u00b7\u30bd\u30eb\u30b8\u30e3\u30fc][\u5f39\u5c04\u9f9f/\u30ab\u30bf\u30d1\u30eb\u30c8\u00b7\u30bf\u30fc\u30c8\u30eb]\u7b49\u6548\u679c\u4f24\u5bb3\u3002\n\u00b7\u53d7[\u9f99\u4e4b\u652f\u914d\u8005/\u30ed\u30fc\u30c9\u00b7\u30aa\u30d6\u00b7\u30c9\u30e9\u30b4\u30f3]\u52b9\u679c\u5f71\u54cd\u7684\u9f99\u65cf\u602a\u517d\u4e5f\u4e0d\u80fd\u4ea7\u751f\u6218\u6597\u4f24\u5bb3\u3002\uff08\u4e0d\u5c5e\u4e8e\u6307\u5b9a\u5bf9\u8c61\u6548\u679c\uff09\n\u25c7[\u548c\u7766\u306e\u4f7f\u8005/\u548c\u7766\u7684\u4f7f\u8005]\u6548\u679c\u9002\u7528\u540e\uff0c\u5bf9\u624b\u7684\u602a\u7269\u53ef\u4ee5\u653b\u51fb\u5ba3\u8a00\u3002\u8fdb\u884c\u653b\u51fb\u3001\u8fdb\u5165\u4f24\u5bb3\u8ba1\u7b97\u3001[\u5f02\u6b21\u5143\u7684\u6218\u58eb/\u7570\u6b21\u5143\u306e\u6226\u58eb][\u5e7b\u5f71\u58c1/\u5e7b\u5f71\u306e\u58c1][\u54e5\u5e03\u6797\u7a81\u51fb\u90e8\u961f/\u30b4\u30d6\u30ea\u30f3\u7a81\u6483\u90e8\u968a]\u7b49\u52b9\u679c\u53d1\u52a8\u3002\n\u88cf\u5074\u8868\u793a\u7684\u602a\u517d\u5728\u53d7\u5230\u653b\u51fb\u540e\u3001\u5728\u4f24\u5bb3\u8ba1\u7b97\u9636\u6bb5\u7ffb\u5f00\u3002\uff08\u8986\u76d6\u52b9\u679c\u53d1\u52a8\uff09\uff08\u867d\u7136\u8fdb\u5165\u4f24\u5bb3\u8ba1\u7b97\u9636\u6bb5\u3001\u4f46\u7531\u4e8e[\u548c\u7766\u7684\u4f7f\u8005/\u548c\u7766\u306e\u4f7f\u8005]\u7684\u53d1\u52a8\u73a9\u5bb6\u4e0d\u4f1a\u53d7\u5230\u6218\u6597\u4f24\u5bb3\u3001\u602a\u517d\u4e0d\u4f1a\u7834\u574f\uff09\n\u25c7\u88cf\u5074\u5b88\u5099\u8868\u793a\u7684[\u7cbe\u795e\u5bc4\u751f\u4f53/\u7cbe\u795e\u5bc4\u751f\u4f53][\u7403\u4f53\u65f6\u9650\u70b8\u5f39/\u30b9\u30d5\u30a3\u30a2\u00b7\u30dc\u30e0\u7403\u4f53\u6642\u9650\u7206\u5f3e]\u6548\u679c\u53d1\u52a8\u3002\uff08\u539f\u672c\u5c31\u6709\u4e0d\u8ba1\u7b97\u4f24\u5bb3\u7684\u52b9\u679c\uff09\n\u25c7[\u548c\u7766\u7684\u4f7f\u8005/\u548c\u7766\u306e\u4f7f\u8005]\u6548\u679c\u9002\u7528\u540e\u88cf\u5074\u5b88\u5099\u8868\u793a\u602a\u517d\u88ab[\u4e00\u5200\u4e24\u65ad\u4f8d/\u4e00\u5200\u4e21\u65ad\u4f8d]\u653b\u6483\u7684\u5834\u5408\u3001\u4f24\u5bb3\u8ba1\u7b97\u9636\u6bb5[\u4e00\u5200\u4e24\u65ad\u4f8d/\u4e00\u5200\u4e21\u65ad\u4f8d]\u7684\u201d\u52b9\u679c\u201d\u628a\u88cf\u5074\u5b88\u5099\u8868\u793a\u602a\u517d\u4ee5\u88cf\u5074\u5b88\u5099\u8868\u793a\u7684\u72b6\u6001\u7834\u58ca\u3002\uff08\u4e0d\u8fdb\u884c\u4f24\u5bb3\u8ba1\u7b97\u3001\u8ddf\u6218\u6597\u4f24\u5bb3\u65e0\u5173\uff09\n\u25c7[\u5b88\u5893\u7684\u4ece\u8005/\u5893\u5b88\u306e\u5f93\u8005]\u8fdb\u884c\u6218\u6597\u7684\u5834\u5408\uff08\u6218\u6597\u4f24\u5bb3\u53d8\u4e3a\u6548\u679c\u4f24\u5bb3\uff09\n[\u5b88\u5893\u7684\u4ece\u8005/\u5893\u5b88\u306e\u5f93\u8005]\u653b\u51fb\u602a\u517d\u7684\u5834\u5408\u3001\u602a\u517d\u53d7\u5230\u7684\u4f24\u5bb3\u80fd\u88ab[\u548c\u7766\u7684\u4f7f\u8005/\u548c\u7766\u306e\u4f7f\u8005]\u9632\u6b62\u3001\u4f46\u7531\u4e8e\u73a9\u5bb6\u53d7\u5230\u7684\u662f\u6548\u679c\u4f24\u5bb3\u3001[\u548c\u7766\u7684\u4f7f\u8005/\u548c\u7766\u306e\u4f7f\u8005]\u4e0d\u80fd\u9632\u6b62\u3002\n\u25c7[\u548c\u7766\u7684\u4f7f\u8005/\u548c\u7766\u306e\u4f7f\u8005]\u52b9\u679c\u89e3\u6c7a\u3001\u9002\u7528\u7684\u72b6\u6001\u4e0b\u3001\u4e4b\u540e\u5373\u4f7f[\u738b\u5bae\u306e\u304a\u89e6\u308c]\u53d1\u52a8\u3001\u6548\u679c\u9002\u7528\u540e\u6548\u679c\u4e5f\u4e0d\u4f1a\u65e0\u6548\u3002\uff08\u5982\u679c\u662f[\u548c\u7766\u7684\u4f7f\u8005/\u548c\u7766\u306e\u4f7f\u8005]\u53d1\u52a8\u65f6[\u738b\u5bab\u7684\u901a\u544a/\u738b\u5bae\u306e\u304a\u89e6\u308c]\u8fde\u9501\u7684\u573a\u5408\u3001\u6548\u679c\u65e0\u6548\uff09 \n\u25c7[\u4e00\u5200\u4e24\u65ad\u4f8d/\u4e00\u5200\u4e21\u65ad\u4f8d][\u767d\u9f99\u4e4b\u5723\u9a91\u58eb/\u767d\u7adc\u306e\u8056\u9a0e\u58eb]\u5bf9\u88cf\u5074\u5b88\u5099\u8868\u793a\u7684\u602a\u517d\u653b\u6483\u7684\u5834\u5408\u3001\u4e0d\u5c5e\u4e8e\u6218\u6597\u7834\u574f\u3001\u800c\u662f\u6548\u679c\u7834\u574f\u3002\u6b64\u65f6\u5373\u4f7f[\u548c\u7766\u7684\u4f7f\u8005/\u548c\u7766\u306e\u4f7f\u8005]\u9002\u7528\u3001\u4e5f\u65e0\u6cd5\u9632\u6b62\u602a\u517d\u88ab\u6548\u679c\u7834\u574f\u3002\n</poem>\n\n\n\n==\u5173\u8054\u5361\u7247==\n*[[\u96f6\u70b9\u5b88\u536b\u58eb]]\n*[[\u82f1\u8c6a\u6311\u6218\u8005 \u5251\u76fe\u5175]]\n*[[\u706b\u7bad\u6218\u58eb]]\n*[[\u94e0\u7532\u6cb3\u7ae5]]\n*[[\u795e\u5723\u751f\u547d