uff0c\u539f\u672c\u5b81\u9759\u7684\u96fe\u4e4b\u6e56\u4e0a\uff0c\u51fa\u73b0\u4e86\u4e00\u4e2a\u7531\u6cb3\u7ae5\u91cd\u5de5\u627f\u5efa\u7684\u5de5\u5730\u3002\n\n\u8fd9\u4e2a\u9879\u76ee\u7684\u6240\u5c5e\u65e0\u4eba\u77e5\u6653\uff0c\u5176\u76ee\u7684\u66f4\u662f\u4e00\u4e2a\u8c1c\u3002\n\n\u9ed1\u767d\u7684\u597d\u4e8b\u8005\u96fe\u96e8\u9b54\u7406\u6c99\uff0c\u60f3\u90fd\u6ca1\u6709\u60f3\u5c31\u7ed9\u81ea\u5df1\u52a0\u4e0a\u4e86\u798f\u5c14\u6469\u65af\u7684\u5934\u8854\u51b2\u5728\u4e86\u524d\u7ebf\uff0c\u201c\u5e7b\u60f3\u4e61\u6700\u901f\u201d\u7684\u79f0\u53f7\u90fd\u5dee\u70b9\u56e0\u6b64\u6613\u4e3b\u4e86\u3002\n\n\u81f3\u4e8e\u88ab\u4e00\u5ea6\u5c0a\u79f0\u4e3a\u201c\u5e7b\u60f3\u4e61\u7684\u6551\u4e16\u4e3b\u201d\u3001\u597d\u4e0d\u5bb9\u6613\u6062\u590d\u4e86\u5e73\u9759\u7684\u751f\u6d3b\u3001\u52a0\u5165\u4e86\u4e00\u4e2a\u72e9\u730e\u8005\u540c\u76df\u3001\u6253\u7b97\u5b89\u5b89\u5fc3\u5fc3\u5f53\u4e00\u4e2a\u597d\u730e\u4eba\u7684\u9b4f\u6f47\u627f\uff0c\u672c\u6765\u662f\u6839\u672c\u4e0d\u613f\u610f\u53c2\u4e0e\u8fd9\u4ef6\u4e8b\u60c5\u7684\u3002\n\n\u4f46\u662f\uff0c\u4ed6\u7684\u59b9\u59b9\u9b4f\u6f47\u5bc2\uff0c\u53c8\u5931\u8e2a\u4e86\u2026\u2026\n\n\n== \u767b\u573a\u4eba\u7269 ==\n\n[\u9b4f\u6f47\u627f\u00b7\u5b88\u62a4\u68a6\u60f3\u7684\u9ed1\u767d]\n\n\u6210\u4e3a\u4e86\u542f\u793a\u8005-8\u7684\u4e00\u77ac\u95f4\u3001\u4e0e\u542f\u793a\u8005-7\u540c\u5f52\u4e8e\u5c3d\u800c\u5931\u53bb\u4e86\u5b58\u5728\u3002\u5728\u5373\u5c06\u6d88\u5931\u7684\u65f6\u5019\uff0c\u5f97\u5230\u4e86\u5076\u7136\u7ed3\u8bc6\u7684\u670b\u53cb\u516b\u4e91\u7d2b\u7684\u5e2e\u52a9\uff0c\u8fdb\u5165\u4e86\u5e7b\u60f3\u4e61\u3002\u867d\u7136\u5931\u53bb\u4e86\u300a\u4e16\u754c\u6811\u9ed8\u793a\u5f55\u300b\uff0c\u4f46\u662f\u8fd8\u662f\u62e5\u6709\u5f3a\u5927\u7684\u5b9e\u529b\u3002\n\n\u542f\u793a\u8005\u7684\u8bdd\uff0c\u5927\u6982\u5c31\u662f\u5361\u5bc6\u8428\u739b\u4e4b\u7c7b\u7684\u4e1c\u897f\u5427\u3002\u4f46\u662f\u5361\u5bc6\u8428\u739b\u53ea\u80fd\u7ba1\u7406\u8f83\u5c11\u7684\u4e16\u754c\uff0c\u542f\u793a\u8005\u4eec\u5219\u53ef\u4ee5\u63a7\u5236\u5168\u90e8\u7684\u4e16\u754c\u3002\n\n\n\u4e3b\u8981\u80fd\u529b\uff1a\n\n\u3010\u542f\u793a\u8005\u3011\n\n\u80fd\u591f\u4efb\u610f\u64cd\u63a7\u7269\u8d28\u3001\u80fd\u91cf\u3001\u65f6\u95f4\u3001\u7a7a\u95f4\u3001\u610f\u8bc6\u3001\u865a\u7a7a\u7a0b\u5ea6\u7684\u80fd\u529b\u3002\u57fa\u672c\u4e0a\u662f\u65e0\u89e3\u7684\u3002\u5373\u4fbf\u6ca1\u6709\u4e86\u300a\u9ed8\u793a\u5f55\u300b\u7684\u6b63\u4f53\uff0c\u5a01\u529b\u6709\u6240\u4e0b\u964d\uff0c\u4f46\u662f\u5982\u679c\u5bf9\u624b\u4e0d\u662f\u542f\u793a\u8005\u7684\u8bdd\uff0c\u591a\u534a\u90fd\u662f\u5e9f\u4e86\u3002\n\n\u8fd9\u4e2a\u80fd\u529b\u8fd8\u53ef\u4ee5\u7528\u6765\u5b9e\u73b0\u613f\u671b\u3002\u53ea\u9700\u8981\u4fee\u6539\u4e00\u4e0b\u4e16\u754c\u7684\u6cd5\u5219\u2014\u2014\u5bf9\u542f\u793a\u8005\u6765\u8bf4\u5c31\u50cf\u662f\u4fee\u6539\u4e00\u4e2a\u670d\u52a1\u5668\u7684\u914d\u7f6e\u6587\u4ef6\u4e00\u6837\u7b80\u5355\u3002\n\n\u201c\u6211\u5df2\u7ecf\u6210\u4e3a\u4e86\u65b0\u4e16\u754c\u7684\u5361\u5bc6\u8428\u739b\u3002\u201d\n\n\u3010Lancer\u7684\u8bc5\u5492\u3011\n\n\u7531\u4e8e\u4e3b\u8981\u4f7f\u7528\u6b66\u5668\u4e8c\u4e94\u67aa\uff0c\u9b4f\u6f47\u627f\u4e5f\u4e0d\u53ef\u5e78\u514d\u5730\u5e26\u4e0a\u4e86Lancer\u7684\u8bc5\u5492\u3002\u5728\u539f\u4f5c\u5267\u60c5\u4e2d\uff0c\u4ed6\u4e3a\u4e86\u53bb\u5f80\u4e16\u754c\u6811\u7684\u9876\u7aef\uff0c\u7528\u4e8c\u4e94\u67aa\u81ea\u6233\u4e0a\u4e07\u6b21\u3002\n\n\u201cLancer\u53c8\u6b7b\u4e86\uff01\u771f\u6ca1\u4eba\u6027\uff01\u201d\n\n\u3010\u59d0\u63a7\u3011\n\n\u9b4f\u6f47\u627f\u662f\u9b4f\u6f47\u73a5\u5e7b\u60f3\u7684\u4ea7\u7269\uff0c\u5728\u539f\u4f5c\u4e2d\u79f0\u547c\u9b4f\u6f47\u73a5\u4e3a\u59d0\u59d0\u3002\u53ea\u8981\u6709\u4efb\u4f55\u4eba\u6562\u6253\u6f47\u73a5\u7684\u4e3b\u610f\uff0c\u54fc\u54fc\u2026\u2026\n\n\u201c\u4f60\u5df2\u7ecf\u6b7b\u4e86\u3002\u201d\n\n\n[\u9b4f\u6f47\u73a5\u00b7\u5145\u6ee1\u5e7b\u60f3\u7684\u5c11\u5973]\n\n\u5e7b\u60f3\u51fa\u9b4f\u6f47\u627f\uff0c\u5e76\u5728\u88abREBORN\u793e\u7b2c\u4e8c\u6b21\u590d\u6d3b\u540e\u3001\u4f7f\u7528\u83b7\u5f97\u7684\u201c\u73b0\u5b9e\u5316\u201d\u80fd\u529b\u5c06\u4ed6\u5177\u73b0\u3002\u57287-8\u4e4b\u6218\u4e4b\u540e\uff0c\u88ab\u7d2b\u4e00\u8d77\u5e26\u5165\u4e86\u5e7b\u60f3\u4e61\u3002\u80fd\u529b\u4fdd\u7559\u3002\n\n\u5b9e\u9645\u4e0a\uff0c\u6839\u636e\u201c\u4f46\u51e1\u80fd\u64cd\u63a7\u6839\u4e16\u754c\u7cbe\u795e\u4f53\u7684\u5b58\u5728\u90fd\u79f0\u4e3a\u4e16\u754c\u6811\u9ed8\u793a\u5f55\u201d\u7684\u5b9a\u4e49\uff0c\u5979\u672c\u8eab\u5c31\u662f\u4e00\u672c\u300a\u4e16\u754c\u6811\u9ed8\u793a\u5f55\u300b\u3002\n\n\n\u4e3b\u8981\u80fd\u529b\uff1a\n\n\u3010\u73b0\u5b9e\u5316\u3011\n\n\u5c06\u60f3\u8c61\u4e2d\u7684\u4e1c\u897f\u5316\u4e3a\u73b0\u5b9e\u3002\u975e\u5e38\u65b9\u4fbf\u7684\u80fd\u529b\u3002\n\n\u4e00\u5207\u60f3\u8c61\u4e4b\u4e2d\u7684\u4e1c\u897f\uff0c\u5373\u4fbf\u4e0d\u9700\u8981\u5177\u4f53\u5730\u601d\u7d22\u6bcf\u4e00\u4e2a\u7ec6\u8282\uff0c\u4e5f\u80fd\u591f\u73b0\u5b9e\u5316\u6210\u529f\u3002\n\n\u8981\u662f\u7ed9\u5979\u4e00\u4e2a\u79f0\u547c\u7684\u8bdd\uff0c\u53eb\u505a\u201c\u9020\u7269\u4e3b\u201d\u6216\u8bb8\u4e0d\u9519\u3002\n\n\u3010\u4e16\u754c\u6811\u9ed8\u793a\u5f55\u3011\n\n\u80fd\u591f\u4fee\u6539\u6839\u4e16\u754c\u7cbe\u795e\u4f53\uff08\u5b83\u4e4b\u4e8e\u6211\u4eec\u5c31\u597d\u6bd4\u670d\u52a1\u5668\u4e4b\u4e8e\u73a9\u5bb6\uff09\u7684\u610f\u5fd7\u3002\u4f46\u662f\u8fd9\u9879\u80fd\u529b\u4e0d\u80fd\u4e3b\u52a8\u4f7f\u7528\u3002\u5728\u88abREBORN\u4e4b\u540e\uff0c\u53ef\u4ee5\u4f7f\u7528\u201c\u73b0\u5b9e\u5316\u201d\u7684\u80fd\u529b\u3002\n\n\u4e0e\u9b4f\u6f47\u627f\u5408\u2640\u4f53\u540e\u80fd\u591f\u91ca\u653e\u5168\u90e8\u7684\u80fd\u529b\u3002\n\n\n\n\n[\u9b4f\u6f47\u5bc2\u00b7\u4e0d\u660e\u6b63\u4f53\u7684\u59b9\u59b9]\n\n\u539f\u672c\u53ea\u662fREBORN\u793e\u6240\u5236\u4f5c\u7684\u3001\u9b4f\u6f47\u73a5\u7684\u4e00\u4e2a\u6ca1\u6709\u7075\u9b42\u7684\u590d\u5236\u8eab\u4f53\uff0c\u88ab\u542f\u793a\u8005-7\u5f3a\u884c\u6324\u5165\uff1b\u540e\u6765\u5f53\u542f\u793a\u8005-7\u4ece\u4e2d\u8131\u79bb\u7684\u65f6\u5019\uff0c\u5979\u4fbf\u6709\u4e86\u81ea\u5df1\u7684\u4eba\u683c\u3002\u590d\u5236\u4e86\u9b4f\u6f47\u73a5\u7684\u8bb0\u5fc6\uff0c\u5c06\u9b4f\u6f47\u73a5\u53eb\u505a\u59d0\u59d0\uff0c\u5c06\u9b4f\u6f47\u627f\u53eb\u505a\u54e5\u54e5\u3002\n\n\u4e8c\u6b21\u8bbe\u5b9a\u4e2d\uff0c\u9b4f\u6f47\u5bc2\u6240\u690d\u5165\u7684\u8fdc\u53e4\u5438\u8840\u9b3c\u4e4b\u8840\u6765\u81ea\u8299\u5170\u6735\u9732\u00b7\u84dd\u84dd\u8def\u00b7\u62b1\u5934\u8e72\u9632\u00b7\u65af\u5361\u96f7\u7279\u3002\n\n\n\u4e3b\u8981\u80fd\u529b\uff1a\n\n\u3010\u72c2\u5316\u3011\n\n\u4e0e\u5176\u8bf4\u662f\u80fd\u529b\uff0c\u5012\u4e0d\u5982\u8bf4\u662f\u4e00\u4e2a\u8d1f\u9762\u7684\u88ab\u52a8\u6548\u679c\u3002\u5728\u53d7\u5230\u72c2\u6c14\u7684\u5f71\u54cd\u4e0b\uff0c\u53ef\u4ee5\u7206\u53d1\u51fa\u60ca\u4eba\u7684\u6740\u4f24\u529b\u548c\u6062\u590d\u529b\uff0c\u4ee3\u4ef7\u662f\u5b58\u5728\u7684\u7834\u635f\u3002\n\n\u3010\u8840\u3001\u7130\u3001\u9b42\u3011\n\n\u8fd9\u4e09\u79cd\u8c61\u5f81\u7740\u751f\u547d\u7684\u5b58\u5728\u5e38\u5e38\u88ab\u76f8\u63d0\u5e76\u8bba\u3002\u800c\u9b4f\u6f47\u5bc2\u7684\u80fd\u529b\u5219\u662f\u53ef\u4ee5\u968f\u610f\u64cd\u63a7\u5b83\u4eec\u3002\u771f\u6b63\u610f\u4e49\u4e0a\u7684\u968f\u610f\uff0c\u751a\u81f3\u53ef\u4ee5\u505c\u6b62\u8840\u6d41\u3001\u5c06\u6574\u4e2a\u4e16\u754c\u9677\u5165\u706b\u6d77\uff0c\u751a\u81f3\u6d88\u706d\u6389\u6240\u6709\u751f\u7075\u7684\u7075\u9b42\uff01\n\n\n\n\n[\u8d75\u84e6\u4e91\u00b7\u672b\u4e16\u5c01\u5370\u7684\u5149\u8292]\n\n\u53c8\u79f0Spirit Of Light\u5e7d\u5149\u7cbe\u9b44\uff08\u8fd9\u5c31\u662f\u4e3a\u4ec0\u4e48\u4f5c\u8005\u7684\u5e38\u7528ID\u53ebSOL\uff09\u3002\u6ca1\u80fd\u963b\u6b62\u542f\u793a\u8005-6\u89e3\u5f00\u201c\u672b\u65e5\u5c01\u5370\u201d\uff0c\u4e16\u754c\u6811\u8fdb\u5165\u5927\u5d29\u574f\u540e\uff0c\u5411\u4e0a\u6765\u5230\u4e86\u9b4f\u6f47\u73a5\u6240\u5728\u7684\u4e16\u754c\u3002\n\n\u548c\u9b4f\u6f47\u627f\u7684\u5173\u7cfb\u975e\u5e38\u5fae\u5999\uff1a\u867d\u7136\u770b\u8d77\u6765\u65e0\u8bba\u4f55\u65f6\u4e24\u4eba\u90fd\u5904\u5728\u4e0d\u5171\u6234\u5929\u7684\u4ec7\u6068\u4e2d\uff0c\u4f46\u662f\u2026\u2026\u6050\u6015\u8fd9\u5c31\u53eb\u505a\u2642\u7231\u5427\uff1f\n\n\n\n\u4e3b\u8981\u80fd\u529b\uff1a\n\n\u3010\u5149\u548f\u5531\u3011\n\n\u4f7f\u7528\u5149\u5728\u7a7a\u4e2d\u6784\u6210\u9b54\u6cd5\u9635\uff0c\u5e76\u7528\u5149\u6ce2\u4ee3\u66ff\u58f0\u6ce2\u8fdb\u884c\u9b54\u6cd5\u548f\u5531\uff1b\u8fd9\u5e94\u8be5\u662f\u4f7f\u7528\u9b54\u6cd5\u7684\u6700\u7b80\u4fbf\u7684\u65b9\u5f0f\u4e86\uff0c\u56e0\u4e3a\u4ed6\u5e76\u6ca1\u6709\u4f7f\u7528\u81ea\u5df1\u7684\u6cd5\u529b\uff0c\u800c\u53ea\u662f\u5f15\u5bfc\u4e86\u7a7a\u6c14\u4e2d\u7684\u9b54\u529b\u5b50\u3002\u552f\u4e00\u7684\u6d88\u8017\u53ea\u662f\u5728\u7a7a\u4e2d\u7ed8\u5236\u9b54\u6cd5\u9635\u6240\u9700\u8981\u7684\u5149\u7f62\u4e86\uff1b\u5374\u53ef\u4ee5\u8f7b\u677e\u91ca\u653e\u5bf9\u57ce\u4ee5\u4e0a\u7684\u9b54\u6cd5\u3002\n\n\u3010\u5149\u5316\u3011\n\n\u5c06\u81ea\u8eab\u5316\u8eab\u4e3a\u5149\u3002\u53ef\u4ee5\u589e\u5f3a\u81ea\u5df1\u7684\u6218\u6597\u529b\uff0c\u6700\u5173\u952e\u7684\u662f\uff0c\u5982\u679c\u4e07\u4e00\u6b7b\u6389\u7684\u8bdd\uff0c\u53ea\u8981\u5c06\u81ea\u5df1\u91cd\u6784\u5c31\u597d\u2014\u2014\u6bd4\u84ec\u83b1\u836f\u66f4\u7b80\u5355\u7c97\u66b4\u7684\u590d\u6d3b\u65b9\u5f0f\uff1f\uff01\uff08\u8bdd\u8bf4\u6700\u7b80\u5355\u7c97\u66b4\u7684\u5e94\u8be5\u662f\u76f4\u63a5\u7528\u4e16\u754c\u6811\u9ed8\u793a\u5f55\u4fee\u6539\u56e0\u679c\u5427\u2026\u2026\uff09\n\n\n\n\n\n[\u516b\u4e91\u7d2b\u00b7\u82e5\u5b9e\u82e5\u865a\u7684\u8001\u8005]\n\n\u4e16\u754c\u6811\u4e2d\u7684\u7b2c\u4e00\u4e2a\u4e16\u754c\u51fa\u73b0\u7684\u65f6\u5019\uff0c\u7d2b\u5a46\u5a46\u6216\u8bb8\u5c31\u5df2\u7ecf\u5b58\u5728\u4e86\u3002\u4ee4\u4eba\u597d\u5947\u7684\u662f\uff0c\u5979\u8ddf\u6bcf\u4e00\u4ee3\u542f\u793a\u8005\u7684\u5173\u7cfb\u90fd\u8fd8\u4e0d\u9519\uff0c\u5728\u770b\u5230\u4e86\u542f\u793a\u8005-6\u6240\u72af\u4e0b\u7684\u9519\u8bef\u540e\u51b3\u5b9a\u5e2e\u52a9\u9b4f\u6f47\u627f\u6bc1\u6389\u4ed6\u3002\u636e\u8bf4\u6709\u5c01\u53f7Spirit Of Exist\u5b58\u5728\u7cbe\u9b44\uff0c\u4f46\u662f\u7531\u4e8e\u4e0d\u592a\u597d\u542c\u6240\u4ee5\u672c\u4eba\u662f\u62d2\u7edd\u7684\u3002\n\n\u5728\u9b4f\u6f47\u627f\u5373\u5c06\u6d88\u5931\u7684\u65f6\u5019\u5c06\u4ed6\u7684\u788e\u7247\u5f15\u5165\u4e86\u5e7b\u60f3\u4e61\u3002\u7b97\u662f\u6551\u547d\u6069\u4eba\u4e86\u5427\u3002\n\n\n\n\u4e3b\u8981\u80fd\u529b\uff1a\n\n\u3010\u64cd\u7eb5\u5883\u754c\u7a0b\u5ea6\u7684\u80fd\u529b\u3011\n\n\u751a\u81f3\u53ef\u4ee5\u5236\u9020\u51fa\u7b2c\u4e8c\u68f5\u4e16\u754c\u6811\u7684\u5f3a\u5927\u80fd\u529b\uff1b\u5927\u90e8\u5206\u7684\u4e16\u754c\u90fd\u7531\u7d2b\u5728\u7ef4\u62a4\u3002\n\n\u3010\u8bbe\u5b9a\u5b58\u5728\u7a0b\u5ea6\u7684\u80fd\u529b\u3011\n\n\u53ef\u4ee5\u4ec5\u51ed\u610f\u5fd7\u63a7\u5236\u6982\u5ff5\u6216\u5b9e\u4f53\u7684\u5b58\u5728\u4ee5\u53ca\u6d88\u5931\u3002\n\n\u4e0d\u8fc7\u57fa\u672c\u4e0a\u6ca1\u600e\u4e48\u4f7f\u7528\u8fc7\u6d88\u5931\u7684\u80fd\u529b\u3002\n\n\n\n\n[\u8299\u5170\u6735\u9732\u00b7\u84dd\u84dd\u8def\u00b7\u62b1\u5934\u8e72\u9632\u00b7\u65af\u5361\u96f7\u7279]\n\n\u5168\u540d\u5b9e\u5728\u592a\u957f\uff0c\u5171\u6709\u4e0a\u767e\u4e2a\u5355\u8bcd\u3002\u8fd8\u662f\u5c31\u8fd9\u6837\u5c31\u53ef\u4ee5\u4e86\u3002\n\n\u638c\u63a7\u7740\u6bc1\u706d\u529b\u91cf\u7684\u5c0f\u5438\u8840\u9b3c\u3002\u6216\u8bb8\u662f\u56e0\u4e3a\u8eab\u4e0a\u7684\u8840\u6db2\u548c\u9b4f\u6f47\u5bc2\u7c7b\u4f3c\uff0c\u6240\u4ee5\u4e24\u4eba\u7279\u522b\u5408\u62cd\uff0c\u751a\u81f3\u8fd8\u80fd\u591f\u4f7f\u7528\u5408\u2640\u4f53\u6280\u80fd\u3002\u5c3d\u7ba1\u8fd9\u4e00\u4e8b\u5b9e\u8ba9\u8eab\u4e3a\u59d0\u59d0\u7684\u857e\u7c73\u8389\u4e9a\u00b7\u84dd\u84dd\u8def\u00b7\u62b1\u5934\u8e72\u9632\u00b7\u65af\u5361\u96f7\u7279\u5f88\u4f24\u5fc3\u3002\n\n\n\n\u4e3b\u8981\u80fd\u529b\uff1a\n\n\u3010\u7834\u574f\u7a0b\u5ea6\u7684\u80fd\u529b\u3011\n\nDuang\u7684\u4e00\u4e0b\uff0c\u4f60\u788e\u4e86\u3002\u5c31\u8fd9\u4e48\u7b80\u5355\u3002\n\n\u5e76\u6ca1\u6709\u8fd9\u4e48\u7b80\u5355\u3002\u76ee\u6807\u8d8a\u5f3a\u5927\uff0c\u5219\u5979\u7834\u574f\u65f6\u6240\u9700\u8981\u6d88\u8017\u7684\u529b\u91cf\u4e5f\u8d8a\u5927\u3002\n\n\u53ef\u80fd\u4f1a\u5927\u5230\u5979\u627f\u53d7\u4e0d\u4f4f\u7684\u5730\u6b65\u3002\n\n\u3010\u770b\u89c1\u201c\u70b9\u201d\u7a0b\u5ea6\u7684\u80fd\u529b\u3011\n\n\u53ef\u4ee5\u770b\u89c1\u6bcf\u4e00\u4e2a\u7269\u4f53\u7684\u70b9\u3002\u751a\u81f3\u4e9a\u539f\u5b50\u7c92\u5b50\u7684\u90fd\u53ef\u4ee5\uff0c\u53ea\u8981\u8ba4\u771f\u53bb\u770b\u3002\n\n\u5982\u679c\u81ea\u5df1\u6ca1\u6709\u8db3\u591f\u7684\u529b\u91cf\u5c06\u76ee\u6807\u7834\u574f\uff0c\u4f1a\u627e\u9b4f\u6f47\u627f\uff0c\u8ba9\u4ed6\u7528\u865a\u6d6e\u7834\u706d\u4e4b\u7bad\u5c04\u6389\u3002\n\n\n\n\n\n[\u96fe\u96e8\u9b54\u7406\u6c99]\n\n\u4e00\u4e2a\u52a9\u4eba\u4e3a\u4e50\uff0c\u591a\u7ba1\u95f2\u4e8b\u7684\u4eba\u7c7b\u3002\n\u559c\u6b22\u5077\uff0c\u6216\u8005\u8bf4\u501f\uff0c\u4e1c\u897f\u4ec0\u4e48\u7684\uff0c\u90fd\u53ea\u662f\u865a\u5047\u7684\u4f20\u95fb\uff08\u53ea\u662f\u81ea\u5df1\u4e0d\u627f\u8ba4\u7f62\u4e86\u5427\uff01\uff09\u3002\n\u64c5\u957f\u4f7f\u7528\u4e94\u989c\u516d\u8272\u7684\u9b54\u6cd5\u7684\u5979\uff0c\u4f3c\u4e4e\u5e76\u6ca1\u6709\u5916\u8868\u4e0a\u770b\u8d77\u6765\u90a3\u4e48\u7b80\u5355\u2026\u2026\n\n\n\n\u4e3b\u8981\u80fd\u529b\uff1a\n\n\u3010\u9b54\u6cd5\u64cd\u7eb5\u3011\n\n\u63a7\u5236\u73af\u5883\u5185\u7684\u9b54\u6cd5\u3002\u80fd\u591f\u963b\u6b62\u4eba\u4eec\u65bd\u6cd5\uff0c\u4e5f\u53ef\u4ee5\u62bd\u7a7a\u9644\u8fd1\u7684\u6240\u6709\u9b54\u529b\u5b50\u3002\n\n\u3010\u540e\u5bab\u738b\u3011\n\n\u80fd\u591f\u52a8\u5458\u4ed6\u4eba\u7a0b\u5ea6\u7684\u80fd\u529b\u3002\u65e0\u8bba\u6027\u522b\u3001\u65e0\u8bba\u8001\u5c11\uff0c\u90fd\u80fd\u591f\u88ab\u5979\u52a8\u5458\u3002\u4f3c\u4e4e\u662f\u975e\u5e38\u65b9\u4fbf\uff0c\u5982\u679c\u4f7f\u7528\u5f97\u5f53\u7684\u8bdd\uff0c\u6210\u4e3a\u5e7b\u60f3\u4e61\u7684\u7edf\u6cbb\u8005\u4e5f\u4e0d\u6210\u95ee\u9898\u3002\n\n\u4e0d\u8fc7\u8fd9\u4f4d\u9ed1\u767d\u7684\u5c0f\u5077\u8c8c\u4f3c\u5e76\u6ca1\u6709\u90a3\u4e2a\u96c4\u5fc3\u58ee\u5fd7\u5427\u3002\n\n\u3010\u9053\u5177\u4f7f\u7528\u3011\n\n\u53ef\u4ee5\u5f04\u6765\u8bb8\u591a\u6765\u5386\u4e0d\u660e\u7684\u9053\u5177\u5e76\u52a0\u4ee5\u4f7f\u7528\u3002\n\n\u3010\u8c1c\u4e4b\u80fd\u529b\u3011\n\n\u4e00\u79cd\u795e\u5947\u7684\u80fd\u529b\uff0c\u5927\u6982\u5c31\u662f\u4e3b\u89d2\u5149\u73af\u90a3\u6837\u5427\u3002\n\n\n\n\n[\u5e7b\u60f3\u4e61\u7684\u5176\u4ed6BBA\u4eec\uff08\u4e00\u83b2\u4e8c\u5e7d\u4e09\u5927\u516b\uff09]\n\n\u5750\u9547\u5e7b\u60f3\u4e61\u7684\u6700\u5f3a\u5b9e\u529b\u7684\u603b\u79f0\u3002\n\n\u5f53\u7136\uff0c\u90a3\u662f\u5728\u9b4f\u6f47\u627f\u6765\u5230\u5e7b\u60f3\u4e61\u4e4b\u524d\u7684\u4e8b\u4e86\u3002\n\n\u5e74\u9f84\u8d8a\u5927\uff0c\u5b9e\u529b\u5c31\u8d8a\u5f3a\uff0c\u4f3c\u4e4e\u662f\u6c38\u6052\u7684\u771f\u7406\u5462\u3002\n\n\n\n\u4e3b\u8981\u80fd\u529b\uff1a\n\n\u3010\u5632\u8bbd\u8138\u7a0b\u5ea6\u7684\u80fd\u529b\u3011\n\n\u4e0d\u77e5\u9053\u4e3a\u4ec0\u4e48\uff0c\u65b0\u6765\u7684\u5bb6\u4f19\u4eec\u603b\u662f\u548c\u8fd9\u4e9bBBA\u4eec\u8fc7\u4e0d\u53bb\uff0c\u9694\u4e09\u5dee\u4e94\u5c31\u8981\u6709\u90a3\u4e48\u4e00\u4f4d\u60e8\u906d\u6bd2\u624b\u3002\u53ea\u6709\u7d2b\u5a46\u5a46\u80fd\u591f\u5e78\u514d\u9047\u96be\uff0c\u6216\u8bb8\u662f\u56e0\u4e3a\u6709\u9b4f\u6f47\u627f\u62a4\u7740\u7684\u7f18\u6545\u5427\u3002\n\n\u3010\u9a91\u6469\u7684\u7a0b\u5ea6\u7684\u80fd\u529b\u3011\n\n\u8fd9\u9879\u4ece\u5723\u767d\u83b2\u53d1\u8d77\u7684\u8fd0\u52a8\u98ce\u9761\u4e8e\u5e7b\u60f3\u4e61\u7684\u5927\u5988\u4eec\u4e2d\u95f4\uff1b\u751a\u81f3\u8fd8\u4e13\u95e8\u5f04\u51fa\u4e86\u73af\u5e7b\u60f3\u4e61\u9ad8\u901f\u8def\uff0c\u4f9b\u5979\u4eec\u548c\u5c0f\u516c\u5a46\u6bd4\u4e00\u6bd4\uff08\u7136\u540e\u88ab\u4e00\u811a\u8e22\u4e0b\u5730\uff1f\uff09\u3002\n\n== \u6e38\u620f\u7cfb\u7edf ==\n\u00b7\u6413\u62db\u7cfb\u7edf\n\u8f83\u4e3a\u5b8c\u5584\u7684\u6413\u62db\u8868\uff0c\u5927\u591a\u6570\u6280\u80fd\u4ee5\u65b9\u5411\u952e\u7684\u5e8f\u5217\u548c\u4e00\u4e2a\u653b\u51fb\u952e\uff08Z\u3001X\u3001C\uff09\u6784\u6210\u3002\n\n\u00b7\u7279\u6b8a\u6280\u80fd\n\u6839\u636e\u9635\u8425\u51b3\u5b9a\u6b64\u6280\u80fd\uff0c\u6bcf\u4e00\u4e2a\u82f1\u96c4\u6bcf\u4e00\u5c40\u6e38\u620f\u53ef\u4ee5\u4f7f\u7528\u4e00\u6b21\u3002\u4e1c\u65b9\u4fa7\u4e3a\u7075\u51fb\uff08\u51fb\u9000\u5e76\u6e05\u5f39\uff09\u3001\u4e16\u754c\u6811\u4fa7\u4e3a\u5929\u542f\uff08\u6682\u65f6\u514d\u75ab\u4e00\u5207\u63a7\u5236\u6548\u679c\uff09\u3002\n\n\u00b7\u683c\u6321\u673a\u5236\n\u5f53\u6309\u4e0bS\u7684\u65f6\u5019\u5c06\u4f1a\u5f00\u542f\u683c\u6321\u6a21\u5f0f\u3002\u7729\u6655\u3001\u65f6\u505c\u72b6\u6001\u4e0b\u4e5f\u53ef\u5f00\u542f\u3002\u5f00\u542f\u540e\u6d88\u8017\u9b54\u6cd5\u62b5\u6d88\u4f24\u5bb3\uff1b\u5982\u679c\u84dd\u91cf\u4e0d\u591f\u5219\u4f1a\u9677\u5165\u957f\u671f\u7684\u7729\u6655\uff08\u6b64\u7729\u6655\u671f\u95f4\u4e0d\u53ef\u5f00\u542f\u683c\u6321\uff09\uff0c\u76f4\u5230\u84dd\u91cf\u6062\u590d\u52301/2\u9b54\u6cd5\u4e0a\u9650\u4e3a\u6b62\u3002\n\n\u00b7\u6d6e\u7a7a\u683c\u6597\n\u82f1\u96c4\u4f1a\u81ea\u52a8\u5730\u79fb\u52a8\u5411\u5c4f\u5e55\u7684\u4e2d\u95f4\u9ad8\u5ea6\u3002\n\n\u00b7\u7b26\u5361\u7cfb\u7edf\n\u4ecd\u5728\u5236\u4f5c\uff0c\u656c\u8bf7\u671f\u5f85\uff01\n\n== \u5386\u53f2\u7248\u672c ==\n[[File:\u5e7b\u60f3\u68a6\u58832 1.JPG|\u7f29\u7565\u56fe|\u622a\u56fe1]]\n\n[[File:\u5e7b\u60f3\u68a6\u58832 2.JPG|\u7f29\u7565\u56fe|\u622a\u56fe2]]\n\n[[File:\u5e7b\u60f3\u68a6\u58832 3.JPG|\u7f29\u7565\u56fe|\u622a\u56fe3]]\n\n\u00b7\u7248\u672c \u6d4b2.0.8 - 2016.03.22\n\n\u52a0\u5165\u4e86BGM\u548c\u97f3\u6548\u7cfb\u7edf\u3002\n\u5927\u91cf\u7684\u5e73\u8861\u6027\u8c03\u6574\u3002\n\u4fee\u6539\u54b2\u591c\u5fc5\u6740\u6548\u679c\u3002\n\u52a0\u5165\u4e24\u4e2a\u65b0\u7684\u6218\u573a\uff08\u6682\u65f6\u4e