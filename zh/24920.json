\u300a[[\u9b54\u6cd5\u4f7f\u4e4b\u591c]]\u300b\uff082012\u5e74\uff09\n\n\u300a[[Fate/Grand Order]]\u300b\u90e8\u5206\u7ae0\u8282\uff082016\u5e74\uff09\n\n=== \u5176\u4ed6 ===\n\u5e7f\u64ad\u5267\u300a\u7a7a\u4e4b\u5883\u754c \u4fef\u77b0\u98ce\u666f\u300b\u811a\u672c\uff082002\u5e74\uff09\n\n\u5e7f\u64ad\u5267\u300aAhnenerbe\u7684\u4e00\u65e5\u300b\u811a\u672c\uff082007\u5e74\uff09\n\n\u52a8\u753b\u300a[[CANAAN]]\u300b\uff082008\u5e74\uff09\n\n\u5e7f\u64ad\u5267\u300a\u30b9\u30bf\u30fc\u30c0\u30b9\u30c8\u30fb\u30aa\u30da\u30ec\u30c3\u30bf\u300b\u811a\u672c\uff082013\u5e74\uff09\n\n\u5e7f\u64ad\u5267\u300aVariety Sound Drama Fate/EXTRA CCC \u30eb\u30ca\u30c6\u30a3\u30c3\u30af\u30b9\u30c6\u30fc\u30b7\u30e7\u30f32013\u300b\u811a\u672c\uff082013\u5e74\uff09\n\n\u52a8\u753b\u300a[[Fate/Zero]]\u300b\u6620\u50cf\u7279\u5178\u300a[[\u62dc\u6258\u4e86\uff01\u7231\u56e0\u5179\u8d1d\u4f26\u54a8\u8be2\u5ba4]]\u300b\u811a\u672c\n\n\u52a8\u753b\u300a[[Fate/EXTRA Last Encore]]\u300b\u7cfb\u5217\u6784\u6210\n\n== \u4e2a\u4eba\u4fe1\u606f ==\n\nTYPE-MOON\u5267\u672c\u62c5\u5f53\u3002\n \n\u7af9\u5e1a\u5c0f\u8bf4\u8d1f\u8d23\u4eba\u3002\n \n\u5e74\u9f84:\u5341\u516b\u5c81\u4ee5\u4e0a\uff0c\u4e09\u5341\u5c81\u672a\u6ee1\u3002\n \n\u6027\u522b:\u96c4\u3002\n \n\u559c\u6b22\u7684\u4e1c\u897f\uff1a\u676f\u9762\uff0c\u5bc6\u5ba4\uff0c\u5c11\u5e74\uff0c\u7ef7\u5e26\uff0c\u5c0f\u5200\u3002\n \n\u8ba8\u538c\u7684\u4e1c\u897f\uff1a\u4eba\u95f4\u7684\u751f\u6d3b\u3002\n \n\u559c\u6b22\u7684\u683c\u8a00\uff1a\u4eba\u7c7b\u7686\u5f3a\u5927\n\n\u64c5\u957f\u7684\u4e8b\uff1a\u505a\u8bbe\u5b9a\uff0c\u5851\u9020\u4eba\u7269\uff0c\u5fc3\u7406\u63cf\u5199\n\n== \u6027\u522b\u4e4b\u4e89 ==\n\n\u66fe\u4ea7\u751f\u4ece\u540d\u5b57\u4ee5\u5f15\u7533\u5948\u987b\u771f\u5b9e\u6027\u522b\u4e4b\u8ba8\u8bba\uff1a\n\n\u8611\u83c7\u7684\u540d\u5b57\u4e3a\u201c\u304d\u306e\u3053\u201d\uff08Kinoko\uff09\uff0c\u5373\u201c\u6728\uff08\u304d\uff09\u4e4b\uff08\u306e\uff09\u5b50\uff08\u3053\uff09\u201d\u7684\u610f\u601d\uff0c\u4ea6\u5373\u83cc\u7c7b\u3002\u65e5\u672c\u4eba\u540d\u5b57\u4ee5\u201c\u3053\uff08\u5b50\uff09\u201d\u7ed3\u5c3e\u7684\u591a\u4e3a\u5973\u6027\u3002\u56e0\u800c\u6709\u4eba\u8ba4\u4e3a\u5948\u987b\u662f\u5973\u6027\u3002\n \n\u5728\u8f83\u65e9\u524d\u7684\u957f\u7bc7\u8bbf\u95ee\u4e2dOKSG\u66f4\u620f\u8a00\u5948\u987b\u662f\u5973\u6027\u3002\u539f\u540d\u5948\u987b\u304d\u306e\u5b50\u3002 \n \n\u4ece\u540e\u6765\u53e6\u4e00\u7bc7\u7684\u8bbf\u95ee\u4e2d\u520a\u767b\u7684\u7167\u7247\uff08\u867d\u7136\u53ea\u6709\u80cc\u9762\uff09\u4e5f\u80fd\u770b\u51fa\u5948\u987b\u4e0d\u592a\u53ef\u80fd\u662f\u5973\u6027\u3002\n \n\u5948\u987b\u5728\u7af9\u5e1a\u7684\u7f51\u9875\u7684PROFILE\u4e2d\u81ea\u79f0\u6027\u522b\u662f\u96c4\u3002\n\n\u57282018\u5e748\u6708\u7684[[\u8fe6\u52d2\u5e95\u5e7f\u64ad\u5c40]]\u4e2d\uff0c[[\u5ddd\u6f84\u7eeb\u5b50]]\u4e00\u65f6\u53e3\u8bef\u8bf4\u51fa\u4e86\u201c{{lj|\u5948\u9808\u306d}}\u201d<ref>[https://www.bilibili.com/video/av31252199 Fate/Grand Order \u8fe6\u52d2\u5e95\u5e7f\u64ad\u5c40 #086] \u53e3\u8bef\u51fa\u73b0\u572813\u520604</ref>\uff0c\u53ef\u4ee5\u88ab\u89e3\u8bfb\u4e3a\u5ddd\u6f84\u7eeb\u5b50\u66fe\u60f3\u8bf4\u7684\u662f\u5948\u987b\u59d0\uff0c\u5948\u987b\u5973\u6027\u8bf4\u6709\u4e86\u65b0\u7684\u8bc1\u636e\u652f\u6301\u3002\n\n== \u4e2a\u4eba\u7ecf\u5386 ==\n\n\u548c\u57fa\u53cb\u6b66\u5185\u5728\u521d\u4e2d\u65f6\u4ee3\u8ba4\u8bc6<del>\u60c5\u5b9a\u7ec8\u8eab</del>\uff0c\u4e00\u4e2a\u753b\u753b\u4e00\u4e2a\u5199\u5c0f\u8bf4\u3002\u8fd9\u4e24\u4e2a\u4eba\u662f\u540c\u5e74\u7ea7\u5b66\u751f\uff0c\u5f53\u65f6\u7684\u8611\u83c7\u5728\u5b66\u751f\u5708\u5b50\u91cc\u5df2\u7ecf\u5c0f\u6709\u540d\u6c14\uff0c\u4e00\u5e2e\u4eba\u8ffd\u7740\u770b\u4ed6\u7684\u5c0f\u8bf4\u3002\u7531\u4e8e\u559c\u6b22\u4e91\u96c6\u4e86\u65e5\u672c\u5251\u8c6a\u5927\u6218\u7684\u6f2b\u753b\u7248\u300a\u9b54\u754c\u8f6c\u751f\u300b\uff0c\u4e8e\u662f\u5c31\u60f3\u5199\u4e00\u90e8\u5c0f\u8bf4\u4e91\u96c6\u53e4\u4eca\u4e1c\u897f\u7684\u82f1\u96c4\u6765\u8c6a\u5feb\u5730\u5927\u6218\u3002\u5723\u676f\uff0c\u9b54\u672f\u5e08\uff0cServant\u7b49\u7b49\u7cfb\u7edf\u5728\u5f53\u65f6\u5c31\u5df2\u7ecf\u5b9a\u4e86\u4e0b\u6765\u3002\u6ca1\u9519\uff0c\u5c31\u662f\u6211\u4eec\u559c\u95fb\u4e50\u89c1\u7684fsn<del>\u7684\u524d\u8eab</del>\u3002\u81f3\u4e8e\u5267\u60c5\u5219\u57fa\u672c\u4e0a\u662fFate\u7ebf\u7684\u5185\u5bb9\uff0c\u4e3b\u89d2\u5219\u662f\u4e2a\u773c\u955c\u59b9\uff0c<del>\u5173\u952e\u662f\u543e\u738b\u662f\u7537\u7684\u554a\u554a\u554a\u554a\u554a</del>\u3002\u56e0\u4e3a\u79cd\u79cd\u539f\u56e0\uff0c\u8611\u83c7\u53ea\u5199\u5230\u521d\u6218\u4f50\u4f50\u6728\u5c0f\u6b21\u90ce\u90a3\u91cc\u5b8c\u7ed3\u5c31\u6ca1\u6709\u518d\u5199\u4e0b\u53bb\u4e86\u3002\u5c31\u8fd9\u4e48\u653e\u7f6e\u4e86\u5341\u51e0\u5e74\u4e4b\u4e45\u3002\n\n\u540e\u6bd5\u4e1a\u4e8e\u6cd5\u653f\u5927\u5b66\u5927\u5b66\u9662\u4eba\u6587\u79d1\u5b66\u7814\u7a76\u79d1\u3002\u4e0e\u6b66\u5185\u518d\u6b21\u76f8\u4f1a\u540e\u51b3\u5b9a\u6210\u7acb\u540c\u4eba\u793e\u56e2\u3002\u4e8e\u662f\u5728\u4e24\u4eba\u7684\u52aa\u529b\u4e0b\uff0cTYPE-MOON\u6b63\u5f0f\u8bde\u751f\u3002\n\n\u867d\u7136\u662f\u4e00\u4e2a\u5f88\u6709\u624d\u534e\u7684\u4eba\uff08\u8fd9\u662f\u771f\u7684\uff09\uff0c\u4f46\u662f\u4ed6\u4e4b\u6240\u4ee5\u4f1a\u6210\u4e3a\u6838\u5fc3\u7edd\u4e0d\u53ea\u662f\u56e0\u4e3a\u4ed6\u7684\u624d\u534e\uff0c\u66f4\u662f\u56e0\u4e3a\u4ed6\u7684\u60c5\u5546\u5f88\u9ad8\uff0c\u5f88\u4f1a\u805a\u4eba\u5fc3\uff0c\u4f7f\u5f97\u5176\u4ed6\u4eba\u90fd\u613f\u610f\u4e3a\u4e86\u4ed6\u800c\u52aa\u529b\u3002\u8fd9\u79cd\u98ce\u6c14\u4e00\u76f4\u5728\u578b\u6708\u5546\u4e1a\u5316\u540e\u8fd8\u4fdd\u7559\u7740\u3002\n\n\u540e\u6765\u5c31\u662f\u4eba\u5c3d\u7686\u77e5\u7684\u6545\u4e8b\u4e86\u3002\u8611\u83c7\u51ed\u501f\u7740\u6027\u8f6c\u4e9a\u745f\u738b\u7684\u5de8\u5927\u9b45\u529b\uff0c\u6210\u529f\u5546\u4e1a\u5316TYPE-MOON\u3002{{\u9ed1\u5e55|\u6210\u4e3a\u4eba\u751f\u8d62\u5bb6\uff0c\u8fc7\u4e0a\u4e86\u5403\u8001\u672c\u7684\u5e78\u798f\u751f\u6d3b\u3002}}\n\n==\u5176\u4ed6==\n\u5728\u300a\u767d\u8272\u6708\u59ec\u300b\uff08[[\u5e7b\u60f3\u5609\u5e74\u534e]]\u7684\u5267\u4e2d\u5267\uff09\u4e2d\uff0c[[\u7231\u5c14\u594e\u7279\u00b7\u5e03\u4f26\u53f2\u5854\u5fb7]]\u8f6c\u804c\u4e3a[[\u9b54\u6cd5\u5c11\u5973|\u9a6c\u7334\u70e7\u9152]]\uff0c\u6d88\u706d\u4e86\u5404\u79cd\u8611\u83c7\u602a\u3002<del>\u53d1\u6cc4\u4e86\u5bf9\u4e8e\u5948\u987b\u8611\u83c7\u4e45\u4e45\u4e0d\u80af\u91cd\u5236[[\u6708\u59ec(\u4f5c\u54c1)|\u6708\u59ec]]\u7684\u6124\u6012\u3002</del>\n\n\u5728\u300a\u98ce\u4e91\u4f0a\u8389\u96c5\u57ce\u300b\uff08[[Fate/hollow ataraxia]]\u4e2d\u7684\u5c0f\u6e38\u620f\uff09\u4e2d\uff0c\u5948\u987b\u8611\u83c7\u7684\u865a\u62df\u5f62\u8c61\u4e5f\u4e00\u5ea6\u767b\u573a\u3002\n\n\u5728\u6f2b\u753b\u300aTYPE-MOON\u5b66\u56ed Chibichuki!\u300b\u4e2d\uff0c\u4e16\u754c\u89c2\u4e3a'''\u8611\u83c7'''\u5927\u9646\u4e1c\u7684\u65b0\u6708\u5c9b\uff1b\u578b\u6708\u5b66\u56ed\u7684\u6821\u957f\u4e3a'''\u8611\u83c7'''\uff0c\u540c\u65f6\u4e5f\u662f\u57fa\u7840\u8d27\u5e01'''\u8611\u83c7\u5e01'''\uff0c\u5982\u679c\u5b66\u5458\u65e0\u6cd5\u81ea\u98df\u5176\u529b\u5c31\u8981\u53d1\u914d\u53bb\u79cd'''\u8611\u83c7'''\u3002\n\n{{\u65e5\u672c\u811a\u672c\u5bb6}}\n{{\u65e5\u672c\u8f7b\u5c0f\u8bf4\u4e1a\u754c}}\n\n== \u6ce8\u91ca\u4e0e\u5916\u90e8\u94fe\u63a5 ==\n[[\u5206\u7c7b:\u4f5c\u5bb6]]"}}}