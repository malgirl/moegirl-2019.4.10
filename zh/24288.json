/>\n{{ruby|{{lj|\u58c7\u4e0a\u767b\u5834\u3001\u9280\u6cb3\u304c\u9b45\u305b\u305f\u30d2\u30fc\u30ed\u30fc\u30b7\u30e7\u30fc}}|\u7c89\u58a8\u767b\u573a \u98ce\u9761\u94f6\u6cb3\u7684Hero show}}\uff0c\uff08\u539f\u4f5c\u4e2d\u5feb\u6597\u4f7f\u7528\u201c\u94f6\u6cb3/\u5149\u5b50\u201d\u4e3b\u9898\u5361\u7ec4\uff09<br/>\n{{ruby|{{lj|\u4eca\u3001\u672a\u6765\u3092\u5909\u3048\u308b\u304b\u3089}}|\u6b64\u523b \u5c31\u6539\u53d8\u672a\u6765\u5427}}\uff0c\uff08\u5feb\u6597\u7684\u5f00\u59cb\u7684\u738b\u724c\u5361\u7247[[\u94f6\u6cb3\u773c\u5149\u5b50\u9f99]]\uff09<br/>\n{{ruby|{{lj|\u5927\u4e08\u592b\u3060\u3001\u5927\u4e08\u592b\u3068\u3000\u541b\u306b\u8a00\u3044\u805e\u304b\u305b\u308b}}|\u201c\u6ca1\u5173\u7cfb\uff0c\u6ca1\u4e8b\u7684\u3002\u201d \u6211\u8fd9\u6837\u5bf9\u4f60\u8bf4\u9053}}\uff0c\uff08\u5929\u57ce\u5144\u5f1f\u7684\u9003\u96be\u56de\u5fc6\uff09<br/>\n{{ruby|{{lj|\u5929\u771f\u721b\u6f2b\u3000\u30ad\u30e3\u30e9\u30e1\u30eb\u982c\u5f35\u308a\u904a\u5712\u5730}}|\u5929\u771f\u70c2\u6f2b \u53cc\u988a\u585e\u6ee1\u5976\u7cd6\u5728\u8fd9\u6e38\u4e50\u56ed}}\uff0c\uff08\u5976\u7cd6\u51fa\u73b0\u5728\u9003\u96be\u7684\u56de\u5fc6\u548cOP2\u7684\u955c\u5934\u95ea\u73b0\u4e2d\uff0c\u5e76\u4e14\u5728OP\u4e2d\u8be5\u955c\u5934\u4e00\u5f00\u59cb\u662f\u6a21\u7cca\u5904\u7406\uff09<br/>\n{{ruby|{{lj|\u5951\u7d04\u66f8\u3092\u7834\u308a\u6368\u3066\uff0c\u4eca\u3001\u5947\u8de1\u3092\u8d77\u3053\u3059\u304b\u3089}}|\u5c06\u5951\u7ea6\u4e66\u6495\u6bc1\uff0c\u6b64\u523b \u5c31\u5f15\u53d1\u5947\u8ff9\u5427}}\u3002\uff08\u5feb\u6597\u7684\u540e\u6765\u7684\u738b\u724c\u5361\u7247[[\u8d85\u94f6\u6cb3\u773c\u5149\u5b50\u9f99]]\uff09\n</poem>\n{{Hide|\u6807\u9898=\u76f8\u5173\u4e3b\u8981CP\uff08\u6b63\u5e38\u5411&\u8150\u5411\uff09\uff08\u614e\uff01\uff09<br/>\u6ca1\u6709\u5148\u540e\u6b21\u5e8f\u4e4b\u5206|\u5185\u5bb9=\n\n\u6b63\u5e38\u5411\uff1a\n\n\u5feb\u6597 X [[\u5fb7\u9c81\u74e6]] / [[\u5fb7\u9c81\u74e6]] X \u5feb\u6597\uff1a\u5355\u76f8\u601d\uff0c\u6ca1\u6709\