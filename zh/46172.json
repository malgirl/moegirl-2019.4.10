027\u53d6\u5411\u4e5f\u968f\u4f60\u5b9a\u3002\u81f3\u4e8e\u4f60\u60f3\u662f\u4e00\u4e2a\u770b\u8c01\u4e0d\u987a\u773c\u5c31\u6740\u3001\u4e00\u5207\u5411\u94b1\u770b\u7684\u9b54\u738b\uff0c\u8fd8\u662f\u60f3\u662f\u4e00\u4e2a\u9ad8\u98ce\u4eae\u8282\u7684\u9053\u5fb7\u6977\u6a21\uff0c\u5f53\u7136\u4e5f\u968f\u4f60\u5b9a\u3002\n\n\u4e0d\u8fc7\u81ea\u7136\u5730\uff0c\u4ed6/\u5979\u8fd8\u662f\u6709\u4e00\u4e9b\u80cc\u666f\u6545\u4e8b\u5c31\u662f\u4e86\u3002\n\n== \u80cc\u666f\u6545\u4e8b ==\n\u5c3d\u7ba1\u4f60\u53ef\u4ee5\u9009\u5948\u7279\u4e5f\u53ef\u4ee5\u9009\u8bfa\u62c9\uff0c\u4e5f\u53ef\u4ee5\u968f\u610f\u53d8\u66f4\u540d\u5b57\uff0c\u4f46\u56fd\u5185\u7684\u4e3b\u6d41\u89c2\u70b9\u8ba4\u4e3a\u5b98\u65b9\u662f\u6bd4\u8f83\u503e\u5411\u4e8e\u5948\u7279\u800c\u4e0d\u662f\u8bfa\u62c9\u3002\n\n\u5948\u7279\u662f\u7f8e\u56fd\u9646\u519b\u7b2c108\u6b65\u5175\u56e2\u7b2c\u4e8c\u8425\u6210\u5458\uff0c\u66fe\u53c2\u4e0e\u5728\u5b89\u514b\u96f7\u5947\u7684\u6218\u5f79\u30022076\u5e74\uff0c\u88ab\u56fd\u5bb6\u89c6\u4e3a\u6218\u4e89\u82f1\u96c4\u7684\u5948\u7279\u5149\u8363\u9000\u5f79\uff0c\u4e0e\u6cd5\u5f8b\u7cfb\u6bd5\u4e1a\u7684\u5f8b\u5e08\u8bfa\u62c9\u7ed3\u5a5a\uff0c\u540c\u5e74\u751f\u4e0b\u513f\u5b50\u5c1a\u6069\u3002\u4ed6\u4eec\u8fd8\u8d2d\u4e70\u4e86\u4e00\u4e2a\u5de7\u624b\u5148\u751f\u201c\u560e\u6293\uff08\u5361\u5179\u6c83\u5c14\uff09\u201d\u66ff\u4ed6\u4eec\u5e72\u5bb6\u52a1\u6d3b\u3002\n\n[[\u6587\u4ef6:Bandicam 2019-01-18 02-34-31-680.png|left|thumb|800px|\u5948\u7279\u548c\u6218\u53cb\u4eec\u5728\u4e00\u8d77]]\n{{clear}}\n\u56e0\u6b64\uff0c\u4e0d\u50cf\u524d\u51e0\u90e8\u300a\u8f90\u5c04\u300b\u4e2d\u7684\u4e3b\u89d2\u8981\u4e48\u662f\u6218\u540e\u907f\u96be\u6240\u5c45\u6c11\u3001\u8981\u4e48\u662f\u5e9f\u571f\u4eba\uff0c\u72ec\u5b58\u8005\u662f\u767e\u5206\u4e4b\u767e\u7684\u6218\u524d\u4eba\u3002\u540c\u65f6\uff0c\u72ec\u5b58\u8005\u4e5f\u662f\u300a\u8f90\u5c04\u300b\u5386\u4ee3\u4e3b\u89d2\u4e2d\u5bff\u547d\u6700\u957f\u7684\u3002\n\n2077\u5e7410\u670823\u65e5\uff0c\u5948\u7279\u5728\u4e3a\u5373\u5c06\u5728\u5eb7\u79d1\u5fb7\u8001\u5175\u7eaa\u5ff5\u9986\u8fdb\u884c\u7684\u6f14\u8bb2\u505a\u5f69\u6392\uff0c\u8fd9\u4e5f\u5c31\u662f\u4f60\u9009\u62e9\u4e3b\u89d2\u6027\u522b\uff0c\u4e5f\u5c31\u662f\u9009\u62e9\u626e\u6f14\u5948\u7279\u8fd8\u662f\u626e\u6f14\u8bfa\u62c9\u7684\u73af\u8282\u3002\n\n\u9009\u62e9\u5b8c\u6bd5\u4e4b\u540e\uff0c\u4e00\u540d\u907f\u96be\u6240\u79d1\u6280\u4ee3\u8868\u627e\u4e0a\u6765\uff0c\u5411\u4f60\u5ba3\u5e03\u4f60\u4eec\u4e00\u5bb6\u4e09\u53e3\u88ab\u8bb8\u53ef\u5165\u4f4f111\u53f7\u907f\u96be\u6240\u3002\u7d27\u8ddf\u7740\uff0c\u6838\u5927\u6218\u7a81\u7136\u7206\u53d1\uff0c\u4f60\u548c\u914d\u5076\u8fd8\u6709\u513f\u5b50\u5c1a\u6069\u7acb\u5373\u524d\u5f80\u907f\u96be\u6240\uff0c\u987a\u5229\u9003\u8fc7\u4e86\u6838\u7206\u7684\u6467\u6b8b\u3002\n\n\u5c3d\u7ba1\u907f\u96be\u6240\u5458\u5de5\u544a\u8bc9\u4f60\u201c\u4f60\u4eec\u9996\u5148\u9700\u8981\u8fdb\u884c\u6d88\u6bd2\u624d\u80fd\u7ee7\u7eed\u5f80\u91cc\u8d70\u201d\uff0c\u7136\u800c\u90a3\u4e2a\u6240\u8c13\u7684\u201c\u6d88\u6bd2\u8231\u201d\u5176\u5b9e\u662f\u51b7\u51bb\u8231\uff0c\u800c111\u53f7\u907f\u96be\u6240\u5efa\u9020\u7684\u76ee\u7684\u5b9e\u9645\u4e0a\u662f\u8bd5\u9a8c\u6d3b\u4f53\u51b7\u51bb\u3002\u5c31\u8fd9\u6837\uff0c\u4f60\u548c\u4f60\u7684\u914d\u5076\u8fd8\u6709\u513f\u5b50\u5c31\u8fd9\u4e48\u6210\u4e3a\u4e86\u4e09\u53ea\u767d\u8001\u9f20\uff0c\u9677\u5165\u4e86\u6f2b\u957f\u7684\u51b7\u51bb\u4f11\u7720\u4e2d\u3002\n\n\u4e00\u6bb5\u65f6\u95f4\u4e4b\u540e\uff0c\u51b7\u51bb\u88ab\u505c\u6389\uff0c\u4f60\u9192\u8fc7\u6765\uff0c\u5374\u770b\u89c1\u51e0\u4e2a\u795e\u79d8\u4eba\u6253\u6b7b\u4e86\u4f60\u7684\u914d\u5076\u3001\u62a2\u8d70\u4e86\u4f60\u7684\u513f\u5b50\u3002\u5176\u4e2d\u5f00\u67aa\u5c04\u6740\u4f60\u914d\u5076\u7684\u90a3\u4e00\u4eba\u51b2\u7740\u4f60\u7529\u4e0b\u4e00\u53e5\u201c\u81f3\u5c11\u6211\u4eec\u8fd8\u6709\u5907\u80ce\u201d\u4e4b\u540e\u4fbf\u79bb\u53bb\uff0c\u91cd\u542f\u4e86\u51b7\u51bb\uff0c\u4f7f\u4f60\u518d\u4e00\u6b21\u8fdb\u5165\u4f11\u7720\u3002\n\n\u5f53\u4f60\u518d\u5ea6\u82cf\u9192\u65f6\uff0c\u907f\u96be\u6240\u91cc\u53ea\u5269\u4e0b\u4f60\u4e00\u4e2a\u6d3b\u4eba\u3002\u4f60\u5411\u7740\u914d\u5076\u7684\u9057\u4f53\u53d1\u8a93\u590d\u4ec7\u5e76\u627e\u56de\u513f\u5b50\uff0c\u62ff\u8d70\u4e86\u4e00\u53f0\u88ab\u9057\u5f03\u4e0d\u77e5\u591a\u4e45\u7684\u54d4\u54d4\u5c0f\u5b50\uff0c\u6253\u5f00\u907f\u96be\u6240\u5927\u95e8\uff0c\u56de\u5f52\u5730\u9762\u3002\n\n\u6b64\u65f6\uff0c\u662f2287\u5e7410\u670823\u65e5\uff0c\u800c\u4f60\uff0c\u662f111\u53f7\u907f\u96be\u6240\u6700\u540e\u4e00\u4e2a\u6d3b\u7740\u7684\u4eba\u3002\u56e0\u6b64\uff0c\u4f60\u88ab\u79f0\u4e3a\u72ec\u5b58\u8005\u2014\u2014111\u53f7\u907f\u96be\u6240\u7684\u552f\u4e00\u5e78\u5b58\u8005\u3002{{\u9ed1\u5e55|\u540c\u65f6\u4e5f\u662f\u51b0\u5c01\u4e86\u4e24\u4e16\u7eaa\u4e4b\u4e45\u7684\u2014\u2014\u8001\u51b0\u68cd\u3002}}\n\n\u6218\u524d\u7684\u7f8e\u597d\u65f6\u5149\u65e9\u5df2\u4e00\u53bb\u4e0d\u8fd4\uff0c\u800c\u4f60\uff0c\u4e3a\u4e86\u5bfb\u56de\u513f\u5b50\u3001\u4e3a\u4e86\u7ed9\u60e8\u906d\u8c0b\u6740\u7684\u53e6\u4e00\u534a\u590d\u4ec7\uff0c\u5f00\u59cb\u4e86\u95ef\u8361\u6ce2\u58eb\u987f\u7684\u5e9f\u571f\u751f\u6d3b\u3002{{\u9ed1\u5e55|<s>\u9a97\u8c01\u5462\u4f60\uff01\u53bb\u4ed6\u5a18\u7684\u513f\u5b50\uff0c\u6361\u5783\u573e\u624d\u662f\u8001\u51b0\u68cd\u4f60\u771f\u6b63\u7684\u7cbe\u795e\u652f\u67f1\uff01</s>}}\n\n== \u7ec6\u8282 & \u8f76\u4e8b ==\n*\u901a\u8fc7\u83b7\u53d6\u6447\u5934\u5a03\u5a03\uff0c\u4f60\u53ef\u4ee5\u8ba9\u72ec\u5b58\u8005\u5df2\u5347\u6ee1\u7ea7\u7684\u4e03\u5927\u57fa\u672c\u53c2\u6570\u5373S.P.E.C.I.A.L.\u4e2d\u5bf9\u5e94\u7684\u90a3\u4e00\u9879\u5f3a\u884c\u518d\u5347\u4e00\u7ea7\uff0c\u8fd9\u4e2a\u5347\u7ea7\u662f\u6c38\u4e45\u7684\uff0c\u4e14\u4e0d\u4f1a\u5f15\u8d77BUG\u3002\uff08\u867d\u7136\u836f\u7269\u548c\u300a{{ruby|\u4f60\u6700special\uff01|\u4f60\u5f88\u7279\u522b\uff01}}\u300b\uff08\u540e\u9762\u4f1a\u63d0\u53ca\uff09\u4e5f\u53ef\u4ee5\u5347\u7ea7\uff0c\u4f46\u836f\u7269\u7684\u4f5c\u7528\u662f\u6682\u65f6\u7684\uff0c\u540e\u8005\u5219\u53ea\u80fd\u5347\u7ea7\u672a\u5347\u6ee1\u7684\u9879\u76ee\uff09\n*\u5c3d\u7ba1\u5b98\u65b9\u7ed9\u4e3b\u89d2\u7684\u79f0\u53f7\u662f\u201c{{ruby|\u72ec\u5b58\u8005|Sole Survivor}}\u201d\uff0c\u4f46\u6e38\u620f\u4e2d\u666e\u904d\u51fa\u73b0\u5bf9\u4f60\u7684\u79f0\u547c\u53ea\u6709\u4e00\u4e2a\uff0c\u5373\u201c{{ruby|\u90a3\u4e2a\u907f\u96be\u6240\u5c45\u6c11|That The Vault Dweller}}\u201d\u3002\n*\u72ec\u5b58\u8005\u662f\u300a\u8f90\u5c04\u300b\u7cfb\u5217\u4e2d\u9996\u4e2a\u5b8c\u5168\u914d\u97f3\u7684\u4e3b\u89d2\u3002\n*\u4e0d\u59a8\u62ac\u4e2a\u6760\uff0c\u5176\u5b9e\u201c\u72ec\u5b58\u8005\u201d\u4e4b\u540d\u6839\u672c\u5c31\u662f\u540d\u4e0d\u526f\u5b9e\uff0c\u56e0\u4e3a111\u53f7\u907f\u96be\u6240\u6839\u672c\u4e0d\u662f\u53ea\u6709\u4e3b\u89d2\u4e00\u4eba\u5e78\u5b58\uff0c\u4f60\u628a\u540c\u6837\u6d3b\u4e0b\u6765\u4e86\u7684[[\u5723\u7236|\u4e3b\u89d2\u7684\u513f\u5b50]]\u4e22\u5230\u4e5d\u9704\u4e91\u5916\u53bb\u4e86\u662f\u5427\u2192_\u2192\n*\u5f00\u5c40\u7684\u6218\u524d\u5f71\u50cf\u4e2d\uff0c\u5948\u7279\u7a7f\u7740\u58eb\u5175\u670d\u5916\u5957\u6574\u5957\u6218\u6597\u88c5\u7532\uff08\u7591\u4f3c\u8f7b\u578b\u6216\u575a\u5b9e\u80f8\u7532\u3001\u91cd\u578b\u81c2\u94e0\u3001\u8f7b\u578b\u817f\u7532\uff09\uff0c\u62ff\u4e00\u628a\u957f\u7ba1\u6563\u5c04\u5934AER-9\u6fc0\u5149\u6b65\u67aa\u3002\n*\u7531\u4e8e\u201cSole Survivor\u201d\u53d1\u97f3\u592a\u590d\u6742\uff0c\u6ca1\u529e\u6cd5\u5f04\u51fa\u7c7b\u4f3c\u90b1\u751f\u65fa\u3001\u9f99\u4e07\u5fb7\u3001\u90ae\u5dee\u8fd9\u6837\u7684\u8bd9\u8c10\u97f3\u8bd1\uff0c\u56e0\u6b64\u56fd\u4eba\u53d6\u51b0\u5c01210\u5e74\u8fd9\u4e2a\u7ecf\u5386\u53d6\u4e86\u4e00\u4e2a\u540c\u6837\u8bd9\u8c10\u7684\u7ef0\u53f7\u201c\u8001\u51b0\u68cd\u201d\u3002\u5982\u679c\u4f60\u9009\u7537\u7684\uff0c\u5c31\u662f\u7537\u51b0\u68cd\uff1b\u800c\u5973\u7684\u81ea\u7136\u5c31\u662f\u5973\u51b0\u68cd\u4e86\u3002\u5f53\u7136\uff0c\u5982\u679c\u4f60\u4e0d\u559c\u6b22\u201c\u5973\u51b0\u68cd\u201d\u8fd9\u4e2a\u8bcd\uff0c\u4e5f\u53ef\u4ee5\u53eb\u5979\u201c\u51b0\u6dc7\u6dcb\u201d\u3002\n*\u7537\u51b0\u68cd\u96b6\u5c5e\u7f8e\u56fd\u9646\u519b\u7b2c108\u6b65\u5175\u56e2\u7b2c\u4e8c\u8425\uff0c\u5973\u51b0\u68cd\u662f\u5f8b\u5e08\u2014\u2014\u53ef\u5982\u4f55\u5f97\u77e5\u4f60\u7684\u8eab\u4efd\uff1f\u5f88\u7b80\u5355\uff0c\u5c31\u5728\u5baa\u6cd5\u53f7\u90a3\u91cc\u3002\u5728\u5baa\u6cd5\u53f7\u4e0b\u8fb9\u6709\u4e00\u4e2a\u7ad9\u54e8\u7684\u5de7\u624b\u5148\u751f\uff0c\u4f60\u9996\u6b21\u8fc7\u53bb\u7684\u65f6\u5019\u4ed6\u5c31\u4f1a\u62a5\u51fa\u4f60\u7684\u8eab\u4efd\u3002\n*\u6838\u5f39\u843d\u4e0b\u4e4b\u524d\uff0c\u5973\u51b0\u68cd\u7684\u6bd5\u4e1a\u8bc1\u4e66\u5c31\u6446\u5728\u5bb6\u95e8\u53f3\u624b\u7684\u8fb9\u67b6\u5b50\u4e0a\u3002\n*\u9b45\u529b\u503c\u8d85\u8fc710\u70b9\u7684\u8001\u51b0\u68cd\u62e5\u6709\u8fd1\u4e4e\u767e\u5206\u4e4b\u767e\u7684\u5634\u70ae\u6210\u529f\u7387\u3002\n*\u5982\u679c\u8001\u51b0\u68cd\u559d\u9ad8\u4e86\uff0c\u5728\u8bf4\u8bdd\u65f6\u4f1a\u5410\u5b57\u4e0d\u6e05\u3002\n*\u6309\u7406\u8bf4VATS\u53ea\u6709\u62ff\u8d77\u54d4\u54d4\u5c0f\u5b50\u4e4b\u540e\u624d\u80fd\u4f7f\u7528\uff0c\u5927\u6982\u662f\u5b98\u65b9\u5077\u61d2\u4e86\uff0c\u5b9e\u9645\u4e0a\u5728\u4f60\u62ff\u8d77\u54d4\u54d4\u5c0f\u5b50\u4e4b\u524d\u4e5f\u53ef\u4ee5\u4f7f\u7528VATS\u3002{{\u9ed1\u5e55|\u6b63\u56e0\u4e3a\u5982\u6b64\u751a\u81f3\u6709\u8001\u5916\u53d1\u8868\u957f\u7bc7\u5927\u8bba\u8bd5\u56fe\u8bc1\u660e\u8001\u51b0\u68cd\u662f\u4e2a\u5408\u6210\u4eba}}\n*\u513f\u5b50\u5c1a\u6069\uff08\u4ee5\u53ca\u5728\u5b66\u9662\u91cc\u9047\u5230\u7684\u5408\u6210\u4eba\u5c1a\u6069\uff09\u7684\u5f62\u8c61\u4f1a\u6839\u636e\u4f60\u5f00\u5c40\u65f6\u7ed9\u5948\u7279/\u8bfa\u62c9\u8bbe\u5b9a\u7684\u5f62\u8c61\u6765\u51b3\u5b9a\u3002\n*\u6838\u5f39\u843d\u4e0b\u4e4b\u524d\uff0c\u8001\u51b0\u68cd\u5bb6\u91cc\u6709\u4e00\u53ea\u72d7\u7cae\u76c6\uff0c\u70b9\u51fb\u5b83\u8fd8\u4f1a\u8bf4\u201c\u518d\u4e5f\u627e\u4e0d\u5230\u90a3\u53ea\u72d7\u4e86\u201d\u3002\u53ef\u89c1\u8001\u51b0\u68cd\u6218\u524d\u6709\u517b\u72d7\u7684\u8bb0\u5f55\u3002\n*\u8001\u51b0\u68cd\u7684\u6545\u5c45\u91cc\u4e0d\u4f46\u6709\u4e00\u672c\u300a{{ruby|\u86ee\u4eba\u53e4\u53e4\u90a3|\u91ce\u86ee\u4eba\u67ef\u5357}}\u300b\uff0c\u8fd8\u6709\u4e00\u672c\u300a{{ruby|\u4f60\u6700special\uff01|\u4f60\u5f88\u7279\u522b\uff01}}\u300b\u3002\n*\u300a\u94f6\u8863\u602a\u5ba2\u300b\u662f\u8001\u51b0\u68cd\u4e00\u5bb6\u4eba\u6218\u524d\u559c\u6b22\u8ffd\u7684\u7535\u53f0\u8282\u76ee\u3002\n*\u4f60\u5e72\u8fc7\u7684\u5927\u4e8b\uff08\u6bd4\u5982\u5e26\u9886[[\u8054\u90a6\u4e49\u52c7\u519b]]\u6536\u590d\u57ce\u5821\u3001\u6216\u8005\u5728\u845b\u6797\u79d1\u6280\u8ffd\u6740\u8ffd\u730e\u8005\uff09\uff0c\u94bb\u77f3\u57ce\u7535\u53f0\u90fd\u4f1a\u64ad\u62a5\u3002\u5f53\u7136\uff0cDLC\u9644\u52a0\u7684\u4e0d\u4f1a\u64ad\u62a5{{\u9ed1\u5e55|\u5b98\u65b9\u5077\u61d2\u8bc1\u636e+1}}\u3002\u505a\u5b8c\u4efb\u52a1\u201c\u7537\u4eba\u7684\u81ea\u4fe1\u201d\u540e\uff0c\u5f97\u5230\u4f60\u5e2e\u52a9\u7684\u7535\u53f0DJ\u5d14\u7ef4\u65af\u00b7\u8fc8\u5c14\u65af\u4e5f\u4f1a\u5728\u5e7f\u64ad\u4e2d\u611f\u8c22\u4f60\u3002\n*\u5b98\u65b9\u780d\u6389\u4e86\u94c1\u8def\u628a\u8001\u51b0\u68cd\u7684\u914d\u5076\u590d\u6d3b\u3001\u6216\u4ee5\u5408\u6210\u4eba\u7684\u5f62\u5f0f\u518d\u73b0\u7684\u5267\u60c5\u3002\n*\u5173\u4e8e\u8001\u51b0\u68cd\u548c\u5144\u5f1f\u4f1a\uff0c\u5b98\u65b9\u780d\u6389\u4e86\u8ba9\u8001\u51b0\u68cd\u62c5\u4efb\u957f\u8001\u7684\u5267\u60c5\u3002\u5177\u4f53\u6765\u8bf4\uff0c\u539f\u672c\u5728\u201c\u76f2\u76ee\u7684\u80cc\u53db\u201d\u4efb\u52a1\u4e2d\uff0c\u6e38\u620f\u91cc\u505a\u51fa\u6765\u7684\u53ea\u6709\u5904\u6b7b\u4e39\u65af\u548c\u5634\u70ae\u9ea6\u514b\u68ee\u957f\u8001\u653e\u4e39\u65af\u4e00\u9a6c\u7684\u9009\u9879\uff1b\u5b9e\u9645\u4e0a\u8fd8\u6709\u7b2c\u4e09\u79cd\u5373\u88ab\u780d\u6389\u7684\u9009\u9879\uff0c\u5373\u8001\u51b0\u68cd\u548c\u4e39\u65af\u8054\u624b\u6218\u80dc\u9ea6\u514b\u68ee\uff0c\u4e4b\u540e\u4e39\u65af\u6216\u8001\u51b0\u68cd\u6765\u63a5\u9ea6\u514b\u68ee\u7684\u73ed\u3002\u8be5\u5267\u60c5\u5728\u56fd\u5916\u5df2\u6709\u7231\u597d\u8005\u7528mod\u300a\u4e39\u65af\u7684\u56f0\u5883\u300b\u8fd8\u539f\u3002\n*\u4ecd\u7136\u662f\u8001\u51b0\u68cd\u548c\u5144\u5f1f\u4f1a\uff0c\u5267\u60c5\u91cc\u662f\u8001\u51b0\u68cd\u6740\u4e86\u514b\u7f57\u683c\u4e4b\u540e\uff0c\u79bb\u5f00\u9ed1\u6839\u5821\u65f6\u5144\u5f1f\u4f1a\u7684\u5927\u98de\u8247\u666e\u5229\u5fb7\u6e29\u53f7\u62b5\u8fbe\u6ce2\u58eb\u987f\u3002\u4e8b\u5b9e\u4e0a\u98de\u8247\u5149\u4e34\u7684\u4e8b\u4ef6\u662f\u5728\u4f60\u6253\u5f00\u9ed1\u6839\u5821\u5929\u53f0\u4e0a\u7531\u7ec8\u7aef\u673a\u63a7\u5236\u7684\u5b89\u5168\u95e8\u7684\u52a8\u4f5c\u89e6\u53d1\u7684\uff0c\u56e0\u6b64\u4f60\u53ef\u4ee5\u7528\u8457\u540d\u7684\u201c\u52a8\u529b\u7532\u7a7f\u5899\u6cd5\u201d\u5728\u4e00\u5f00\u5c40\u5c31\u8dd1\u53bb\u9ed1\u6839\u5821\uff0c\u7a7f\u5899\u5230\u5b89\u5168\u95e8\u540e\u9762\u518d\u7528\u7ec8\u7aef\u673a\u6253\u5f00\uff0c\u7531\u6b64\u5f3a\u884c\u89e6\u53d1\u98de\u8247\u5149\u4e34\u7684\u4e8b\u4ef6\u3002\n*\u8bbe\u5b9a\u4e0a\uff0c\u5f97\u76ca\u4e8e\u6218\u524d\u7684\u519b\u4e8b\u8bad\u7ec3\uff0c\u8001\u51b0\u68cd\u662f\u5386\u5c4a\u300a\u8f90\u5c04\u300b\u4e3b\u89d2\u4e2d\u552f\u4e00\u7684\u4e00\u4e0a\u6765\u5c31\u80fd\u5f00\u52a8\u529b\u88c5\u7532\u7684\u3002{{\u9ed1\u5e55|\u90a3\u600e\u4e48\u8bf4\u4e5f\u5e94\u8be5\u662f\u7537\u51b0\u68cd\u7684\u4e13\u4eab\u7279\u6743\u554a\uff0c\u5973\u51b0\u68cd\u600e\u4e48\u4e5f\u80fd\u4e00\u4e0a\u6765\u5c31\u5f00\u5f97\u52a8\u52a8\u529b\u88c5\u7532\u7684\u554a(\u256f\u2035\u25a1\u2032)\u256f\ufe35\u253b\u2501\u253b}}\n*\u6839\u636e\u548c\u6770\u514b\u00b7\u51ef\u4f2f\u7684\u8c08\u8bdd\u5224\u65ad\uff0c\u8001\u51b0\u68cd\u6700\u6709\u53ef\u80fd\u662f\u51fa\u751f\u5728\u4e8c\u5341\u4e00\u4e16\u7eaa\u56db\u4e94\u5341\u5e74\u4ee3\u3002\n*\u5982\u679c\u4f60\u548c115\u53f7\u5144\u5f1f\u54e8\u7ad9\u4e2d\u8bb2\u53f0\u65c1\u7684\u8bdd\u7b52\u4e92\u52a8\uff0c\u8001\u51b0\u68cd\u4f1a\u8bf4\u51fa\u90a3\u53e5\u540d\u8a00\u201c\u6218\u4e89\uff0c\u6218\u4e89\u6c38\u8fdc\u4e0d\u53d8\u201d\u3002\u4e14\u5728\u6b64\u5730\u7684\u4e00\u53f0\u7ec8\u7aef\u673a\u4e0a\u53ef\u4ee5\u67e5\u5230\uff0c\u6218\u524d\u8fd9\u91cc\u66fe\u9080\u8bf7\u5948\u7279\u6765\u6b64\u6f14\u8bb2\uff08\u4f3c\u4e4e\u5c31\u662f\u7206\u70b8\u5f53\u5929\u51c6\u5907\u53c2\u52a0\u7684\u90a3\u4e2a\u805a\u4f1a\uff09\u3002\n*\u8001\u51b0\u68cd\u65e0\u6cd5\u548c\u673a\u5668\u4eba\u961f\u53cb\u3001\u72d7\u8089\u3001[[\u58ee\u58ee|\u7eff\u5de8\u4eba]]\u548c\u5c3c\u514b\u00b7\u74e6\u4f26\u5766\u5efa\u7acb\u604b\u7231\u5173\u7cfb\u3002\u4e5f\u4e0d\u80fd\u548c\u4e24\u4e2a\u4e27\u5076\u7684\u4eba\u604b\u7231\uff08\u6559\u58eb\u548c\u8001\u6e14\u7fc1\uff09\u3002\n*\u9664\u4e86\u604b\u7231\u7684\u961f\u53cb\u53ef\u4ee5\u7761\uff0c\u8001\u51b0\u68cd\u4e5f\u53ef\u4ee5\u901a\u8fc7\u5634\u70ae\u6765\u7761\u6728\u5170\u82b1{{\u9ed1\u5e55|\u548c118\u53f7\u907f\u96be\u6240\u91cc\u7684\u67d0\u4e2a\u8111\u63a7\u673a\u5668\u4eba}}\u3002\u9700\u8981\u6ce8\u610f\u7684\u662f\u6709\u4e9b\u961f\u53cb\u4e0d\u559c\u6b22\u8fd9\u4e48\u505a\u3002\n*\u5728\u7b2c\u4e09\u94c1\u8f68\u9152\u5427\u9996\u6b21\u548c\u6728\u5170\u82b1\u4ea4\u8c08\u65f6\uff0c\u6728\u5170\u82b1\u4f1a\u6839\u636e\u4f60\u5f53\u524d\u7684S.P.E.C.I.A.L.\u4e2d\u70b9\u6570\u6700\u9ad8\u7684\u4e00\u9879\u505a\u8bc4\u8bae\u3002\u5982\u679c\u4f60\u7684\u591a\u9879\u6700\u9ad8S.P.E.C.I.A.L.\u7b49\u503c\u751a\u81f3\u662f\u5168\u90e8\u7b49\u503c\u5462\uff1f\u5219\u4f1a\u968f\u673a\u9009\u53d6\u4e00\u9879\u3002\n*\u5f00\u5c40\u90a3\u4e2a\u7ed9\u4f60\u5206\u914dS.P.E.C.I.A.L.\u70b9\u6570\u548c\u5b9a\u540d\u5b57\u7684\u907f\u96be\u6240\u63a8\u9500\u5458\u867d\u7136\u88ab\u907f\u96be\u6240\u62d2\u4e4b\u95e8\u5916\uff0c\u4f46\u4f60\u8fd8\u662f\u53ef\u4ee5\u5728\u82b3\u90bb\u9547\u7684\u745e\u798f\u5927\u996d\u5e97\u627e\u5230\u53d8\u6210\u5c38\u9b3c\u7684\u4ed6\uff0c\u5e76\u9080\u8bf7\u4ed6\u56de\u53bb\u5e87\u62a4\u5c71\u4e18\u4ee5\u53ca\u7ed9\u4ed6\u4e00\u4efd\u5de5\u4f5c\u3002\n*\u503c\u5f97\u4e00\u63d0\u7684\u662f\uff0c\u5982\u679c\u4f60\u62d2\u7edd\u4e86\u90a3\u4e2a\u63a8\u9500\u5458\uff0c\u90a3\u4e48\u4f60\u7684\u53e6\u4e00\u534a\u4f1a\u5f3a\u8feb\u4f60\u540c\u610f\u3002\u5982\u679c\u4f60\u5728\u6838\u5f39\u5feb\u843d\u4e0b\u6765\u65f6\u4e0d\u53bb\u907f\u96be\u6240\uff0c\u90a3\u4e48\u4f60\u4f1a\u6b7b\u4e8e\u6838\u7206{{\u9ed1\u5e55|\u6700\u5feb\u901a\u5173\u6210\u5c31get}}\u3002\n*\u540c\u6837\u662f\u90a3\u4e2a\u63a8\u9500\u5458\uff0c\u6709\u4e0d\u9760\u8c31\u4f46\u6d41\u4f20\u5e7f\u6cdb\u7684\u6d41\u8a00\u79f0\u5f53\u6838\u5f39\u5373\u5c06\u843d\u4e0b\u65f6\u3001\u4ed6\u8981\u6c42\u8fdb\u5165\u907f\u96be\u6240\u65f6\u88ab\u53c2\u4e0e\u9547\u66b4\u884c\u52a8\u7684\u58eb\u5175\u8f70\u8d70\u4e4b\u540e\uff0c\u5728\u4f17\u76ee\u777d\u777d\u4e4b\u4e0b\uff0c\u9876\u7740\u5632\u7b11\u6316\u4e86\u4e00\u4e2a\u53ef\u5bb9\u7eb3\u4e00\u4eba\u7684\u5751\uff0c\u7136\u540e\u8df3\u8fdb\u5751\u91cc\uff0c\u4ece\u800c\u6210\u529f\u5730\u5728\u6838\u7206\u4e4b\u65f6\u9003\u51fa\u751f\u5929\u3002\u5f53\u7136\uff0c\u5207\u8bb0\u8fd9\u662f\u4e2a\u65e0\u6cd5\u9a8c\u8bc1\u7684\u6d41\u8a00\u3002\n*\u5982\u679c\u8001\u51b0\u68cd\u7a7f\u7740\u94f6\u8863\u602a\u5ba2\u670d\u88c5\u9762\u5bf9[[\u4f0a\u838e\u8d1d\u5c14\u00b7\u514b\u9c81\u5179|\u673a\u68b0\u5927\u5e08]]\uff0c\u5219\u4f1a\u51fa\u73b0\u201c\u4ee5\u94f6\u8863\u602a\u5ba2\u8eab\u4efd\u8bf4\u8bdd\u201d\u7684\u9009\u9879\uff0c\u56e0\u4e3a\u94f6\u8863\u602a\u5ba2\u548c\u673a\u68b0\u5927\u5e08\u90fd\u662f\u540c\u4e00\u7cfb\u5217\u4f5c\u54c1\u4e2d\u767b\u573a\u7684\u89d2\u8272\uff0c\u4e14\u4e92\u4e3a\u654c\u5bf9\u5173\u7cfb\u3002\n*\u56fd\u4eba\u6000\u7591\uff0c\u300a\u8f90\u5c043\u300bDLC\u300a\u5b89\u514b\u96f7\u5947\u884c\u52a8\u300b\u4e2d\u6240\u626e\u6f14\u7684\u90a3\u4e2a\u7f8e\u519b\u58eb\u5175\u5c31\u662f\u5948\u7279\u672c\u5c0a\u3002\n*\u5728\u4e3b\u7ebf\u5b8c\u7ed3\u65f6\u64ad\u51fa\u7684\u5f71\u7247\u7684\u7ed3\u5c3e\u5904\uff0c\u8001\u51b0\u68cd\u5e26\u7740\u72d7\u8089\u8d70\u8fdb\u4e86\u53d1\u5149\u6d77\u3002\n[[\u6587\u4ef6:20190310142027 1.jpg|centre|thumb|1000px|\u8001\u51b0\u68cd\u548c\u72d7\u8089\u5411\u53d1\u5149\u6d77\u8d70\u53bb]]\n\n== \u540d\u8a00\u91d1\u53e5 ==\n*\u6218\u4e89\uff0c\u6218\u4e89\u6c38\u8fdc\u4e0d\u53d8\u3002<br/>1945\u5e74\uff0c\u6211\u7684\u66fe\u66fe\u66fe\u7956\u7236\uff0c\u4e3a\u56fd\u51fa\u5f81\uff0c\u4e0d\u77e5\u4f55\u65f6\u80fd\u591f\u56de\u5bb6\u966a\u4f34\u4ed6\u7684\u59bb\u5b50\u3001\u770b\u770b\u4ed6\u672a\u89c1\u8fc7\u7684\u513f\u5b50\u3002<br/>\u540e\u6765\u4ed6\u5982\u613f\u4ee5\u507f\uff0c\u56e0\u4e3a\u7f8e\u56fd\u5728\u5e7f\u5c9b\u3001\u957f\u5d0e\u6295\u4e0b\u4e86\u4e24\u679a\u539f\u5b50\u5f39\uff0c\u7ec8\u7ed3\u4e86\u7b2c\u4e8c\u6b21\u4e16\u754c\u5927\u6218\u3002<br/>\u90a3\u65f6\u5019\u5168\u4e16\u754c\u90fd\u7b49\u5f85\u7740\u672b\u65e5\u964d\u4e34\u3002<br/>\u4f46\u662f\uff0c\u5947\u8ff9\u53d1\u751f\u4e86\uff1a\u6211\u4eec\u4e0d\u518d\u5c06\u6838\u80fd\u5f53\u4f5c\u6b66\u5668\uff0c\u53cd\u800c\u5f53\u6210\u7528\u4e4b\u4e0d\u7aed\u7684\u80fd\u6e90\u3002<br/>\u90a3\u4e9b\u66fe\u4ee5\u4e3a\u53ea\u4f1a\u51fa\u73b0\u5728\u79d1\u5e7b\u6545\u4e8b\u4e2d\u7684\u5962\u4f88\u54c1\uff0c\u5168\u4e3a\u4eba\u4eec\u6240\u4eab\u7528\u2014\u2014\u5bb6\u7528\u673a\u5668\u4eba\u3001\u6838\u80fd\u8f66\u3001\u4fbf\u643a\u5f0f\u7535\u8111\u3002<br/>\u7136\u800c\uff0c\u5230\u4e86\u4e8c\u5341\u4e00\u4e16\u7eaa\uff0c\u4eba\u4eec\u4ece\u7f8e\u56fd\u68a6\u4e2d\u60ca\u9192\u8fc7\u6765\u3002<br/>\u5e38\u5e74\u7684\u6ee5\u7528\u5bfc\u81f4\u6240\u6709\u91cd\u5927\u8d44\u6e90\u77ed\u7f3a\u3002\u5168\u7403\u5f00\u59cb\u5206\u5d29\u79bb\u6790\u3002<br/>\u548c\u5e73\u6210\u4e86\u9065\u8fdc\u7684\u56de\u5fc6\u3002<br/>\u73b0\u5728\u662f2077\u5e74\u3002\u6218\u4e89\u5373\u5c06\u5168\u9762\u7206\u53d1\u3002<br/>\u6211\u5f88\u5bb3\u6015\u3002\u4e3a\u81ea\u5df1\u3001\u4e3a\u59bb\u5b50\u3001\u4e3a\u8941\u8913\u4e2d\u7684\u513f\u5b50\u5bb3\u6015\u3002<br/>\u56e0\u4e3a\u5982\u679c\u6211\u7684\u519b\u65c5\u751f\u6daf\u6559\u4f1a\u6211\u4e00\u4ef6\u4e8b\u2026\u2026\u90a3\u5c31\u662f\u6218\u4e89\uff0c\u6218\u4e89\u6c38\u8fdc\u4e0d\u53d8\u3002\u2014\u2014\u5f00\u5c40\n*\u8fd8\u771f\u662f\u7b49\u4e0d\u53ca\u4e16\u754c\u672b\u65e5\u6765\u4e34\u5462\uff01\u2014\u2014\u6838\u5f39\u843d\u4e0b\u524d\u4e0d\u4e45\uff0c\u5bf9\u907f\u96be\u6240\u79d1\u6280\u4ee3\u8868{{\u9ed1\u5e55|\u7136\u540e\u4e16\u754c\u672b\u65e5\u679c\u7136\u5c31\u6765\u4e86}}\n*\uff08\u7537\uff09\u5c31\u50cf\u4e00\u5e74\u524d\u5728\u516c\u56ed\u5ea6\u8fc7\u7684\u90a3\u4e2a\u591c\u665a\u5417\uff1f/\uff08\u5973\uff09\u5662\uff0c\u597d\u554a\u3002\u5230\u516c\u56ed\u3002\u8ddf\u4f60\u53bb\u3002\u56e0\u4e3a\u6211\u8fd8\u60f3\u518d\u88ab\u4f60\u641e\u5927\u809a\u5b50\u3002\u2014\u2014\u540c\u4e0a\uff0c\u5bf9\u914d\u5076\uff08\u6697\u793a\u4e86\u513f\u5b50\u5c1a\u6069\u662f\u592b\u59bb\u4e8c\u4eba\u91ce\u6218\u7684\u6210\u679c\uff09\n*\u6211\u6700\u559c\u6b22\u7684\u7403\u573a\u53d8\u6210\u5783\u573e\u573a\u4e86\uff0c\u6240\u4ee5\uff0c\u4eca\u5929\u771f\u4e0d\u9519\u3002/\u5c31\u53ea\u662f\u6211\u548c\u4e00\u5343\u4e2a\u5b9e\u9a8c\u767d\u8001\u9f20\u3002\u5b83\u4eec\u53d8\u6210\u2026\u2026\u8089\u98df\u6027\u7684\u3002/\u770b\u5427\uff0c\u6211\u662f\u957f\u751f\u4e0d\u8001\u7684\uff01\u2014\u2014\u201c\u4e16\u7eaa\u5927\u62a5\u9053\u201d\u4efb\u52a1\uff0c\u5bf9[[\u6d3e\u666e\u00b7\u83b1\u7279]]\n*\u591a~\u559d~\u70ed~\u6c34~\u2014\u2014\u5728\u94bb\u77f3\u57ce\u5632\u8bbd\u8c22\u83f2\u5c14\u5fb7\n*\u8fd9\u662f\u4e0d\u662f\u540e\u9762\u6709\u6697\u5ba4\u548c\u4e00\u7968\u5355\u8eab\u6c49\uff1f\u2014\u2014\u5728\u8bb0\u5fc6\u4fdd\u7ba1\u5e93\u9996\u6b21\u548c\u4f0a\u739b\u5bf9\u8bdd\uff08\u4e0d\u80fd\u662f\u201c\u5371\u9669\u5fc3\u7075\u201d\u4efb\u52a1\u4e4b\u540e\uff0c\u5426\u5219\u5c31\u6ca1\u6709\u8fd9\u5219\u5bf9\u8bdd\u4e86\uff09\n*\u6211\u6bcf\u5929\u90fd\u62ff\u5371\u9669\u5f53\u996d\u5403\uff0c\u5c1d\u8d77\u6765\u8ddf\u9e21\u8089\u5473\u9053\u5dee\u4e0d\u591a\u3002\u2014\u2014\u5bf9H2-22{{\u9ed1\u5e55|[[\u560e\u5623\u8106\uff0c\u9e21\u8089\u5473|\u560e\u5623\u8106\uff0c\u9e21\u8089\u5473\uff01]]}}\n*\u6211\uff1f\u4e0d\uff0c\u6211\u6765\u81ea\u6708\u7403\uff0c\u4f60\u770b\u6211\u811a\u4e0a\u8fd8\u6709\u5e72\u916a\u5462\u3002\u2014\u2014\u572881\u53f7\u907f\u96be\u6240\u9996\u6b21\u548c\u5c0f\u7537\u5b69\u5965\u65af\u6c40\u4ea4\u8c08\uff08\u73a9\u4e86\u53e4\u8fd1\u4ee3\u897f\u65b9\u4eba\u76f8\u4fe1\u6708\u7403\u662f\u4e73\u916a\u505a\u7684\u6897\uff09\n*\u542c\u4f60\u8fd9\u4e48\u8bf4\u771f\u597d\u7b11\uff0c\u6211\u600e\u4e48\u89c9\u5f97\u6240\u6709\u4e8b\u60c5\u90fd\u662f\u6211\u4e00\u4e2a\u4eba\u5728\u625b\uff1f\u2014\u2014\u8ddf[[\u6d3e\u666e\u00b7\u83b1\u7279]]\u597d\u611f\u5ea6\u8fbe\u5230\u4e00\u5b9a\u7a0b\u5ea6\u540e\u7684\u5bf9\u8bdd\n*\u53ea\u8981\u4e0d\u4f1a\u7528\u5230\u51e1\u58eb\u6797\u548c\u6a61\u80f6\u624b\u5957\uff0c\u6211\u968f\u65f6\u5949\u966a\u3002\u2014\u2014\u201c\u4efb\u804c\u671f\u95f4\u201d\u4efb\u52a1\uff0c\u5bf9\u51ef\u5fb7\u9a91\u58eb\u961f\u957f{{\u9ed1\u5e55|\u4ece\u8fd9\u53e5\u8bdd\u770b\u8001\u51b0\u68cd\u4f3c\u4e4e\u5f88\u6015\u524d\u5217\u817a\u68c0\u67e5}}{{\u9ed1\u5e55|\u800c\u4e14\u5f88\u53ef\u80fd\u662f\u4e2a\u6709\u201c\u6545\u4e8b\u201d\u7684\u4eba}}\n*\u6211\u5927\u5b66\u65f6\u6253\u8d4c\u8f93\u4e86\uff0c\u66fe\u7ecf\u548c\u4e00\u4e2a\u4e11\u7537/\u4e11\u5973\uff08\u4e0e\u4f60\u9009\u62e9\u7684\u6027\u522b\u76f8\u53cd\uff09\u53d1\u751f\u4e86\u5173\u7cfb\u3002\u2014\u2014\u540c\u4e0a\uff0c\u5bf9\u51ef\u5fb7\u9a91\u58eb\u961f\u957f\uff08\u521d\u6b65\u6000\u7591\u662f\u5728\u9ed1\u914d\u5076\uff09\n*\u6211\u53ea\u5bf9\u201c\u9632\u6b62\u50bb\u74dc\u63a5\u8fd1\u201d\u7684\u4fdd\u9669\u6709\u5174\u8da3\u3002\u2014\u2014\u9996\u6b21\u62b5\u8fbe\u82b3\u90bb\u9547\u65f6\uff0c\u5bf9\u82ac\u6069\n*\u6ca1\u9519\uff0c\u4eca\u5e74\u6700\u70ed\u95e8\u7684\u6b3e\u5f0f\uff0c\u60f3\u4e70\u8981\u5feb\u5594\u3002\u2014\u2014\u201c\u5927\u6316\u6398\u201d\u4efb\u52a1\uff0c\u56de\u5e94\u6885\u5c14\u63d0\u95ee\u201c\u6240\u4ee5\u4f60\u662f\u82ad\u6bd4\u7684\u65b0\u73a9\u5177\u54af\u201d\u65f6\n*\u76ae\u514b\u66fc\u7684\u90a3\u4e9b\u5c38\u4f53\u827a\u672f\u54c1\u8981\u662f\u8150\u8d25\u4e86\uff0c\u90a3\u4ef7\u503c\u53ef\u5c31\u5927\u6253\u6298\u6263\u4e86\u3002\u2014\u2014\u76ae\u514b\u66fc\u753b\u5eca\u8c03\u67e5\u4efb\u52a1\uff0c\u5bf9\u7ea6\u7ff0\u00b7\u6c49\u8003\u514b\n*\u6211\u559c\u6b22\u5de5\u4f5c\uff0c\u8ba9\u6211\u5750\u7740\u770b\u6574\u5929\u90fd\u884c\u3002\u2014\u2014\u201c\u5e87\u62a4\u5c71\u4e18\u201d\u4efb\u52a1\uff0c\u5bf9\u53f8\u7279\u5409\n*\u9664\u6b64\u4e4b\u5916\uff0c\u6211\u4e5f\u5f88\u4f1a\u5531\u6b4c\u3001\u5f88\u4f1a\u8c03\u9a6c\u4e01\u5c3c\u3001\u7a7f\u4e0a\u71d5\u5c3e\u670d\u8fd8\u5f88\u5e05\u5462\u3002\u2014\u2014\u548c\u666e\u96f7\u65af\u987f\u00b7\u52a0\u7ef4\u4eb2\u5bc6\u5ea6\u8fbe\u5230\u4e00\u5b9a\u7a0b\u5ea6\u540e\u7684\u5bf9\u8bdd\uff08\u9a6c\u4e01\u5c3c\u662f\u9e21\u5c3e\u9152\u7684\u4e00\u79cd\uff09\n*\u6211\u628a\u6211\u5bb6\u7684\u672a\u6765\u5bc4\u6258\u5728\u4e00\u6761\u72d7\u8eab\u4e0a\uff0c\u5c31\u662f\u8fd9\u6837\u3002\u6211\u6b63\u5f0f\u5ba3\u5e03\u6211\u75af\u4e86\u3002\u2014\u2014\u201c\u91cd\u9022\u201d\u4efb\u52a1\uff0c\u5bf9\u72d7\u8089\n*\u5f53\u6211\u4e00\u767e\u5e74\u540e\u7ec8\u4e8e\u6b7b\u4e86\uff0c\u6211\u53ea\u5e0c\u671b\u6211\u80fd\u4e0b\u5730\u72f1\uff0c\u7136\u540e\u6740\u4f60\u4e2a\u5343\u767e\u56de\uff0c\u4f60\u8fd9\u4e2a\u5783\u573e\u3002\u2014\u2014\u540c\u4e0a\uff0c\u5bf9\u5eb7\u62c9\u5fb7\u00b7\u514b\u7f57\u683c\n*\u6211\u6765\u8fd9\u91cc\u53d6\u9910\u7684\uff0c\u4e24\u5f20\u9999\u80a0\u5927\u6bd4\u8428\u8ddf\u4e00\u4e2a\u5e72\u916a\u9985\u997c\u3002\u8ba2\u9910\u5927\u540d\u662f\u201c\u53bb\u4f60\u7684\u201d\u3002\u2014\u2014\u201c\u5206\u5b50\u4e16\u754c\u201d\u4efb\u52a1\uff0c\u5bf9Z2-47\n*\u56e0\u4e3a\u8ffd\u730e\u8005\u8116\u5b50\u91cc\u7684\u82af\u7247\u770b\u8d77\u6765\u597d\u95ea\u4eae\uff0c\u6211\u5fcd\u4e0d\u4f4f\u60f3\u8981\u4e48\u3002\u2014\u2014\u540c\u4e0a\uff0c\u5bf9\u6234\u745f\u8499\u5a1c\n*\u5929\u54ea\uff0c\u6211\u4eec\u73b0\u5728\u5c31\u53ea\u5dee\u51e0\u4e2a\u8336\u676f\u548c\u90a3\u53ea\u767d\u5154\u4e86\u2026\u2026\u2014\u2014\u201c\u5b66\u9662\u5316\u201d\u4efb\u52a1\uff0c\u5bf9[[\u5723\u7236]]\n*\u662f\u554a\uff0c\u6ca1\u6709\u4ec0\u4e48\u6bd4\u770b\u5230\u5047\u5927\u7329\u7329\u66f4\u52a0\u8ba9\u4eba\u5174\u594b\u4e86\u3002\u2014\u2014\u540c\u4e0a\uff0c\u5bf9\u514b\u83b1\u987f\u00b7\u970d\u96bc\n*\u6240\u4ee5\u5c31\u8ddf\u6240\u6709\u4e0d\u53ef\u601d\u8bae\u7684\u5c0f\u73a9\u610f\u513f\u4e00\u6837\uff0c\u4f60\u4eec\u79d1\u5b66\u5bb6\u4ec0\u4e48\u65f6\u5019\u624d\u4f1a\u5b66\u4e56\uff1f\u2014\u2014\u201c\u56de\u6536\u5408\u6210\u4eba\u201d\u4efb\u52a1\uff0c\u5bf9\u5723\u7236\n*\u6211\u4eec\u662f\u63a0\u593a\u8005\u6539\u8fc7\u81ea\u65b0\u4f1a\u3002\u544a\u8bc9\u6211\uff0c\u5341\u4e00\u6708\u65f6\u4f60\u4f1a\u6295\u6211\u4e00\u7968\u5417\uff1f\u2014\u2014\u201c\u6a21\u8303\u516c\u6c11\u201d\u4efb\u52a1\uff0c\u5bf9\u7b2c\u4e00\u4e2a\u9762\u8bd5\u8005\n*\u53ef\u6076\uff0c\u6211\u76f8\u4fe1\u8fd9\u62fc\u51fa\u6765\u7684\u5b57\u662f\u201c{{ruby|\u65b0\u9c9c\u751f\u59dc|fresh ginger}}\u201d\u3002\u653e\u5230\u8336\u91cc\u5f88\u597d\u559d\uff0c\u518d\u52a0\u8702\u871c\u3001\u67e0\u6aac\u548c\u4e00\u70b9\u767d\u5170\u5730\u2026\u2026\u5f88\u591a\u767d\u5170\u5730\u2026\u2026\u2014\u2014\u201c\u5927\u6d77\u635e\u963f\u76d6\u201d\u4efb\u52a1\uff0c\u5bf9\u897f\u96c5\u62c9\u00b7\u4f69\u7279\u6d1b\u7ef4\u5854\uff08\u8fd9\u662f\u4e2a\u5728\u6e38\u4e50\u573a\u641c\u96c6\u7ec4\u6210\u5bc6\u7801\u7684\u5b57\u6bcd\u6765\u89e3\u5f00\u6838\u5b50\u53ef\u4e50\u53d1\u660e\u8005\u529e\u516c\u5ba4\u7684\u4efb\u52a1\uff0c\u5728\u6e38\u4e50\u56ed\u641c\u96c6\u5230\u7684\u5b57\u6bcd\u62fc\u51fa\u6765\u7684\u5bc6\u7801\u662f\u201c{{ruby|\u6e05\u51c9\u7545\u5feb|refreshing}}\u201d\uff09\n*\u6211\u5931\u53bb\u4e86\u5c1a\u6069\uff0c\u53c8\u4e00\u6b21\u3002/\u6211\u611f\u5230\u4e00\u5207\u5411\u6211\u88ad\u6765\u2014\u2014\u51b2\u51fb\u6ce2\u3001\u70ed\u6d6a\u3001\u8f90\u5c04\u2026\u2026\u6050\u60e7\u3002\u4e16\u754c\u672b\u65e5\u518d\u6b21\u6765\u4e34\u4e86\u3002<br/>\u6211\u95ed\u4e0a\u53cc\u773c\uff0c\u770b\u89c1\u5728\u8fd9\u4e00\u5207\u4e4b\u524d\uff0c\u6838\u5f39\u843d\u4e0b\u524d\u7684\u4eba\u751f\u3002</br>\u4ec0\u4e48\u4e8b\u90fd\u53ef\u80fd\u5728\u8f6c\u77ac\u95f4\u98ce\u4e91\u53d8\u8272\u3002\u4f60\u6240\u89c4\u5212\u7684\u672a\u6765\u4e5f\u4f1a\u77ac\u95f4\u53d8\u6837\uff0c\u4e0d\u8bba\u4f60\u662f\u5426\u505a\u597d\u51c6\u5907\u3002<br/>\u4eba\u7684\u4e00\u751f\u4e2d\uff0c\u603b\u662f\u4f1a\u78b0\u5230\u8fd9\u79cd\u4e8b\u3002<br/>\u8fd9\u4e0d\u662f\u6211\u60f3\u8981\u7684\u4e16\u754c\u3002\u4f46\u6211\u6df1\u9677\u5176\u4e2d\u3002\u8054\u90a6\u3002\u6211\u7684\u5bb6\u56ed\u3002\u5206\u5206\u5408\u5408\u3002<br/>\u6211\u4ee5\u4e3a\uff0c\u6211\u5e0c\u671b\u2026\u2026\u6211\u80fd\u627e\u5230\u6211\u7684\u5bb6\u4eba\uff0c\u9a97\u8fc7\u65f6\u5149\u3001\u518d\u6b21\u56e2\u5706\u3001\u56de\u5230\u4ece\u524d\u7684\u6837\u5b50\u3002<br/>\u4f46\u73b0\u5728\u6211\u660e\u767d\u4e86\u3002\u6211\u660e\u767d\u6211\u56de\u4e0d\u53bb\u4e86\u3002<br/>\u6211\u660e\u767d\u4e16\u754c\u5df2\u7ecf\u53d8\u4e86\u3002\u524d\u65b9\u7684\u8def\u5c06\u4f1a\u8270\u96be\u9669\u963b\u3002<br/>\u8fd9\u4e00\u6b21\u6211\u51c6\u5907\u597d\u4e86\u3002\u56e0\u4e3a\u6211\u4e86\u89e3\u6218\u4e89\u3002\u6218\u4e89\u6c38\u8fdc\u4e0d\u53d8\u3002\u2014\u2014\u4e3b\u7ebf\u5b8c\u7ed3\uff08\u659c\u6760\u524d\u662f\u9009\u62e9\u5e2e\u52a9\u5b66\u9662\uff0c\u659c\u6760\u540e\u662f\u9009\u62e9\u6467\u6bc1\u5b66\u9662\uff09\n\uff08\u5f85\u8865\u5145\uff09\n\n== \u6d3b\u52a8\u5730\u533a ==\n\u6218\u524d\uff0c\u5948\u7279\u66fe\u7ecf\u5728\u963f\u62c9\u65af\u52a0\u5dde\u7684\u5b89\u514b\u96f7\u5947\u53c2\u52a0\u8fc7\u6218\u4e89\uff0c\u8bbe\u5b9a\u4e2d\u8fd9\u573a\u6218\u4e89\u8ba9\u7f8e\u56fd\u7ec8\u6b62\u4e86\u4e0d\u65ad\u7684\u8d25\u9000\u3002\u5176\u59bb\u5b50\u5219\u662f\u4e00\u540d\u521a\u6bd5\u4e1a\u4e8e\u8428\u798f\u514b\u5927\u5b66\u7684\u5f8b\u5e08\u3002\n\n\u6218\u540e\u8001\u51b0\u68cd\u7684\u6d3b\u52a8\u5730\u533a\u4e3b\u8981\u5728{{\u6ce8\u91ca|\u9a6c\u8428\u8bf8\u585e\u5dde|\u786e\u5207\u6765\u8bf4\u662f\u9a6c\u8428\u8bf8\u585e\u8054\u90a6\u3002\u300a\u8f90\u5c04\u300b\u4e16\u754c\u89c2\u91cc\uff0c\u7f8e\u56fd\u5168\u56fd\u5404\u5dde\u5728\u5927\u6218\u4e4b\u524d\u91cd\u7ec4\u6210\u5341\u4e09\u4e2a\u8054\u90a6\uff08\u56e0\u6b64\u6e38\u620f\u4e2d\u7684\u7f8e\u56fd\u56fd\u65d7\u4e0a\u9762\u662f\u5341\u4e09\u9897\u661f\uff09\u3002}}\u4ee5\u6ce2\u58eb\u987f\u4e3a\u4e2d\u5fc3\u7684\u4e00\u5c0f\u5757\u533a\u57df\uff0c\u800c\u5728DLC\u300a\u8fdc\u6e2f\u60ca\u9b42\u300b\u4e2d\u53c8\u6269\u589e\u5230\u7f05\u56e0\u5dde\u7684\u963f\u5361\u8fea\u4e9a\u56fd\u5bb6\u516c\u56ed\u4e0a\uff08\u73b0\u5b9e\u4e16\u754c\u91cc\u7684\u963f\u5361\u8fea\u4e9a\u56fd\u5bb6\u516c\u56ed\u4e3b\u8981\u7531\u4e09\u5ea7\u5c9b\u5c7f\u7ec4\u6210\uff0c\u8001\u51b0\u68cd\u767b\u4e0a\u7684\u53ea\u662f\u4e09\u5ea7\u5c9b\u5c7f\u7684\u5176\u4e2d\u4e4b\u4e00\uff09\u3002\u81f3\u4e8e\u6838\u5b50\u4e16\u754c\u2014\u2014\u90a3\u5176\u5b9e\u4e5f\u8fd8\u662f\u5728\u6ce2\u58eb\u987f\u5730\u533a\uff0c\u786e\u5207\u8bf4\u662f\u6ce2\u58eb\u987f\u897f\u4fa7\u7684\u8fdc\u90ca\u3002\n\n{{\u300a\u8f90\u5c044\u300b\u77e5\u540d\u4eba\u7269\u540d\u5355}}\n{{\u8f90\u5c04}}\n\n== \u6ce8\u91ca\u4e0e\u5916\u90e8\u94fe\u63a5 ==\n<references />\n[[\u5206\u7c7b:\u8f90\u5c04\u7cfb\u5217]]\n[[\u5206\u7c7b:\u519b\u4eba]]\n[[\u5206\u7c7b:\u5f8b\u5e08]]"}}}