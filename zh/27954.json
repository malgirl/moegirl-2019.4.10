u67aa\u4e4b\u4e00\u3002\u6bd4\u8d77\u90fd\u5e02\u4f20\u8bf4\u5185\u683c\u592b\u7565\u5fae\u597d\u51fa\u4e00\u4e9b\u3002<del>\u975e\u6d32\u4eba\uff1a\u4f60\u5728\u9017\u6211\uff1f</del>\n{{Hide|\u6807\u9898={{color|#AD5A5A|\u5496\u5561\u5385\u6545\u4e8b}}-{{color|blue|MG5\u201c\u8352\u91ce\u730e\u4eba\u201d}}|\u5185\u5bb9=\n\u8b66\u65b9\u5bf9\u5267\u9662\u88ad\u51fb\u4e8b\u4ef6\u6b8b\u515a\u7684\u6293\u6355\u544a\u4e00\u6bb5\u843d\uff0c\u683c\u91cc\u82ac\u7684\u5c0f\u961f\u6309\u7167\u9884\u5b9a\u7684\u5b89\u6392\u8fdb\u884c\u6536\u5c3e\u5de5\u4f5c\u3002\n\nMG5\uff1a\u8bf7\u5f80\u8fd9\u8fb9\u8d70\uff0c\u957f\u5b98\u3002\n\nMG5\u8d70\u5728\u6211\u7684\u524d\u65b9\uff0c\u7528\u624b\u4e2d\u7684\u5f2f\u5200\u5288\u5f00\u4f4e\u77ee\u7684\u704c\u6728\u4e1b\u6e05\u51fa\u4e00\u6761\u9053\u8def\u3002\n\n\u6307\u6325\u5b98\uff1a\u8dd1\u5230\u8fd9\u79cd\u8352\u90ca\u91ce\u5916\uff0c\u8fd9\u7fa4\u4eba\u8fd8\u771f\u662f\u4f1a\u7ed9\u4eba\u627e\u9ebb\u70e6\u3002\n\nMG5\uff1a\u8fd9\u79cd\u7a0b\u5ea6\u7684\u641c\u5bfb\u5de5\u4f5c\uff0c\u6211\u4eec\u4eba\u5f62\u53ef\u4ee5\u72ec\u7acb\u5b8c\u6210\u3002\n::\u957f\u5b98\u4f60\uff0c\u5176\u5b9e\u6ca1\u5fc5\u8981\u4eb2\u81ea\u6765\u73b0\u573a\u7684\u3002\n\n\u6307\u6325\u5b98\uff1a\u8981\u548c\u5176\u4ed6\u7ec4\u7ec7\u7684\u4eba\u7c7b\u6253\u4ea4\u9053\uff0c\u8fd9\u79cd\u5de5\u4f5c\u8fd8\u662f\u6709\u6211\u5728\u573a\u6bd4\u8f83\u597d\u3002\n::\u800c\u4e14\u2026\u2026\u8fd9\u6b21\u7684\u76ee\u6807\u4e5f\u662f\u4eba\u7c7b\uff0c\u6211\u8fd8\u662f\u60f3\u4eb2\u81ea\u6765\u89c1\u8bc1\u4e00\u4e0b\u3002\n\nMG5\uff1a\u8fd9\u6837\u554a\u2026\u2026\u4eba\u7c7b\u7684\u4e8b\u60c5\u8fd8\u771f\u662f\u96be\u4ee5\u7406\u89e3\u3002\n::\u957f\u5b98\uff0c\u8bf7\u4e0d\u8981\u79bb\u6211\u592a\u8fdc\uff0c\u9644\u8fd1\u53ef\u80fd\u8fd8\u4f1a\u6709\u4e00\u4e9b\u88ad\u51fb\u8005\u7684\u6b8b\u515a\u3002\n\n\u6307\u6325\u5b98\uff1a\u5509\u2026\u2026\u597d\u4e45\u6ca1\u8fd9\u4e48\u51fa\u6765\u6d3b\u52a8\uff0c\u611f\u89c9\u4f53\u529b\u6709\u70b9\u8ddf\u4e0d\u4e0a\u4e86\u3002\n::\u53cd\u6b63\u9047\u5230\u88ad\u51fb\u8005\u7684\u8bdd\uff0c\u4f60\u4e5f\u4f1a\u4fdd\u62a4\u6211\u7684\u5427\u3002\n\nMG5\uff1a\u2026\u2026\u90a3\u662f\u81ea\u7136\u3002\u4e0d\u8fc7\u957f\u5b98\u4e5f\u4e0d\u8981\u56e0\u6b64\u6389\u4ee5\u8f7b\u5fc3\u3002\n::\u9664\u4e86\u4eba\u7c7b\u7684\u88ad\u51fb\u8005\uff0c\u5728\u8fd9\u79cd\u91ce\u5916\u5730\u533a\u53ef\u80fd\u4f1a\u6709\u4e00\u4e9b\u5176\u4ed6\u7684\u5371\u9669\u751f\u7269\u7684\u3002\n\n\u6307\u6325\u5b98\uff1a\u5443\uff0c\u662f\u5417\uff1f\u90a3\u6211\u53ef\u5f97\u8ddf\u7d27\u4f60\u4e86\u3002\n::\u8bf4\u6765\u2026\u2026MG5\u4f60\u770b\u4e0a\u53bb\u5f88\u719f\u6089\u91ce\u5916\u73af\u5883\u554a\u3002\n::\u8fd9\u8eab\u8863\u670d\u770b\u8d77\u6765\u5c31\u50cf\u662f\u91ce\u8425\u4e13\u7528\u7684\u3002\n\nMG5\u6b64\u523b\u7a7f\u7740\u4e00\u8eab\u5bbd\u677e\u7684\u670d\u88c5\uff0c\u6234\u7740\u4e00\u9876\u9e2d\u820c\u5e3d\uff0c\u8eab\u4e0a\u80cc\u5305\u6c34\u58f6\u6298\u53e0\u51f3\u4e4b\u7c7b\u7684\u88c5\u5907\u4e00\u5e94\u4ff1\u5168\u3002\n\n\u5982\u679c\u4e0d\u662f\u8eab\u540e\u80cc\u7740\u7684\u90a3\u633a\u673a\u67aa\uff0c\u770b\u4e0a\u53bb\u5b8c\u5168\u5c31\u662f\u4e00\u4e2a\u6237\u5916\u63a2\u9669\u8005\u3002\n\nMG5\uff1a\u5728\u8fd9\u79cd\u73af\u5883\u4e0b\uff0c\u8fd9\u6837\u7684\u7a7f\u7740\u66f4\u52a0\u65b9\u4fbf\u884c\u52a8\u3002\n\n\u6307\u6325\u5b98\uff1a\u8981\u51d1\u9f50\u8fd9\u8eab\u884c\u5934\uff0c\u5f00\u9500\u53ef\u4e0d\u5c11\u5462\uff0c\u662f\u56e0\u4e3a\u7ecf\u5e38\u6765\u5916\u9762\u5417\uff1f\n\nMG5\uff1a\u55ef\u2026\u2026\u56e0\u4e3a\u6211\u7ecf\u5e38\u8fd9\u6837\u4e00\u4e2a\u4eba\u51fa\u6765\u8f6c\u8f6c\u3002\n\n\u6307\u6325\u5b98\uff1a\u90fd\u662f\u72ec\u81ea\u91ce\u8425\uff1f\n\nMG5\uff1a\u662f\u7684\uff0c\u957f\u5b98\u3002\n\n\u6307\u6325\u5b98\uff1a\u6211\u8fd8\u4ee5\u4e3a\u4f60\u4f1a\u66f4\u559c\u6b22\u548c\u5176\u4ed6\u4eba\u5f62\u5f85\u5728\u4e00\u8d77\u5462\u3002\n\nMG5\uff1a\u6211\u5e76\u4e0d\u8ba8\u538c\u548c\u540c\u4f34\u4eec\u5f85\u5728\u4e00\u8d77\u2026\u2026\n::\u53ea\u662f\u5076\u5c14\u2026\u2026\u4e5f\u9700\u8981\u4e00\u4e9b\u72ec\u5904\u7684\u65f6\u95f4\u3002\n::\u554a\uff0c\u957f\u5b98\uff0c\u6211\u4eec\u5230\u8fbe\u76ee\u6807\u5730\u70b9\u4e86\u3002\n\n\u6307\u6325\u5b98\uff1a\u597d\uff0c\u641c\u5bfb\u5b8c\u8fd9\u7247\u5730\u533a\uff0c\u6211\u4eec\u5c31\u80fd\u4f11\u606f\u4e00\u4e0b\u4e86\u3002\n\n\u4e00\u5c0f\u65f6\u540e\uff0c\u641c\u7d22\u7ed3\u675f\u3002\n\nMG5\uff1a\u957f\u5b98\uff0c\u641c\u7d22\u5b8c\u6bd5\u3002\u6ca1\u6709\u53d1\u73b0\u654c\u4eba\u7684\u8e2a\u8ff9\u3002\n\n\u6307\u6325\u5b98\uff1a\u6211\u8fd9\u8fb9\u4e5f\u6ca1\u6709\u53d1\u73b0\u4ec0\u4e48\u3002\n\nMG5\uff1a\u5176\u4ed6\u4eba\u7684\u641c\u67e5\u8fd8\u6ca1\u6709\u7ed3\u675f\uff0c\u957f\u5b98\u4f60\u8981\u4e0d\u8981\u5728\u8fd9\u91cc\u4f11\u606f\u4e0b\uff1f\n\n\u6307\u6325\u5b98\uff1a\u597d\uff0c\u7b49\u5176\u4ed6\u4eba\u7684\u8054\u7edc\u5427\u3002\n::\u5982\u679c\u5176\u4ed6\u4eba\u90a3\u8fb9\u90fd\u6ca1\u4ec0\u4e48\u95ee\u9898\uff0c\u6211\u4eec\u5c31\u53ef\u4ee5\u6536\u5de5\u56de\u53bb\u4e86\u3002\n\nMG5\u53d6\u4e0b\u4e86\u6298\u53e0\u51f3\u653e\u7f6e\u5728\u6797\u95f4\u7684\u7a7a\u5730\u4e0a\u3002\n\nMG5\uff1a\u957f\u5b98\uff0c\u8bf7\u5728\u8fd9\u91cc\u7a0d\u7b49\u4e00\u4e0b\u3002\n::\u6211\u51c6\u5907\u4e00\u4e9b\u996e\u6599\u6696\u6696\u8eab\u5b50\u3002\n\n\u6307\u6325\u5b98\uff1a\u9ebb\u70e6\u4f60\u4e86\uff0cMG5\u3002\n\nMG5\uff1a\u8bf7\u4e0d\u7528\u5ba2\u6c14\uff0c\u957f\u5b98\u3002\u6211\u7684\u529b\u91cf\u672c\u5c31\u662f\u4e3a\u60a8\u800c\u4f7f\u7528\u7684\u3002\n\nMG5\u5a34\u719f\u5730\u67b6\u8d77\u7089\u706b\uff0c\u5f88\u5feb\u4e00\u676f\u70ed\u817e\u817e\u7684\u5496\u5561\u5c31\u9001\u5230\u4e86\u6211\u7684\u624b\u91cc\u3002\n\nMG5\uff1a\u62b1\u6b49\uff0c\u957f\u5b98\u3002\u6211\u73b0\u5728\u53ea\u80fd\u51c6\u5907\u8fd9\u6837\u7684\u996e\u54c1\u3002\n::\u867d\u7136\u662f\u666e\u901a\u7684\u901f\u6eb6\u5496\u5561\uff0c\u4e0d\u8fc7\u6211\u76f8\u4fe1\u5b83\u8fd8\u662f\u80fd\u8d77\u5230\u4e00\u4e9b\u63d0\u795e\u6548\u679c\u3002\n\n\u6307\u6325\u5b98\uff1a\u6ca1\u5173\u7cfb\uff0cMG5\u3002\u8fd9\u5df2\u7ecf\u662f\u5f88\u68d2\u7684\u996e\u6599\u4e86\u3002\n\nMG5\uff1a\u4e0d\u8fc7\u6211\u6ca1\u6709\u5e26\u5907\u7528\u7684\u5976\u7cbe\u548c\u7802\u7cd6\u2026\u2026\u6ca1\u6cd5\u66f4\u4ed4\u7ec6\u5730\u8c03\u6574\u5473\u9053\u3002\n\n\u6307\u6325\u5b98\uff1a\u867d\u7136\u662f\u4eba\u7c7b\uff0c\u4e0d\u8fc7\u6211\u6ca1\u6709\u90a3\u4e48\u591a\u9ad8\u8981\u6c42\u5566\u3002\n::\u5514\uff0c\u5514\u2026\u2026\u8fd9\u5496\u5561\u7684\u5473\u9053\u5df2\u7ecf\u5f88\u597d\u4e86\u3002\n\nMG5\uff1a\u80fd\u5408\u4f60\u7684\u53e3\u5473\u5c31\u597d\u3002\n\nMG5\u7aef\u7740\u5496\u5561\uff0c\u5728\u6211\u5bf9\u9762\u5750\u4e0b\u6765\u3002\n\nMG5\uff1a\u957f\u5b98\u2026\u2026\u4f60\u5bf9\u8fd9\u6b21\u7684\u4e8b\u4ef6\u600e\u4e48\u770b\uff1f\n\n\u6307\u6325\u5b98\uff1a\u8fd9\u6b21\u7684\u4e8b\u4ef6\u2026\u2026\u5c31\u662f\u4e00\u6b21\u666e\u901a\u7684\u6050\u6016\u88ad\u51fb\u5427\u3002\n\nMG5\uff1a\u5176\u5b9e\uff0c\u6211\u6709\u70b9\u4e0d\u592a\u660e\u767d\u2026\u2026\n::\u660e\u660e\u8fd8\u6709\u94c1\u8840\u8fd9\u6837\u7684\u654c\u4eba\uff0c\u4e3a\u4ec0\u4e48\u4eba\u7c7b\u4e4b\u95f4\u4e5f\u8981\u4e92\u76f8\u6218\u6597\uff1f\n\n\u6307\u6325\u5b98\uff1a\u2026\u2026\u4f60\u8fd8\u771f\u662f\u63d0\u4e86\u4e2a\u96be\u4ee5\u56de\u7b54\u7684\u95ee\u9898\u3002\n::\u600e\u4e48\u8bf4\u5462\uff0c\u5176\u5b9e\u4eba\u7c7b\u548c\u4eba\u7c7b\u4e4b\u95f4\u7684\u6218\u6597\uff0c\u540c\u4f60\u4eec\u548c\u94c1\u8840\u7684\u6218\u6597\u5f88\u76f8\u4f3c\u3002\n::\u6216\u8005\u8bf4\uff0c\u4f60\u4eec\u7684\u6218\u6597\u5c31\u662f\u4eba\u7c7b\u4e4b\u95f4\u6218\u6597\u7684\u5ef6\u4f38\u5427\u3002\n\nMG5\uff1a\u2026\u2026\u679c\u7136\u6211\u8fd8\u662f\u4e0d\u592a\u80fd\u7406\u89e3\u554a\u3002\n::\u5bf9\u6211\u4eec\u6765\u8bf4\uff0c\u8fd8\u662f\u6267\u884c\u547d\u4ee4\u66f4\u52a0\u7b80\u5355\u4e00\u4e9b\u3002\n\n\u6307\u6325\u5b98\uff1a\u5176\u5b9e\uff0c\u5bf9\u6211\u4eec\u8fd9\u6837\u7684\u4eba\u6765\u8bf4\uff0c\u80fd\u4fdd\u62a4\u8eab\u8fb9\u7684\u4eba\u5c31\u8db3\u591f\u4e86\u3002\n\nMG5\uff1a\u6bd4\u5982\uff0c\u4fdd\u62a4\u540c\u4f34\u5417\u3002\n\n\u6307\u6325\u5b98\uff1a\u55ef\uff0c\u8fd9\u4e5f\u662f\u5176\u4e2d\u7684\u4e00\u79cd\u3002\n::\u53ea\u8981\u6709\u81ea\u5df1\u671f\u671b\u8fbe\u6210\u7684\u76ee\u6807\uff0c\u6218\u6597\u5c31\u4e0d\u662f\u6beb\u65e0\u4ef7\u503c\u7684\u3002\n::\u6211\u89c9\u5f97\u5728\u8fd9\u4e2a\u65b9\u9762\uff0c\u65e0\u8bba\u662f\u4eba\u5f62\u8fd8\u662f\u4eba\u7c7b\uff0c\u90fd\u662f\u4e00\u6837\u7684\u3002\n\nMG5\uff1a\u6211\u7684\u8bdd\u2026\u2026\u5c31\u662f\u5e0c\u671b\u6bcf\u6b21\u5171\u540c\u6218\u6597\u7684\u4f19\u4f34\u90fd\u80fd\u5b89\u5168\u5f52\u6765\u3002\n::\u5305\u62ec\u957f\u5b98\u4f60\u5728\u5185\uff0c\u5e0c\u671b\u5927\u5bb6\u80fd\u591f\u5e73\u5b89\u5730\u53d6\u5f97\u80dc\u5229\u3002\n\n\u6307\u6325\u5b98\uff1a\u8fd8\u771f\u662f\u6e29\u67d4\u554a\uff0cMG5\u3002\n\nMG5\uff1a\u2026\u2026\u60a8\u7a81\u7136\u5728\u8bf4\u4e9b\u4ec0\u4e48\u554a\u3002\n::\u90a3\u4e2a\uff0c\u518d\u4e0d\u8d76\u5feb\u559d\u5b8c\u7684\u8bdd\uff0c\u5496\u5561\u5c31\u8981\u51c9\u4e86\u3002\n\n\u6307\u6325\u5b98\uff1a\u597d\uff0c\u597d\uff0c\u6211\u8fd9\u5c31\u559d\u3002\n\n\u2026\u2026\u6ef4\u3002\u6536\u5230\u4e86\u5f00\u59cb\u884c\u52a8\u7684\u8baf\u53f7\u3002\n\nMG5\uff1a\u4f60\u770b\uff0c\u8981\u6765\u4e0d\u53ca\u4e86\u3002\n\n\u6211\u8fde\u5fd9\u5927\u53e3\u5c06\u5496\u5561\u704c\u8fdb\u809a\u5b50\u91cc\uff0c\u64e6\u4e86\u64e6\u5634\u3002\n\n\u6307\u6325\u5b98\uff1a\u4e0b\u6b21\u6765\u4e00\u6b21\u771f\u6b63\u7684\u91ce\u5916\u8fdc\u8db3\u5427\uff0c\u6ca1\u6709\u654c\u4eba\uff0c\u53ea\u6709\u670b\u53cb\u7684\u90a3\u79cd\u3002\n\nMG5\uff1a\u6307\u6325\u5b98\uff0c\u4f60\u613f\u610f\u628a\u4eba\u5f62\u5f53\u4f5c\u670b\u53cb\uff1f\n\n\u6307\u6325\u5b98\uff1a\u4e0d\u53ef\u4ee5\u5417\uff1f\u6211\u4e5f\u559c\u6b22\u7b80\u5355\u7684\u4e8b\u60c5\u554a\u3002\n\nMG5\uff1a\u957f\u5b98\u2026\u2026\n::\u6211\u4e5f\u975e\u5e38\u559c\u6b22\u2026\u2026\u548c\u4f60\u5171\u5904\u7684\u65f6\u5149\u3002\n\nMG5\uff1a\u90a3\u5c31\u8fd9\u6837\u8bf4\u597d\u4e86\u3002\n::\u4e0b\u4e00\u6b21\uff0c\u662f\u771f\u6b63\u7684\u8fdc\u8db3\u3002\n\n}}\n{{Hide|\u6807\u9898={{color|#AD5A5A|\u5496\u5561\u5385\u6545\u4e8b}}-{{color|blue|MG5\u201c\u7eef\u7ea2\u5b88\u591c\u4eba\u201d}}|\u5185\u5bb9=\n\u683c\u91cc\u82ac\u6307\u6325\u5ba4\u3002\n\nMG5\uff1a\u957f\u5b98\uff0c\u60a8\u6709\u4efb\u52a1\u4ea4\u7ed9\u6211\uff1f\n\n\u8054\u7edc\u8fc7\u4e4b\u540e\uff0c\u7a7f\u7740\u5723\u8bde\u88c5\u675f\u7684MG5\u5f88\u5feb\u5c31\u8d76\u5230\u4e86\u6211\u7684\u529e\u516c\u5ba4\u3002\n\n\u5979\u4e00\u5b9a\u4e5f\u5f88\u671f\u5f85\u8fd9\u4e2a\u8282\u65e5\uff0c\u6211\u4e00\u60f3\u5230\u63a5\u4e0b\u6765\u8981\u62dc\u6258\u5979\u7684\u4e8b\u60c5\uff0c\u603b\u89c9\u5f97\u6709\u4e9b\u5bf9\u4e0d\u8d77\u5979\u2026\u2026\n\n\u6307\u6325\u5b98\uff1a\u4f60\u77e5\u9053\uff0c\u57fa\u5730\u91cc\u7684\u90a3\u4e9b\u5b69\u5b50\u4eec\u6bcf\u5230\u8fc7\u8282\u5c31\u4f1a\u6709\u70b9\u4e0d\u5b89\u5206\u3002\n::\u6240\u4ee5\u4e3a\u4e86\u9632\u6b62\u5e73\u5b89\u591c\u7684\u5e86\u5178\u51fa\u73b0\u610f\u5916\uff0c\u6211\u5e0c\u671b\u4f60\u80fd\u591f\u534f\u52a9\u7ef4\u62a4\u73b0\u573a\u79e9\u5e8f\u3002\n\nMG5\uff1a\u539f\u6765\u5982\u6b64\u3002\u8fd9\u6ca1\u6709\u4ec0\u4e48\u95ee\u9898\u3002\n\n\u6307\u6325\u5b98\uff1a\u4e0d\u8fc7\uff0c\u9762\u5bf9\u90a3\u4e9b\u4f01\u56fe\u72e9\u730e\u5723\u8bde\u8001\u4eba\u7684\u718a\u5b69\u5b50\uff0c\u5230\u65f6\u5019\u4f60\u53ef\u80fd\u4f1a\u9700\u8981\u52a8\u624b\u2026\u2026\n\nMG5\u6c89\u9ed8\u4e86\u7247\u523b\u3002\n\nMG5\uff1a\u6ca1\u5173\u7cfb\uff0c\u957f\u5b98\u3002\n::\u6ca1\u88ab\u6253\u8fc7\u7684\u4eba\u662f\u4e0d\u4f1a\u6210\u957f\u7684\uff0c\u4eba\u5f62\u4e5f\u662f\u4e00\u6837\u3002\n::\u4e0e\u5176\u56e0\u4e3a\u5fc3\u667a\u4e91\u56fe\u7684\u4e0d\u7a33\u5b9a\u800c\u72af\u4e0b\u8fc7\u9519\uff0c\u4e0d\u5982\u8ba9\u8eab\u4e3a\u540c\u4f34\u7684\u6211\u4eec\u5148\u6559\u8bad\u4e00\u4e0b\u3002\n\nMG5\u7684\u53d1\u8a00\u8ba9\u6211\u83ab\u540d\u611f\u5230\u4e00\u4e1d\u60b2\u58ee\uff0c\u5fc3\u4e2d\u7684\u6b49\u610f\u53cd\u800c\u6108\u53d1\u589e\u957f\u4e86\u3002\n\n\u6307\u6325\u5b98\uff1a\u62b1\u6b49\u554a\u2026\u2026MG5\uff0c\u8ba9\u4f60\u6267\u884c\u8fd9\u79cd\u4efb\u52a1\u3002\n::\u5982\u679c\u4e0d\u613f\u610f\u7684\u8bdd\u4e5f\u6ca1\u5173\u7cfb\u2026\u2026\n\nMG5\uff1a\u574f\u5b69\u5b50\u5c31\u662f\u9700\u8981\u6572\u6253\u6572\u6253\uff0c\u4f5c\u4e3a\u540c\u4f34\u66f4\u5e94\u5982\u6b64\u3002\n::\u957f\u5b98\u4f60\u4e5f\u662f\u4e3a\u5979\u4eec\u597d\uff0c\u6240\u4ee5\u4e0d\u7528\u9053\u6b49\u3002\n::\u8fd9\u4e2a\u4efb\u52a1\uff0c\u8bf7\u52a1\u5fc5\u8981\u4ea4\u7ed9\u6211\u3002\n\n\u6307\u6325\u5b98\uff1a\u597d\u7684\uff0cMG5\u3002\u4e00\u5207\u5c31\u62dc\u6258\u4f60\u4e86\u3002\n::\u5230\u65f6\u5019\u4e0d\u4f1a\u8ba9\u4f60\u5355\u6253\u72ec\u6597\u7684\uff0c\u4e3a\u4e86\u786e\u4fdd\u4e07\u65e0\u4e00\u5931\uff0c\u6211\u4e5f\u51c6\u5907\u4e86\u522b\u7684\u5e2e\u624b\u3002\n\n12\u670824\u65e5\u665a\u3002\n\n\u4eba\u5f62\u4eec\u56f4\u7ed5\u7740\u5723\u8bde\u6811\uff0c\u805a\u96c6\u5728\u57fa\u5730\u524d\u7684\u5c0f\u5e7f\u573a\u4e0a\u7b49\u5f85\u7740\u5723\u8bde\u8001\u4eba\u7684\u5230\u6765\u3002\n\nMG5\u5e76\u6ca1\u6709\u52a0\u5165\u62e5\u6324\u7684\u4eba\u7fa4\uff0c\u5979\u5750\u5728\u5899\u8fb9\u5792\u8d77\u7684\u7269\u8d44\u7bb1\u4e0a\uff0c\u89c2\u5bdf\u5c0f\u5e7f\u573a\u4e2d\u7684\u60c5\u51b5\u3002\n\n\u5723\u8bde\u8001\u4eba\u7684\u96ea\u6a47\u6ee1\u8f7d\u7740\u793c\u7269\u5f00\u8fdb\u4e86\u5c0f\u5e7f\u573a\uff0c\u4eba\u7fa4\u987f\u65f6\u9a9a\u52a8\u8d77\u6765\u3002\n\n\u5728\u6b22\u547c\u96c0\u8dc3\u7684\u4eba\u5f62\u4e2d\uff0cMG5\u53d1\u73b0\u4e86\u51e0\u4e2a\u9b3c\u9b3c\u795f\u795f\u7684\u8eab\u5f71\u3002\n\nMG5\uff1a\u867d\u7136\u4e0d\u60f3\u5411\u540c\u4f34\u5f00\u67aa\u2026\u2026\u4f46\u662f\u770b\u6765\u4e0d\u5f97\u4e0d\u5f00\u5de5\u4e86\u554a\u3002\n\nMG5\u5c06\u586b\u6ee1\u6a61\u80f6\u5f39\u7684\u5f39\u94fe\u88c5\u5165\u67aa\u5323\u3002\n\n\u51e0\u4e2a\u8eab\u5f62\u8f83\u4e3a\u77ee\u5c0f\u7684\u4eba\u5f62\u62b1\u7740\u9999\u69df\u9152\u7684\u74f6\u5b50\u51fa\u73b0\u5728\u62e5\u6324\u7684\u4eba\u7fa4\u4e2d\u3002\n\n\u5728\u57fa\u5730\u91cc\u4eba\u5f62\u662f\u4e0d\u80fd\u9886\u53d6\u5230\u95ea\u5149\u5f39\u3001\u70df\u96fe\u5f39\u4e4b\u7c7b\u7684\u88c5\u5907\u7684\u3002\u90a3\u4e48\u8981\u60f3\u5236\u9020\u6df7\u4e71\uff0c\u5979\u4eec\u5c31\u53ea\u80fd\u5229\u7528\u80fd\u63a5\u89e6\u5230\u7684\u4e1c\u897f\u4e86\u3002\n\nMG5\uff1a\u771f\u662f\u7684\uff0c\u8fd9\u79cd\u8fd0\u7b97\u6548\u7387\u600e\u4e48\u4e0d\u653e\u5728\u6218\u6597\u4e0a\u3002\n\n\u773c\u770b\u7740\u5979\u4eec\u5f00\u59cb\u6447\u6643\u9999\u69df\u74f6\uff0c\u5c06\u74f6\u53e3\u5bf9\u51c6\u5723\u8bde\u8001\u4eba\uff0cMG5\u6447\u6447\u5934\u67b6\u8d77\u673a\u67aa\u3002\n\nMG5\uff1a\u60f3\u51fa\u8fd9\u79cd\u998a\u4e3b\u610f\u2026\u2026\u770b\u6765\u8fd9\u5c31\u662f\u5979\u4eec\u5fc3\u667a\u7684\u6781\u9650\u4e86\u3002\n::\u505a\u4e8b\u4e4b\u524d\u4e5f\u4e0d\u8003\u8651\u4e0b\u4f1a\u7ed9\u522b\u4eba\u5e26\u6765\u591a\u5927\u7684\u9ebb\u70e6\uff0c\u770b\u6765\u7684\u786e\u5f97\u597d\u597d\u6559\u80b2\u4e00\u4e0b\u624d\u884c\u3002\n\n\u5c31\u5728\u4eba\u5f62\u4eec\u51c6\u5907\u62d4\u6389\u74f6\u585e\u7684\u65f6\u5019\uff0c\u4e00\u58f0\u6c89\u95f7\u7684\u67aa\u54cd\u8d2f\u7a7f\u4e86\u5e7f\u573a\u7684\u4e0a\u7a7a\u3002\n\n\u8fd8\u6ca1\u6765\u5f97\u53ca\u78b0\u5230\u74f6\u53e3\uff0c\u4f01\u56fe\u5e72\u6270\u6d3b\u52a8\u7684\u4eba\u5f62\u4fbf\u5e94\u58f0\u5012\u5730\uff0c\u6574\u4e2a\u5e7f\u573a\u90fd\u7a81\u7136\u5b89\u9759\u4e86\u4e0b\u6765\u3002\n\n\u800c\u5728\u77ed\u6682\u7684\u5bc2\u9759\u540e\uff0c\u53cd\u5e94\u8fc7\u6765\u72b6\u51b5\u7684\u4eba\u5f62\u4eec\u4fbf\u51b2\u5411\u5723\u8bde\u8001\u4eba\u7684\u96ea\u6a47\u3002\n\n\u683c\u91cc\u82ac\u4eba\u5f62\uff1a\u4e8b\u5230\u5982\u4eca\uff0c\u53ea\u80fd\u62fc\u4e00\u628a\u4e86\uff01\n\n\u683c\u91cc\u82ac\u4eba\u5f62\uff1a\u4e0d\u80fd\u8ba9\u540c\u5fd7\u767d\u767d\u5012\u4e0b\uff01\n\n\u683c\u91cc\u82ac\u4eba\u5f62\uff1a\u4e3a\u4e86\u5723\u8bde\u8282\u7684\u793c\u7269\uff01\u51b2\u554a\uff01\n\nMG5\uff1a\u2026\u2026\u5979\u4eec\u7684\u5fc3\u667a\u4e91\u56fe\u662f\u63a5\u9519\u7ebf\u8def\u4e86\u5417\uff1f\n\nMG5\u6263\u4e0b\u4e86\u6273\u673a\uff0c\u6a61\u80f6\u5f39\u4ece\u67aa\u53e3\u503e\u6cfb\u800c\u51fa\u3002\n\n\u4e0e\u6b64\u540c\u65f6\u4ece\u5e7f\u573a\u7684\u53e6\u4e00\u7aef\u4e5f\u4f20\u6765\u4e86\u67aa\u58f0\uff0c\u62a2\u593a\u793c\u7269\u7684\u5c0f\u5206\u961f\u5f88\u5feb\u4fbf\u5168\u519b\u8986\u6ca1\u4e86\u3002\n\nFAL\uff1a\u8fd8\u771f\u662f\u4e00\u7fa4\u4f1a\u7ed9\u4eba\u6dfb\u9ebb\u70e6\u7684\u5bb6\u4f19\u3002\n\nMG5\uff1a\u539f\u6765\u957f\u5b98\u8bf4\u7684\u5e2e\u624b\u662f\u4f60\u554a\uff0cFAL\u3002\n\nFAL\uff1a\u9664\u4e86\u6211\uff0c\u8fd8\u6709\u623f\u9876\u4e0a\u7684\u975e\u6d32\u9a6f\u9e7f\u3002\n\nFAL\u5c06\u5916\u5957\u7684\u9886\u5b50\u5411\u4e0a\u62c9\u4e86\u62c9\u3002\n\nFAL\uff1a\u771f\u51b7\u3002\u8fd9\u91cc\u4ea4\u7ed9\u4f60\u4e86\uff0cMG5\u3002\n::\u6211\u7684\u88c5\u675f\u4eca\u5929\u4e0d\u592a\u9002\u5408\u5f53\u642c\u8fd0\u5de5\u3002\n\nFAL\u8f6c\u8eab\u79bb\u5f00\u4e86\u3002\n\nMG5\u770b\u4e86\u770b\u5728\u5730\u4e0a\u4e1c\u5012\u897f\u6b6a\u8eba\u7740\u7684\u4eba\u5f62\uff0c\u53f9\u53e3\u6c14\u8e72\u4e0b\u8eab\u62cd\u4e86\u62cd\u5979\u4eec\u7684\u8111\u888b\u3002\n\nMG5\uff1a\u8d77\u6765\u4e86\uff0c\u522b\u8d56\u5728\u5730\u4e0a\u3002\n\n\u683c\u91cc\u82ac\u4eba\u5f62\uff1a\u545c\u545c\u2026\u2026\u597d\u75db\uff01\n\nMG5\uff1a\u77e5\u9053\u75db\u7684\u8bdd\u5c31\u6536\u655b\u4e9b\uff0c\u4e0b\u6b21\u4e0d\u8981\u505a\u8fd9\u79cd\u8822\u4e8b\u3002\n\n\u683c\u91cc\u82ac\u4eba\u5f62\uff1a\u53ef\u662f\u5723\u8bde\u793c\u7269\u2026\u2026\u597d\u60f3\u8981\u561b\u3002\n\nMG5\uff1a\u5723\u8bde\u8001\u4eba\u4f1a\u53d1\u7ed9\u6bcf\u4e2a\u4eba\u5979\u5e94\u5f97\u7684\u90a3\u4efd\u3002\n::\u800c\u4e14\u6bd4\u8d77\u72ec\u5360\u793c\u7269\uff0c\u548c\u540c\u4f34\u4eec\u4e00\u8d77\u5206\u4eab\u559c\u60a6\u4e0d\u662f\u66f4\u597d\u5417\uff1f\n\n\u683c\u91cc\u82ac\u4eba\u5f62\uff1a\u5514\uff0c\u597d\u50cf\u4e5f\u6709\u9053\u7406\u3002\n::\u90a3\u2026\u2026\u6211\u4eec\u8fd8\u80fd\u62ff\u5230\u793c\u7269\u5417\uff1f\u5723\u8bde\u8001\u4eba\u4f1a\u4e0d\u4f1a\u751f\u6211\u4eec\u7684\u6c14\u5440\uff1f\n\nMG5\uff1a\u5723\u8bde\u8001\u4eba\u4f1a\u539f\u8c05\u77e5\u9519\u80fd\u6539\u7684\u5b69\u5b50\u3002\n::\u53bb\u5411\u5723\u8bde\u8001\u4eba\u9053\u6b49\uff0c\u7136\u540e\u6392\u961f\u9886\u4f60\u4eec\u7684\u793c\u7269\u5427\u3002\n\n\u521a\u521a\u8fd8\u5931\u843d\u5730\u6a2a\u4e03\u7ad6\u516b\u762b\u5728\u5730\u4e0a\u7684\u4eba\u5f62\u4eec\u91cd\u65b0\u5145\u6ee1\u4e86\u6d3b\u529b\uff0c\u8e66\u8e66\u8df3\u8df3\u5730\u52a0\u5165\u5230\u9886\u53d6\u793c\u7269\u7684\u884c\u5217\u4e2d\u4e86\u3002\n\n\u770b\u7740\u5979\u4eec\u7684\u80cc\u5f71\uff0cMG5\u5982\u91ca\u91cd\u8d1f\u5730\u957f\u51fa\u4e00\u53e3\u6c14\u3002\n\nMG5\uff1a\u7ec8\u4e8e\u89e3\u51b3\u4e86\u2026\u2026\n\n\u6307\u6325\u5b98\uff1a\u5e72\u5f97\u4e0d\u9519\u554a\uff0cMG5\u3002\n\nMG5\uff1a\u957f\u5b98\uff0c\u5176\u5b9e\u6211\u4e0d\u592a\u64c5\u957f\u5bf9\u4ed8\u5c0f\u5b69\u5b50\u2026\u2026\n::\u5982\u679c\u662fG36\u6216\u8005\u6625\u7530\u7684\u8bdd\uff0c\u5e94\u8be5\u80fd\u505a\u5f97\u66f4\u597d\u5427\u3002\n\n\u6307\u6325\u5b98\uff1a\u4f60\u505a\u5f97\u5f88\u597d\u3002\n::\u4e0d\u5982\u8bf4\uff0c\u4f60\u7684\u786e\u662f\u4eca\u665a\u8fd9\u9879\u5de5\u4f5c\u7684\u6700\u4f73\u4eba\u9009\u3002\n\nMG5\uff1a\u2026\u2026\u54ce\uff1f\n\n\u6307\u6325\u5b98\uff1a\u591a\u4e8f\u4e86\u4f60\u7684\u884c\u52a8\u548c\u5f00\u5bfc\uff0c\u5979\u4eec\u4eca\u5929\u4e0a\u4e86\u91cd\u8981\u7684\u4e00\u8bfe\u3002\n::\u4eca\u540e\u6709\u4f60\u5728\u8eab\u8fb9\u7684\u8bdd\uff0c\u5979\u4eec\u4e5f\u4f1a\u66f4\u52a0\u660e\u767d\u540c\u4f34\u7684\u91cd\u8981\u6027\u5427\u3002\n\nMG5\u6c89\u9ed8\u4e86\u4e00\u4f1a\u513f\uff0c\u7ee7\u800c\u5fae\u7b11\u7740\u70b9\u70b9\u5934\u3002\n\nMG5\uff1a\u5e0c\u671b\u5982\u6b64\u3002\n::\u8fd9\u6837\u5982\u679c\u5979\u4eec\u5355\u72ec\u51fa\u53bb\u4f5c\u6218\uff0c\u6211\u4e5f\u80fd\u66f4\u52a0\u5b89\u5fc3\u3002\n\n\u6307\u6325\