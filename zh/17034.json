u3068\u8a00\u3046\u5974\u3082\u3044\u308b\u3002}}'''</big></big>}}\n\n'''\u5927\u5305\u5e73'''\u662f\u7f51\u9875\u6e38\u620f'''\u300a[[\u5200\u5251\u4e71\u821e]]\u300b\uff08{{lang|ja|\u5200\u5263\u4e71\u821e-ONLINE-}}\uff09'''\u53ca\u5176\u884d\u751f\u4f5c\u54c1\u7684\u767b\u573a\u89d2\u8272\u3002\n\n== \u5386\u53f2\u539f\u578b ==\n=== \u516c\u5f0f\u7279\u8bbe\u7f51\u7ad9\u4ecb\u7ecd ===\n{{Hide|\u6807\u9898=\u65e5\u6587\u539f\u6587|\u5185\u5bb9=<poem>\u53e4\u5099\u524d\u6d3e\u306e\u5200\u5de5\u5305\u5e73\u4f5c\u306e\u592a\u5200\u3002\n\u5171\u306b\u5200\u5263\u306e\u6a2a\u7db1\u3068\u3044\u308f\u308c\u306a\u304c\u3089\u3001\u5929\u4e0b\u4e94\u5263\u3067\u3082\u3042\u308b\u7ae5\u5b50\u5207\u5b89\u7db1\u3092\u30e9\u30a4\u30d0\u30eb\u8996\u3059\u308b\u3002\n\u6b74\u53f2\u7684\u306a\u9038\u8a71\u3084\u4f1d\u8aac\u306b\u3084\u3084\u4e4f\u3057\u304f\u3001\u6c60\u7530\u8f1d\u653f\u306b\u898b\u51fa\u3055\u308c\u305f\u3068\u3044\u3046\u4e00\u8aac\u306f\u5f7c\u306e\u62e0\u308a\u6240\u3002\n\u540c\u90f7\u306e\u9daf\u4e38\u306b\u89b3\u5bdf\u3055\u308c\u3066\u3044\u308b\u3002</poem>}}\n\u53e4\u5907\u524d\u6d3e\u7684\u5200\u5de5\u5305\u5e73\u6240\u4f5c\u7684\u592a\u5200\u3002\n\n\u5c06\u540c\u8a89\u4e3a\u5200\u5251\u6a2a\u7eb2\u7684\u7ae5\u5b50\u5207\u5b89\u7eb2\u89c6\u4e3a\u5bf9\u624b\u3002\n\n\u5386\u53f2\u4e0a\u7684\u4f20\u8bf4\u9038\u95fb\u8f83\u5c11\uff0c\u539f\u4e3a\u6c60\u7530\u8f89\u653f\u7231\u5200\u4e00\u8bf4\u662f\u5176\u51fa\u5904\u3002\n\n\u88ab\u540c\u4e61\u7684\u83ba\u4e38\u89c2\u5bdf\u7740\u3002\n=== \u5386\u53f2\u76f8\u5173 ===\n\u94ed\u201c\u5907\u524d\u56fd\u5305\u5e73\u4f5c\u201d\uff0c\u5e73\u5b89\u65f6\u4ee3\u53e4\u5907\u524d\u6d3e\u5200\u5320\u5305\u5e73\u6240\u4f5c\u7684\u65e5\u672c\u5200\uff08\u592a\u5200\uff09\uff0c\u88ab\u6307\u5b9a\u4e3a\u56fd\u5b9d\u3002\u56fd\u5b9d\u6307\u5b9a\u65e5\u4e3a1951\u5e746\u67089\u65e5\u3002\u88ab\u79f0\u4e3a\u201c\u65e5\u672c\u5200\u4e2d\u7684\u6700\u9ad8\u6770\u4f5c\u3001\u4e0e\u7ae5\u5b50\u5207\u5b89\u7eb2\u5e76\u79f0\u4e3a\u300c\u65e5\u672c\u5200\u7684\u4e1c\u897f\u4e24\u6a2a\u7eb2\u300d{{\u9ed1\u5e55|\u300c\u7ae5\u5b50\u5207\u2014\uff01\u7ae5\u5b50\u5207\u3001\u3044\u306a\u3044\u304b\uff01\u300d}}\u3002\n\n\u540d\u7a31\u4e2d\u51a0\u4e0a\u300c\u5927\u300d\u4e43\u662f\u6709\u300c\u5305\u5e73\u4e2d\u7684\u5305\u5e73\u300d\u7684\u610f\u7fa9\u3002\u636e\u300a\u4eab\u4fdd\u540d\u7269\u5e10\u300b\u4e4b\u8bb0\u8f7d\uff0c\u5176\u540d\u53f7\u4e4b\u7531\u6765\u4e3a\u201c\u5bf8\u9577\u304d\u6545\u540d\u4ed8\u304f\uff08\u56e0\u5176\u5c3a\u5bf8\u957f\u800c\u5f97\u5176\u540d\uff09\u201d\uff0c\u4e0d\u8fc7\uff0c\u6291\u6216\u53ef\u89e3\u91ca\u4e3a\u201c\u5927\u6770\u4f5c\u201d\u4e4b\u610f\u3002\n\n\u8fd9\u4f4d\u5305\u5e73\uff08\u5200\u953b\u51b6\uff09\u662f\u88ab\u79f0\u4e3a\u53e4\u5907\u524d\u7684\u5e73\u5b89\u65f6\u4ee3\u7684\u5907\u524d\u56fd\uff08\u5188\u5c71\u53bf\u4e1c\u5357\u90e8\uff09\u7684\u5200\u5320\uff0c\u73b0\u5b58\u7684\u53e4\u5907\u524d\u5305\u5e73\u4f5c\u4e4b\u5200\u5251\u4e5f\u6bd4\u8f83\u591a\u3002\u666e\u901a\u7684\u5907\u524d\u56fd\u5305\u5e73\u4e4b\u94ed\u4ec5\u4e3a\u5c0f\u5b57\u4e66\u5199\u4e4b\u201c\u5305\u5e73\u201d\u4e8c\u5b57\u94ed\uff0c\u800c\u8fd9\u67c4\u592a\u5200\u7684\u94ed\u5b57\u4e0e\u5236\u4f5c\u98ce\u683c\u90fd\u975e\u540c\u5bfb\u5e38\uff0c\u53cd\u5e38\u5730\u523b\u4e0a\u4e86\u300c\u5099\u524d\u56fd\u5305\u5e73\u4f5c\u300d\u7684\u516d\u5b57\u5200\u9298\uff0c\u4e00\u822c\u8a8d\u70ba\u9019\u4ee3\u8868\u5927\u5305\u5e73\u5c0d\u540d\u5320\u5305\u5e73\u4f86\u8aaa\u4e5f\u5c6c\u65bc\u4ed6\u7684\u5f97\u610f\u4e4b\u4f5c\u3002\u6240\u4ee5\u4e0e\u90a3\u4e9b\u201c\u5c0f\u94ed\u201d\u4e4b\u4f5c\u76f8\u6bd4\uff0c\u65e2\u6709\u8bf4\u6cd5\u662f\u540c\u4e00\u4eba\u7684\u4f5c\u54c1\uff0c\u4e5f\u6709\u8bf4\u662f\u4e0d\u540c\u8005\u4e4b\u4f5c\u3002\n\n\u6700\u65e9\u7684\u8a18\u8f09\u662f\u5f9e\u6c60\u7530\u4e09\u5de6\u885b\u9580\u8f1d\u653f\u958b\u59cb\u5728\u6c60\u7530\u5bb6\u4ee3\u4ee3\u76f8\u50b3\uff0c\u88ab\u8f1d\u653f\u7a31\u70ba\u662f\u50f9\u53ef\u6575\u570b\u7684\u540d\u5200\uff0c\u4eab\u4fdd\u540d\u7269\u5e33\u4e2d\u4e5f\u6709\u6240\u8a18\u8f09\u3002\u4f5c\u70ba\u8f1d\u653f\u7684\u914d\u5200\u6703\u5728\u65b0\u5e74\u6642\u7684\u5177\u8db3\u59cb\u4e2d\u62ff\u51fa\u4f86\u64fa\u8a2d\uff0c\u4e26\u8207\u6c60\u7530\u6b63\u5b97\u548c\u6c60\u7530\u6765\u56fd\u5149\u7b49\u5200\u4e00\u540c\u4ee3\u4ee3\u76f8\u50b3\u3002\u662d\u548c42\u5e74\u6642\u7531\u7576\u6642\u7684\u6587\u90e8\u7701\u4ee56,500\u842c\u65e5\u5713\u8cb7\u4e0b\uff0c\u4e26\u65bc\u6771\u4eac\u570b\u7acb\u535a\u7269\u9928\u73cd\u85cf\u81f3\u4eca\u3002\n\n\u636e\u8bf4\u5927\u5305\u5e73\u66fe\u6709\u4e00\u4e2a\u534e\u4e3d\u7684\u6843\u5c71\u62f5\uff0c\u4f46\u662f\u5728\u6218\u540e\u9057\u5931\u4e86\u3002\n\n{{Hide|\u6807\u9898=\u5927\u5305\u5e73\u7684\u5200\u5251\u771f\u8eab|\u5185\u5bb9=[[File:\u5927\u5305\u5e73_\u672c\u4f53.jpeg|500px]]}}\n\n== \u6e38\u620f\u6570\u636e ==\n{| class=\"wikitable\" style=\"text-align:center\"\n|-\n! colspan=5 |'''\u5927\u5305\u5e73'''\n|-\n| colspan=2 | '''CV\uff1a\u5c0f\u91ce\u53cb\u6a39'''\n| colspan=2 | '''\u4eba\u8bbe\uff1a\u5c0f\u5bae\u56fd\u6625'''\n| rowspan=10 | [[File:\u5927\u5305\u5e73.jpg]]\n|-\n| colspan=4 | '''\u5927\u5305\u5e73\uff08lv.1\uff09'''\u2192'''\u5927\u5305\u5e73 \u7279\uff08lv.25\uff09'''\n|-\n| colspan=2 | '''\u7a00\u6709\u5ea6\uff085\u21925\uff09|| colspan=2 | '''\u56fe\u9274\u7f16\u53f7:53\u756a'''\n|-\n| colspan=2 | '''\u7c7b\u578b\uff1a\u592a\u5200'''\n| colspan=2 | '''\u5200\u6d3e\uff1a\u53e4\u5099\u524d'''\n|-\n! colspan=4 | \u521d\u59cb\u6570\u636e\u2192\u7279\n|-\n| '''\u751f\u5b58''' || 51(57)\u219257(63) || '''\u6253\u51fb''' || 57(69)\u219263(75)\n|-\n| '''\u7edf\u7387''' || 52(64)\u219258(70) || '''\u673a\u52a8''' || 23(32)\u219229(38)\n|-\n| '''\u885d\u529b''' || 45(52)\u219251(58) ||'''\u8303\u56f4'''|| \u72ed\n|-\n| '''\u5fc5\u6740''' || 43 || '''\u4fa6\u67e5''' || 20(26)\u219226(32) \n|-\n| '''\u9690\u853d''' || 21\u219227 || '''\u5200\u69fd'''|| 3\n|-\n! colspan=5 | '''\u88c5\u5907\u53ef\u80fd''' \n|-\n| colspan=5 | \u8f7b\u6b65\u5175\u3001\u91cd\u6b65\u5175\u3001\u8f7b\u9a91\u5175\u3001\u91cd\u9a91\u5175\u3001\u76fe\u5175\n|-\n! colspan=5 | '''\u5165\u624b\u65b9\u5f0f'''\n|-\n| colspan=2 | '''\u953b\u9020'''|| colspan=3 | \u65e0\n|-\n| colspan=2 | '''\u6389\u843d'''|| colspan=3 | 2017/12/19~2018/1/16\u8fde\u961f\u6218\uff08\u65e5\u670d\uff09\u8d85\u96be\u5730\u56fe\u6982\u7387\u6389\u843d\n|-\n| colspan=2 | '''\u6d3b\u52a8'''|| colspan=3 | 2016/12/20~2017/1/17\u8fde\u961f\u6218\uff08\u65e5\u670d\uff0910\u4e07\u786e\u5b9a\u62a5\u916c<br/>2017/5/30~6/13 \u8fde\u961f\u6226 \uff5e\u521d\u590f\u306e\u9635\uff5e\uff08\u65e5\u670d\uff096\u4e07\u786e\u5b9a\u62a5\u916c\n|-\n! colspan=5 | '''\u5200\u8d26\u8bf4\u660e''' \n|-\n| colspan=5 |  \u4ffa\u306e\u540d\u524d\u306f\u5927\u5305\u5e73\u3002\u7ae5\u5b50\u5207\u5b89\u7db1\u3068\u4e26\u3076\u3001\u540d\u5200\u306e\u4e2d\u306e\u540d\u5200\u3002<br>\u5200\u5263\u306e\u6a2a\u7db1\u3068\u3082\u547c\u3070\u308c\u3066\u3044\u308b\u3002\u5929\u4e0b\u4e94\u5263\u306e\u4e2d\u306b\u306f\u5165\u3063\u3066\u3044\u306a\u3044\u2026\u2026\u304c\u3001\u898b\u51fa\u3055\u308c\u305f\u306e\u304c\u9045\u304b\u3063\u305f\u3060\u3051\u306a\u3093\u3060\n|-\n|}\n*\u62ec\u53f7\u5185\u6570\u636e\u4e3a\u8be5\u9879\u80fd\u529b\u6240\u80fd\u8fbe\u5230\u7684\u6700\u5927\u503c(\u70bc\u7ed3MAX\u3001Lv99)\n\n== \u7acb\u7ed8\u4e00\u89c8 ==\n{|style=\"text-align:center\"\n|-\n|[[File:Ookanehira-1.png|160px]]\n|[[File:Ookanehira-5.png|150px]]\n|[[File:Ookanehira-2.png|250px]]\n|[[File:Ookanehira-3.png|250px]]\n|[[File:Ookanehira-4.png|260px]]\n|-\n! \u901a\u5e38 !! \u5185\u756a !! \u6218\u6597 !!\u53d7\u4f24 !! \u771f\u5251\u5fc5\u6740\n|}\n\n== \u89d2\u8272\u76f8\u5173 ==\n===\u5200\u7eb9===\n[[File:\u5927\u5305\u5e73_\u5200\u7eb9.png|100px]]\n\n\u53c2\u8003\u81ea\u6c60\u7530\u6c0f\u5bb6\u7eb9\"\u5907\u524d\u8776\"\u3002\n\n===\u8ba9\u4f17\u591a\u5ba1\u795e\u8005\u548c\u83ba\u4e38\u82e6\u82e6\u7b49\u5f85\u7684\u5927\u5305\u5e73===\n\u5728\u6e38\u620f\u5f00\u670d\u521d\u671f\u5c31\u51fa\u73b0\u5728\u83ba\u4e38\u7684\u53f0\u8bcd\u4e2d\uff0c\u51e0\u4e2a\u6708\u540e\u7684\u751f\u653e\u9001\u8868\u793a\u4f1a\u5728\u660e\u5e74\uff0816\u5e74\uff09\u5b9e\u88c5\u5927\u5305\u5e73\u3002\u7136\u800c16\u5e74\u5f00\u59cb\u5230\u5feb\u7ed3\u675f\u4e86\u8fd9\u679a\u536b\u661f\u4f9d\u7136\u8fdf\u8fdf\u4e0d\u843d\u5730\u8ba9\u5a76\u5a76\u5341\u5206\u5fc3\u75bc\u83ba\u4e38\u3002<del>\u4e8e\u662f\u7ec8\u4e8e\u572816\u5e74\u7684\u6700\u540e\u4e00\u4e2a\u6708\u5b9e\u88c5\u4e86\uff0c\u5b98\u65b9\uff1a\u6211\u8bf416\u5e74\u5b9e\u88c5\uff0c12\u6708\u6708\u5e95\u4e5f\u662f16\u5e74</del>\n\n\u6b63\u56e0\u4e3a\u8fd9\u4efd\u7b49\u5f85\u592a\u957f\uff0c2016/12/16\u53d1\u8868\u90e8\u5206\u7acb\u7ed8\u7acb\u9a6c\u5c31\u6709\u773c\u5c16\u7684\u5ba1\u795e\u8005\u53d1\u73b0\u670d\u88c5\u7684\u8bbe\u8ba1\u548c\u83ba\u4e38\u5341\u5206\u76f8\u4f3c\uff0c\u77ed\u77ed\u51e0\u4e2a\u5c0f\u65f6\u5185\u8f6c\u63a8\u6570\u77ac\u95f4\u8d85\u8fc715\u4e07\uff0c\u5728\u65e5\u672c\u8f66\u7ad9\u7b49\u5730\u7684\u63a8\u7279\u6f6e\u6d41\u901f\u62a5\u6eda\u52a8\u5c4f\u4e0a\u5168\u90fd\u662f\u5927\u5305\u5e73\u6240\u9738\u5c4f\uff0c\u751a\u81f3\u4e0a\u4e86\u4e16\u754c\u70ed\u95e8\u5173\u952e\u5b57\u2026\u2026<del>\u8fd9\u4e00\u65e5\uff0c\u662f\u5a76\u5a76\u7684\u72c2\u6b22</del>{{Hide|\u6807\u9898=\u5409\u7965\u5bfa\u8f66\u7ad9\u7684\u63a8\u7279\u70ed\u95e8\u901f\u62a5\u6eda\u52a8\u5c4f\u88ab\u5927\u5305\u5e73\u5360\u636e<ref>https://twitter.com/Our_16bit_Wars/status/809709850359758848</ref>|\u5185\u5bb9=[[File:\u5409\u7965\u5bfa\u99c5 \u5927\u5305\u5e73.jpg|600px]]}}\n\n\u4e00\u5468\u540e\u5168\u90e8\u60c5\u62a5\u516c\u5f00\u66f4\u662f\u8ba9\u5404\u65b9\u5ba1\u795e\u8005\u6b22\u547c\u7206\u70b8\u5e76\u4e3a\u83ba\u4e38\u5e86\u795d\uff0c\u7eb7\u7eb7\u8868\u793a\u7edd\u5bf9\u4ee5\u6700\u5feb\u7684\u901f\u5ea6\u628a\u5927\u5305\u5e73\u63a5\u56de\u672c\u4e38\u3002<del>\u7136\u800c\u540e\u7eed\u7684\u5341\u4e07\u7389\u8981\u6c42\u2026\u2026\u5728\u6b64\u6309\u4e0b\u4e0d\u8868\u3002</del>\n\n===\u5341\u4e07\u7537===\n\u5927\u5305\u5e73\u4e8e2016/12/20\u5b9e\u88c5\u5728\u65e5\u670d\u8fde\u961f\u6218\u6d3b\u52a8\u4e2d\uff0c\u4e3a\u7b2c\u4e94\u628a\u4e94\u82b1\u592a\u5200\u3002\u9700\u898110w\u7389\u5373\u53ef\u83b7\u5f97\u3002\u76f8\u6bd4\u4e4b\u524d\u8fde\u961f\u62185w\u7389\u7684\u819d\u4e38\uff0c10w\u7389\u7684\u8981\u6c42\u5bf9\u4e8e\u5c0f\u5224\u548c\u65f6\u95f4\u4e0d\u8db3\u7684\u5ba1\u795e\u8005\u662f\u4e2a\u6253\u51fb\u3002\n\n\u4e0d\u5c11\u5a76\u5a76\u4e3a\u4e86\u8ba9[[\u5200\u5251\u4e71\u821e:\u83ba\u4e38|\u83ba\u4e38]]\u4e0e\u5927\u5305\u5e73\u65e9\u65e5\u76f8\u4f1a\u800c\u7206\u809d\u3002\u636e\u95fb\u6709\u6d3b\u52a8\u5f00\u59cb\u4e0d\u5230\u4e00\u5929\u5c31\u51b2\u523a\u523010w\u7389\u7684\u5723\u50e7\u5ba1\u795e\u8005\u2026\u2026\n\n\u4ece\u90a3\u6b21\u4e4b\u540e\uff0c\u5927\u5305\u5e73\u5f97\u5230\u4e86\u4e00\u4e2a\u5916\u53f7\uff1a\u5341\u4e07\u7537\u3002<del>\u751a\u81f3\u4ea7\u751f\u4e86\u5947\u602a\u7684\u8ba1\u6570\u5355\u4f4d\uff1a\u4e00\u4e2a\u5927\u5305\u5e73=\u5341\u4e07</del>\n\n\u540e\u6765\u5927\u5305\u5e73\u590d\u523b\u4e8e2017/5/30\u65e5\u670d\u7684\u8424\u706b\u866b\u8fde\u961f\u6218\u6d3b\u52a8\u4e2d\uff0c\u53ea\u97006w\u8424\u706b\u866b<del>\u5341\u4e07\u7537\u6253\u516d\u6298</del>\u5373\u53ef\u83b7\u5f97\u3002 {{\u9ed1\u5e55|\u4f46\u5341\u4e07\u7537\u7684\u79f0\u53f7\u4f9d\u7136\u6ca1\u53d8}}\n\n2017/12/19\u65e5\u670d\u7684\u8fde\u961f\u6218\u518d\u6b21\u590d\u523b\uff0c\u751a\u81f3\u76f4\u63a5\u53d8\u4e3a\u4e86\u8d85\u96be\u6a21\u5f0fboss\u70b9\u6389\u843d\u3002{{\u9ed1\u5e55|\u5927\u5305\u5e73\u8eab\u4ef7\u4e00\u964d\u518d\u964d}}\n\n===\u5bf9\u5929\u4e0b\u4e94\u5251\u7684\u7ade\u4e89\u610f\u8bc6===\n\u867d\u7136\u88ab\u79f0\u4e3a\u201c\u65e5\u672c\u5200\u4e2d\u7684\u6700\u9ad8\u6770\u4f5c\u201d\u3001\u548c\u7ae5\u5b50\u5207\u5b89\u7eb2\u5e76\u79f0\u4e3a\u300c\u65e5\u672c\u5200\u7684\u4e1c\u897f\u4e24\u6a2a\u7eb2\u300d\uff0c\u4f46\uff08\u81ea\u79f0\uff09\u53d1\u6398\u7684\u65f6\u95f4\u8f83\u665a\uff0c\u5e76\u6ca1\u6709\u6210\u4e3a\u5929\u4e0b\u4e94\u5251\u4e4b\u4e00\u3002\u56e0\u6b64\u5bf9\u5929\u4e0b\u4e94\u5251\u62b1\u6709\u5f3a\u70c8\u7684\u7ade\u4e89\u610f\u8bc6\u3002\n\n\u6bd4\u5982\u767b\u5165\u53f0\u8bcd\u65f6\u8bf4\u81ea\u5df1\u624d\u662f\u6700\u7f8e\u7684\u5200\uff0c<del>\u660e\u663e\u662f\u5728\u9488\u5bf9\u4e09\u65e5\u6708\u5b97\u8fd1</del>\n\n\u5728\u548c\u83ba\u4e38\u7684\u56de\u60f3\u4e2d\u5927\u558a\u8981\u548c\u7ae5\u5b50\u5207<del>\u53c8\u662f\u4e00\u679a\u536b\u661f\u653e\u51fa</del>\u4e00\u8f83\u9ad8\u4e0b\uff0c\u5411\u5ba1\u795e\u8005\u8bc1\u660e\u81ea\u5df1\u624d\u662f\u6700\u5f3a\u7684\u3002<del>\u7136\u540e\u88ab\u83ba\u4e38\u5410\u69fd\u4e86</del>\n\n\u5728\u624b\u5408\u756a\u4e2d\u548c\u5929\u4e0b\u4e94\u5251\u4ea6\u6709\u7279\u6b8a\u53f0\u8bcd\uff0c\u4e0d\u8fc7\u5927\u5305\u5e73\u90fd\u662f\u5355\u65b9\u9762\u6311\u8845\u5bf9\u65b9\uff0c\u4f46\u5bf9\u6570\u73e0\u4e38\u5374\u4e00\u53cd\u56a3\u5f20\u6001\u5ea6\u5341\u5206\u6709\u793c\u3002\n\n\u5728\u6570\u503c\u4e0a\u5c5e\u4e8e\u5e73\u5747\u578b\u7684\u592a\u5200\uff0c\u867d\u7136\u7efc\u5408\u6570\u503c\u8d85\u8fc7\u4e86\u4e09\u65e5\u6708\u5b97\u8fd1\uff0c\u4f46\u5355\u65b9\u9762\u6570\u636e\u90fd\u900a\u8272\u4e8e\u5929\u4e0b\u4e94\u5251\u7684\u6570\u73e0\u4e38\uff08\u751f\u5b58\u4ee5\u53ca\u7edf\u7387\uff09\u3001\u5927\u5178\u592a\uff08\u6253\u51fb\uff09\u3001\u4ee5\u53ca\u7956\u5b97\u5c0f\u4e4c\u4e38\uff08\u673a\u52a8\uff09\u3002\n\n<del>\u4f46\u7ae5\u5b50\u5207\u8fd8\u6ca1\u6765\uff0c\u5927\u5305\u5e73\u7684\u6570\u636e\u5c31\u88ab\u5b9e\u88c5\u7684\u56db\u82b1\u592a\u5200[[\u5200\u5251\u4e71\u821e:\u5c0f\u9f99\u666f\u5149|\u5c0f\u9f99\u666f\u5149]]\u8d85\u8fc7\u4e86\u2026\u2026</del>\n\n===\u672c\u4e38\u7bee\u7403\u961f===\n\u5728\u90e8\u5206\u7acb\u7ed8\u653e\u51fa\u65f6\u5c31\u6709\u90e8\u5206\u5ba1\u795e\u8005\u89c9\u5f97\u914d\u8272\u548c\u300a\u9ed1\u5b50\u7684\u7bee\u7403\u300b\u4e2d\u7684\u706b\u795e\u5927\u6211\u5341\u5206\u76f8\u4f3c<del>\u4e0d\u8fc7\u4ece\u90e8\u5206\u56fe\u900f\u7684\u5927\u5c0f\u6765\u770b\u5927\u6982\u53ea\u6709\u53d1\u8272</del>\uff0c\u5728\u5168\u90e8\u60c5\u62a5\u516c\u5f00\u540e\u53d1\u73b0\u4e0d\u4ec5\u7acb\u7ed8\u50cf\uff0c\u8fdecv\u90fd\u662f\u540c\u4e00\u4eba\uff0c\u6027\u683c\u4e5f\u5341\u5206\u76f8\u4f3c\uff0c\u56e0\u6b64\u5305\u542b\u706b\u795e\u5927\u6211\u3001\u706b\u795e\u304f\u3093\u7684\u5173\u952e\u8bcd\u53c8\u4e0a\u4e86\u63a8\u7279\u70ed\u641c\uff0c\u751a\u81f3\u60ca\u52a8\u4e86\u58f0\u4f18\u5c0f\u91ce\u53cb\u6811\u672c\u4eba\u3002\n\n{{\u9ed1\u5e55|<del>\u56e0\u4e3a[[\u5200\u5251\u4e71\u821e:\u6570\u73e0\u4e38\u6052\u6b21|\u6570\u73e0\u4e38]]\u662f\u7bee\u7403\u754c\u7684\u524d\u8f88\u624d\u90a3\u4e48\u6709\u793c\u8c8c\u7684\u5417</del>}}\n\n{{Hide|\u6807\u9898=<del>\u5a76\u5a76\u6253\u7403\u5417</del>|\u5185\u5bb9=[[File:25bd317dd1.jpg]]}}\n\n\n== \u6e38\u620f\u53f0\u8bcd ==\n{| class=\"wikitable\"\n|-\n! \u573a\u5408 !! \u53f0\u8bcd\n|-\n| \u56fe\u9274\u8bf4\u660e\n| <poem>{{lang|ja|\u4ffa\u306e\u540d\u524d\u306f\u5927\u5305\u5e73\u3002\u7ae5\u5b50\u5207\u5b89\u7db1\u3068\u4e26\u3076\u3001\u540d\u5200\u306e\u4e2d\u306e\u540d\u5200\u3002\n\u5200\u5263\u306e\u6a2a\u7db1\u3068\u3082\u547c\u3070\u308c\u3066\u3044\u308b\u3002\u5929\u4e0b\u4e94\u5263\u306e\u4e2d\u306b\u306f\u5165\u3063\u3066\u3044\u306a\u3044\u2026\u2026\u304c\u3001\u898b\u51fa\u3055\u308c\u305f\u306e\u304c\u9045\u304b\u3063\u305f\u3060\u3051\u306a\u3093\u3060\u3002}}</poem>\n----\n<poem>\u6211\u7684\u540d\u5b57\u662f\u5927\u5305\u5e73\uff0c\u548c\u7ae5\u5b50\u5207\u5b89\u7db1\u4e26\u99d5\u9f4a\u9a45\uff0c\u662f\u540d\u5200\u4e2d\u7684\u540d\u5200\u3002\n\u96d6\u7136\u88ab\u7a31\u70ba\u65e5\u672c\u5200\u4e2d\u7684\u6a2a\u7db1\uff0c\u4f46\u537b\u6c92\u88ab\u5217\u5165\u5929\u4e0b\u4e94\u528d\u4e4b\u4e00\u2026\u2026\uff0c\u6bd5\u7adf\uff0c\u6211\u7684\u5b9e\u529b\u88ab\u6ce8\u610f\u5230\u7684\u65f6\u5019\u5df2\u7ecf\u592a\u665a\u4e86\u3002</poem>\n\n|-\n| \u5165\u624b\n| {{lang|ja|\u5927\u5305\u5e73\u3002\u6c60\u7530\u8f1d\u653f\u304c\u898b\u51fa\u3057\u305f\u3001\u5200\u5263\u306e\u7f8e\u306e\u7d50\u6676\u3002\u3082\u3063\u3068\u3082\u7f8e\u3057\u3044\u5263\u306e\u4e00\u3064\u3002\u305f\u3060\u2026\u2026}} <br />\u5927\u5305\u5e73\u3002\u88ab\u6c60\u7530\u8f1d\u653f\u6240\u53d1\u6398\u7684\uff0c\u5200\u528d\u4e4b\u7f8e\u7684\u7ed3\u6676\u3002\u662f\u6700\u7f8e\u7684\u5200\u4e4b\u4e00\u3002\u53ea\u662f\u2026\u2026\n\n|-\n| \u767b\u9646\uff08\u52a0\u8f7d\u4e2d\uff09\n| {{lang|ja|\u30d0\u30ab\u3068\u8a00\u3046\u65b9\u304c\u30d0\u30ab\u3060\uff01\uff01\u2026\u305d\u308c\u306b\u6c17\u3065\u304f\u306e\u304c\u9045\u3059\u304e\u305f}}<br />\u8aaa\u5225\u4eba\u662f\u7b28\u86cb\u7684\u4eba\u81ea\u5df1\u624d\u662f\u7b28\u86cb\uff01\uff01\u2026\u5bdf\u89c9\u5230\u9019\u9ede\u5df2\u7d93\u592a\u665a\u4e86\u3002\n|-\n\n| \u767b\u9646\uff08\u52a0\u8f7d\u5b8c\u6210\uff09\n| {{lang|ja|\u5200\u5263\u4e71\u821e\u3001\u958b\u59cb\u3059\u308b\u3002}}<br />\u5200\u528d\u4e82\u821e\uff0c\u958b\u59cb\u4e86\u3002\n\n|-\n| \u5f00\u59cb\u6e38\u620f\n| {{lang|ja|\u5927\u5305\u5e73\u3053\u305d\u3001\u3082\u3063\u3068\u3082\u7f8e\u3057\u3044\u3068\u8a00\u3046\u5974\u3082\u3044\u308b\u3002}}<br />\u4e5f\u6709\u4eba\u8aaa\uff0c\u5927\u5305\u5e73\u624d\u662f\u6700\u7f8e\u7684\u5200\u3002<br />\n\n|-\n| rowspan=3 |\u672c\u4e38\n| {{lang|ja|\u4e16\u306e\u4e2d\u306f\u7686\u9593\u9055\u3063\u3066\u3044\u308b\uff01}}<br />\u4e16\u4eba\u90fd\u9519\u4e86\uff01{{\u9ed1\u5e55|[[\u5200\u5251\u4e71\u821e:\u83ba\u4e38|\u83ba]]\uff1a\u6211\uff1f\u6211\u5c31\u662f\u8fd9\u6837\u554a\u2026\u4e0d\u8981\u4ecb\u610f\u4ed6\u4eba\u8bf4\u6cd5\uff0c\u5df2\u7ecf\u8bf4\u8fc7\u5f88\u591a\u6b21\u4e86\u5462\u3002}}\n\n|-\n| {{lang|ja|\u300c\u5929\u4e0b\u4e94\u5263\u300d\u304c\u306a\u3093\u3060\uff01\u4ffa\u306f\u6c60\u7530\u8f1d\u653f\u306b\u898b\u51fa\u3055\u308c\u305f\u3093\u3060\u305e\uff01}}<br /> \u5929\u4e0b\u4e94\u5251\u53c8\u5982\u4f55\uff01\u6211\u53ef\u662f\u88ab\u6c60\u7530\u8f89\u653f\u6240\u53d1\u73b0\u7684\uff01{{\u9ed1\u5e55|[[\u5200\u5251\u4e71\u821e:\u83ba\u4e38|\u83ba]]\uff1a\u5927\u5305\u5e73\u8fd8\u662f\u592a\u5728\u610f\u5929\u4e0b\u4e94\u5251\u7684\u79f0\u53f7\u4e86\u3002}}\n\n|-\n| {{lang|ja|\u3063\u3044\u3058\u3051\u3066\u306a\u3069\u3044\u306a\u3044\u3002}}<br />\u6211\u624d\u6ca1\u6709\u5728\u95f9\u522b\u626d\uff01{{\u9ed1\u5e55|[[\u5200\u5251\u4e71\u821e:\u83ba\u4e38|\u83ba]]\uff1a\u95ee\u6211\u5728\u60f3\u4ec0\u4e48\uff1f\u5f53\u7136\u662f\u5728\u60f3\u5927\u5305\u5e73\u4eca\u5929\u4e5f\u5728\u72af\u50bb\u554a\u4ec0\u4e48\u7684\u2026}}\n\n|-\n| \u672c\u4e38\uff08\u653e\u7f6e\uff09\n| {{lang|ja|\u306a\u3093\u3060\u3001\u3044\u3058\u3051\u3066\u308b\u306e\u304b\uff1f}}<br />\u5e79\u561b\uff0c\u662f\u5728\u9b27\u5f46\u626d\u55ce\uff1f\n|-\n| \u672c\u4e38\uff08\u8d1f\u4f24\uff09\n| {{lang|ja|\u2026\u4ffa\u304c\u2026\u3053\u3093\u306a\u59ff\u306b\u306a\u308b\u2026\u3060\u3068\u2026}}<br />...\u6211\u5c45\u7136...\u4f1a...\u641e\u6210\u8fd9\u526f\u6478\u6837...\n\n|-\n| \u7ed3\u6210\uff08\u5165\u66ff\uff09\n| {{lang|ja|\u5c48\u8fb1\u3060\u2026\u968a\u9577\u3067\u7121\u3044\u306a\u3093\u3066\u2026}}<br />\u5c45\u7136\u4e0d\u662f\u961f\u957f...\u5c48\u8fb1\u554a... {{\u9ed1\u5e55|\u5feb\u53bb\u8ddf\u4eba\u5bb6[[\u5200\u5251\u4e71\u821e:\u548c\u6cc9\u5b88\u517c\u5b9a|\u517c\u6851]]\u5b66\u5b66\u963fQ\u7cbe\u795e}}\n\n|-\n| \u7ed3\u6210\uff08\u961f\u957f\uff09\n| {{lang|ja|\u4e86\u89e3\u3057\u305f\u3002\u5f53\u7136\u3060\u3002}}<br />\u4e86\u89e3\u4e86\uff0c\u7406\u6240\u5e94\u5f53\u554a\u3002\n\n|-\n| rowspan=3 | \u88c5\u5907 \n| {{lang|ja|\u88c5\u5099\u54c1\u304b\u3002}}<br />\u662f\u88c5\u5907\u554a\u3002\n|-\n| {{lang|ja|\u305b\u3044\u305c\u3044\u9811\u5f35\u3063\u3066\u5949\u516c\u3059\u308b\u3055\u3002}}<br />\u6211\u4f1a\u52aa\u529b\u5949\u884c\u516c\u4e8b\u3002\n\n|-\n| {{lang|ja|\u3064\u3051\u305f\u305e\u3002}}<br />\u88dd\u597d\u4e86\u3002\n\n|-\n| \u8fdc\u5f81\n| {{lang|ja|\u884c\u3063\u3066\u304f\u308b\u3002}}<br />\u6211\u51fa\u9580\u4e86\u3002\n\n|-\n| \u8fdc\u5f81\u5f52\u8fd8\uff08\u961f\u957f\uff09\n| {{lang|ja|\u623b\u3063\u305f\u3002}}<br />\u56de\u4f86\u4e86\u3002\n\n|-\n| \u8fdc\u5f81\u5f52\u8fd8\uff08\u8fd1\u4f8d\uff09\n| {{lang|ja|\u9060\u5f81\u90e8\u968a\u304c\u5e30\u3063\u305f\u305e\u3002}}<br />\u9060\u5f81\u90e8\u968a\u56de\u4f86\u4e86\u54e6\u3002\n\n|-\n| \u953b\u5200\n| {{lang|ja|\u65b0\u3057\u3044\u5263\u304c\u914d\u5c5e\u3055\u308c\u305f\u305e\u3002}}<br />\u6709\u65b0\u5200\u88ab\u5206\u914d\u6765\u4e86\u54df\u3002\n\n|-\n| \u5200\u88c5\n| {{lang|ja|\u53d7\u3051\u53d6\u308c\u3002}}<br />\u62ff\u53bb\u5427\u3002\n\n|-\n| \u624b\u5165\uff08\u8f7b\u4f24\uff09\n| {{lang|ja|\u2026\u4fee\u5fa9\u306b\u306f\u3044\u308b\u2026}}<br />\u2026\u524d\u53bb\u4fee\u7406\u2026\n\n|-\n| \u624b\u5165\uff08\u4e2d\u4f24\u53ca\u91cd\u4f24\uff09\n| {{lang|ja|\u3063\u672c\u683c\u4fee\u5fa9\u306b\u2026\u306f\u3044\u308b\u2026}}<br />\u6211\u53bb\u597d\u597d\u4fee\u4e00\u4fee\n\n|-\n| \u70bc\u7ed3\n| {{lang|ja|\u3088\u308a\u5b8c\u74a7\u306b\u306a\u308b\u3068\u3044\u3046\u306e\u304b\u3002}}<br />\u662f\u8bf4\u4f1a\u53d8\u5f97\u66f4\u52a0\u5b8c\u7f8e\u5417\u3002\n\n|-\n| \u4efb\u52a1\u5b8c\u6210\n| {{lang|ja|\u4efb\u52d9\u304c\u7d42\u308f\u3063\u305f\u3088\u3046\u3060\u3002}}<br />\u4efb\u52d9\u7d50\u675f\u4e86\u7684\u6a23\u5b50\u3002\n\n|-\n| \u6218\u7ee9\n| {{lang|ja|\u3053\u308c\u304c\u304a\u524d\u306e\u5b9f\u7e3e\u3060\u3002}}<br />\u9019\u5c31\u662f\u4f60\u7684\u5be6\u7e3e\u554a\u3002\n\n|-\n| \u4e07\u5c4b\n| {{lang|ja|\u5e97\u3068\u3044\u3046\u306e\u3082\u305f\u307e\u306b\u306f\u826f\u3044\u3002\u6bce\u65e5\u3060\u3068\u7834\u7523\u3057\u305d\u3046\u3060\u304c\u3002}}<br />\u5076\u723e\u4f86\u5e97\u88cf\u901b\u4e5f\u4e0d\u932f\uff0c\u4f46\u6bcf\u5929\u4f86\u7684\u8a71\u6703\u7834\u7522\u5427\u3002{{\u9ed1\u5e55|\u5a76\uff1a\u624e\u5fc3\u4e86\u8001\u94c1}}\n\n|-\n| \u51fa\u9635\n| {{lang|ja|\u51fa\u9663\u3059\u308b\uff01\u3064\u3044\u3066\u3053\u3044\uff01}}<br />\u51fa\u9635\uff01\u8ddf\u4e0a\u6765\uff01\n\n|-\n| \u53d1\u73b0\u8d44\u6e90\n| {{lang|ja|\u8ab0\u304b\u3001\u62fe\u3063\u3066\u304a\u3051\uff01}}<br />\u8ab0\u554a\uff0c\u5feb\u53bb\u64bf\u8d77\u4f86\u554a\uff01{{\u9ed1\u5e55|\u6765\u4eba\u554a\uff01\u7ed9\u672c\u5bab\u6361\u8d77\u6765\uff01}} {{\u9ed1\u5e55|\u4e00\u9a91\u8ba8\u7684\u65f6\u5019\u5c31\u5f88\u5c34\u5c2c\u4e86}}\n\n|-\n| \u7d22\u654c\n| {{lang|ja|\u9013\u9a0e\u3092\u653e\u3066\uff01\u4eca\u3053\u305d\u8a13\u7df4\u306e\u6210\u679c\u3092\u898b\u305b\u308b\u3068\u304d\u3060\u305e\uff01}}<br />\u6d3e\u4fa6\u5bdf\u5175\uff01\u73b0\u5728\u6b63\u662f\u5c55\u73b0\u8bad\u7ec3\u6210\u679c\u7684\u65f6\u5019\uff01\n\n|-\n| \u5f00\u6218\n| {{lang|ja|\u8a13\u7df4\u901a\u308a\u306b\u3084\u308c\uff01\u7a81\u6483\u3060\uff01}}<br />\u6309\u548c\u8bad\u7ec3\u662f\u4e00\u6837\uff0c\u7a81\u51fb\uff01\n\n|-\n| \u6f14\u7ec3\u5f00\u59cb\n| {{lang|ja|\u8a13\u7df4\u3067\u826f\u304b\u3063\u305f\u306a\u2026}}<br />\u8a13\u7df4\u771f\u662f\u592a\u597d\u4e86\u2026\n\n|-\n| rowspan=2 | \u653b\u51fb\n| {{lang|ja|\u90aa\u9b54\u3060\uff01}}<br />\u522b\u6321\u8def\uff01\n\n|-\n| {{lang|ja|\u6b7b\u306b\u305f\u304f\u306a\u3044\u306a\u3089\u4e0b\u304c\u308c\uff01}}<br />\u4e0d\u60f3\u6b7b\u5c31\u9000\u4e0b\uff01\n\n|-\n| \u4f1a\u5fc3\u4e00\u51fb\n| {{lang|ja|\u4ffa\u306b\u6bba\u3055\u308c\u308b\u3093\u3060\u3001\u540d\u8a89\u306b\u601d\u3048\uff01}}<br />\u6b7b\u5728\u6211\u7684\u624b\u4e2d\u53ef\u662f\u5f88\u5149\u8363\u7684\uff01\n\n|-\n| rowspan=2 | \u8f7b\u4f24\n| {{lang|ja|\u3063\u3061\u3002}}<br />\u5616\u3002\n\n|-\n| {{lang|ja|\u4e2d\u3005\u3084\u308b\u2026}}<br />\u4f60\u8fd8\u6709\u70b9\u6c34\u5e73\u2026\n\n|-\n| \u4e2d\u4f24\u3001\u91cd\u4f24\n| {{lang|ja|\u3063\u304f\u2026\u4ed5\u7559\u3081\u640d\u306a\u3063\u305f\u306a\u2026\u4eca\u5ea6\u306f\u3001\u3053\u3061\u3089\u306e\u756a\u3060\uff01}}<br />\u5514...\u8fd8\u5dee\u4e00\u70b9...\u8fd9\u6b21\uff0c\u8f6e\u5230\u6211\u4e86\uff01{{\u9ed1\u5e55|\u7136\u800c\u5374\u4e56\u4e56\u7684\u4e00\u8840\u9000\u81f3K\u4f4d...}}\n\n|-\n| \u771f\u5251\u5fc5\u6740\n| {{lang|ja|\u30d5\u3