7406\u7eb1\u8fd9\u91cc\u5374\u662f\u53e6\u4e00\u756a\u666f\u8c61\u3002\u6216\u8bb8\u662f\u7531\u4e8e\u4e9a\u7406\u7eb1\u9887\u6709\u81ea\u4fe1\u7684\u76d1\u542c\u6280\u672f{{\u9ed1\u5e55|24\u5c0f\u65f6\u76d1\u542c\u7537\u53cb\u7684\u4e00\u4e3e\u4e00\u52a8\uff0c\u6700\u540e\u76f4\u63a5\u5413\u8dd1\u5bf9\u65b9}}\uff0c\u53c8\u6216\u8bb8\u662f\u4e9a\u7406\u7eb1\u7740\u6025\u8d77\u6765\u6b47\u65af\u5e95\u91cc\u7684\u8868\u73b0{{\u9ed1\u5e55|\u66b4\u8d70}}\u3002\u4e9a\u7406\u7eb1\u5728\u60c5\u573a\u4e0a\u5e76\u4e0d\u5982\u8d5b\u573a\u4e0a\u90a3\u4e48\u4e00\u5e06\u98ce\u987a\u3002\u5f53\u5979\u5728\u6bd4\u8d5b\u4e2d\u4e00\u8def\u72c2\u98d9\u5730\u5927\u53d1\u7262\u9a9a\u4e4b\u540e{{\u9ed1\u5e55|\u5531\u9976\u820c\u6b4c}}\uff0c\u5979\u66fe\u7ecf\u53cc\u624b\u63a9\u9762\u3002\u54ed\u54ed\u557c\u557c\u5730\u62b1\u6028\u8bf4\uff0c\u201c\u4e3a\u4ec0\u4e48\u963f\u5b5d\u559c\u6b22\u90a3\u4e2a\u5973\u5b69\u5b50\u554a\uff1f\u4e3a\u4ec0\u4e48\u5bdf\u89c9\u4e0d\u5230\u6211\u7684\u5fc3\u610f\u554a\uff1f\u201d\uff08\u300a\u5c11\u5973\u4e0e\u6218\u8f66\u300b\u7b2c6\u8bdd9:32\u5de6\u53f3\uff0c\u539f\u6587\uff1a\u201c{{lang|ja|\u306a\u3093\u3067\u5b5d\u306f\u3042\u306e\u5b50\u304c\u597d\u304d\u306a\u306e\u3002\u3069\u3046\u3057\u3066\u79c1\u306e\u6c17\u6301\u3061\u306b\u6c17\u3064\u304b\u306a\u3044\u3088\uff1f\uff01}}\u201d\uff09<del>\u4f30\u8ba1\u5c31\u662f\u8fd9\u65f6\u88ab\u5154\u5b50\u961f{{\u9ed1\u5e55|\u90a3\u4e9b\u64c5\u957f\u628a[[\u5b66\u59d0|\u524d\u8f88]]\u73a9\u574f\u7684[[\u5b66\u59b9|\u4f4e\u73ed\u5c0f\u9b3c]]}}\u542c\u5230\u4e86{{\u9ed1\u5e55|\u6bd5\u7adf\u5154\u5b50\u8033\u6735\u5927\uff08\u96fe\uff09}}\u3002\u6240\u4ee5\u540e\u6765{{\u9ed1\u5e55|\u8fd9\u4e9b[[\u7c89\u7ea2\u7684\u5207\u5f00\u6765\u91cc\u9762\u90fd\u662f\u9ed1\u7684|\u7c89\u5207\u9ed1\u7684]]}}\u5154\u5b50\u961f\u6210\u5458\u53c8\u5728\u5267\u573a\u7248\u91cc\u65e7\u4e8b\u91cd\u63d0\u3002</del>{{\u9ed1\u5e55|\u62ff\u5979\u5931\u604b\u4e86\uff0c\u6ca1\u6709\u7537\u670b\u53cb\u5f00\u73a9\u7b11\u3002}}\n{{Hide